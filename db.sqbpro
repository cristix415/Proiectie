INSERT INTO &quot;verses&quot; VALUES (470,1,1,'&lt;pb/&gt;Cartea neamului&lt;f&gt;ⓐ&lt;/f&gt; lui Isus Hristos, fiul&lt;f&gt;ⓑ&lt;/f&gt; lui David, fiul&lt;f&gt;ⓒ&lt;/f&gt; lui Avraam.');
INSERT INTO &quot;verses&quot; VALUES (470,1,2,'Avraam&lt;f&gt;ⓓ&lt;/f&gt; l-a născut pe Isaac; Isaac&lt;f&gt;ⓔ&lt;/f&gt; l-a născut pe Iacov; Iacov&lt;f&gt;ⓕ&lt;/f&gt; i-a născut pe Iuda și pe frații lui;');
INSERT INTO &quot;verses&quot; VALUES (470,1,3,'Iuda&lt;f&gt;ⓖ&lt;/f&gt; i-a născut pe Fares și Zara, din Tamar; Fares&lt;f&gt;ⓗ&lt;/f&gt; l-a născut pe Esrom; Esrom l-a născut pe Aram;');
INSERT INTO &quot;verses&quot; VALUES (470,1,4,'Aram l-a născut pe Aminadab; Aminadab l-a născut pe Naason; Naason l-a născut pe Salmon;');
INSERT INTO &quot;verses&quot; VALUES (470,1,5,'Salmon l-a născut pe Boaz, din Rahav; Boaz l-a născut pe Obed, din Rut; Obed l-a născut pe Ișai;');
INSERT INTO &quot;verses&quot; VALUES (470,1,6,'Ișai&lt;f&gt;ⓘ&lt;/f&gt; l-a născut pe regele David. Regele David&lt;f&gt;ⓙ&lt;/f&gt; l-a născut pe Solomon, din văduva lui Urie;');
INSERT INTO &quot;verses&quot; VALUES (470,1,7,'Solomon&lt;f&gt;ⓚ&lt;/f&gt; l-a născut pe Roboam; Roboam l-a născut pe Abia; Abia l-a născut pe Asaf;');
INSERT INTO &quot;verses&quot; VALUES (470,1,8,'Asaf l-a născut pe Iosafat; Iosafat l-a născut pe Ioram; Ioram l-a născut pe Ozia;');
INSERT INTO &quot;verses&quot; VALUES (470,1,9,'Ozia l-a născut pe Ioatam; Ioatam l-a născut pe Ahaz; Ahaz l-a născut pe Ezechia;');
INSERT INTO &quot;verses&quot; VALUES (470,1,10,'Ezechia&lt;f&gt;ⓛ&lt;/f&gt; l-a născut pe Manase; Manase l-a născut pe Amos; Amos l-a născut pe Iosia;');
INSERT INTO &quot;verses&quot; VALUES (470,1,11,'Iosia&lt;f&gt;ⓜ&lt;/f&gt; i-a născut pe Iehonia și pe frații lui, pe vremea deportării&lt;f&gt;ⓝ&lt;/f&gt; în Babilon.');
INSERT INTO &quot;verses&quot; VALUES (470,1,12,'După deportarea în Babilon, Iehonia&lt;f&gt;ⓞ&lt;/f&gt; l-a născut pe Salatiel; Salatiel l-a născut pe Zorobabel&lt;f&gt;ⓟ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (470,1,13,'Zorobabel l-a născut pe Abiud; Abiud l-a născut pe Eliachim; Eliachim l-a născut pe Azor;');
INSERT INTO &quot;verses&quot; VALUES (470,1,14,'Azor l-a născut pe Sadoc; Sadoc l-a născut pe Achim; Achim l-a născut pe Eliud;');
INSERT INTO &quot;verses&quot; VALUES (470,1,15,'Eliud l-a născut pe Eleazar; Eleazar l-a născut pe Matan; Matan l-a născut pe Iacov;');
INSERT INTO &quot;verses&quot; VALUES (470,1,16,'Iacov l-a născut pe Iosif, bărbatul Mariei, din care S-a născut Isus, care Se numește Hristos.');
INSERT INTO &quot;verses&quot; VALUES (470,1,17,'Așadar, de la Avraam până la David sunt paisprezece generații cu totul; de la David până la deportarea în Babilon sunt paisprezece generații; iar de la deportarea în Babilon până la Hristos sunt paisprezece generații.');
INSERT INTO &quot;verses&quot; VALUES (470,1,18,'&lt;pb/&gt;Iar nașterea&lt;f&gt;ⓠ&lt;/f&gt; lui Isus Hristos a fost așa: &lt;pb/&gt;Maria, mama Lui, era logodită cu Iosif; și înainte să locuiască ei împreună, ea s-a aflat însărcinată de la Duhul Sfânt&lt;f&gt;ⓡ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,1,19,'Iosif, bărbatul ei, fiindcă era un om drept și nu voia să o facă de rușine&lt;f&gt;ⓢ&lt;/f&gt;, plănuia să o lase pe ascuns.');
INSERT INTO &quot;verses&quot; VALUES (470,1,20,'Dar pe când se frământa el cu aceste &lt;i&gt;gânduri&lt;/i&gt;, i s-a arătat în vis un înger al Domnului, care i-a zis: „Iosife, fiul lui David, nu te teme să o iei pe Maria de soție, căci ce s-a zămislit&lt;f&gt;ⓣ&lt;/f&gt; în ea este de la Duhul Sfânt!');
INSERT INTO &quot;verses&quot; VALUES (470,1,21,'Ea va naște&lt;f&gt;ⓤ&lt;/f&gt; un Fiu și-I vei pune numele Isus, pentru că El&lt;f&gt;ⓥ&lt;/f&gt; va mântui pe poporul Său de păcate.”');
INSERT INTO &quot;verses&quot; VALUES (470,1,22,'Toate acestea s-au întâmplat ca să se împlinească ce spusese Domnul prin profetul care zice:');
INSERT INTO &quot;verses&quot; VALUES (470,1,23,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată, fecioara va rămâne însărcinată, va naște un fiu&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și-i vor pune numele Emanuel&lt;/e&gt; &lt;f&gt;[1]&lt;/f&gt;,&lt;/t&gt; &lt;pb/&gt;care, tradus, înseamnă: &lt;e&gt;Dumnezeu este cu noi&lt;/e&gt;&lt;f&gt;[2]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,1,24,'Când s-a trezit Iosif din somn, a făcut cum îi poruncise îngerul Domnului: a luat-o de soție,');
INSERT INTO &quot;verses&quot; VALUES (470,1,25,'dar n-a cunoscut-o&lt;f&gt;[3]&lt;/f&gt; până ce ea a născut un fiu&lt;f&gt;ⓦ&lt;/f&gt;. Și I-a pus numele Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,2,1,'&lt;pb/&gt;După ce S-a născut&lt;f&gt;ⓧ&lt;/f&gt; Isus în Betleemul din Iudeea, în zilele regelui Irod, iată că au venit niște magi din Răsărit&lt;f&gt;ⓨ&lt;/f&gt; la Ierusalim');
INSERT INTO &quot;verses&quot; VALUES (470,2,2,'și au întrebat: „Unde&lt;f&gt;ⓩ&lt;/f&gt; este Regele de curând născut al iudeilor? Căci I-am văzut steaua&lt;f&gt;ⓐ&lt;/f&gt; în Răsărit și am venit să ne închinăm Lui.”');
INSERT INTO &quot;verses&quot; VALUES (470,2,3,'Când a auzit regele Irod aceasta, s-a tulburat, și tot Ierusalimul &lt;i&gt;s-a tulburat&lt;/i&gt; împreună cu el.');
INSERT INTO &quot;verses&quot; VALUES (470,2,4,'I-a adunat pe toți preoții de seamă&lt;f&gt;ⓑ&lt;/f&gt; și pe&lt;f&gt;ⓒ&lt;/f&gt; cărturarii poporului și a căutat să afle&lt;f&gt;ⓓ&lt;/f&gt; de la ei unde trebuia să Se nască Hristosul.');
INSERT INTO &quot;verses&quot; VALUES (470,2,5,'Ei i-au spus: „În Betleemul din Iudeea, căci iată ce a fost scris prin profetul:');
INSERT INTO &quot;verses&quot; VALUES (470,2,6,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Și tu, Betleeme&lt;/e&gt; &lt;f&gt;ⓔ&lt;/f&gt;, ținut al lui Iuda,&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu ești nicidecum cea mai neînsemnată dintre căpeteniile lui Iuda,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;căci din tine va ieși o Căpetenie&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;care va păstori&lt;/e&gt; &lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;poporul Meu, Israel.&lt;/e&gt;”&lt;f&gt;[4]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,2,7,'&lt;pb/&gt;Atunci, Irod i-a chemat în ascuns pe magi și i-a întrebat amănunțit despre vremea în care se arătase steaua.');
INSERT INTO &quot;verses&quot; VALUES (470,2,8,'Apoi i-a trimis la Betleem, zicându-le: „Duceți-vă și cercetați în amănunt despre prunc și, când îl veți găsi, dați-mi și mie de veste, ca să vin și eu să mă închin lui!”');
INSERT INTO &quot;verses&quot; VALUES (470,2,9,'După ce l-au ascultat pe rege, au plecat. Și iată că steaua pe care o văzuseră în Răsărit mergea înaintea lor, până când a ajuns și s-a oprit deasupra locului unde era Pruncul.');
INSERT INTO &quot;verses&quot; VALUES (470,2,10,'Când au văzut ei steaua, au avut o bucurie nespus de mare.');
INSERT INTO &quot;verses&quot; VALUES (470,2,11,'Au intrat în casă, au văzut Pruncul cu Maria, mama Lui, au căzut cu fața la pământ și I s-au închinat; apoi și-au deschis cuferele și I-au adus&lt;f&gt;ⓖ&lt;/f&gt; daruri: aur, tămâie și smirnă.');
INSERT INTO &quot;verses&quot; VALUES (470,2,12,'După aceea, fiind înștiințați de Dumnezeu în vis&lt;f&gt;ⓗ&lt;/f&gt; să nu se întoarcă pe la Irod, au plecat spre țara lor pe un alt drum.');
INSERT INTO &quot;verses&quot; VALUES (470,2,13,'&lt;pb/&gt;După ce au plecat ei, iată că un înger al Domnului i s-a arătat în vis lui Iosif și i-a zis: „Scoală-te, ia-i pe Prunc și pe mama Lui și fugi în Egipt! Și rămâi acolo până îți voi spune eu, căci Irod are să caute Pruncul ca să-L omoare!”');
INSERT INTO &quot;verses&quot; VALUES (470,2,14,'&lt;i&gt;Iosif&lt;/i&gt; s-a sculat noaptea, i-a luat pe Prunc și pe mama Lui și a plecat în Egipt.');
INSERT INTO &quot;verses&quot; VALUES (470,2,15,'Și a rămas acolo până la moartea lui Irod, ca să se împlinească ce fusese vestit de Domnul prin profetul care zice: &lt;e&gt;Din Egipt L-am chemat pe Fiul Meu.&lt;/e&gt;&lt;f&gt;[5]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,2,16,'&lt;pb/&gt;Atunci Irod, când a văzut că fusese înșelat de magi, s-a mâniat foarte tare și a trimis să-i omoare pe toți pruncii din Betleem și din toate împrejurimile lui care aveau mai puțin de doi ani, după vremea pe care o aflase de la magi.');
INSERT INTO &quot;verses&quot; VALUES (470,2,17,'&lt;pb/&gt;Atunci s-a împlinit ce fusese vestit prin profetul Ieremia, care zice:');
INSERT INTO &quot;verses&quot; VALUES (470,2,18,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Glas s-a auzit în Rama,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;plângere și bocet mult:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Rahela își plângea copiii&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și nu voia să fie mângâiată,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;pentru că nu mai erau.&lt;/e&gt; &lt;f&gt;[6]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,2,19,'&lt;pb/&gt;După ce a murit Irod, iată că un înger al Domnului i s-a arătat în vis lui Iosif, în Egipt,');
INSERT INTO &quot;verses&quot; VALUES (470,2,20,'și i-a zis: „Scoală-te, ia-i pe Prunc și pe mama Lui și du-te în țara lui Israel, căci &lt;e&gt;au murit cei ce căutau să-I ia viața&lt;/e&gt;&lt;f&gt;[7]&lt;/f&gt; Pruncului!”');
INSERT INTO &quot;verses&quot; VALUES (470,2,21,'&lt;i&gt;Iosif&lt;/i&gt; s-a sculat, i-a luat pe Prunc și pe mama Lui și a venit în țara lui Israel.');
INSERT INTO &quot;verses&quot; VALUES (470,2,22,'Dar când a auzit că în Iudeea domnește Arhelau în locul tatălui său, Irod, s-a temut să se ducă acolo; și fiind înștiințat în vis, a plecat în părțile&lt;f&gt;ⓘ&lt;/f&gt; Galileei.');
INSERT INTO &quot;verses&quot; VALUES (470,2,23,'A ajuns acolo și a locuit într-o cetate numită&lt;f&gt;ⓙ&lt;/f&gt; Nazaret, ca să se împlinească ce fusese vestit&lt;f&gt;ⓚ&lt;/f&gt; prin profeți, &lt;i&gt;și anume&lt;/i&gt; că El va fi numit Nazarinean&lt;f&gt;[8]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,3,1,'&lt;pb/&gt;În zilele acelea a venit Ioan&lt;f&gt;ⓛ&lt;/f&gt; Botezătorul și propovăduia în&lt;f&gt;ⓜ&lt;/f&gt; Pustiul Iudeei,');
INSERT INTO &quot;verses&quot; VALUES (470,3,2,'zicând: „Pocăiți-vă, căci Împărăția cerurilor&lt;f&gt;ⓝ&lt;/f&gt; este aproape!”');
INSERT INTO &quot;verses&quot; VALUES (470,3,3,'El este acela care fusese vestit prin profetul Isaia, când zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Glasul&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;celui ce strigă în pustiu:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;„Pregătiți&lt;/e&gt; &lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;calea Domnului,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;îndreptați cărările Lui!”&lt;/e&gt; &lt;f&gt;[9]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,3,4,'&lt;pb/&gt;Ioan&lt;f&gt;ⓠ&lt;/f&gt; avea îmbrăcăminte&lt;f&gt;ⓡ&lt;/f&gt; din păr de cămilă și un brâu de piele în jurul mijlocului și se hrănea cu lăcuste&lt;f&gt;ⓢ&lt;/f&gt; și miere sălbatică&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,3,5,'Au început să iasă la el&lt;f&gt;ⓤ&lt;/f&gt; din Ierusalim, din toată Iudeea și din toate împrejurimile Iordanului');
INSERT INTO &quot;verses&quot; VALUES (470,3,6,'și, după ce își mărturiseau păcatele, erau botezați&lt;f&gt;ⓥ&lt;/f&gt; de el în râul Iordan.');
INSERT INTO &quot;verses&quot; VALUES (470,3,7,'Dar când i-a văzut pe mulți dintre farisei și saduchei venind la botezul lui, le-a zis: „Pui de vipere&lt;f&gt;ⓦ&lt;/f&gt;, cine v-a învățat să fugiți de mânia&lt;f&gt;ⓧ&lt;/f&gt; care vine?');
INSERT INTO &quot;verses&quot; VALUES (470,3,8,'Faceți roade vrednice de pocăință!');
INSERT INTO &quot;verses&quot; VALUES (470,3,9,'Și să nu credeți că &lt;i&gt;puteți&lt;/i&gt; zice în sinea voastră: «Îl avem ca tată&lt;f&gt;ⓨ&lt;/f&gt; pe Avraam»! Vă spun că Dumnezeu din pietrele acestea poate să-i ridice copii lui Avraam.');
INSERT INTO &quot;verses&quot; VALUES (470,3,10,'Securea așteaptă deja la rădăcina pomilor. Orice pom&lt;f&gt;ⓩ&lt;/f&gt; care nu face roade bune va fi tăiat și aruncat în foc!');
INSERT INTO &quot;verses&quot; VALUES (470,3,11,'Eu vă&lt;f&gt;ⓐ&lt;/f&gt; botez în apă, spre pocăință, dar Cel ce vine după mine este mai puternic decât mine, și eu nu sunt vrednic să-I duc încălțămintea. El&lt;f&gt;ⓑ&lt;/f&gt; vă va boteza în Duhul Sfânt și în foc.');
INSERT INTO &quot;verses&quot; VALUES (470,3,12,'Acela&lt;f&gt;ⓒ&lt;/f&gt; are lopata &lt;i&gt;de vânturat&lt;/i&gt; în mână; El Își va curăța aria, Își va strânge grâul în grânar, iar pleava o va arde&lt;f&gt;ⓓ&lt;/f&gt; în focul care nu se stinge.”');
INSERT INTO &quot;verses&quot; VALUES (470,3,13,'&lt;pb/&gt;Atunci&lt;f&gt;ⓔ&lt;/f&gt; a venit Isus din Galileea&lt;f&gt;ⓕ&lt;/f&gt; la Iordan, la Ioan, ca să fie botezat de el.');
INSERT INTO &quot;verses&quot; VALUES (470,3,14,'Dar Ioan căuta să-L oprească, spunând: „Eu am nevoie să fiu botezat de Tine, și Tu vii la mine?”');
INSERT INTO &quot;verses&quot; VALUES (470,3,15,'Isus i-a răspuns zicându-i: &lt;J&gt;„Lasă acum, fiindcă așa se cuvine: să împlinim toată dreptatea.”&lt;/J&gt; Atunci &lt;i&gt;Ioan&lt;/i&gt; a încuviințat.');
INSERT INTO &quot;verses&quot; VALUES (470,3,16,'După ce a fost botezat&lt;f&gt;ⓖ&lt;/f&gt;, Isus a ieșit îndată din apă. Și iată, cerurile s-au deschis și L-a văzut pe Duhul&lt;f&gt;ⓗ&lt;/f&gt; lui Dumnezeu coborând ca un porumbel și venind peste El.');
INSERT INTO &quot;verses&quot; VALUES (470,3,17,'Și iată că din ceruri&lt;f&gt;ⓘ&lt;/f&gt; s-a auzit un glas care zicea: „Acesta&lt;f&gt;ⓙ&lt;/f&gt; este Fiul Meu preaiubit, în care Îmi găsesc plăcerea!”');
INSERT INTO &quot;verses&quot; VALUES (470,4,1,'&lt;pb/&gt;Atunci, Isus&lt;f&gt;ⓚ&lt;/f&gt; a fost dus de Duhul&lt;f&gt;ⓛ&lt;/f&gt; în pustiu, ca să fie ispitit de Diavol.');
INSERT INTO &quot;verses&quot; VALUES (470,4,2,'După ce a postit patruzeci de zile și patruzeci de nopți, în cele din urmă a flămânzit.');
INSERT INTO &quot;verses&quot; VALUES (470,4,3,'Ispititorul s-a apropiat de El și I-a zis: „Dacă ești Fiul lui Dumnezeu, poruncește ca pietrele acestea să devină pâini!”');
INSERT INTO &quot;verses&quot; VALUES (470,4,4,'El i-a răspuns zicându-i: &lt;J&gt;„Este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;Omul nu va trăi numai cu pâine,&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;ci cu orice cuvânt care iese&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;din gura lui Dumnezeu.&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[10]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,5,'&lt;pb/&gt;Atunci, Diavolul L-a dus în sfânta cetate&lt;f&gt;ⓜ&lt;/f&gt;, L-a pus pe acoperișul Templului');
INSERT INTO &quot;verses&quot; VALUES (470,4,6,'și I-a zis: „Dacă ești Fiul lui Dumnezeu, aruncă-Te jos, căci este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;El va porunci îngerilor Săi cu privire la tine&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și ei te vor purta pe mâini,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca nu cumva să-ți lovești piciorul de vreo piatră&lt;/e&gt;!”&lt;f&gt;[11]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,7,'&lt;pb/&gt;Isus i-a zis: &lt;J&gt;„De asemenea este scris: &lt;e&gt;Să nu-L ispitești pe Domnul, Dumnezeul tău.&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[12]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,8,'Diavolul L-a dus din nou pe un munte foarte înalt, I-a arătat toate împărățiile lumii și slava lor și I-a zis:');
INSERT INTO &quot;verses&quot; VALUES (470,4,9,'„Pe toate acestea Ți le voi da Ție dacă vei cădea &lt;i&gt;cu fața&lt;/i&gt; la pământ și mi Te vei închina.”');
INSERT INTO &quot;verses&quot; VALUES (470,4,10,'Atunci Isus i-a zis: &lt;J&gt;„Pleacă, Satano, căci este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Domnului, Dumnezeului tău&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;&lt;J&gt;, să te închini&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și numai Lui să-I slujești.&lt;/J&gt;&lt;/e&gt; &lt;J&gt;”&lt;/J&gt;&lt;f&gt;[13]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,11,'&lt;pb/&gt;Atunci Diavolul L-a lăsat și iată că au venit niște îngeri&lt;f&gt;ⓞ&lt;/f&gt; care Îi slujeau.');
INSERT INTO &quot;verses&quot; VALUES (470,4,12,'&lt;pb/&gt;Auzind&lt;f&gt;ⓟ&lt;/f&gt; că Ioan fusese închis, &lt;i&gt;Isus&lt;/i&gt; a plecat în Galileea.');
INSERT INTO &quot;verses&quot; VALUES (470,4,13,'A părăsit Nazaretul și a venit să locuiască în Capernaum, lângă mare, în ținuturile lui Zabulon și Neftali,');
INSERT INTO &quot;verses&quot; VALUES (470,4,14,'ca să se împlinească ce fusese vestit prin profetul Isaia, care zice:');
INSERT INTO &quot;verses&quot; VALUES (470,4,15,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Țara lui Zabulon și țara lui Neftali,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;drumul spre mare, dincolo de Iordan,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Galileea neamurilor:&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,16,'&lt;t&gt;&lt;e&gt;poporul&lt;/e&gt; &lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;care stătea în întuneric&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;a văzut o mare lumină,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;iar peste cei ce locuiau în ținutul și în umbra morții&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;a răsărit lumina.&lt;/e&gt; &lt;f&gt;[14]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,17,'&lt;pb/&gt;De atunci&lt;f&gt;ⓡ&lt;/f&gt;, Isus a început să propovăduiască și să zică: &lt;J&gt;„Pocăiți-vă&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt;&lt;J&gt;, căci Împărăția cerurilor este aproape!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,18,'&lt;pb/&gt;Pe când umbla pe lângă Marea Galileei, Isus&lt;f&gt;ⓣ&lt;/f&gt; a văzut doi frați — pe Simon, zis&lt;f&gt;ⓤ&lt;/f&gt; Petru, și pe fratele său, Andrei — care aruncau năvodul în mare, căci erau pescari.');
INSERT INTO &quot;verses&quot; VALUES (470,4,19,'El le-a zis: &lt;J&gt;„Veniți&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;după Mine și vă voi face pescari de oameni!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,4,20,'Îndată&lt;f&gt;ⓦ&lt;/f&gt; ei au lăsat mrejele și L-au urmat.');
INSERT INTO &quot;verses&quot; VALUES (470,4,21,'De acolo&lt;f&gt;ⓧ&lt;/f&gt; a mers mai departe și a văzut alți doi frați — pe Iacov, fiul lui Zebedei, și pe Ioan, fratele lui — care erau în barcă împreună cu tatăl lor, Zebedei, și își dregeau mrejele. El i-a chemat.');
INSERT INTO &quot;verses&quot; VALUES (470,4,22,'Îndată, ei au lăsat barca și pe tatăl lor și L-au urmat.');
INSERT INTO &quot;verses&quot; VALUES (470,4,23,'&lt;pb/&gt;Isus străbătea toată Galileea, dând învățături&lt;f&gt;ⓨ&lt;/f&gt; în sinagogile lor, propovăduind Evanghelia&lt;f&gt;ⓩ&lt;/f&gt; Împărăției și vindecând&lt;f&gt;ⓐ&lt;/f&gt; orice boală și orice neputință din popor.');
INSERT INTO &quot;verses&quot; VALUES (470,4,24,'Iar vestea despre El s-a răspândit în toată Siria; și-i aduceau la El pe toți cei ce sufereau de felurite boli și chinuri: pe demonizați, pe epileptici&lt;f&gt;[15]&lt;/f&gt; și pe paralitici, iar El îi vindeca.');
INSERT INTO &quot;verses&quot; VALUES (470,4,25,'După El mergeau&lt;f&gt;ⓑ&lt;/f&gt; mulțimi mari din Galileea, din Decapole, din Ierusalim, din Iudeea și de dincolo de Iordan.');
INSERT INTO &quot;verses&quot; VALUES (470,5,1,'&lt;pb/&gt;Când a văzut &lt;i&gt;Isus&lt;/i&gt; mulțimile, a urcat&lt;f&gt;ⓒ&lt;/f&gt; pe munte și, după ce S-a așezat, ucenicii Lui s-au apropiat de El.');
INSERT INTO &quot;verses&quot; VALUES (470,5,2,'Și ridicându-Și glasul, a început să-i învețe zicând:');
INSERT INTO &quot;verses&quot; VALUES (470,5,3,'&lt;pb/&gt;&lt;t&gt;&lt;J&gt;„Ferice&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;de cei săraci în duh&lt;/J&gt;&lt;f&gt;[16]&lt;/f&gt;&lt;J&gt;, căci a lor este Împărăția cerurilor!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,4,'&lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;de cei ce plâng, căci ei vor fi mângâiați!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,5,'&lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;de cei blânzi, căci ei&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;vor moșteni pământul!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,6,'&lt;t&gt;&lt;J&gt;Ferice de cei flămânzi și însetați după dreptate, căci ei&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;vor fi săturați!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,7,'&lt;t&gt;&lt;J&gt;Ferice de cei milostivi, căci ei&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;vor avea parte de milă!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,8,'&lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;de cei cu inima curată, căci ei&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;Îl vor vedea pe Dumnezeu!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,9,'&lt;t&gt;&lt;J&gt;Ferice de făcătorii de pace, căci ei vor fi chemați fii ai lui Dumnezeu!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,10,'&lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;de cei prigoniți pentru dreptate, căci a lor este Împărăția cerurilor!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,11,'&lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;va fi de voi când, din pricina Mea, oamenii vă vor ocărî, vă vor prigoni și vor spune tot felul de lucruri rele și neadevărate împotriva voastră!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,12,'&lt;t&gt;&lt;J&gt;Bucurați-vă&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;și veseliți-vă, căci răsplata voastră este mare în ceruri, pentru că tot așa&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;i-au prigonit și pe profeții care au fost înainte de voi.&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,13,'&lt;pb/&gt;&lt;J&gt;Voi sunteți sarea pământului. Dar dacă sarea&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;își pierde gustul, cum se va face înapoi sărată? Atunci nu mai este bună decât să fie aruncată afară și călcată în picioare de oameni.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,14,'&lt;pb/&gt;&lt;J&gt;Voi&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;sunteți lumina lumii. O cetate așezată pe un munte nu poate să rămână ascunsă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,15,'&lt;J&gt;Și lampa nu este aprinsă&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;ca să fie pusă sub obroc, ci este pusă în sfeșnic și luminează tuturor celor din casă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,16,'&lt;J&gt;Tot așa să lumineze și lumina voastră înaintea oamenilor, ca ei&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt;&lt;J&gt;, văzând faptele voastre bune, să-L slăvească&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;pe Tatăl vostru din ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,17,'&lt;pb/&gt;&lt;J&gt;Să&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;nu credeți că am venit să desființez Legea sau Profeții; am venit nu să desființez, ci să împlinesc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,18,'&lt;J&gt;Adevărat vă spun: câtă vreme&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;vor dăinui cerul și pământul, nu va trece nicio iotă sau frântură de literă din Lege înainte ca toate să se fi împlinit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,19,'&lt;J&gt;Așa că oricine&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;va strica fie și una din aceste porunci mărunte&lt;/J&gt;&lt;f&gt;[17]&lt;/f&gt; &lt;J&gt;și-i va învăța pe oameni astfel va fi considerat cel mai mic în Împărăția cerurilor, dar oricine le va împlini și-i va învăța pe alții la fel va fi considerat mare în Împărăția cerurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,20,'&lt;J&gt;Căci vă spun: dacă dreptatea voastră nu o va întrece&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;pe cea a cărturarilor și a fariseilor, cu niciun chip nu veți intra în Împărăția cerurilor!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,21,'&lt;pb/&gt;&lt;J&gt;Ați auzit că li s-a zis celor din vechime: «&lt;e&gt;Să nu ucizi&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[18]&lt;/f&gt; &lt;J&gt;; oricine&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;ucide va da socoteală la judecată.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,22,'&lt;J&gt;Dar Eu vă spun că oricine se mânie pe fratele său va da socoteală la judecată; oricine-i va zice fratelui său: «Prostule!»&lt;/J&gt;&lt;f&gt;[19]&lt;/f&gt; &lt;J&gt;va&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;da socoteală &lt;i&gt;înaintea&lt;/i&gt; Sinedriului; iar oricine-i va zice: «Nebunule!» va sfârși în focul gheenei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,23,'&lt;J&gt;Așadar, dacă&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;îți aduci darul la altar și acolo îți amintești că fratele tău are ceva împotriva ta,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,24,'&lt;J&gt;lasă-ți&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;darul acolo, înaintea altarului, mergi mai întâi să te împaci cu fratele tău și apoi vino să-ți aduci darul!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,25,'&lt;J&gt;Caută&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;să te împaci degrabă cu potrivnicul tău, cât&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;timp ești cu el pe drum, ca nu cumva acesta să te dea pe mâna judecătorului, judecătorul — pe mâna temnicerului și să fii aruncat în temniță.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,26,'&lt;J&gt;Adevărat îți spun că nu vei ieși de acolo până nu vei da înapoi și ultimul bănuț&lt;/J&gt;&lt;f&gt;[20]&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,27,'&lt;pb/&gt;&lt;J&gt;Ați auzit că li s-a zis celor din vechime: «&lt;e&gt;Să nu săvârșești adulter.&lt;/e&gt;»&lt;/J&gt;&lt;f&gt;[21]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,28,'&lt;J&gt;Dar Eu vă spun că oricine se uită&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;la o femeie ca s-o poftească a și săvârșit adulter cu ea în inima lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,29,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;ochiul tău drept te face să cazi în păcat, scoate-l&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;și aruncă-l de la tine! Este spre folosul tău să piară unul dintre mădularele tale decât să-ți fie aruncat tot trupul în gheenă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,30,'&lt;J&gt;Și dacă mâna ta dreaptă te face să cazi în păcat, tai-o și arunc-o de la tine! Este spre folosul tău să piară unul dintre mădularele tale decât să-ți fie aruncat tot trupul în gheenă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,31,'&lt;pb/&gt;&lt;J&gt;S-a zis de asemenea: «Oricine&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;își va lăsa&lt;/J&gt;&lt;f&gt;[22]&lt;/f&gt; &lt;J&gt;soția &lt;e&gt;să-i dea un act de despărțire&lt;/e&gt;.»&lt;/J&gt;&lt;f&gt;[23]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,32,'&lt;J&gt;Dar Eu vă spun că oricine&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;își lasă soția din alt motiv decât desfrâul o face să săvârșească adulter. Și cine se căsătorește cu cea lăsată săvârșește adulter.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,33,'&lt;pb/&gt;&lt;J&gt;Ați auzit că li s-a zis celor din vechime&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;: «Să nu juri strâmb&lt;/J&gt;&lt;f&gt;[24]&lt;/f&gt; &lt;J&gt;, ci să-ți împlinești juruințele față de Domnul.»&lt;/J&gt;&lt;f&gt;[25]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,34,'&lt;J&gt;Dar Eu vă spun: să nu jurați&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;deloc: nici pe &lt;e&gt;cer&lt;/e&gt;, fiindcă &lt;e&gt;este tronul lui Dumnezeu&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[26]&lt;/f&gt;&lt;J&gt;,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,35,'&lt;J&gt;nici pe &lt;e&gt;pământ&lt;/e&gt;, fiindcă &lt;e&gt;este reazem pentru picioarele Lui&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[27]&lt;/f&gt;&lt;J&gt;, nici pe Ierusalim, fiindcă este &lt;e&gt;cetatea marelui Împărat&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[28]&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,36,'&lt;J&gt;Să nu juri nici pe capul tău, fiindcă nu poți face un singur fir de păr alb sau negru.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,37,'&lt;J&gt;Cuvântul&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;vostru să fie: «Da, da; nu, nu.» Ce trece peste acestea vine de la Cel Rău.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,38,'&lt;pb/&gt;&lt;J&gt;Ați auzit că s-a zis: «&lt;e&gt;Ochi pentru ochi și dinte pentru dinte.&lt;/e&gt;»&lt;/J&gt;&lt;f&gt;[29]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,39,'&lt;J&gt;Dar Eu vă spun: să&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;nu vă împotriviți &lt;i&gt;celui ce vă face&lt;/i&gt; rău. Ci, dacă&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;te lovește cineva peste obrazul drept, întoarce-i-l și pe celălalt!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,40,'&lt;J&gt;Celui ce vrea să se judece cu tine ca să-ți ia cămașa, lasă-i și haina!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,41,'&lt;J&gt;Dacă te silește&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;cineva să mergi o milă&lt;/J&gt;&lt;f&gt;[30]&lt;/f&gt;&lt;J&gt;, mergi cu el două!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,42,'&lt;J&gt;Celui ce-ți cere dă-i, iar celui ce vrea să se împrumute de la tine nu-i&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;întoarce spatele!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,43,'&lt;pb/&gt;&lt;J&gt;Ați auzit că s-a zis: «&lt;e&gt;Să-l iubești pe aproapele tău&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[31]&lt;/f&gt; &lt;J&gt;și să-l urăști pe vrăjmașul tău.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,44,'&lt;J&gt;Dar Eu vă spun: iubiți-vă&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;vrăjmașii, [binecuvântați-i pe cei ce vă blestemă, faceți-le bine celor ce vă urăsc] și rugați-vă pentru cei ce [vă asupresc&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;și] vă prigonesc,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,45,'&lt;J&gt;ca să fiți fiii Tatălui vostru din ceruri; fiindcă El face să răsară soarele Lui&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;și peste cei răi, și peste cei buni și dă ploaie și peste cei drepți, și peste cei nedrepți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,46,'&lt;J&gt;Dacă îi iubiți&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;numai pe cei ce vă iubesc, ce răsplată mai aveți? Nu fac la fel și vameșii?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,47,'&lt;J&gt;Și dacă îi îmbrățișați numai pe frații voștri, ce lucru neobișnuit faceți? Oare păgânii nu fac la fel?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,5,48,'&lt;J&gt;Voi fiți&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;deci desăvârșiți, după cum&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;și Tatăl vostru ceresc este desăvârșit!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,1,'&lt;pb/&gt;&lt;J&gt;Luați seama să nu îndepliniți dreptatea&lt;/J&gt;&lt;f&gt;[32]&lt;/f&gt; &lt;J&gt;voastră înaintea oamenilor ca să fiți văzuți de ei; altfel, nu veți avea răsplată de la Tatăl vostru din ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,2,'&lt;J&gt;Așadar, când faci&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;milostenie, nu trâmbița înaintea ta, cum fac fățarnicii în sinagogi și pe străzi, ca să fie slăviți de oameni! Adevărat vă spun că și-au primit plata.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,3,'&lt;J&gt;Ci tu, când faci milostenie, să nu știe stânga ta ce face dreapta,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,4,'&lt;J&gt;pentru ca milostenia ta să fie făcută în ascuns; și Tatăl tău, care vede în ascuns, îți va răsplăti&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,5,'&lt;pb/&gt;&lt;J&gt;Când vă rugați, să nu fiți ca fățarnicii, cărora le place să se roage stând în picioare în sinagogi și pe la colțurile străzilor, ca să fie văzuți de oameni. Adevărat vă spun că și-au primit plata.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,6,'&lt;J&gt;Ci tu, când te rogi, intră&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;în odăița ta, încuie ușa și roagă-te Tatălui tău, care este în ascuns; și Tatăl tău, care vede în ascuns, îți va răsplăti.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,7,'&lt;J&gt;Când vă rugați, să nu îndrugați &lt;i&gt;multe&lt;/J&gt;&lt;/i&gt;&lt;f&gt;ⓩ&lt;/f&gt;&lt;J&gt;, ca păgânii, cărora&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;li se pare că pentru mulțimea vorbelor vor fi ascultați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,8,'&lt;J&gt;Să nu vă asemănați cu ei, fiindcă Tatăl vostru știe de ce aveți nevoie mai înainte să-I cereți voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,9,'&lt;J&gt;Voi să vă rugați așa:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;Tatăl&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;nostru care ești în ceruri,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;sfințească-se Numele Tău;&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,10,'&lt;t&gt;&lt;J&gt;vie Împărăția Ta;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;facă-se&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;voia Ta&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;precum&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;în cer, așa și pe pământ.&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,11,'&lt;t&gt;&lt;J&gt;Pâinea noastră&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;cea de toate zilele dă-ne-o nouă astăzi&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,12,'&lt;t&gt;&lt;J&gt;și ne&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;iartă nouă greșelile&lt;/J&gt;&lt;f&gt;[33]&lt;/f&gt; &lt;J&gt;noastre&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;precum și noi iertăm greșiților noștri;&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,13,'&lt;t&gt;&lt;J&gt;și&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;nu ne duce în ispită&lt;/J&gt;&lt;f&gt;[34]&lt;/f&gt;&lt;J&gt;,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;ci izbăvește-ne&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;de Cel Rău!&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;[Căci&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;a Ta este împărăția și puterea și slava în veci. Amin!]&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,14,'&lt;pb/&gt;&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;le iertați oamenilor greșelile, și Tatăl vostru ceresc vă va ierta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,15,'&lt;J&gt;Dar dacă nu-i&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;iertați pe oameni, nici Tatăl vostru nu vă va ierta greșelile.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,16,'&lt;pb/&gt;&lt;J&gt;Când postiți&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;, să nu aveți o înfățișare posomorâtă, ca fățarnicii, care își schimonosesc fețele ca să le arate oamenilor că postesc. Adevărat vă spun că și-au primit plata.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,17,'&lt;J&gt;Ci tu, când postești, unge-ți&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;capul și spală-ți fața,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,18,'&lt;J&gt;ca să te arăți că postești nu oamenilor, ci Tatălui tău, care este în ascuns; și Tatăl tău, care vede în ascuns, îți va răsplăti.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,19,'&lt;pb/&gt;&lt;J&gt;Nu vă strângeți&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;comori pe pământ, unde le mănâncă moliile și rugina și unde le sapă și le fură hoții;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,20,'&lt;J&gt;ci&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;strângeți-vă comori în cer, unde nu le mănâncă moliile și rugina și unde hoții nu le sapă, nici nu le fură.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,21,'&lt;J&gt;Pentru că unde este comoara ta, acolo va fi și inima ta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,22,'&lt;pb/&gt;&lt;J&gt;Ochiul este candela&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;trupului. Dacă ochiul tău este sănătos, tot trupul tău va fi luminat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,23,'&lt;J&gt;Dar dacă ochiul tău este rău, tot trupul tău va fi plin de întuneric. Așadar, dacă lumina din tine este întuneric, cât de mare &lt;i&gt;trebuie să fie&lt;/i&gt; întunericul &lt;i&gt;acesta&lt;/i&gt;!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,24,'&lt;pb/&gt;&lt;J&gt;Nimeni&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;nu poate sluji la doi stăpâni. Căci ori îl va urî pe unul și-l va iubi pe celălalt, ori va ține la unul și-l va disprețui pe celălalt. Nu&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;puteți sluji și lui Dumnezeu, și avuției.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,25,'&lt;pb/&gt;&lt;J&gt;De aceea vă spun: nu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;vă îngrijorați cu privire la viața voastră, &lt;i&gt;gândindu-vă&lt;/i&gt; ce veți mânca sau ce veți bea, nici cu privire la trupul vostru, &lt;i&gt;gândindu-vă&lt;/i&gt; cu ce vă veți îmbrăca! Oare nu este viața mai mult decât hrana, și trupul &lt;i&gt;mai mult&lt;/i&gt; decât îmbrăcămintea?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,26,'&lt;J&gt;Uitați-vă&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;la păsările cerului: ele nu seamănă, nu seceră și nici nu strâng nimic în hambare, și totuși Tatăl vostru ceresc le hrănește. Oare nu sunteți voi cu mult mai de preț decât ele?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,27,'&lt;J&gt;Și apoi, cine dintre voi, îngrijorându-se, poate să adauge măcar un cot la statura lui?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,28,'&lt;J&gt;Și de ce vă îngrijorați cu privire la îmbrăcăminte? Luați aminte cum cresc crinii de pe câmp: ei nici nu trudesc, nici nu torc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,29,'&lt;J&gt;Vă spun însă că nici chiar Solomon, în toată slava lui, nu s-a îmbrăcat ca unul din ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,30,'&lt;J&gt;Iar dacă astfel îmbracă Dumnezeu iarba de pe câmp, care astăzi este, dar mâine va fi aruncată în cuptor, nu &lt;i&gt;vă va îmbrăca&lt;/i&gt; El cu mult mai mult pe voi, puțin credincioșilor?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,31,'&lt;J&gt;Așadar, nu vă îngrijorați, zicând: «Ce vom mânca?» sau: «Ce vom bea?» sau: «Cu ce ne vom îmbrăca?»,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,32,'&lt;J&gt;fiindcă după toate aceste lucruri umblă păgânii! Tatăl vostru ceresc știe că aveți nevoie de ele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,33,'&lt;J&gt;Căutați&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;mai întâi Împărăția lui Dumnezeu și dreptatea Lui, și toate acestea vi se vor da pe deasupra!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,6,34,'&lt;J&gt;Așadar, nu vă îngrijorați pentru ziua de mâine, fiindcă ziua de mâine se va îngrijora pentru ea însăși! Ajunge zilei necazul ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,1,'&lt;pb/&gt;&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;judecați, ca să nu fiți judecați!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,2,'&lt;J&gt;Căci cu ce judecată judecați, &lt;i&gt;cu aceea&lt;/i&gt; veți fi judecați; și&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;cu ce măsură măsurați, &lt;i&gt;cu aceea&lt;/i&gt; vi se va măsura.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,3,'&lt;J&gt;De&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;ce vezi tu paiul din ochiul fratelui tău, dar la bârna din ochiul tău nu te uiți?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,4,'&lt;J&gt;Sau cum îi poți spune fratelui tău: «Lasă-mă să scot paiul din ochiul tău» și, când colo, tu ai o bârnă în al tău?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,5,'&lt;J&gt;Fățarnicule, scoate întâi bârna din ochiul tău, și atunci vei vedea deslușit să scoți paiul din ochiul fratelui tău!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,6,'&lt;pb/&gt;&lt;J&gt;Să nu&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;dați câinilor ce este sfânt și să nu aruncați mărgăritarele voastre înaintea porcilor, ca nu cumva &lt;i&gt;porcii&lt;/i&gt; să le calce în picioare, iar &lt;i&gt;câinii&lt;/i&gt; să se întoarcă și să vă sfâșie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,7,'&lt;pb/&gt;&lt;J&gt;Cereți&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;și vi se va da; căutați și veți găsi; bateți și vi se va deschide!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,8,'&lt;J&gt;Căci oricine&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;cere primește, cine caută găsește, iar celui ce bate i se deschide.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,9,'&lt;J&gt;Cine din voi&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt;&lt;J&gt;, dacă-i cere fiul său pâine, îi dă o piatră?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,10,'&lt;J&gt;Sau, dacă-i cere un pește, îi dă un șarpe?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,11,'&lt;J&gt;Deci dacă voi, care&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;sunteți răi, știți să dați daruri bune copiilor voștri, cu cât mai mult Tatăl vostru din ceruri le va da &lt;i&gt;lucruri&lt;/i&gt; bune celor ce I le cer!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,12,'&lt;pb/&gt;&lt;J&gt;Tot ce&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;vreți să vă facă vouă oamenii, faceți-le și voi lor, căci în aceasta&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;sunt cuprinse Legea și Profeții!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,13,'&lt;pb/&gt;&lt;J&gt;Intrați&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;pe poarta cea strâmtă! Căci largă este poarta, lată este calea care duce la pieire și mulți sunt cei ce intră pe ea,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,14,'&lt;J&gt;dar strâmtă este poarta, îngustă este calea care duce la viață și puțini sunt cei ce o găsesc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,15,'&lt;pb/&gt;&lt;J&gt;Fiți cu băgare de seamă&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;la profeții mincinoși, care&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;vin la voi îmbrăcați în piei de oi, dar pe dinăuntru sunt lupi răpitori!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,16,'&lt;J&gt;După&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;roadele lor îi veți recunoaște. Oare se culeg&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;struguri din mărăcini sau smochine din ciulini?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,17,'&lt;J&gt;Tot așa, orice&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;pom bun face roade bune, dar pomul rău face roade rele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,18,'&lt;J&gt;Pomul bun nu poate face roade rele, nici pomul rău nu poate face roade bune.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,19,'&lt;J&gt;Orice&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;pom care nu face rod bun este tăiat și aruncat în foc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,20,'&lt;J&gt;Așa că după roadele lor îi veți recunoaște.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,21,'&lt;pb/&gt;&lt;J&gt;Nu oricine-Mi zice: «Doamne&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;, Doamne!» va intra în Împărăția cerurilor, ci cel ce face voia Tatălui Meu din ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,22,'&lt;J&gt;Mulți Îmi vor zice în ziua aceea: «Doamne, Doamne, n-am&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;profețit noi în Numele Tău? N-am scos noi demoni în Numele Tău? Și n-am făcut noi multe minuni în Numele Tău?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,23,'&lt;J&gt;Atunci&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;le voi spune deschis: «Niciodată nu v-am cunoscut; &lt;e&gt;plecați&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;&lt;J&gt;de la Mine, &lt;i&gt;voi&lt;/i&gt; care lucrați fărădelege!&lt;/e&gt;»&lt;/J&gt;&lt;f&gt;[35]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,24,'&lt;pb/&gt;&lt;J&gt;De aceea, oricine&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;aude aceste cuvinte ale Mele și le împlinește se va asemăna cu un om chibzuit, care și-a zidit casa pe stâncă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,25,'&lt;J&gt;A venit ploaia, s-au revărsat șuvoaiele, au suflat vânturile și s-au năpustit asupra casei, dar ea nu s-a prăbușit, pentru că avea temelia pe stâncă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,26,'&lt;J&gt;Însă oricine aude aceste cuvinte ale Mele și nu le împlinește se va asemăna cu un om nechibzuit, care și-a zidit casa pe nisip.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,27,'&lt;J&gt;A venit ploaia, s-au revărsat șuvoaiele, au suflat vânturile și au izbit în casa aceea; ea s-a prăbușit, și prăbușirea i-a fost mare.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,7,28,'După ce a sfârșit Isus aceste cuvântări, mulțimile au rămas uimite&lt;f&gt;ⓡ&lt;/f&gt; de învățătura Lui,');
INSERT INTO &quot;verses&quot; VALUES (470,7,29,'fiindcă&lt;f&gt;ⓢ&lt;/f&gt; El îi învăța &lt;i&gt;pe oameni&lt;/i&gt; ca &lt;i&gt;unul&lt;/i&gt; care avea autoritate, nu cum &lt;i&gt;îi învățau&lt;/i&gt; cărturarii lor.');
INSERT INTO &quot;verses&quot; VALUES (470,8,1,'&lt;pb/&gt;Când S-a coborât de pe munte, mulțimi mari au mers după El.');
INSERT INTO &quot;verses&quot; VALUES (470,8,2,'Și&lt;f&gt;ⓣ&lt;/f&gt; iată că un lepros s-a apropiat de El, I s-a închinat și I-a zis: „Doamne, dacă vrei, poți să mă curățești!”');
INSERT INTO &quot;verses&quot; VALUES (470,8,3,'&lt;i&gt;Isus&lt;/i&gt; a întins mâna și l-a atins, zicând: &lt;J&gt;„Da, vreau, fii curățit!”&lt;/J&gt; Îndată a fost curățit de lepra lui.');
INSERT INTO &quot;verses&quot; VALUES (470,8,4,'Apoi, Isus i-a zis: &lt;J&gt;„Vezi&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;să nu spui nimănui, ci mergi, arată-te preotului și adu darul pe care l-a poruncit&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;Moise, ca mărturie pentru ei!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,5,'&lt;pb/&gt;Pe când&lt;f&gt;ⓦ&lt;/f&gt; intra &lt;i&gt;Isus&lt;/i&gt; în Capernaum, s-a apropiat de El un centurion, care L-a rugat');
INSERT INTO &quot;verses&quot; VALUES (470,8,6,'și I-a zis: „Doamne, slujitorul meu zace în casă paralizat și se chinuie cumplit!”');
INSERT INTO &quot;verses&quot; VALUES (470,8,7,'&lt;i&gt;Isus&lt;/i&gt; i-a zis: &lt;J&gt;„O să vin să-l vindec.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,8,'Centurionul i-a răspuns, zicând: „Doamne, nu&lt;f&gt;ⓧ&lt;/f&gt; sunt vrednic să intri sub acoperișul meu; spune&lt;f&gt;ⓨ&lt;/f&gt; numai un cuvânt, și slujitorul meu va fi vindecat!');
INSERT INTO &quot;verses&quot; VALUES (470,8,9,'Căci și eu sunt om sub autoritate și am sub mine soldați; și când îi zic unuia: «Du-te!», se duce; &lt;i&gt;când îi zic&lt;/i&gt; altuia: «Vino!», vine; și &lt;i&gt;când îi zic&lt;/i&gt; robului meu: «Fă asta!», face.”');
INSERT INTO &quot;verses&quot; VALUES (470,8,10,'Auzind Isus &lt;i&gt;aceste vorbe&lt;/i&gt;, S-a mirat și le-a zis celor ce Îl urmau: &lt;J&gt;„Adevărat vă spun că n-am găsit în tot Israelul o asemenea credință!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,11,'&lt;J&gt;Dar vă spun că vor veni mulți&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;de la răsărit și de la apus și vor sta la masă cu Avraam, Isaac și Iacov în Împărăția cerurilor,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,12,'&lt;J&gt;iar&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;fiii Împărăției vor&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;fi aruncați în întunericul de afară. Acolo va fi plânsul și scrâșnirea dinților.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,13,'Apoi i-a zis centurionului: &lt;J&gt;„Du-te și facă-ți-se după cum ai crezut!”&lt;/J&gt; Și slujitorul lui s-a vindecat &lt;i&gt;chiar&lt;/i&gt; în ceasul acela.');
INSERT INTO &quot;verses&quot; VALUES (470,8,14,'&lt;pb/&gt;Isus a mers apoi&lt;f&gt;ⓒ&lt;/f&gt; în casa lui Petru și a văzut-o pe soacra&lt;f&gt;ⓓ&lt;/f&gt; acestuia zăcând în pat, cuprinsă de febră.');
INSERT INTO &quot;verses&quot; VALUES (470,8,15,'&lt;i&gt;Isus&lt;/i&gt; i-a atins mâna și febra a lăsat-o; apoi ea s-a ridicat și a început să-I slujească.');
INSERT INTO &quot;verses&quot; VALUES (470,8,16,'&lt;pb/&gt;Când s-a înserat&lt;f&gt;ⓔ&lt;/f&gt;, au adus la Isus mulți demonizați. Prin cuvântul &lt;i&gt;Său&lt;/i&gt;, El a scos din ei duhuri &lt;i&gt;necurate&lt;/i&gt; și i-a vindecat pe toți cei bolnavi,');
INSERT INTO &quot;verses&quot; VALUES (470,8,17,'ca să se împlinească ce fusese vestit prin profetul Isaia, care zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;El&lt;/e&gt; &lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;a luat asupra Lui neputințele noastre&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și a purtat bolile noastre.&lt;/e&gt; &lt;f&gt;[36]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,18,'&lt;pb/&gt;Văzând Isus mulțimea din jurul Său, le-a poruncit &lt;i&gt;ucenicilor&lt;/i&gt; să treacă pe malul celălalt.');
INSERT INTO &quot;verses&quot; VALUES (470,8,19,'Atunci&lt;f&gt;ⓖ&lt;/f&gt; a venit la El un cărturar, care I-a zis: „Învățătorule, Te voi urma oriunde vei merge.”');
INSERT INTO &quot;verses&quot; VALUES (470,8,20,'Isus i-a răspuns: &lt;J&gt;„Vulpile au vizuini și păsările cerului au cuiburi, dar Fiul Omului n-are unde să-Și plece capul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,21,'Un&lt;f&gt;ⓗ&lt;/f&gt; altul, care era dintre ucenici, I-a zis: „Doamne, dă-mi voie&lt;f&gt;ⓘ&lt;/f&gt; să mă duc mai întâi să-l îngrop pe tatăl meu!”');
INSERT INTO &quot;verses&quot; VALUES (470,8,22,'Dar Isus i-a răspuns: &lt;J&gt;„Vino după Mine și lasă morții să-și îngroape morții!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,8,23,'&lt;pb/&gt;Isus S-a urcat într-o barcă, iar ucenicii Lui L-au urmat.');
INSERT INTO &quot;verses&quot; VALUES (470,8,24,'Și&lt;f&gt;ⓙ&lt;/f&gt; iată că pe mare s-a stârnit o asemenea furtună, încât barca era acoperită de valuri. El însă dormea.');
INSERT INTO &quot;verses&quot; VALUES (470,8,25,'&lt;i&gt;Ucenicii&lt;/i&gt; s-au apropiat de El și L-au trezit, zicându-I: „Doamne, salvează-ne, fiindcă pierim!”');
INSERT INTO &quot;verses&quot; VALUES (470,8,26,'El le-a spus: &lt;J&gt;„De ce sunteți fricoși, puțin credincioșilor?”&lt;/J&gt; Apoi S-a sculat&lt;f&gt;ⓚ&lt;/f&gt;, a mustrat vânturile și marea și s-a lăsat o liniște adâncă.');
INSERT INTO &quot;verses&quot; VALUES (470,8,27,'Oamenii &lt;i&gt;aceia&lt;/i&gt; erau uimiți și ziceau: „Ce fel de om este Acesta, dacă Îl ascultă până și vânturile și marea?”');
INSERT INTO &quot;verses&quot; VALUES (470,8,28,'&lt;pb/&gt;Când&lt;f&gt;ⓛ&lt;/f&gt; a ajuns Isus pe partea cealaltă, în ținutul gadarenilor&lt;f&gt;[37]&lt;/f&gt;, L-au întâmpinat doi demonizați care ieșeau din morminte. Erau atât de înfricoșători, încât nimeni nu mai putea trece pe drumul acela.');
INSERT INTO &quot;verses&quot; VALUES (470,8,29,'Și iată că au început să strige: „Ce avem noi a face cu Tine, [Isuse,] Fiul lui Dumnezeu? Ai venit aici să ne chinuiești înainte de vreme?”');
INSERT INTO &quot;verses&quot; VALUES (470,8,30,'Departe de ei era o turmă mare de porci care pășteau.');
INSERT INTO &quot;verses&quot; VALUES (470,8,31,'Demonii Îl rugau și ziceau: „Dacă ne scoți afară, trimite-ne în turma &lt;i&gt;aceea&lt;/i&gt; de porci!”');
INSERT INTO &quot;verses&quot; VALUES (470,8,32,'&lt;J&gt;„Duceți-vă!”&lt;/J&gt;, le-a zis El. Ei au ieșit și au intrat în porci. Și, deodată, toată turma s-a repezit de pe stâncă în mare și a pierit în ape.');
INSERT INTO &quot;verses&quot; VALUES (470,8,33,'Porcarii au fugit și, ajungând în cetate, au povestit totul, anume cele &lt;i&gt;întâmplate&lt;/i&gt; cu demonizații.');
INSERT INTO &quot;verses&quot; VALUES (470,8,34,'&lt;i&gt;Atunci&lt;/i&gt; toată cetatea a ieșit în întâmpinarea lui Isus și, cum L-au văzut, L-au&lt;f&gt;ⓜ&lt;/f&gt; rugat să plece din ținutul lor.');
INSERT INTO &quot;verses&quot; VALUES (470,9,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; S-a urcat în barcă, a trecut pe malul celălalt și&lt;f&gt;ⓝ&lt;/f&gt; a venit în cetatea Sa.');
INSERT INTO &quot;verses&quot; VALUES (470,9,2,'Și&lt;f&gt;ⓞ&lt;/f&gt; iată că I-au adus un paralitic, care zăcea pe un pat&lt;f&gt;[38]&lt;/f&gt;. Văzându-le&lt;f&gt;ⓟ&lt;/f&gt; Isus credința, i-a zis paraliticului: &lt;J&gt;„Îndrăznește, fiule! Păcatele îți sunt iertate!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,3,'Și iată că unii dintre cărturari ziceau în sinea lor: „&lt;i&gt;Omul&lt;/i&gt; acesta hulește!”');
INSERT INTO &quot;verses&quot; VALUES (470,9,4,'Isus, cunoscându-le&lt;f&gt;ⓠ&lt;/f&gt; gândurile, le-a zis: &lt;J&gt;„De ce aveți gânduri rele în inimile voastre?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,5,'&lt;J&gt;Ce este mai ușor? A zice: «Păcatele îți sunt iertate» sau a zice: «Scoală-te și umblă»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,6,'&lt;J&gt;Dar ca să știți că Fiul Omului are autoritate pe pământ să ierte păcatele, scoală-te”&lt;/J&gt;, i-a zis El paraliticului, &lt;J&gt;„ridică-ți patul și du-te acasă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,7,'&lt;i&gt;Paraliticul&lt;/i&gt; s-a sculat și s-a dus acasă.');
INSERT INTO &quot;verses&quot; VALUES (470,9,8,'Când au văzut mulțimile, s-au înfricoșat și L-au slăvit pe Dumnezeu, care a dat oamenilor o asemenea putere.');
INSERT INTO &quot;verses&quot; VALUES (470,9,9,'&lt;pb/&gt;Isus a plecat&lt;f&gt;ⓡ&lt;/f&gt; de acolo și a văzut un om pe nume Matei stând la vamă și i-a zis: &lt;J&gt;„Vino după Mine!”&lt;/J&gt; El s-a ridicat și L-a urmat.');
INSERT INTO &quot;verses&quot; VALUES (470,9,10,'Pe&lt;f&gt;ⓢ&lt;/f&gt; când stătea Isus la masă în casă, iată că au venit mulți vameși și păcătoși și s-au așezat la masă cu El și cu ucenicii Lui.');
INSERT INTO &quot;verses&quot; VALUES (470,9,11,'Văzând &lt;i&gt;acest lucru&lt;/i&gt;, fariseii le-au zis ucenicilor Lui: „De ce mănâncă învățătorul vostru cu vameșii&lt;f&gt;ⓣ&lt;/f&gt; și cu păcătoșii&lt;f&gt;ⓤ&lt;/f&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (470,9,12,'&lt;i&gt;Isus&lt;/i&gt; i-a auzit și le-a zis: &lt;J&gt;„Nu cei sănătoși au nevoie de doctor, ci bolnavii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,13,'&lt;J&gt;Mergeți și învățați ce înseamnă: &lt;e&gt;Milă&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;&lt;J&gt;vreau, nu jertfe!&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[39]&lt;/f&gt; &lt;J&gt;Eu n-am venit să-i chem [la pocăință] pe cei drepți, ci&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;pe cei păcătoși.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,14,'&lt;pb/&gt;Atunci au venit la El ucenicii lui Ioan și I-au zis: „De&lt;f&gt;ⓧ&lt;/f&gt; ce noi și fariseii postim mult, iar ucenicii Tăi nu postesc?”');
INSERT INTO &quot;verses&quot; VALUES (470,9,15,'Isus le-a răspuns: &lt;J&gt;„Oare pot jeli nuntașii&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;câtă vreme mirele este cu ei? Vor veni zile când mirele va fi luat de la ei, și atunci&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;vor posti.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,16,'&lt;J&gt;Nimeni nu pune un petic de țesătură nouă la o haină veche, căci &lt;i&gt;peticul&lt;/i&gt; trage din &lt;i&gt;materialul&lt;/i&gt; hainei, și ruptura se face și mai mare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,17,'&lt;J&gt;Și nimeni nu pune vin nou în burdufuri vechi, altminteri burdufurile se sparg, vinul se varsă, iar burdufurile se pierd; ci vinul nou se pune în burdufuri noi și &lt;i&gt;astfel&lt;/i&gt; se păstrează și unul, și celelalte.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,18,'&lt;pb/&gt;Pe când&lt;f&gt;ⓐ&lt;/f&gt; le spunea &lt;i&gt;Isus&lt;/i&gt; aceste &lt;i&gt;cuvinte&lt;/i&gt;, iată că a venit un conducător &lt;i&gt;al sinagogii&lt;/i&gt;, I s-a închinat și I-a zis: „Fiica mea tocmai a murit, însă vino să-Ți pui mâna peste ea, și va trăi!”');
INSERT INTO &quot;verses&quot; VALUES (470,9,19,'Isus S-a ridicat și a plecat după el, împreună cu ucenicii Lui.');
INSERT INTO &quot;verses&quot; VALUES (470,9,20,'&lt;pb/&gt;Și&lt;f&gt;ⓑ&lt;/f&gt; iată că o femeie care de doisprezece ani avea o scurgere de sânge s-a apropiat din spate și I-a atins poala hainei,');
INSERT INTO &quot;verses&quot; VALUES (470,9,21,'căci își zicea în sinea ei: „Dacă mă voi atinge fie și numai de haina Lui, voi fi vindecată.”');
INSERT INTO &quot;verses&quot; VALUES (470,9,22,'Isus S-a întors și, văzând-o, i-a zis: &lt;J&gt;„Îndrăznește, fiică! Credința&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;ta te-a mântuit!”&lt;/J&gt; Și femeia s-a vindecat chiar în ceasul acela.');
INSERT INTO &quot;verses&quot; VALUES (470,9,23,'&lt;pb/&gt;Când&lt;f&gt;ⓓ&lt;/f&gt; a ajuns Isus în casa conducătorului &lt;i&gt;sinagogii&lt;/i&gt; și când i-a văzut pe cei ce cântau din fluier și mulțimea care se tânguia&lt;f&gt;ⓔ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (470,9,24,'le-a zis: &lt;J&gt;„Plecați&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;, căci fetița n-a murit, ci doarme!”&lt;/J&gt; Dar ei Îl luau în râs.');
INSERT INTO &quot;verses&quot; VALUES (470,9,25,'După ce a fost scoasă mulțimea, &lt;i&gt;Isus&lt;/i&gt; a intrat, a luat-o &lt;i&gt;pe fetiță&lt;/i&gt; de mână, și fetița s-a sculat.');
INSERT INTO &quot;verses&quot; VALUES (470,9,26,'Și vestea s-a răspândit în tot ținutul acela.');
INSERT INTO &quot;verses&quot; VALUES (470,9,27,'&lt;pb/&gt;Când Isus a plecat de acolo, au venit după El doi orbi, care strigau și ziceau: „Ai milă de noi, Fiul&lt;f&gt;ⓖ&lt;/f&gt; lui David!”');
INSERT INTO &quot;verses&quot; VALUES (470,9,28,'După ce a intrat în casă, orbii au venit la El. Și Isus le-a zis: &lt;J&gt;„Credeți că pot să fac lucrul acesta?”&lt;/J&gt; „Da, Doamne”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (470,9,29,'Atunci le-a atins ochii, zicând: &lt;J&gt;„Facă-vi-se după credința voastră!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,30,'Și li s-au deschis ochii. Isus le-a poruncit cu tărie și le-a zis: &lt;J&gt;„Vedeți&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;să nu afle nimeni!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,31,'Dar&lt;f&gt;ⓘ&lt;/f&gt; ei, cum au ieșit, au răspândit vestea despre El în tot ținutul acela.');
INSERT INTO &quot;verses&quot; VALUES (470,9,32,'&lt;pb/&gt;Pe&lt;f&gt;ⓙ&lt;/f&gt; când plecau ei, iată că I-au adus &lt;i&gt;lui Isus&lt;/i&gt; un mut &lt;i&gt;care era&lt;/i&gt; demonizat.');
INSERT INTO &quot;verses&quot; VALUES (470,9,33,'După ce demonul a fost scos din el, mutul a vorbit. Mulțimile se minunau și ziceau: „Niciodată nu s-a &lt;i&gt;mai&lt;/i&gt; văzut așa ceva în Israel!”');
INSERT INTO &quot;verses&quot; VALUES (470,9,34,'Dar fariseii spuneau: „Cu &lt;i&gt;ajutorul&lt;/i&gt; căpeteniei demonilor scoate el&lt;f&gt;ⓚ&lt;/f&gt; demonii!”');
INSERT INTO &quot;verses&quot; VALUES (470,9,35,'&lt;pb/&gt;Isus străbătea&lt;f&gt;ⓛ&lt;/f&gt; toate cetățile și satele, dând învățătură&lt;f&gt;ⓜ&lt;/f&gt; în sinagogi, propovăduind Evanghelia Împărăției și vindecând orice boală și orice neputință [care erau în popor].');
INSERT INTO &quot;verses&quot; VALUES (470,9,36,'Când&lt;f&gt;ⓝ&lt;/f&gt; a văzut mulțimile, I s-a făcut milă de ele, pentru că erau chinuite și neajutorate, &lt;e&gt;ca&lt;/e&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;niște oi care n-au păstor&lt;/e&gt;&lt;f&gt;[40]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,9,37,'Atunci le-a zis ucenicilor Săi: &lt;J&gt;„De secerat este mult&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt;&lt;J&gt;, dar lucrători sunt puțini!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,9,38,'&lt;J&gt;Rugați-L deci&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;pe Stăpânul secerișului să scoată lucrători la secerișul Lui!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; i-a chemat&lt;f&gt;ⓡ&lt;/f&gt; pe cei doisprezece ucenici ai Săi și le-a dat autoritate să scoată duhurile necurate și să vindece orice boală și orice neputință.');
INSERT INTO &quot;verses&quot; VALUES (470,10,2,'Iar numele celor doisprezece apostoli sunt: mai întâi, Simon, zis&lt;f&gt;ⓢ&lt;/f&gt; Petru, și Andrei, fratele lui; Iacov, fiul lui Zebedei, și Ioan, fratele lui;');
INSERT INTO &quot;verses&quot; VALUES (470,10,3,'Filip și Bartolomeu; Toma și Matei, vameșul; Iacov, fiul lui Alfeu, și [Levi, zis] Tadeu;');
INSERT INTO &quot;verses&quot; VALUES (470,10,4,'Simon&lt;f&gt;ⓣ&lt;/f&gt; Cananeul&lt;f&gt;[41]&lt;/f&gt; și Iuda Iscariotul&lt;f&gt;ⓤ&lt;/f&gt;, cel care L-a și vândut pe Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,10,5,'&lt;pb/&gt;Aceștia sunt cei doisprezece pe care i-a trimis Isus, după ce le-a poruncit și le-a zis: &lt;J&gt;„Să&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;nu mergeți pe calea păgânilor și să nu intrați în vreo cetate a samaritenilor&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt;&lt;J&gt;;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,6,'&lt;J&gt;să mergeți&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;mai degrabă la oile&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;pierdute ale casei lui Israel.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,7,'&lt;J&gt;Mergeți&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;și propovăduiți zicând: «Împărăția&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;cerurilor este aproape!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,8,'&lt;J&gt;Vindecați-i pe bolnavi, înviați-i pe morți, curățiți-i pe leproși și scoateți afară demonii! Fără plată&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;ați primit, fără plată să dați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,9,'&lt;J&gt;Să nu&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;luați nici &lt;i&gt;bani de&lt;/i&gt; aur, de argint sau de aramă&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;în brâiele voastre,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,10,'&lt;J&gt;nici traistă pentru drum, nici două haine, nici sandale, nici toiag, căci&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;vrednic este lucrătorul de hrana lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,11,'&lt;J&gt;În&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;toate cetățile sau satele în care veți intra, cercetați ce &lt;i&gt;om&lt;/i&gt; este vrednic acolo și rămâneți la el până veți pleca!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,12,'&lt;J&gt;Când intrați în casă, salutați-i &lt;i&gt;pe cei de acolo&lt;/i&gt;;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,13,'&lt;J&gt;și&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;dacă acea casă este vrednică, pacea voastră să vină peste ea, iar&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;dacă nu este vrednică, pacea voastră să se întoarcă la voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,14,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;cineva nu vă va primi, nici nu va asculta cuvintele voastre, să ieșiți din casa ori din cetatea aceea și să vă scuturați&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;praful de pe picioare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,15,'&lt;J&gt;Adevărat vă spun că, în ziua judecății, va fi mai&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;ușor pentru ținutul Sodomei și al Gomorei decât pentru cetatea aceea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,16,'&lt;pb/&gt;&lt;J&gt;Iată&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;, Eu vă trimit ca pe niște oi în mijlocul lupilor. Fiți dar&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;chibzuiți ca șerpii și fără răutate&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;ca porumbeii!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,17,'&lt;J&gt;Păziți-vă de oameni, fiindcă vă&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vor da pe mâna sinedriilor și vă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;vor biciui în sinagogile lor!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,18,'&lt;J&gt;Din pricina Mea, veți&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;fi duși înaintea dregătorilor și a regilor, ca mărturie pentru ei și pentru neamuri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,19,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;când vă vor da pe mâna lor, să nu vă îngrijorați cum sau ce veți vorbi, fiindcă vi&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;se va da chiar în ceasul acela ce să vorbiți;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,20,'&lt;J&gt;căci&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;nu voi veți vorbi, ci Duhul Tatălui vostru va vorbi în voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,21,'&lt;pb/&gt;&lt;J&gt;Fratele&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;își va da la moarte fratele, și tatăl — copilul; copiii se vor ridica împotriva părinților și-i vor omorî.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,22,'&lt;J&gt;Veți&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;fi urâți de toți din pricina Numelui Meu, dar&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;cine va răbda până la sfârșit va fi mântuit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,23,'&lt;pb/&gt;&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;vă vor prigoni într-o cetate, să fugiți în alta! Adevărat vă spun că nu veți sfârși cetățile lui Israel până&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;va veni Fiul Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,24,'&lt;pb/&gt;&lt;J&gt;Ucenicul nu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;este mai presus de învățătorul său, nici robul mai presus de stăpânul său.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,25,'&lt;J&gt;Îi este de ajuns ucenicului să fie ca învățătorul lui și robului să fie ca stăpânul lui. Dacă pe&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;Stăpânul casei L-au numit Beelzebul, cu cât mai mult &lt;i&gt;îi vor numi&lt;/i&gt; așa pe cei din casa Lui!?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,26,'&lt;pb/&gt;&lt;J&gt;Așa că nu vă temeți de ei. Căci&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;nu este nimic acoperit care nu va fi descoperit și nimic tăinuit care nu va fi cunoscut.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,27,'&lt;J&gt;Ce vă spun Eu la întuneric, voi să spuneți la lumină, și ce auziți &lt;i&gt;șoptindu-se&lt;/i&gt; la ureche, voi să propovăduiți de pe acoperișuri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,28,'&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;vă temeți de cei ce ucid trupul, dar care nu pot ucide sufletul; temeți-vă mai degrabă de Acela care poate să nimicească și sufletul, și trupul în gheenă!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,29,'&lt;J&gt;Nu se vând oare două vrăbii pe un ban&lt;/J&gt;&lt;f&gt;[42]&lt;/f&gt;&lt;J&gt;? Totuși, niciuna dintre ele nu cade la pământ fără &lt;i&gt;voia&lt;/i&gt; Tatălui vostru.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,30,'&lt;J&gt;Vouă&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;până și perii capului toți vă sunt numărați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,31,'&lt;J&gt;Deci nu vă temeți; voi sunteți mai de preț decât multe vrăbii!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,32,'&lt;pb/&gt;&lt;J&gt;De aceea&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;, pe oricine Mă va mărturisi înaintea oamenilor îl voi mărturisi și&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;Eu înaintea Tatălui Meu, care este în ceruri;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,33,'&lt;J&gt;dar&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;pe oricine Mă va tăgădui înaintea oamenilor îl voi tăgădui și Eu înaintea Tatălui Meu, care este în ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,34,'&lt;pb/&gt;&lt;J&gt;Să nu&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;credeți că am venit să aduc pace pe pământ; n-am venit să aduc pace, ci sabie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,35,'&lt;J&gt;Căci am venit să despart&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;pe om de tatăl său,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;pe fiică de mama sa&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și pe noră de soacra sa.&lt;/J&gt;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,36,'&lt;t&gt;&lt;e&gt;&lt;J&gt;Și omului îi vor fi vrăjmași&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓘ&lt;/f&gt; &lt;i&gt;&lt;e&gt;&lt;J&gt;chiar&lt;/i&gt; cei din casa lui.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[43]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,37,'&lt;pb/&gt;&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;își iubește tatăl sau mama mai mult decât pe Mine nu este vrednic de Mine și cine își iubește fiul sau fiica mai mult decât pe Mine nu este vrednic de Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,38,'&lt;J&gt;De asemenea, cine&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;nu-și ia crucea și nu Mă urmează nu este vrednic de Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,39,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;își va câștiga viața o va pierde și cine își va pierde viața pentru Mine o va câștiga.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,40,'&lt;pb/&gt;&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;vă primește pe voi Mă primește pe Mine, și cine Mă primește pe Mine Îl primește pe Acela care M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,41,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;primește un profet pentru că este profet&lt;/J&gt;&lt;f&gt;[44]&lt;/f&gt; &lt;J&gt;va primi răsplata unui profet, și cine primește un &lt;i&gt;om&lt;/i&gt; drept pentru că este un &lt;i&gt;om&lt;/i&gt; drept va primi răsplata unui om drept.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,10,42,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;oricine va da de băut fie și numai un pahar cu apă rece unuia dintre acești neînsemnați &lt;i&gt;ucenici&lt;/i&gt; pentru că este ucenic adevărat vă spun că nu-și va pierde răsplata.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,1,'&lt;pb/&gt;După ce a sfârșit de dat învățături celor doisprezece ucenici ai Săi, Isus a plecat de acolo, ca să-i învețe &lt;i&gt;pe oameni&lt;/i&gt; și să propovăduiască în cetățile lor.');
INSERT INTO &quot;verses&quot; VALUES (470,11,2,'&lt;pb/&gt;Ioan&lt;f&gt;ⓟ&lt;/f&gt; a auzit în&lt;f&gt;ⓠ&lt;/f&gt; temniță despre lucrările lui Hristos și i-a trimis pe ucenicii săi');
INSERT INTO &quot;verses&quot; VALUES (470,11,3,'să-L întrebe: „Tu ești Acela&lt;f&gt;ⓡ&lt;/f&gt; care vine sau să așteptăm pe altul?”');
INSERT INTO &quot;verses&quot; VALUES (470,11,4,'Isus le-a răspuns zicând: &lt;J&gt;„Mergeți și vestiți-i lui Ioan ce auziți și vedeți:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,5,'&lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;orbii&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;&lt;J&gt;își capătă vederea, șchiopii umblă,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;leproșii sunt curățiți, &lt;e&gt;surzii aud&lt;/e&gt;,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;morții învie, iar &lt;e&gt;săracilor&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;&lt;J&gt;li se aduc vești bune&lt;/e&gt;.&lt;/J&gt;&lt;f&gt;[45]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,6,'&lt;pb/&gt;&lt;J&gt;Ferice de cel ce nu se poticnește&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;din pricina Mea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,7,'&lt;pb/&gt;Pe când&lt;f&gt;ⓥ&lt;/f&gt; plecau ei, Isus a început să le vorbească mulțimilor despre Ioan: &lt;J&gt;„Ce ați ieșit să vedeți în pustiu? O trestie&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;clătinată de vânt?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,8,'&lt;J&gt;Atunci ce ați ieșit să vedeți? Un om îmbrăcat în &lt;i&gt;haine&lt;/i&gt; moi? Iată, cei ce poartă &lt;i&gt;haine&lt;/i&gt; moi sunt în casele regilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,9,'&lt;J&gt;Atunci ce ați ieșit să vedeți? Un profet? Da, vă spun, chiar&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;mai mult decât un profet.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,10,'&lt;J&gt;El este acela despre care s-a scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Iată&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;&lt;J&gt;, trimit înaintea Feței Tale pe solul Meu,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;care va pregăti calea înaintea Ta.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[46]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,11,'&lt;pb/&gt;&lt;J&gt;Adevărat vă spun că, dintre cei născuți din femeie, nu s-a ridicat niciunul mai mare decât Ioan Botezătorul. Totuși, cel mai mic în Împărăția cerurilor este mai mare decât el.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,12,'&lt;J&gt;Din&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;zilele lui Ioan Botezătorul până acum, Împărăția cerurilor este atacată cu înverșunare, și atacatorii pun mâna pe ea.&lt;/J&gt;&lt;f&gt;[47]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,13,'&lt;J&gt;Căci toți&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;Profeții și Legea până la Ioan au profețit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,14,'&lt;J&gt;Și dacă vreți să primiți &lt;i&gt;cuvântul acesta&lt;/i&gt;: el este Ilie&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt;&lt;J&gt;, care trebuia să vină.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,15,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;are urechi [de auzit] să audă!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,16,'&lt;pb/&gt;&lt;J&gt;Cu&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;cine voi asemăna generația aceasta? Este asemenea unor copii care stau în piețe și strigă &lt;i&gt;unii&lt;/i&gt; către alții:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,17,'&lt;pb/&gt;&lt;t&gt;&lt;J&gt;«V-am cântat din fluier, și n-ați jucat;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;v-am cântat de jale, și nu v-ați tânguit.»&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,18,'&lt;pb/&gt;&lt;J&gt;Căci a venit Ioan, care nici nu mănâncă, nici nu bea, iar ei zic: «Are demon!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,19,'&lt;J&gt;A venit Fiul Omului, care mănâncă și bea, iar ei zic: «Iată un mâncău și un bețiv, prieten&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;cu vameșii și cu păcătoșii!» Dar&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;înțelepciunea se dovedește dreaptă prin lucrările ei.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,20,'&lt;pb/&gt;Atunci&lt;f&gt;ⓖ&lt;/f&gt;, &lt;i&gt;Isus&lt;/i&gt; a început să mustre cetățile în care fuseseră făcute cele mai multe din minunile Lui, pentru că nu se pocăiseră:');
INSERT INTO &quot;verses&quot; VALUES (470,11,21,'&lt;J&gt;„Vai de tine, Horazine! Vai de tine, Betsaido! Căci dacă s-ar fi făcut în Tir și în Sidon minunile care au fost făcute în voi, de mult s-ar fi pocăit în&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;sac și cenușă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,22,'&lt;J&gt;De aceea vă spun că, în ziua judecății, va&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;fi mai ușor pentru Tir și Sidon decât pentru voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,23,'&lt;J&gt;Și tu, Capernaume, &lt;e&gt;vei fi oare înălțat până la cer&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[48]&lt;/f&gt;&lt;J&gt;? &lt;i&gt;Nu, ci&lt;/i&gt; &lt;e&gt;până în Locuința Morților vei fi coborât&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[49]&lt;/f&gt;&lt;J&gt;; căci dacă s-ar fi făcut în Sodoma minunile care au fost făcute în tine, ea ar fi rămas până în ziua de azi&lt;/J&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,11,24,'&lt;J&gt;De aceea vă spun că, în ziua judecății, va fi mai ușor&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;pentru ținutul Sodomei decât pentru tine.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,25,'&lt;pb/&gt;În&lt;f&gt;ⓚ&lt;/f&gt; vremea aceea, Isus a zis: &lt;J&gt;„Te laud, Tată, Domn al cerului și al pământului, pentru că ai&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;ascuns aceste &lt;i&gt;lucruri&lt;/i&gt; de cei înțelepți și pricepuți și&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;le-ai descoperit pruncilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,26,'&lt;J&gt;Da, Tată, pentru că așa ai binevoit Tu!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,27,'&lt;J&gt;Toate&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;Mi-au fost încredințate de Tatăl Meu și&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;nimeni nu-L cunoaște pe Fiul în afară de Tatăl; &lt;i&gt;tot așa,&lt;/i&gt; nimeni nu-L cunoaște pe Tatăl în afară de Fiul și de cel căruia vrea Fiul să i-L descopere.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,28,'&lt;pb/&gt;&lt;J&gt;Veniți la Mine, toți cei osteniți și împovărați, și Eu vă voi da odihnă!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,29,'&lt;J&gt;Luați jugul Meu asupra voastră și&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;învățați de la Mine, căci Eu sunt blând și smerit&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;cu inima, &lt;e&gt;și veți găsi odihnă pentru sufletele voastre&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[50]&lt;/f&gt;&lt;J&gt;!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,11,30,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;jugul Meu &lt;i&gt;este&lt;/i&gt; plăcut și povara Mea este ușoară.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,1,'&lt;pb/&gt;În vremea aceea, Isus trecea&lt;f&gt;ⓢ&lt;/f&gt; prin lanurile de grâu într-o zi de sabat. Ucenicii Lui, care erau flămânzi, au început să smulgă spice și să mănânce.');
INSERT INTO &quot;verses&quot; VALUES (470,12,2,'Fariseii, văzând &lt;i&gt;aceasta&lt;/i&gt;, I-au zis: „Uite că ucenicii Tăi fac ce nu este îngăduit să se facă în ziua sabatului!”');
INSERT INTO &quot;verses&quot; VALUES (470,12,3,'Dar Isus le-a răspuns: &lt;J&gt;„Oare n-ați citit ce&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;a făcut David când el și cei ce erau împreună cu el au flămânzit?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,4,'&lt;J&gt;Cum a intrat în Casa lui Dumnezeu și a mâncat pâinile pentru înfățișare&lt;/J&gt;&lt;f&gt;[51]&lt;/f&gt;&lt;J&gt;, care nu s-ar fi cuvenit să fie mâncate nici de el, nici de cei ce erau cu el, ci&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;numai de preoți?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,5,'&lt;J&gt;Sau n-ați citit în Lege&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;că în zilele de sabat preoții încalcă sabatul în Templu, și &lt;i&gt;totuși&lt;/i&gt; sunt fără vină?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,6,'&lt;J&gt;Dar Eu vă spun că aici este Unul&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;mai mare decât Templul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,7,'&lt;J&gt;Dacă ați fi știut ce înseamnă: &lt;e&gt;Milă&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;&lt;J&gt;vreau, nu jertfe&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[52]&lt;/f&gt;&lt;J&gt;, n-ați fi osândit niște nevinovați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,8,'&lt;J&gt;Căci Fiul Omului este Domn al sabatului.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,9,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; a plecat&lt;f&gt;ⓨ&lt;/f&gt; de acolo și a mers în sinagoga lor.');
INSERT INTO &quot;verses&quot; VALUES (470,12,10,'Și iată că &lt;i&gt;acolo&lt;/i&gt; era un om care avea o mână uscată. Ca să-L poată învinui &lt;i&gt;pe Isus&lt;/i&gt;, ei L-au întrebat și I-au zis: „Este îngăduit&lt;f&gt;ⓩ&lt;/f&gt; să vindeci în sabat?”');
INSERT INTO &quot;verses&quot; VALUES (470,12,11,'El le-a răspuns: &lt;J&gt;„Cine din voi, dacă are o oaie și&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;îi cade într-o groapă în ziua sabatului, n-o apucă și n-o scoate afară?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,12,'&lt;J&gt;Cu cât mai de preț este un om decât o oaie? De aceea este îngăduit să faci bine în sabat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,13,'Atunci i-a zis omului: &lt;J&gt;„Întinde-ți mâna!”&lt;/J&gt; El a întins-o și &lt;i&gt;mâna&lt;/i&gt; s-a făcut sănătoasă ca și cealaltă.');
INSERT INTO &quot;verses&quot; VALUES (470,12,14,'Fariseii&lt;f&gt;ⓑ&lt;/f&gt; au ieșit afară și s-au sfătuit cum să-L omoare &lt;i&gt;pe Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,12,15,'&lt;pb/&gt;Dar Isus, știind &lt;i&gt;aceasta&lt;/i&gt;, a plecat&lt;f&gt;ⓒ&lt;/f&gt; de acolo. După&lt;f&gt;ⓓ&lt;/f&gt; El a mers o mulțime numeroasă, iar El i-a vindecat pe toți &lt;i&gt;bolnavii&lt;/i&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,16,'și le-a poruncit&lt;f&gt;ⓔ&lt;/f&gt; să nu răspândească veste despre El,');
INSERT INTO &quot;verses&quot; VALUES (470,12,17,'ca să se împlinească ce fusese vestit prin profetul Isaia, care zice:');
INSERT INTO &quot;verses&quot; VALUES (470,12,18,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată Slujitorul Meu, pe care L-am ales,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Preaiubitul Meu, în&lt;/e&gt; &lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;care sufletul Meu își găsește plăcerea.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Voi pune Duhul Meu peste El&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și va vesti neamurilor judecata.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,19,'&lt;t&gt;&lt;e&gt;El nu Se va certa, nici nu va striga,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și nimeni nu-I va auzi glasul pe ulițe.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,20,'&lt;t&gt;&lt;e&gt;Trestia îndoită nu o va frânge&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și fitilul fumegând nu-l va stinge,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;până când nu va duce la biruință judecata.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,21,'&lt;t&gt;&lt;e&gt;Și neamurile vor nădăjdui în Numele Lui.&lt;/e&gt; &lt;f&gt;[53]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,22,'&lt;pb/&gt;Atunci&lt;f&gt;ⓖ&lt;/f&gt; I-au adus un demonizat orb și mut, și &lt;i&gt;Isus&lt;/i&gt; l-a vindecat, așa că mutul vorbea și vedea.');
INSERT INTO &quot;verses&quot; VALUES (470,12,23,'Mulțimile toate, uimite, ziceau: „Nu cumva acesta este Fiul lui David?”');
INSERT INTO &quot;verses&quot; VALUES (470,12,24,'Fariseii, auzind&lt;f&gt;ⓗ&lt;/f&gt;, au zis: „Acesta nu scoate demonii decât cu Beelzebul, căpetenia demonilor!”');
INSERT INTO &quot;verses&quot; VALUES (470,12,25,'Isus, care le cunoștea&lt;f&gt;ⓘ&lt;/f&gt; gândurile, le-a zis: &lt;J&gt;„Orice împărăție dezbinată împotriva ei înseși este pustiită, și orice cetate sau casă dezbinată împotriva ei înseși nu poate dăinui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,26,'&lt;J&gt;Dacă Satana îl scoate pe Satana, este dezbinat împotriva lui însuși; deci cum va dăinui împărăția lui?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,27,'&lt;J&gt;Și dacă Eu scot demonii cu ajutorul lui Beelzebul, fiii voștri cu ajutorul cui îi scot? De aceea, ei vor fi judecătorii voștri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,28,'&lt;J&gt;Dar dacă Eu scot demonii prin Duhul lui Dumnezeu, atunci Împărăția&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;lui Dumnezeu a ajuns la voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,29,'&lt;J&gt;Sau&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;cum poate cineva să intre în casa celui puternic și să-l jefuiască de bunuri, dacă nu l-a legat mai întâi pe cel puternic? Numai atunci îi va jefui casa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,30,'&lt;J&gt;Cine nu este cu Mine este împotriva Mea și cine nu adună cu Mine risipește.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,31,'&lt;J&gt;De aceea vă spun: toate&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;păcatele și blasfemiile li se vor ierta oamenilor, dar&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;blasfemia împotriva Duhului Sfânt nu li se va ierta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,32,'&lt;J&gt;Cine va vorbi&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;împotriva Fiului Omului va fi iertat&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt;&lt;J&gt;, dar oricine va vorbi împotriva Duhului Sfânt nu va fi iertat nici în veacul acesta, nici în cel viitor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,33,'&lt;pb/&gt;&lt;J&gt;Ori ziceți că pomul e bun și rodul&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;lui e bun, ori ziceți că pomul e rău și rodul lui e rău; căci pomul se cunoaște după rod.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,34,'&lt;J&gt;Pui&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;de vipere, cum puteți voi să spuneți lucruri bune, când voi sunteți răi? Căci&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;din prisosul inimii vorbește gura.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,35,'&lt;J&gt;Omul bun scoate lucruri bune din vistieria bună [a inimii lui], dar omul rău scoate lucruri rele din vistieria rea [a inimii lui].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,36,'&lt;J&gt;Vă spun că, în ziua judecății, oamenii vor da socoteală de orice cuvânt nesăbuit pe care-l vor fi rostit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,37,'&lt;J&gt;Căci din cuvintele tale vei fi îndreptățit și din cuvintele tale vei fi osândit.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,38,'&lt;pb/&gt;Atunci&lt;f&gt;ⓢ&lt;/f&gt;, unii dintre cărturari și farisei I-au răspuns zicând: „Învățătorule, vrem să vedem un semn de la tine!”');
INSERT INTO &quot;verses&quot; VALUES (470,12,39,'&lt;i&gt;Isus&lt;/i&gt; le-a răspuns și le-a zis: &lt;J&gt;„O generație rea și adulteră&lt;/J&gt;&lt;f&gt;[54]&lt;/f&gt; &lt;J&gt;cere un semn&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt;&lt;J&gt;, dar nu i se va da alt semn decât semnul profetului Iona.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,40,'&lt;J&gt;Căci, după cum &lt;e&gt;Iona a fost trei zile și trei nopți în pântecele unui pește mare&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[55]&lt;/f&gt;&lt;J&gt;, la fel și Fiul Omului va fi trei zile și trei nopți în inima pământului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,41,'&lt;J&gt;Bărbații&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;din Ninive se vor ridica la judecată împotriva generației acesteia și o&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;vor osândi, pentru că&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;ei s-au pocăit la propovăduirea lui Iona; și iată că aici &lt;i&gt;este&lt;/i&gt; Unul mai mare decât Iona.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,42,'&lt;J&gt;Regina&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;de la miazăzi se va ridica la judecată împotriva generației acesteia și o va osândi, pentru că ea a venit de la marginile pământului ca să asculte înțelepciunea lui Solomon; și iată că aici &lt;i&gt;este&lt;/i&gt; Unul mai mare decât Solomon.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,43,'&lt;pb/&gt;&lt;J&gt;Când a ieșit din om, duhul&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;necurat umblă&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;prin locuri fără apă, căutând odihnă, și n-o găsește.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,44,'&lt;J&gt;Atunci zice: «Mă voi întoarce în casa mea, de unde am ieșit.» Și când ajunge, o găsește nelocuită, măturată și împodobită.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,45,'&lt;J&gt;Atunci se duce, ia cu sine alte șapte duhuri mai rele decât el, intră &lt;i&gt;în casă&lt;/i&gt;, își fac locuința acolo, iar&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;starea din urmă a acelui om &lt;i&gt;ajunge&lt;/i&gt; mai rea decât cea dintâi. La fel va fi și cu generația aceasta rea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,46,'&lt;pb/&gt;Pe când încă le vorbea El mulțimilor, iată&lt;f&gt;ⓑ&lt;/f&gt; că mama și frații Lui stăteau afară și căutau să-I vorbească.');
INSERT INTO &quot;verses&quot; VALUES (470,12,47,'Atunci cineva I-a zis: „Iată, mama Ta și frații&lt;f&gt;ⓒ&lt;/f&gt; Tăi sunt afară și vor să stea de vorbă cu Tine!”');
INSERT INTO &quot;verses&quot; VALUES (470,12,48,'Dar &lt;i&gt;Isus&lt;/i&gt; i-a răspuns celui ce-I vorbise: &lt;J&gt;„Cine este mama Mea și cine sunt frații Mei?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,49,'Și întinzându-Și mâna spre ucenicii Săi, a zis: &lt;J&gt;„Iată-i pe mama Mea și pe frații Mei!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,12,50,'&lt;J&gt;Căci oricine&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;face voia Tatălui Meu din ceruri, acela Îmi este frate și soră și mamă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,1,'&lt;pb/&gt;În ziua aceea, Isus a ieșit din casă și S-a așezat&lt;f&gt;ⓔ&lt;/f&gt; pe &lt;i&gt;țărmul&lt;/i&gt; mării.');
INSERT INTO &quot;verses&quot; VALUES (470,13,2,'Mulțimi mari s-au&lt;f&gt;ⓕ&lt;/f&gt; adunat la El, așa că S-a&lt;f&gt;ⓖ&lt;/f&gt; suit și S-a așezat într-o barcă, în timp ce mulțimea toată stătea pe țărm.');
INSERT INTO &quot;verses&quot; VALUES (470,13,3,'&lt;pb/&gt;El le-a spus multe, în pilde, zicând: &lt;J&gt;„Iată, semănătorul a ieșit să semene.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,4,'&lt;J&gt;Pe când semăna el, o parte din semințe au căzut lângă drum, iar păsările au venit și le-au mâncat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,5,'&lt;J&gt;O altă parte &lt;i&gt;din semințe&lt;/i&gt; au căzut pe loc pietros, unde n-aveau mult pământ: au răsărit îndată, pentru că n-aveau pământ adânc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,6,'&lt;J&gt;Dar când a răsărit soarele, s-au veștejit și, pentru că n-aveau rădăcină, s-au uscat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,7,'&lt;J&gt;O altă&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;parte &lt;i&gt;din semințe&lt;/i&gt; au căzut între spini: spinii au crescut și le-au înăbușit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,8,'&lt;J&gt;O altă parte &lt;i&gt;din semințe&lt;/i&gt; au căzut pe pământ bun și au adus rod: una o sută&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt;&lt;J&gt;, una șaizeci și alta treizeci.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,9,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;are urechi [de auzit] să audă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,10,'&lt;pb/&gt;Apropiindu-se de El, ucenicii I-au zis: „De ce le vorbești în pilde?”');
INSERT INTO &quot;verses&quot; VALUES (470,13,11,'&lt;i&gt;Isus&lt;/i&gt; le-a răspuns zicând: &lt;J&gt;„Pentru că vouă&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;vi s-a dat să cunoașteți tainele Împărăției cerurilor, dar lor nu li s-a dat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,12,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;celui ce are i se va da și va avea din belșug, dar celui ce nu are i se va lua și ce are.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,13,'&lt;J&gt;De aceea le vorbesc în pilde, pentru că &lt;i&gt;ei&lt;/i&gt;, deși văd, nu văd și, deși aud, nu aud, nici nu înțeleg.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,14,'&lt;J&gt;Cu ei se împlinește profeția lui Isaia, care zice:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;De auzit veți&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;e&gt;&lt;J&gt;auzi, și nu veți înțelege;&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;de privit veți privi, și nu veți vedea.&lt;/e&gt;&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,15,'&lt;t&gt;&lt;J&gt;&lt;e&gt;Căci inima acestui popor s-a împietrit;&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;cu urechile greu au auzit&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;&lt;e&gt;,&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;iar ochii și i-au închis,&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;ca nu cumva să vadă cu ochii,&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;să audă cu urechile,&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;să înțeleagă cu inima și să se întoarcă&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;la Mine ca să-i vindec.&lt;/e&gt;&lt;/J&gt;&lt;f&gt;[56]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,16,'&lt;pb/&gt;&lt;J&gt;Dar ferice&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;de ochii voștri că văd și de urechile voastre că aud!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,17,'&lt;J&gt;Adevărat vă spun că mulți&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;profeți și &lt;i&gt;oameni&lt;/i&gt; drepți au dorit să vadă ce vedeți voi, și nu au văzut, și să audă ce auziți voi, și nu au auzit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,18,'&lt;pb/&gt;&lt;J&gt;Voi însă ascultați&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;pilda semănătorului!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,19,'&lt;J&gt;Când cineva aude Cuvântul Împărăției&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;și nu-l înțelege, vine Cel Rău și răpește ce a fost semănat în inima lui. Acesta este cel semănat lângă drum.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,20,'&lt;J&gt;Cel semănat în loc pietros este cel ce aude Cuvântul și-l primește îndată cu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;bucurie,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,21,'&lt;i&gt;&lt;J&gt;dar&lt;/i&gt; nu are rădăcină în el, ci ține până la o vreme și, când vine un necaz sau o prigoană din cauza Cuvântului, se&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;poticnește îndată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,22,'&lt;J&gt;Cel&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;semănat între&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;spini este cel ce aude Cuvântul, dar îngrijorările veacului [acestuia] și înșelăciunea bogățiilor înăbușă Cuvântul și-l fac astfel neroditor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,23,'&lt;J&gt;Iar cel semănat în pământ bun este cel ce aude Cuvântul și-l înțelege; el aduce rod: unul o sută, unul șaizeci și altul treizeci.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,24,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; le-a mai înfățișat o pildă, zicând: &lt;J&gt;„Împărăția cerurilor se aseamănă cu un om care a semănat sămânță bună în ogorul lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,25,'&lt;J&gt;Dar pe când dormeau oamenii, a venit vrăjmașul lui, a semănat neghină&lt;/J&gt;&lt;f&gt;[57]&lt;/f&gt; &lt;J&gt;în mijlocul grâului și a plecat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,26,'&lt;J&gt;Când au crescut firele &lt;i&gt;de grâu&lt;/i&gt; și au făcut rod, s-a arătat și neghina.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,27,'&lt;J&gt;Robii stăpânului casei s-au apropiat și i-au zis: «Stăpâne, n-ai semănat sămânță bună în ogorul tău? Cum de are neghină?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,28,'&lt;J&gt;El le-a răspuns: «Un vrăjmaș a făcut asta.» Și robii i-au zis: «Vrei dar să mergem să o adunăm?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,29,'&lt;J&gt;«Nu», le-a zis el, «ca nu cumva, adunând neghina, să smulgeți și grâul&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;odată cu ea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,30,'&lt;J&gt;Lăsați-le să crească împreună până la seceriș, iar la vremea secerișului le voi spune secerătorilor: ‘Adunați întâi neghina și legați-o în snopi ca să fie arsă, iar grâul adunați-l în hambarul meu!’»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,31,'&lt;pb/&gt;Isus le-a mai înfățișat o pildă, zicând: &lt;J&gt;„Împărăția&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;cerurilor se aseamănă cu un grăunte de muștar pe care l-a luat un om și l-a semănat în ogorul său.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,32,'&lt;J&gt;Acesta, deși este cea mai mică dintre toate semințele, când crește se face mai mare decât verdețurile și devine copac, așa că păsările cerului vin și își fac cuiburi în ramurile lui.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,33,'&lt;pb/&gt;Le-a mai spus&lt;f&gt;ⓨ&lt;/f&gt; o pildă: &lt;J&gt;„Împărăția cerurilor se aseamănă cu un aluat pe care l-a luat o femeie și l-a pus în trei măsuri&lt;/J&gt;&lt;f&gt;[58]&lt;/f&gt; &lt;J&gt;de făină de grâu, până a dospit totul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,34,'Isus le-a spus toate acestea mulțimilor în pilde și nu le vorbea deloc fără pildă,');
INSERT INTO &quot;verses&quot; VALUES (470,13,35,'ca să se împlinească ce fusese vestit prin profetul care zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Îmi voi deschide gura în pilde,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;voi&lt;/e&gt; &lt;f&gt;ⓩ&lt;/f&gt; &lt;e&gt;rosti lucruri ascunse de la întemeierea lumii.&lt;/e&gt;&lt;f&gt;[59]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,36,'&lt;pb/&gt;Atunci &lt;i&gt;Isus&lt;/i&gt; a lăsat mulțimile și a intrat în casă. Ucenicii Lui s-au apropiat de El și I-au zis: „Tâlcuiește-ne pilda cu neghina din ogor!”');
INSERT INTO &quot;verses&quot; VALUES (470,13,37,'El le-a răspuns: &lt;J&gt;„Cel ce seamănă sămânța bună este Fiul Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,38,'&lt;J&gt;Ogorul&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;este lumea; sămânța bună sunt fiii Împărăției; neghina sunt fiii&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;Celui Rău.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,39,'&lt;J&gt;Vrăjmașul care a semănat-o este Diavolul; secerișul&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;este sfârșitul veacului, iar secerătorii sunt îngerii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,40,'&lt;J&gt;Așadar, după cum se adună neghina și se arde în foc, așa va fi și la sfârșitul veacului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,41,'&lt;J&gt;Fiul Omului îi va trimite pe îngerii Săi și&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;ei vor strânge din Împărăția Lui toate pricinile de poticnire și pe cei ce săvârșesc fărădelegea&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,42,'&lt;e&gt;&lt;J&gt;și-i&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;e&gt;&lt;J&gt;vor arunca în cuptorul cu foc&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[60]&lt;/f&gt;&lt;J&gt;; acolo&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;va fi plânsul și scrâșnirea dinților.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,43,'&lt;J&gt;Atunci&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;cei drepți vor străluci ca soarele în Împărăția Tatălui lor. Cine&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;are urechi [de auzit] să audă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,44,'&lt;pb/&gt;&lt;J&gt;Împărăția cerurilor se aseamănă cu o comoară ascunsă într-un ogor. Omul care a găsit-o a ascuns-o și, de bucurie, merge, vinde&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;tot ce are și cumpără&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;ogorul acela.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,45,'&lt;pb/&gt;&lt;J&gt;Împărăția cerurilor se mai aseamănă cu un negustor care caută mărgăritare frumoase.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,46,'&lt;J&gt;Când găsește un&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;mărgăritar de mare preț, merge, vinde tot ce are și-l cumpără.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,47,'&lt;pb/&gt;&lt;J&gt;Împărăția cerurilor se mai aseamănă cu un năvod aruncat în mare, care prinde&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;tot felul &lt;i&gt;de vietăți&lt;/i&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,48,'&lt;J&gt;După ce s-a umplut, &lt;i&gt;pescarii&lt;/i&gt; îl trag la mal, se așază, adună în coșuri ce este bun și aruncă ce este rău.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,49,'&lt;J&gt;La fel va fi și la sfârșitul veacului. Îngerii vor ieși, îi vor despărți&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;pe cei răi din mijlocul celor drepți&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,50,'&lt;e&gt;&lt;J&gt;și-i&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;&lt;J&gt;vor arunca în cuptorul cu foc&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[61]&lt;/f&gt;&lt;J&gt;; acolo va fi plânsul și scrâșnirea dinților.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,51,'&lt;pb/&gt;&lt;J&gt;Ați înțeles toate acestea?”&lt;/J&gt; „Da”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (470,13,52,'El le-a zis: &lt;J&gt;„De aceea, orice cărturar care a devenit ucenic al Împărăției cerurilor este asemenea unui gospodar care scoate din vistieria lui &lt;i&gt;lucruri&lt;/i&gt; noi&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;și &lt;i&gt;lucruri&lt;/i&gt; vechi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,53,'&lt;pb/&gt;După ce a sfârșit Isus pildele acestea, a plecat de acolo.');
INSERT INTO &quot;verses&quot; VALUES (470,13,54,'A&lt;f&gt;ⓟ&lt;/f&gt; venit în ținutul Său natal și îi învăța &lt;i&gt;pe oameni&lt;/i&gt; în sinagogă, iar ei erau uimiți și ziceau: „De unde &lt;i&gt;are&lt;/i&gt; el înțelepciunea aceasta și &lt;i&gt;puterea cu care face&lt;/i&gt; minuni?');
INSERT INTO &quot;verses&quot; VALUES (470,13,55,'Oare&lt;f&gt;ⓠ&lt;/f&gt; nu este el fiul tâmplarului? Pe mama lui oare n-o cheamă Maria, iar pe frații lui — Iacov&lt;f&gt;ⓡ&lt;/f&gt;, Iosif, Simon și Iuda&lt;f&gt;ⓢ&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (470,13,56,'Și surorile lui nu sunt oare toate printre noi? De unde are el atunci toate aceste lucruri?”');
INSERT INTO &quot;verses&quot; VALUES (470,13,57,'Și se poticneau în El&lt;f&gt;ⓣ&lt;/f&gt;. Dar Isus le-a zis: &lt;J&gt;„Un profet nu&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;este disprețuit decât în patria și în casa Lui.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,13,58,'Și n-a&lt;f&gt;ⓥ&lt;/f&gt; făcut multe minuni acolo, din cauza necredinței lor.');
INSERT INTO &quot;verses&quot; VALUES (470,14,1,'&lt;pb/&gt;În vremea aceea, tetrarhul Irod&lt;f&gt;ⓦ&lt;/f&gt; a auzit de faima lui Isus');
INSERT INTO &quot;verses&quot; VALUES (470,14,2,'și le-a spus slujitorilor săi: „Acesta este Ioan Botezătorul! A înviat din morți și de aceea lucrează aceste puteri prin el!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,3,'Căci Irod&lt;f&gt;ⓧ&lt;/f&gt; îl prinsese pe Ioan, îl legase și-l aruncase în temniță din pricina Irodiadei, soția fratelui său Filip,');
INSERT INTO &quot;verses&quot; VALUES (470,14,4,'pentru că Ioan îi zicea: „Nu-ți&lt;f&gt;ⓨ&lt;/f&gt; este îngăduit să trăiești cu ea!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,5,'&lt;i&gt;Irod&lt;/i&gt; ar fi vrut să-l omoare, dar se temea de mulțime, pentru că&lt;f&gt;ⓩ&lt;/f&gt; &lt;i&gt;oamenii&lt;/i&gt; îl considerau &lt;i&gt;pe Ioan&lt;/i&gt; profet.');
INSERT INTO &quot;verses&quot; VALUES (470,14,6,'Dar când a fost ziua de naștere a lui Irod, fiica Irodiadei a dansat în mijlocul &lt;i&gt;oaspeților&lt;/i&gt; și lui Irod i-a plăcut.');
INSERT INTO &quot;verses&quot; VALUES (470,14,7,'De aceea, i-a promis cu jurământ că îi va da orice îi va cere.');
INSERT INTO &quot;verses&quot; VALUES (470,14,8,'Iar ea, îndemnată de mama ei, a zis: „Dă-mi aici, pe o tavă, capul lui Ioan Botezătorul!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,9,'Regele s-a întristat, dar din pricina jurămintelor și a oaspeților, a poruncit să-i fie dat;');
INSERT INTO &quot;verses&quot; VALUES (470,14,10,'și a trimis să i se taie capul lui Ioan în temniță.');
INSERT INTO &quot;verses&quot; VALUES (470,14,11,'Capul a fost adus pe o tavă și i-a fost dat fetei, care l-a dus la mama ei.');
INSERT INTO &quot;verses&quot; VALUES (470,14,12,'Ucenicii lui &lt;i&gt;Ioan&lt;/i&gt; au venit, i-au luat trupul, l-au îngropat și au mers să-I dea de veste lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,14,13,'&lt;pb/&gt;La auzul &lt;i&gt;veștii&lt;/i&gt;&lt;f&gt;ⓐ&lt;/f&gt;, Isus a plecat de acolo cu barca spre un loc pustiu, deoparte. Când au aflat mulțimile, &lt;i&gt;au ieșit&lt;/i&gt; din cetăți și L-au urmat pe jos.');
INSERT INTO &quot;verses&quot; VALUES (470,14,14,'Coborând din barcă, [Isus] a văzut o mulțime mare &lt;i&gt;de oameni&lt;/i&gt;, I s-a făcut milă de ei&lt;f&gt;ⓑ&lt;/f&gt; și i-a vindecat pe cei bolnavi.');
INSERT INTO &quot;verses&quot; VALUES (470,14,15,'Pe&lt;f&gt;ⓒ&lt;/f&gt; înserate, ucenicii s-au apropiat de El și I-au zis: „Locul acesta este pustiu, și ora este înaintată. Dă-le drumul mulțimilor să se ducă prin sate, ca să-și cumpere de mâncare!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,16,'Isus le-a răspuns: &lt;J&gt;„Nu e nevoie să meargă; dați-le voi să mănânce!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,14,17,'Dar ei I-au zis: „Nu avem aici decât cinci pâini și doi pești!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,18,'Și El le-a zis: &lt;J&gt;„Aduceți-Mi-i aici!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,14,19,'A poruncit mulțimilor să se așeze pe iarbă, a luat cele cinci pâini și cei doi pești și, ridicându-Și ochii spre cer, a rostit binecuvântarea&lt;f&gt;ⓓ&lt;/f&gt;, a frânt pâinile și le-a dat ucenicilor, iar ucenicii, mulțimilor.');
INSERT INTO &quot;verses&quot; VALUES (470,14,20,'Au mâncat toți și s-au săturat, și s-au ridicat douăsprezece coșuri pline cu bucățile rămase.');
INSERT INTO &quot;verses&quot; VALUES (470,14,21,'Cei ce mâncaseră erau cam cinci mii de bărbați, în afară de femei și copii.');
INSERT INTO &quot;verses&quot; VALUES (470,14,22,'&lt;pb/&gt;Îndată, Isus i-a silit pe ucenicii Săi să intre în barcă și să treacă înaintea Lui pe cealaltă parte până când El avea să le dea drumul mulțimilor.');
INSERT INTO &quot;verses&quot; VALUES (470,14,23,'După&lt;f&gt;ⓔ&lt;/f&gt; ce le-a dat drumul mulțimilor, S-a dus pe munte, deoparte, ca să Se roage. Se înserase&lt;f&gt;ⓕ&lt;/f&gt; și El era singur acolo.');
INSERT INTO &quot;verses&quot; VALUES (470,14,24,'Barca era deja la multe stadii&lt;f&gt;[62]&lt;/f&gt; departe de țărm, bătută de valuri, căci vântul era potrivnic.');
INSERT INTO &quot;verses&quot; VALUES (470,14,25,'La a patra strajă din noapte, &lt;i&gt;Isus&lt;/i&gt; a venit la ei umblând&lt;f&gt;ⓖ&lt;/f&gt; pe mare.');
INSERT INTO &quot;verses&quot; VALUES (470,14,26,'Când L-au văzut umblând pe mare, ucenicii s-au înspăimântat, zicând că este o nălucă, și au țipat de frică.');
INSERT INTO &quot;verses&quot; VALUES (470,14,27,'Isus le-a vorbit numaidecât și le-a zis: &lt;J&gt;„Îndrăzniți, Eu Sunt, nu vă temeți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,14,28,'„Doamne”, I-a răspuns Petru, „dacă ești Tu, poruncește-mi să vin la Tine pe ape!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,29,'&lt;J&gt;„Vino!”&lt;/J&gt;, i-a zis &lt;i&gt;Isus&lt;/i&gt;. Petru a coborât din barcă și a mers pe apă către Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,14,30,'Văzând însă că vântul &lt;i&gt;era&lt;/i&gt; puternic, i s-a făcut frică și, fiindcă începea să se scufunde, a strigat: „Doamne, scapă-mă!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,31,'Îndată, Isus a întins mâna, l-a apucat și i-a zis: &lt;J&gt;„Puțin credinciosule, de ce te-ai îndoit?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,14,32,'Și când au urcat ei în barcă, vântul s-a potolit.');
INSERT INTO &quot;verses&quot; VALUES (470,14,33,'Cei ce erau în barcă I s-au&lt;f&gt;ⓗ&lt;/f&gt; închinat și au zis: „Cu adevărat, Tu ești Fiul lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (470,14,34,'&lt;pb/&gt;După&lt;f&gt;ⓘ&lt;/f&gt; ce au trecut marea, au venit în ținutul Ghenezaretului.');
INSERT INTO &quot;verses&quot; VALUES (470,14,35,'Recunoscându-L pe Isus, oamenii din acele locuri au dat de veste în toate împrejurimile și au adus la El pe toți bolnavii.');
INSERT INTO &quot;verses&quot; VALUES (470,14,36,'&lt;i&gt;Aceștia&lt;/i&gt; Îl rugau să le dea voie măcar să se atingă de poala hainei Lui. Și toți câți&lt;f&gt;ⓙ&lt;/f&gt; se atingeau erau vindecați.');
INSERT INTO &quot;verses&quot; VALUES (470,15,1,'&lt;pb/&gt;Atunci au&lt;f&gt;ⓚ&lt;/f&gt; venit la Isus niște farisei și niște cărturari din Ierusalim și I-au zis:');
INSERT INTO &quot;verses&quot; VALUES (470,15,2,'„De ce&lt;f&gt;ⓛ&lt;/f&gt; calcă ucenicii Tăi tradiția&lt;f&gt;ⓜ&lt;/f&gt; din bătrâni, căci nu-și spală mâinile când mănâncă?”');
INSERT INTO &quot;verses&quot; VALUES (470,15,3,'El le-a răspuns zicând: &lt;J&gt;„Și voi de ce călcați porunca lui Dumnezeu pentru tradiția voastră?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,4,'&lt;J&gt;Căci Dumnezeu a zis: &lt;e&gt;Cinstește-i&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;&lt;J&gt;pe tatăl tău și pe mama ta&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[63]&lt;/f&gt; &lt;J&gt;și: &lt;e&gt;Cine&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;&lt;J&gt;își va blestema tatăl sau mama să fie pedepsit cu moartea.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[64]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,5,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;voi ziceți: «Cine îi va zice tatălui său ori mamei sale: ‘Orice &lt;i&gt;lucru&lt;/i&gt; cu care ți-aș putea fi de folos este &lt;i&gt;deja&lt;/i&gt; dăruit &lt;i&gt;lui Dumnezeu&lt;/i&gt;’&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,6,'&lt;J&gt;nu mai trebuie să-și cinstească tatăl [sau mama].» Și ați desființat astfel Cuvântul lui Dumnezeu pentru tradiția voastră.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,7,'&lt;J&gt;Fățarnicilor&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt;&lt;J&gt;, bine a profețit Isaia despre voi, când a zis:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,8,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Poporul acesta&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓡ&lt;/f&gt; &lt;e&gt;&lt;J&gt;Mă cinstește cu buzele,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;însă inima lui este departe de Mine.&lt;/J&gt;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,9,'&lt;t&gt;&lt;e&gt;&lt;J&gt;Degeaba Mă cinstesc &lt;i&gt;ei&lt;/i&gt;&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;dând învățături &lt;i&gt;care sunt&lt;/J&gt;&lt;/i&gt;&lt;/e&gt; &lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;&lt;J&gt;porunci omenești.”&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[65]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,10,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; a chemat&lt;f&gt;ⓣ&lt;/f&gt; mulțimea la Sine și a zis: &lt;J&gt;„Ascultați și înțelegeți:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,11,'&lt;J&gt;nu&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;ce intră în gura omului îl face necurat; ce iese din gură, aceea-l face necurat!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,12,'Atunci, ucenicii Lui s-au apropiat și I-au zis: „Știi că fariseii, auzind cuvântul &lt;i&gt;acesta&lt;/i&gt;, s-au poticnit?”');
INSERT INTO &quot;verses&quot; VALUES (470,15,13,'Drept răspuns, El le-a zis: &lt;J&gt;„Orice&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;răsad pe care nu l-a sădit Tatăl Meu ceresc va fi smuls din rădăcină.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,14,'&lt;J&gt;Lăsați-i! Sunt&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;niște călăuze oarbe; iar când un orb îl călăuzește pe un alt orb, vor cădea amândoi în groapă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,15,'Petru&lt;f&gt;ⓧ&lt;/f&gt; i-a zis: „Deslușește-ne pilda aceasta!”');
INSERT INTO &quot;verses&quot; VALUES (470,15,16,'El le-a zis: &lt;J&gt;„Și&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;voi sunteți încă fără pricepere?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,17,'&lt;J&gt;Nu înțelegeți că orice&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;intră în gură merge în pântece și apoi este aruncat afară, în hazna?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,18,'&lt;J&gt;Dar ce&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;iese din gură vine din inimă, și aceea îl spurcă pe om.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,19,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;din inimă ies gândurile rele, uciderile, adulterele, desfrânările, furturile, mărturiile mincinoase, blasfemiile.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,20,'&lt;J&gt;Acestea &lt;i&gt;sunt&lt;/i&gt; lucrurile care-l spurcă pe om, pe când mâncatul cu mâinile nespălate nu-l spurcă pe om.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,21,'&lt;pb/&gt;După&lt;f&gt;ⓒ&lt;/f&gt; ce a plecat de acolo, Isus S-a dus în părțile Tirului și ale Sidonului.');
INSERT INTO &quot;verses&quot; VALUES (470,15,22,'Și iată că o femeie canaaneancă din ținuturile acelea a venit și a început să strige &lt;i&gt;către El&lt;/i&gt;: „Ai milă de mine, Doamne, Fiul lui David! Fiica mea este chinuită rău de un demon.”');
INSERT INTO &quot;verses&quot; VALUES (470,15,23,'Dar El nu i-a răspuns niciun cuvânt. Atunci ucenicii Lui s-au apropiat și au început să-L roage, zicând: „Dă-i un răspuns, căci strigă în urma noastră!”');
INSERT INTO &quot;verses&quot; VALUES (470,15,24,'El le-a răspuns: &lt;J&gt;„Eu&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;n-am fost trimis decât la oile pierdute ale casei lui Israel.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,25,'Dar ea a venit și I s-a închinat, zicând: „Doamne, ajută-mă!”');
INSERT INTO &quot;verses&quot; VALUES (470,15,26,'Drept răspuns, El i-a zis: &lt;J&gt;„Nu este bine să iei pâinea copiilor și s-o arunci la căței&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt;&lt;J&gt;!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,27,'„Da, Doamne”, a zis ea, „dar și cățeii mănâncă din firimiturile ce cad de la masa stăpânilor lor.”');
INSERT INTO &quot;verses&quot; VALUES (470,15,28,'Atunci, Isus i-a răspuns: &lt;J&gt;„Femeie, mare este credința ta! Facă-ți-se după cum dorești!”&lt;/J&gt; Și fiica ei s-a vindecat chiar în ceasul acela.');
INSERT INTO &quot;verses&quot; VALUES (470,15,29,'&lt;pb/&gt;Isus&lt;f&gt;ⓕ&lt;/f&gt; a plecat de acolo și a venit lângă&lt;f&gt;ⓖ&lt;/f&gt; Marea Galileei. Și suindu-Se pe munte, S-a așezat acolo.');
INSERT INTO &quot;verses&quot; VALUES (470,15,30,'Atunci&lt;f&gt;ⓗ&lt;/f&gt; au venit la El mulțimi numeroase, având cu ele șchiopi, orbi, ciungi, muți și mulți alți &lt;i&gt;bolnavi&lt;/i&gt;. I-au pus la picioarele Lui și El i-a vindecat,');
INSERT INTO &quot;verses&quot; VALUES (470,15,31,'iar mulțimea se minuna văzând că muții vorbesc, ciungii sunt vindecați, șchiopii umblă și orbii văd. Și Îl slăveau pe Dumnezeul lui Israel.');
INSERT INTO &quot;verses&quot; VALUES (470,15,32,'&lt;pb/&gt;Isus&lt;f&gt;ⓘ&lt;/f&gt; i-a chemat pe ucenicii Săi și le-a zis: &lt;J&gt;„Îmi este milă de mulțimea aceasta, pentru că &lt;i&gt;acești oameni&lt;/i&gt; stau de trei zile lângă Mine și nu au ce mânca. Nu vreau să-i trimit flămânzi, ca nu cumva să leșine &lt;i&gt;de foame&lt;/i&gt; pe drum.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,15,33,'Ucenicii&lt;f&gt;ⓙ&lt;/f&gt; I-au zis: „De unde &lt;i&gt;să luăm&lt;/i&gt; în pustiu atâtea pâini încât să săturăm atâta mulțime?”');
INSERT INTO &quot;verses&quot; VALUES (470,15,34,'&lt;J&gt;„Câte pâini aveți?”&lt;/J&gt;, i-a întrebat Isus. „Șapte”, I-au răspuns ei, „și puțini pești.”');
INSERT INTO &quot;verses&quot; VALUES (470,15,35,'Atunci, Isus a poruncit mulțimii să se așeze pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (470,15,36,'A luat&lt;f&gt;ⓚ&lt;/f&gt; cele șapte pâini și peștii și, aducând mulțumire&lt;f&gt;ⓛ&lt;/f&gt;, le-a frânt și le-a dat ucenicilor, iar ucenicii, mulțimilor.');
INSERT INTO &quot;verses&quot; VALUES (470,15,37,'Au mâncat toți și s-au săturat; și au strâns șapte coșuri pline cu bucățile rămase.');
INSERT INTO &quot;verses&quot; VALUES (470,15,38,'Cei ce mâncaseră erau patru mii de bărbați, în afară de femei și de copii.');
INSERT INTO &quot;verses&quot; VALUES (470,15,39,'Apoi&lt;f&gt;ⓜ&lt;/f&gt;, Isus a dat drumul mulțimilor, S-a suit în barcă și S-a dus în ținutul Magadanului&lt;f&gt;[66]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,16,1,'&lt;pb/&gt;Fariseii&lt;f&gt;ⓝ&lt;/f&gt; și saducheii s-au apropiat de Isus și, ca să-L ispitească, I-au cerut să le arate un semn din cer.');
INSERT INTO &quot;verses&quot; VALUES (470,16,2,'Drept răspuns, El le-a zis: &lt;J&gt;„Când se înserează, voi ziceți: «Va fi vreme bună, căci cerul este roșu.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,3,'&lt;J&gt;Și dimineața ziceți: «Astăzi va fi furtună, căci cerul este roșu-posomorât.» [Fățarnicilor], chipul cerului știți să-l deosebiți, dar semnele vremurilor nu puteți?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,4,'&lt;J&gt;O generație&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;rea și adulteră cere un semn, dar nu i se va da &lt;i&gt;alt&lt;/i&gt; semn decât semnul profetului Iona.”&lt;/J&gt; Apoi i-a lăsat și a plecat.');
INSERT INTO &quot;verses&quot; VALUES (470,16,5,'&lt;pb/&gt;Ucenicii trecuseră&lt;f&gt;ⓟ&lt;/f&gt; pe cealaltă parte și uitaseră să ia pâini.');
INSERT INTO &quot;verses&quot; VALUES (470,16,6,'Iar Isus le-a zis: &lt;J&gt;„Vedeți&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;să vă păziți de aluatul fariseilor și al saducheilor!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,7,'Ei se gândeau în sinea lor și își ziceau: „&lt;i&gt;Spune așa&lt;/i&gt; pentru că nu am luat pâini!”');
INSERT INTO &quot;verses&quot; VALUES (470,16,8,'Isus, știind &lt;i&gt;aceasta&lt;/i&gt;, le-a zis: &lt;J&gt;„De ce vă gândiți în sinea voastră că nu aveți pâini, puțin credincioșilor?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,9,'&lt;J&gt;Tot&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;nu pricepeți, nici nu vă mai aduceți aminte de cele cinci pâini pentru cei cinci mii și câte coșuri ați adunat?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,10,'&lt;J&gt;Nici&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;de cele șapte pâini pentru cei patru mii și câte coșuri ați adunat?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,11,'&lt;J&gt;Cum nu pricepeți că nu despre pâini v-am zis, ci să vă păziți de aluatul fariseilor și al saducheilor?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,12,'Atunci au înțeles ei că nu le zisese să se păzească de aluatul pâinilor, ci de învățătura fariseilor și a saducheilor.');
INSERT INTO &quot;verses&quot; VALUES (470,16,13,'&lt;pb/&gt;Isus a venit în părțile Cezareei lui Filip și i-a întrebat pe ucenicii Săi: &lt;J&gt;„Cine&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;zic oamenii că este Fiul Omului?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,14,'Ei&lt;f&gt;ⓤ&lt;/f&gt; I-au răspuns: „Unii &lt;i&gt;zic că&lt;/i&gt; Ioan Botezătorul, alții — Ilie, iar alții — Ieremia sau unul dintre profeți.”');
INSERT INTO &quot;verses&quot; VALUES (470,16,15,'&lt;J&gt;„Dar voi”&lt;/J&gt;, le-a zis El, &lt;J&gt;„cine ziceți că sunt?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,16,'Simon Petru I-a răspuns și I-a zis: „Tu&lt;f&gt;ⓥ&lt;/f&gt; ești Hristosul, Fiul Dumnezeului celui viu!”');
INSERT INTO &quot;verses&quot; VALUES (470,16,17,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Ferice de tine, Simone, Bar-Iona&lt;/J&gt;&lt;f&gt;[67]&lt;/f&gt;&lt;J&gt;, fiindcă&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;nu carnea și sângele ți-au descoperit &lt;i&gt;aceasta&lt;/i&gt;, ci Tatăl&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Meu din ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,18,'&lt;J&gt;Și Eu îți spun: tu&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;ești Petru, și pe&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;această piatră&lt;/J&gt;&lt;f&gt;[68]&lt;/f&gt; &lt;J&gt;voi zidi Biserica Mea, și&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;porțile Locuinței Morților nu o vor birui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,19,'&lt;J&gt;Îți&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;voi da cheile Împărăției cerurilor, și orice vei lega pe pământ va fi legat în ceruri, iar orice vei dezlega pe pământ va fi dezlegat în ceruri.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,20,'Atunci&lt;f&gt;ⓒ&lt;/f&gt; le-a poruncit ucenicilor Săi să nu spună nimănui că El este Hristosul.');
INSERT INTO &quot;verses&quot; VALUES (470,16,21,'&lt;pb/&gt;De atunci &lt;i&gt;încolo&lt;/i&gt;, Isus a început să le arate&lt;f&gt;ⓓ&lt;/f&gt; ucenicilor Săi că El trebuie să meargă la Ierusalim, să pătimească multe din partea bătrânilor, a preoților de seamă și a cărturarilor, să fie omorât și a treia zi să învie.');
INSERT INTO &quot;verses&quot; VALUES (470,16,22,'Petru L-a luat deoparte și a început să-L mustre, zicând: „Să Te ferească Dumnezeu, Doamne! Să nu Ți se întâmple asta!”');
INSERT INTO &quot;verses&quot; VALUES (470,16,23,'Dar El, întorcându-Se, i-a zis lui Petru: &lt;J&gt;„Înapoia Mea, Satano&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt;&lt;J&gt;! Tu&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;ești o piatră de poticnire pentru Mine, fiindcă tu nu te gândești la lucrurile lui Dumnezeu, ci la cele ale oamenilor.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,24,'&lt;pb/&gt;Atunci&lt;f&gt;ⓖ&lt;/f&gt; Isus le-a zis ucenicilor Săi: &lt;J&gt;„Dacă vrea cineva să vină după Mine, să se lepede de sine, să-și ia crucea și să Mă urmeze!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,25,'&lt;J&gt;Pentru că cine&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;va vrea să-și scape viața o va pierde, dar oricine își va pierde viața pentru Mine și-o va păstra.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,26,'&lt;J&gt;Căci la ce i-ar folosi unui om să câștige toată lumea dacă și-ar pierde sufletul? Sau ce&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;ar &lt;i&gt;putea&lt;/i&gt; da un om în schimb pentru sufletul său?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,27,'&lt;J&gt;Căci Fiul&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;Omului are să vină în slava Tatălui Său, cu&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;îngerii Săi, și&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;atunci &lt;e&gt;îi va răsplăti fiecăruia după faptele lui&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[69]&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,16,28,'&lt;J&gt;Adevărat vă spun că&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;sunt unii dintre cei ce stau aici care nu vor gusta moartea până nu-L vor vedea pe Fiul Omului venind în Împărăția Sa.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,1,'&lt;pb/&gt;După&lt;f&gt;ⓝ&lt;/f&gt; șase zile, Isus i-a luat cu Sine pe Petru, pe Iacov și pe Ioan, fratele lui, și i-a dus deoparte pe un munte înalt.');
INSERT INTO &quot;verses&quot; VALUES (470,17,2,'Și S-a schimbat la față înaintea lor: fața Lui a strălucit ca soarele, iar hainele I s-au făcut albe ca lumina.');
INSERT INTO &quot;verses&quot; VALUES (470,17,3,'Și iată că li s-au arătat Moise și Ilie, care stăteau de vorbă cu El.');
INSERT INTO &quot;verses&quot; VALUES (470,17,4,'Petru I-a zis lui Isus: „Doamne, este bine să fim aici; dacă vrei, voi face aici trei colibe: una pentru Tine, una pentru Moise și una pentru Ilie.”');
INSERT INTO &quot;verses&quot; VALUES (470,17,5,'Pe când&lt;f&gt;ⓞ&lt;/f&gt; încă vorbea el, iată că un nor luminos i-a acoperit cu umbra lui și un glas din nor a spus: „Acesta&lt;f&gt;ⓟ&lt;/f&gt; este Fiul Meu preaiubit, în&lt;f&gt;ⓠ&lt;/f&gt; care Îmi găsesc plăcerea: de&lt;f&gt;ⓡ&lt;/f&gt; El să ascultați!”');
INSERT INTO &quot;verses&quot; VALUES (470,17,6,'Auzind&lt;f&gt;ⓢ&lt;/f&gt;, ucenicii au căzut cu fața &lt;i&gt;la pământ&lt;/i&gt; și s-au înspăimântat foarte tare.');
INSERT INTO &quot;verses&quot; VALUES (470,17,7,'Dar Isus S-a apropiat, i-a&lt;f&gt;ⓣ&lt;/f&gt; atins și le-a zis: &lt;J&gt;„Ridicați-vă și nu vă temeți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,8,'Când și-au înălțat privirile, n-au mai văzut pe nimeni altcineva decât pe Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,17,9,'&lt;pb/&gt;Pe când coborau ei de pe munte, Isus&lt;f&gt;ⓤ&lt;/f&gt; le-a poruncit și le-a zis: &lt;J&gt;„Să nu spuneți nimănui despre vedenia &lt;i&gt;aceasta&lt;/i&gt; până când Fiul Omului va învia din morți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,10,'Ucenicii L-au întrebat, zicând: „De ce&lt;f&gt;ⓥ&lt;/f&gt; spun cărturarii că mai întâi trebuie să vină Ilie?”');
INSERT INTO &quot;verses&quot; VALUES (470,17,11,'&lt;i&gt;Isus&lt;/i&gt; le-a răspuns: &lt;J&gt;„Ilie va veni și le va așeza&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;pe toate.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,12,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Eu vă spun că Ilie a și venit, dar ei nu l-au recunoscut, ci au&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;făcut cu el ce au vrut. La fel are să pătimească și&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;Fiul Omului din partea lor.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,13,'Atunci ucenicii au înțeles&lt;f&gt;ⓐ&lt;/f&gt; că le vorbise despre Ioan Botezătorul.');
INSERT INTO &quot;verses&quot; VALUES (470,17,14,'&lt;pb/&gt;Când&lt;f&gt;ⓑ&lt;/f&gt; au ajuns la mulțime, s-a apropiat de El un om, care a îngenuncheat înaintea lui &lt;i&gt;Isus&lt;/i&gt; și I-a zis:');
INSERT INTO &quot;verses&quot; VALUES (470,17,15,'„Doamne, ai milă de fiul meu, căci este epileptic&lt;f&gt;[70]&lt;/f&gt; și pătimește rău: adesea cade în foc și adesea în apă.');
INSERT INTO &quot;verses&quot; VALUES (470,17,16,'L-am adus la ucenicii Tăi, dar n-au putut să-l vindece.”');
INSERT INTO &quot;verses&quot; VALUES (470,17,17,'Isus a răspuns: &lt;J&gt;„O, generație necredincioasă și pornită la rău! Până când voi mai fi cu voi? Până când vă voi mai răbda? Aduceți-l aici, la Mine!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,18,'Isus a certat demonul, care a ieșit din el, și băiatul s-a vindecat chiar în ceasul acela.');
INSERT INTO &quot;verses&quot; VALUES (470,17,19,'Atunci, ucenicii au venit la Isus și I-au zis deoparte: „Noi de ce n-am putut să-l scoatem?”');
INSERT INTO &quot;verses&quot; VALUES (470,17,20,'El le-a zis: &lt;J&gt;„Din pricina puținei voastre credințe. Adevărat vă spun că, dacă&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;ați avea credință cât un grăunte de muștar, i-ați zice muntelui acestuia: «Mută-te de aici acolo!» și s-ar muta; nimic nu v-ar fi cu neputință. [&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,21,'&lt;J&gt;Dar acest soi &lt;i&gt;de demoni&lt;/i&gt; nu iese &lt;i&gt;cu nimic altceva&lt;/i&gt; decât cu rugăciune și cu post.”]&lt;/J&gt;&lt;f&gt;[71]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,22,'&lt;pb/&gt;Pe&lt;f&gt;ⓓ&lt;/f&gt; când erau ei împreună în Galileea, Isus le-a zis: &lt;J&gt;„Fiul Omului va fi dat în mâinile oamenilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,23,'&lt;J&gt;Ei Îl vor omorî, dar a treia zi va învia.”&lt;/J&gt; &lt;i&gt;Ucenicii&lt;/i&gt; s-au întristat foarte mult.');
INSERT INTO &quot;verses&quot; VALUES (470,17,24,'&lt;pb/&gt;Când au ajuns&lt;f&gt;ⓔ&lt;/f&gt; în Capernaum, cei ce strângeau dările&lt;f&gt;[72]&lt;/f&gt; &lt;i&gt;pentru Templu&lt;/i&gt; s-au apropiat de Petru și i-au zis: „Învățătorul vostru nu plătește dările?”');
INSERT INTO &quot;verses&quot; VALUES (470,17,25,'El le-a spus: „Ba da.” Și când a intrat în casă, Isus i-a luat-o înainte, zicând: &lt;J&gt;„Ce crezi, Simone? Regii pământului de la cine iau dări sau biruri? De la fiii lor sau de la străini?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,26,'&lt;i&gt;Petru&lt;/i&gt; I-a răspuns: „De la străini.” Și Isus i-a zis: &lt;J&gt;„Așadar, fiii sunt scutiți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,17,27,'&lt;J&gt;Dar ca să nu le fim pricină de poticnire, du-te la mare, aruncă undița și scoate primul pește care va veni! Când îi vei deschide gura, vei găsi un statér&lt;/J&gt;&lt;f&gt;[73]&lt;/f&gt;&lt;J&gt;; ia-l și dă-li-l pentru Mine și pentru tine!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,1,'&lt;pb/&gt;În ceasul acela&lt;f&gt;ⓕ&lt;/f&gt;, ucenicii s-au apropiat de Isus și I-au zis: „Cine este mai mare în Împărăția cerurilor?”');
INSERT INTO &quot;verses&quot; VALUES (470,18,2,'Iar El a chemat un copilaș, l-a pus în mijlocul lor');
INSERT INTO &quot;verses&quot; VALUES (470,18,3,'și le-a zis: &lt;J&gt;„Adevărat vă spun că, dacă&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;nu vă veți schimba și nu vă veți face ca niște copilași, nu veți intra nicidecum în Împărăția cerurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,4,'&lt;J&gt;Așadar, cine&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;se va smeri ca acest copilaș va fi cel mai mare în Împărăția cerurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,5,'&lt;J&gt;Și cine&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;primește un copilaș ca acesta în Numele Meu pe Mine Mă primește.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,6,'&lt;pb/&gt;&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;dacă cineva îl va face să păcătuiască pe unul dintre acești micuți care cred în Mine, ar fi mai de folos pentru el să i se atârne o piatră de moară de gât și să fie scufundat în adâncul mării.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,7,'&lt;J&gt;Vai de lume, din pricina prilejurilor&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;de păcătuire! Trebuie să vină și prilejuri de păcătuire, dar vai&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;de omul prin care vine prilejul de păcătuire!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,8,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;mâna ta sau piciorul tău te fac să cazi în păcat, taie-le și aruncă-le de la tine! Este mai bine pentru tine să intri în viață șchiop sau ciung decât să ai două mâini sau două picioare și să fii aruncat în focul veșnic.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,9,'&lt;J&gt;Și dacă ochiul tău te face să cazi în păcat, scoate-l și aruncă-l de la tine! Este mai bine pentru tine să intri în viață cu un singur ochi decât să ai doi ochi și să fii aruncat în gheena de foc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,10,'&lt;pb/&gt;&lt;J&gt;Vedeți să nu disprețuiți pe vreunul dintre acești micuți, căci vă spun că îngerii&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;lor din ceruri privesc&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;necontenit fața Tatălui Meu, care este în ceruri!&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;[&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,11,'&lt;J&gt;Fiindcă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;Fiul Omului a venit să mântuiască ce era pierdut.]&lt;/J&gt;&lt;f&gt;[74]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,12,'&lt;J&gt;Ce&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;credeți? Dacă un om are o sută de oi și i se rătăcește una din ele, nu le va lăsa el pe cele nouăzeci și nouă pe munți și va merge s-o caute pe cea rătăcită?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,13,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;dacă ajunge s-o găsească, adevărat vă spun că se bucură de ea mai mult decât de cele nouăzeci și nouă care nu s-au rătăcit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,14,'&lt;J&gt;Tot astfel, nu este voia Tatălui vostru din ceruri să se piardă vreunul din acești micuți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,15,'&lt;pb/&gt;&lt;J&gt;Dacă fratele tău greșește împotriva ta, du-te și mustră-l între tine și el! Dacă te ascultă, l-ai&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;câștigat pe fratele tău.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,16,'&lt;J&gt;Dacă nu te ascultă, mai ia cu tine pe încă unul sau doi, &lt;e&gt;pentru ca din gura&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;&lt;J&gt;a doi sau trei martori să se întărească orice vorbă&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[75]&lt;/f&gt;&lt;J&gt;!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,17,'&lt;J&gt;Dacă nu-i ia în seamă, spune-l bisericii; și dacă nu ia în seamă nici biserica, să fie pentru tine ca păgânul&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;și ca vameșul!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,18,'&lt;pb/&gt;&lt;J&gt;Adevărat vă spun: ce&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;veți lega pe pământ va fi legat și în cer și ce veți dezlega pe pământ va fi dezlegat și în cer.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,19,'&lt;J&gt;Iarăși vă&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;spun: dacă doi dintre voi se învoiesc pe pământ să ceară un lucru oarecare, le va fi&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;i&gt;&lt;J&gt;dat&lt;/i&gt; de Tatăl Meu, care este în ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,20,'&lt;J&gt;Căci acolo unde sunt doi sau trei adunați în Numele Meu, sunt &lt;i&gt;și&lt;/i&gt; Eu în mijlocul lor.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,21,'&lt;pb/&gt;Atunci, Petru s-a apropiat &lt;i&gt;de El&lt;/i&gt; și I-a zis: „Doamne, de câte ori să-l iert pe fratele meu când va păcătui împotriva mea? De&lt;f&gt;ⓨ&lt;/f&gt; șapte &lt;i&gt;ori&lt;/i&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (470,18,22,'Isus i-a zis: &lt;J&gt;„Eu nu-ți spun de șapte ori, ci&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;de șaptezeci de ori câte șapte.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,23,'&lt;pb/&gt;&lt;J&gt;De aceea, Împărăția cerurilor se aseamănă cu un rege care a vrut să-și facă socotelile cu robii săi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,24,'&lt;J&gt;Când a început el socotelile, i-a fost adus unul care îi datora zece mii de talanți&lt;/J&gt;&lt;f&gt;[76]&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,25,'&lt;J&gt;Fiindcă acesta n-avea &lt;i&gt;de unde&lt;/i&gt; să dea înapoi, stăpânul lui a poruncit să fie&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;vânduți el, soția lui, copiii lui și tot ce avea, ca să plătească &lt;i&gt;datoria&lt;/i&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,26,'&lt;J&gt;Robul a căzut &lt;i&gt;la pământ&lt;/i&gt; și i s-a închinat, zicând: «Păsuiește-mă, și-ți voi da înapoi tot!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,27,'&lt;J&gt;Cuprins de milă, stăpânul robului aceluia i-a dat drumul și i-a iertat datoria.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,28,'&lt;J&gt;Când a ieșit, robul acela l-a întâlnit pe unul dintre tovarășii lui de slujbă, care îi datora o sută de dinari&lt;/J&gt;&lt;f&gt;[77]&lt;/f&gt;&lt;J&gt;. L-a înșfăcat de gât și îl strângea, zicând: «Înapoiază-mi ce-mi datorezi!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,29,'&lt;J&gt;Tovarășul lui a căzut &lt;i&gt;la pământ&lt;/i&gt; și îl ruga zicând: «Păsuiește-mă, și-ți voi plăti!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,30,'&lt;J&gt;Dar el n-a vrut, ci s-a dus și l-a aruncat în temniță până când avea să îi înapoieze ce îi datora.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,31,'&lt;J&gt;Văzând tovarășii lui cele întâmplate, s-au întristat foarte tare și au mers să-i spună stăpânului lor toate cele petrecute.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,32,'&lt;J&gt;Atunci, stăpânul său l-a chemat și i-a zis: «Rob rău! Eu ți-am iertat toată datoria, fiindcă m-ai rugat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,33,'&lt;J&gt;Oare nu se cădea să-ți fie și ție milă de tovarășul tău așa cum mi-a fost mie milă de tine?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,34,'&lt;J&gt;Atunci, stăpânul s-a mâniat și l-a dat pe mâna schingiuitorilor până când avea să înapoieze tot ce datora.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,18,35,'&lt;J&gt;Tot așa&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;vă va face și Tatăl Meu ceresc dacă nu-l iertați din inimă pe fratele vostru.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,1,'&lt;pb/&gt;După&lt;f&gt;ⓒ&lt;/f&gt; ce a terminat Isus &lt;i&gt;de zis&lt;/i&gt; cuvintele acestea, a plecat din Galileea și a venit în ținuturile Iudeei, dincolo de Iordan.');
INSERT INTO &quot;verses&quot; VALUES (470,19,2,'Mulțimi mari &lt;i&gt;de oameni&lt;/i&gt; mergeau după&lt;f&gt;ⓓ&lt;/f&gt; El; și i-a vindecat acolo.');
INSERT INTO &quot;verses&quot; VALUES (470,19,3,'&lt;pb/&gt;Niște farisei au venit la El ca să-L ispitească și I-au zis: „Îi este îngăduit unui bărbat să-și lase soția din orice pricină?”');
INSERT INTO &quot;verses&quot; VALUES (470,19,4,'El le-a răspuns zicând: &lt;J&gt;„N-ați citit că, de la început&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt;&lt;J&gt;, Ziditorul &lt;e&gt;i-a creat bărbat și femeie&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[78]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,5,'&lt;J&gt;și a zis: &lt;e&gt;De aceea&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;&lt;J&gt;va lăsa omul pe tatăl său și pe mama sa și se va lipi de nevasta sa, și cei doi vor&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;&lt;J&gt;fi un singur trup&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[79]&lt;/f&gt;&lt;J&gt;?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,6,'&lt;J&gt;Așa că nu mai sunt doi, ci un singur trup. Așadar, ce a unit Dumnezeu, omul să nu despartă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,7,'&lt;i&gt;Ei&lt;/i&gt; I-au zis: „De&lt;f&gt;ⓗ&lt;/f&gt; ce a poruncit Moise &lt;e&gt;să-i dea un act de despărțire și s-o lase&lt;/e&gt;&lt;f&gt;[80]&lt;/f&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (470,19,8,'[Isus] le-a răspuns: &lt;J&gt;„Din pricina împietririi inimii voastre a îngăduit Moise să vă lăsați soțiile, dar de la început n-a fost așa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,9,'&lt;J&gt;Eu însă&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;vă spun că oricine își lasă soția &lt;i&gt;din alt motiv&lt;/i&gt; decât desfrâul și se căsătorește cu alta săvârșește adulter; [și cine se căsătorește cu cea lăsată &lt;i&gt;de bărbat&lt;/i&gt; săvârșește adulter]&lt;/J&gt;&lt;f&gt;[81]&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,10,'&lt;pb/&gt;Ucenicii Lui I-au zis: „Dacă&lt;f&gt;ⓙ&lt;/f&gt; așa stau lucrurile cu bărbatul și femeia, nu-i de niciun folos să se căsătorească.”');
INSERT INTO &quot;verses&quot; VALUES (470,19,11,'El le-a răspuns: &lt;J&gt;„Nu&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;toți pot primi cuvântul acesta, ci numai aceia cărora le este dat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,12,'&lt;J&gt;Fiindcă sunt eunuci care din pântecele mamei &lt;i&gt;lor&lt;/i&gt; s-au născut așa, sunt eunuci care au fost făcuți așa&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;de oameni și sunt eunuci care s-au făcut ei înșiși eunuci pentru Împărăția cerurilor. Cine poate primi să primească.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,13,'&lt;pb/&gt;Atunci&lt;f&gt;ⓜ&lt;/f&gt; I-au adus niște copii ca să-Și pună mâinile peste ei și să Se roage, dar ucenicii i-au certat.');
INSERT INTO &quot;verses&quot; VALUES (470,19,14,'Însă Isus le-a zis: &lt;J&gt;„Lăsați copiii și nu-i opriți să vină la Mine, căci Împărăția cerurilor este a&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;celor ca ei!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,15,'După ce Și-a pus mâinile peste ei, a plecat de acolo.');
INSERT INTO &quot;verses&quot; VALUES (470,19,16,'&lt;pb/&gt;Și iată&lt;f&gt;ⓞ&lt;/f&gt; că un &lt;i&gt;om&lt;/i&gt; s-a apropiat de El și I-a zis: „Învățătorule&lt;f&gt;ⓟ&lt;/f&gt;, ce &lt;i&gt;lucru&lt;/i&gt; bun să fac ca să am viață veșnică?”');
INSERT INTO &quot;verses&quot; VALUES (470,19,17,'El i-a răspuns: &lt;J&gt;„De ce Mă întrebi &lt;i&gt;ce lucru&lt;/i&gt; bun &lt;i&gt;să faci&lt;/i&gt;? Unul &lt;i&gt;singur&lt;/i&gt; este Cel Bun. Deci dacă vrei să intri în viață, păzește poruncile&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt;&lt;J&gt;!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,18,'„Care?”, I-a zis el. Și Isus i-a răspuns: &lt;e&gt;&lt;J&gt;„Să nu ucizi; să nu săvârșești adulter; să nu furi; să nu depui mărturie mincinoasă;&lt;/J&gt;&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,19,'&lt;e&gt;&lt;J&gt;să-i cinstești&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;e&gt;&lt;J&gt;pe tatăl tău și pe mama ta&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[82]&lt;/f&gt; &lt;J&gt;și &lt;e&gt;să-l&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;&lt;J&gt;iubești pe aproapele tău ca pe tine însuți.”&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[83]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,20,'Tânărul i-a zis: „Pe toate acestea le-am păzit [din tinerețea mea]&lt;f&gt;[84]&lt;/f&gt;. Ce-mi mai lipsește?”');
INSERT INTO &quot;verses&quot; VALUES (470,19,21,'Isus i-a zis: &lt;J&gt;„Dacă vrei să fii desăvârșit, du-te&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt;&lt;J&gt;, vinde-ți averile, dă-le săracilor &lt;i&gt;banii&lt;/i&gt; și vei avea o comoară în ceruri! Apoi vino și urmează-Mă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,22,'Când a auzit tânărul vorba aceasta, a plecat întristat, pentru că avea multe averi.');
INSERT INTO &quot;verses&quot; VALUES (470,19,23,'&lt;pb/&gt;Atunci Isus le-a spus ucenicilor Săi: &lt;J&gt;„Adevărat vă spun că foarte greu&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;va intra un bogat în Împărăția cerurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,24,'&lt;J&gt;Vă mai spun iarăși că este mai ușor să treacă o cămilă prin urechea acului decât să intre un bogat în Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,25,'Auzind, ucenicii se mirau foarte mult și ziceau: „Atunci cine poate să fie mântuit?”');
INSERT INTO &quot;verses&quot; VALUES (470,19,26,'Isus i-a țintuit cu privirea și le-a zis: &lt;J&gt;„La oameni acest lucru este cu neputință, dar la&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;Dumnezeu toate sunt cu putință.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,27,'&lt;pb/&gt;Atunci&lt;f&gt;ⓦ&lt;/f&gt; Petru I-a răspuns: „Iată, noi&lt;f&gt;ⓧ&lt;/f&gt; am lăsat totul și Te-am urmat. Care ne va fi &lt;i&gt;răsplata&lt;/i&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (470,19,28,'Isus le-a zis: &lt;J&gt;„Adevărat vă spun că, la înnoirea &lt;i&gt;lumii&lt;/i&gt;, când va sta Fiul Omului pe tronul slavei Sale, voi care M-ați urmat veți&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;sta pe douăsprezece tronuri și veți judeca cele douăsprezece seminții ale lui Israel.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,29,'&lt;J&gt;Și oricine&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;a lăsat case, frați, surori, tată, mamă, [soție,] copii sau ogoare pentru Numele Meu va primi însutit și va moșteni viața veșnică.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,19,30,'&lt;J&gt;Mulți&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;dintre cei dintâi vor fi cei de pe urmă și mulți dintre cei de pe urmă vor fi cei dintâi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,1,'&lt;pb/&gt;&lt;J&gt;Împărăția cerurilor se aseamănă cu un gospodar care a ieșit dis-de-dimineață să-și tocmească lucrători la vie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,2,'&lt;J&gt;S-a înțeles cu lucrătorii cu un dinar pe zi și i-a trimis în via lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,3,'&lt;J&gt;Când a ieșit pe la ceasul al treilea&lt;/J&gt;&lt;f&gt;[85]&lt;/f&gt;&lt;J&gt;, i-a văzut pe alții stând în piață fără lucru&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,4,'&lt;J&gt;și le-a spus: «Mergeți și voi la vie, și vă voi da plata cuvenită!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,5,'&lt;J&gt;Și ei s-au dus. A ieșit iarăși pe la ceasul al șaselea și al nouălea și a făcut la fel.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,6,'&lt;J&gt;Când a ieșit pe la ceasul al unsprezecelea, i-a găsit pe alții care stăteau și le-a zis: «De ce stați aici toată ziua fără lucru?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,7,'&lt;J&gt;Ei i-au răspuns: «Pentru că nu ne-a tocmit nimeni.» El le-a zis: «Mergeți și voi în via mea, [și veți primi plata cuvenită]!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,8,'&lt;J&gt;Când a venit seara, stăpânul viei i-a zis ispravnicului său: «Cheamă-i pe lucrători și dă-le plata, începând cu cei de pe urmă până la cei dintâi!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,9,'&lt;J&gt;Au venit cei din ceasul al unsprezecelea și au primit câte un dinar.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,10,'&lt;J&gt;Când au venit cei dintâi, credeau că vor primi mai mult, dar au primit și ei &lt;i&gt;tot&lt;/i&gt; câte un dinar.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,11,'&lt;J&gt;După ce și-au primit &lt;i&gt;plata&lt;/i&gt;, cârteau împotriva gospodarului,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,12,'&lt;J&gt;zicând: «Aceștia din urmă au lucrat un singur ceas, și i-ai plătit la fel ca pe noi, care am îndurat povara și arșița zilei!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,13,'&lt;J&gt;El i-a răspuns unuia dintre ei: «Prietene, nu te nedreptățesc. Nu te-ai înțeles cu mine cu un dinar?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,14,'&lt;J&gt;Ia-ți-l și pleacă! Eu vreau să-i dau și acestuia din urmă ca și ție.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,15,'&lt;J&gt;Nu pot oare&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;să fac ce vreau cu ce este al meu? Sau&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;este ochiul tău rău fiindcă eu sunt bun?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,16,'&lt;J&gt;Tot așa&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt;&lt;J&gt;, cei din urmă vor fi cei dintâi, și cei dintâi vor fi cei din urmă; [pentru că&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;mulți sunt chemați, dar puțini &lt;i&gt;sunt&lt;/i&gt; aleși].”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,17,'&lt;pb/&gt;Pe drum, pe când&lt;f&gt;ⓕ&lt;/f&gt; urca la Ierusalim, Isus i-a luat deoparte pe cei doisprezece ucenici &lt;i&gt;ai Săi&lt;/i&gt; și le-a zis:');
INSERT INTO &quot;verses&quot; VALUES (470,20,18,'&lt;J&gt;„Iată&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt;&lt;J&gt;, urcăm la Ierusalim, și Fiul Omului va fi dat în mâinile preoților de seamă și ale cărturarilor. Ei Îl vor osândi la moarte&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,19,'&lt;J&gt;și-L&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;vor da în mâinile păgânilor, ca să-L batjocorească, să-L biciuiască și să-L răstignească, dar a treia zi va învia.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,20,'&lt;pb/&gt;Atunci&lt;f&gt;ⓘ&lt;/f&gt;, mama fiilor&lt;f&gt;ⓙ&lt;/f&gt; lui Zebedei s-a apropiat de &lt;i&gt;Isus&lt;/i&gt; împreună cu fiii ei și I s-a închinat, vrând să-I facă o cerere.');
INSERT INTO &quot;verses&quot; VALUES (470,20,21,'El a întrebat-o: &lt;J&gt;„Ce vrei?”&lt;/J&gt; Ea a zis: „Poruncește ca, în Împărăția Ta, acești doi fii ai mei să stea&lt;f&gt;ⓚ&lt;/f&gt; unul la dreapta și altul la stânga Ta!”');
INSERT INTO &quot;verses&quot; VALUES (470,20,22,'Drept răspuns, Isus le-a zis: &lt;J&gt;„Nu știți ce cereți! Puteți voi să beți paharul&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;pe care îl voi bea Eu [sau să fiți botezați cu botezul&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;cu care voi fi botezat Eu]?”&lt;/J&gt; „Putem”, I-au zis ei.');
INSERT INTO &quot;verses&quot; VALUES (470,20,23,'Atunci El le-a răspuns: &lt;J&gt;„&lt;i&gt;Da,&lt;/i&gt; veți bea paharul Meu&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;[și veți fi botezați cu botezul cu care voi fi botezat Eu], dar cine stă la dreapta și la stânga Mea nu Eu pot&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;hotărî, ci &lt;i&gt;acest lucru&lt;/i&gt; este pentru aceia pentru care a fost pregătit de Tatăl Meu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,24,'&lt;pb/&gt;Auzind &lt;i&gt;aceasta&lt;/i&gt;, ceilalți zece s-au mâniat pe cei doi frați.');
INSERT INTO &quot;verses&quot; VALUES (470,20,25,'Dar Isus i-a chemat la Sine și le-a zis: &lt;J&gt;„Știți că domnitorii popoarelor domnesc peste ele și mai-marii lor stăpânesc asupra lor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,26,'&lt;J&gt;Dar între&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;voi să nu fie așa. Cine&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;vrea să fie mare între voi să fie slujitorul vostru;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,27,'&lt;J&gt;și&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;cine vrea să fie cel dintâi între voi să fie robul vostru.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,28,'&lt;J&gt;Pentru&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;că nici Fiul&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;Omului n-a venit să I se slujească, ci&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;El să slujească și să-Și&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;dea viața ca răscumpărare pentru&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;mulți.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,29,'&lt;pb/&gt;Când&lt;f&gt;ⓧ&lt;/f&gt; au plecat ei din Ierihon, o mare mulțime a mers după &lt;i&gt;Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,20,30,'Și iată, doi&lt;f&gt;ⓨ&lt;/f&gt; orbi care stăteau lângă drum au auzit că trece Isus și au strigat, zicând: „Ai milă de noi, Doamne, Fiul lui David!”');
INSERT INTO &quot;verses&quot; VALUES (470,20,31,'Mulțimea îi certa să tacă, dar ei mai tare strigau, zicând: „Ai milă de noi, Doamne, Fiul lui David!”');
INSERT INTO &quot;verses&quot; VALUES (470,20,32,'Isus S-a oprit, i-a chemat și le-a zis: &lt;J&gt;„Ce vreți să fac pentru voi?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,20,33,'„Doamne”, I-au răspuns ei, „&lt;i&gt;vrem&lt;/i&gt; să ni se deschidă ochii!”');
INSERT INTO &quot;verses&quot; VALUES (470,20,34,'Cuprins de milă, Isus le-a atins ochii și îndată &lt;i&gt;orbii&lt;/i&gt; au început să vadă și au mers după El.');
INSERT INTO &quot;verses&quot; VALUES (470,21,1,'&lt;pb/&gt;Când&lt;f&gt;ⓩ&lt;/f&gt; s-au apropiat de Ierusalim și au ajuns la Betfaghe, înspre Muntele&lt;f&gt;ⓐ&lt;/f&gt; Măslinilor, Isus a trimis doi ucenici');
INSERT INTO &quot;verses&quot; VALUES (470,21,2,'și le-a spus: &lt;J&gt;„Mergeți în satul de dinainte și veți găsi îndată o măgăriță legată și un mânz împreună cu ea; dezlegați-i și aduceți-i la Mine!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,3,'&lt;J&gt;Dacă vă întreabă cineva, să spuneți: «Domnul are nevoie de ei.» Și îi va trimite îndată.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,4,'Aceasta s-a petrecut ca să se împlinească ce fusese vestit prin profetul care zice:');
INSERT INTO &quot;verses&quot; VALUES (470,21,5,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Spuneți&lt;/e&gt; &lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;fiicei Sionului:&lt;/e&gt;&lt;f&gt;[86]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;„Iată, Regele tău vine la tine,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;blând și călare pe un măgar,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;pe un măgăruș, mânzul celei de sub jug.”&lt;/e&gt; &lt;f&gt;[87]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,6,'&lt;pb/&gt;Ucenicii&lt;f&gt;ⓒ&lt;/f&gt; s-au dus și au făcut după cum le poruncise Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,21,7,'Au adus măgărița și măgărușul, și-au&lt;f&gt;ⓓ&lt;/f&gt; pus hainele peste ei și El S-a așezat deasupra.');
INSERT INTO &quot;verses&quot; VALUES (470,21,8,'Majoritatea &lt;i&gt;oamenilor&lt;/i&gt; din mulțime își așterneau hainele pe drum; alții&lt;f&gt;ⓔ&lt;/f&gt; tăiau ramuri din copaci și le așterneau pe drum.');
INSERT INTO &quot;verses&quot; VALUES (470,21,9,'Mulțimile care mergeau înaintea lui &lt;i&gt;Isus&lt;/i&gt; și cele care-L urmau strigau: &lt;pb/&gt;&lt;t&gt;„&lt;e&gt;Osana&lt;/e&gt; Fiului lui David!&lt;/t&gt; &lt;t&gt;&lt;e&gt;Binecuvântat&lt;/e&gt; &lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;este Cel ce vine în Numele Domnului!&lt;/e&gt;&lt;f&gt;[88]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Osana&lt;/e&gt; în înălțimi!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,10,'&lt;pb/&gt;Când&lt;f&gt;ⓖ&lt;/f&gt; a intrat &lt;i&gt;Isus&lt;/i&gt; în Ierusalim, toată cetatea a început să freamăte și &lt;i&gt;toți&lt;/i&gt; se întrebau: „Cine este acesta?”');
INSERT INTO &quot;verses&quot; VALUES (470,21,11,'„Este profetul&lt;f&gt;ⓗ&lt;/f&gt; Isus, din Nazaretul Galileei”, răspundeau mulțimile.');
INSERT INTO &quot;verses&quot; VALUES (470,21,12,'&lt;pb/&gt;Isus a intrat&lt;f&gt;ⓘ&lt;/f&gt; în Templul [lui Dumnezeu], i-a scos afară pe toți cei ce vindeau și cumpărau în Templu, a răsturnat mesele schimbătorilor&lt;f&gt;ⓙ&lt;/f&gt; de bani și scaunele celor ce vindeau porumbei');
INSERT INTO &quot;verses&quot; VALUES (470,21,13,'și le-a zis: &lt;J&gt;„Este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;Casa Mea se va numi o casă de rugăciune.&lt;/e&gt;&lt;/J&gt;&lt;f&gt;[89]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;voi faceți din ea &lt;e&gt;o peșteră de tâlhari&lt;/e&gt;.”&lt;/J&gt;&lt;f&gt;[90]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,14,'&lt;pb/&gt;Au venit la El în Templu niște orbi și șchiopi, iar El i-a vindecat.');
INSERT INTO &quot;verses&quot; VALUES (470,21,15,'Însă preoții de seamă și cărturarii, văzând minunile pe care le făcea, precum și pe copii strigând în Templu și zicând: „Osana Fiului lui David!”, s-au mâniat');
INSERT INTO &quot;verses&quot; VALUES (470,21,16,'și I-au zis: „Auzi ce zic aceștia?” Isus le-a răspuns: &lt;J&gt;„Da. Oare n-ați citit niciodată: &lt;e&gt;Din gura pruncilor și a sugarilor Ți-ai scos laudă&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[91]&lt;/f&gt;&lt;J&gt;?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,17,'El i-a lăsat, a ieșit din cetate spre Betania&lt;f&gt;ⓛ&lt;/f&gt; și a înnoptat acolo.');
INSERT INTO &quot;verses&quot; VALUES (470,21,18,'&lt;pb/&gt;Dimineața&lt;f&gt;ⓜ&lt;/f&gt;, pe când Se întorcea în cetate, I s-a făcut foame.');
INSERT INTO &quot;verses&quot; VALUES (470,21,19,'Văzând&lt;f&gt;ⓝ&lt;/f&gt; un smochin lângă drum, S-a apropiat de el, dar nu a găsit nimic decât frunze și i-a zis: &lt;J&gt;„În veac să nu &lt;i&gt;mai&lt;/i&gt; dea rod din tine!”&lt;/J&gt; Și smochinul s-a uscat pe loc.');
INSERT INTO &quot;verses&quot; VALUES (470,21,20,'Când&lt;f&gt;ⓞ&lt;/f&gt; au văzut &lt;i&gt;lucrul acesta&lt;/i&gt;, ucenicii au rămas uimiți și au zis: „Cum de s-a uscat smochinul pe loc?”');
INSERT INTO &quot;verses&quot; VALUES (470,21,21,'Isus le-a răspuns: &lt;J&gt;„Adevărat vă spun că, dacă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;veți avea credință și nu&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;vă veți îndoi, veți face nu numai &lt;i&gt;ce am făcut Eu&lt;/i&gt; smochinului, ci&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;îi veți zice acestui munte: «Ridică-te și aruncă-te în mare!» și se va face.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,22,'&lt;J&gt;Tot&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;ce veți cere prin rugăciune, cu credință, veți primi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,23,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; &lt;f&gt;ⓣ&lt;/f&gt; a mers în Templu și, pe când îi învăța &lt;i&gt;pe oameni&lt;/i&gt;, au venit la El preoții de seamă și bătrânii poporului și I-au zis&lt;f&gt;ⓤ&lt;/f&gt;: „Cu ce autoritate faci tu aceste lucruri și cine ți-a dat această autoritate?”');
INSERT INTO &quot;verses&quot; VALUES (470,21,24,'Isus le-a răspuns zicând: &lt;J&gt;„Vă voi întreba și Eu un lucru, iar dacă-Mi veți răspunde, vă voi spune și Eu cu ce autoritate fac toate acestea:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,25,'&lt;J&gt;botezul lui Ioan de unde era — din cer sau de la oameni?”&lt;/J&gt; Ei însă vorbeau între ei și ziceau: „Dacă spunem: «Din cer», ne va zice: «Atunci de ce n-ați crezut în el?»');
INSERT INTO &quot;verses&quot; VALUES (470,21,26,'Dacă vom spune: «De la oameni», ne temem de mulțime, pentru că&lt;f&gt;ⓥ&lt;/f&gt; toți îl consideră pe Ioan profet.”');
INSERT INTO &quot;verses&quot; VALUES (470,21,27,'&lt;i&gt;Așa că&lt;/i&gt; I-au răspuns lui Isus: „Nu știm!” Atunci și El le-a zis: &lt;J&gt;„Nici Eu nu vă spun cu ce autoritate fac aceste lucruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,28,'&lt;pb/&gt;&lt;J&gt;Ce credeți? Un om avea doi fii. Venind la cel dintâi, i-a zis: «Fiule, du-te și lucrează astăzi la vie!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,29,'&lt;J&gt;El i-a răspuns: «Nu vreau», dar mai târziu i-a părut rău și s-a dus.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,30,'&lt;J&gt;A venit și la celălalt și i-a spus la fel. Acesta a răspuns: «Mă &lt;i&gt;duc&lt;/i&gt;, stăpâne», și nu s-a dus.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,31,'&lt;J&gt;Care dintre cei doi a făcut voia tatălui?”&lt;/J&gt; „Cel dintâi”, au răspuns ei. Iar Isus le-a zis: &lt;J&gt;„Adevărat&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;vă spun că vameșii și desfrânatele merg înaintea voastră în Împărăția lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,32,'&lt;J&gt;Fiindcă Ioan&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;a venit la voi pe calea dreptății, și nu i-ați dat crezare, dar&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;vameșii și desfrânatele l-au crezut. Voi însă, deși ați văzut &lt;i&gt;lucrul acesta&lt;/i&gt;, nu v-ați căit mai târziu ca să-l credeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,33,'&lt;pb/&gt;&lt;J&gt;Ascultați o altă pildă! Era &lt;i&gt;odată&lt;/i&gt; un gospodar care&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;e&gt;&lt;J&gt;a sădit o vie, a împrejmuit-o cu un gard, a săpat un teasc în ea și a zidit un turn&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[92]&lt;/f&gt;, &lt;J&gt;apoi a arendat-o unor lucrători și a plecat&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;în călătorie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,34,'&lt;J&gt;Când s-a apropiat vremea roadelor, Și-a trimis robii la lucrători, ca&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;să ia roadele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,35,'&lt;J&gt;Dar lucrătorii&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;i-au luat pe robi și pe unul l-au bătut, pe altul l-au omorât, iar pe altul l-au ucis cu pietre.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,36,'&lt;J&gt;A trimis apoi alți robi, mai mulți decât cei dintâi, dar &lt;i&gt;lucrătorii&lt;/i&gt; le-au făcut la fel.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,37,'&lt;J&gt;În cele din urmă, l-a trimis la ei pe fiul său, zicându-și: «De fiul meu se vor rușina!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,38,'&lt;J&gt;Dar lucrătorii, văzându-l pe fiu, au zis între ei: «Iată-l pe&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;moștenitor; haideți&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;să-l omorâm și să punem mâna pe moștenirea lui!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,39,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;prinzându-l, l-au scos afară din vie și l-au omorât.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,40,'&lt;i&gt;&lt;J&gt;Acum&lt;/i&gt;, când va veni stăpânul viei, ce le va face lucrătorilor acelora?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,41,'Ei I-au răspuns&lt;f&gt;ⓖ&lt;/f&gt;: „Pe ticăloșii aceia ticălos îi va pierde&lt;f&gt;ⓗ&lt;/f&gt;, iar&lt;f&gt;ⓘ&lt;/f&gt; via o va da altor lucrători, care-i vor da roadele la vreme.”');
INSERT INTO &quot;verses&quot; VALUES (470,21,42,'Isus le-a zis: &lt;J&gt;„Oare n-ați&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;citit niciodată în Scripturi:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Piatra pe care au lepădat-o zidarii,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;tocmai ea a ajuns în capul unghiului;&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;de la Domnul s-a făcut lucrul acesta&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și este minunat în ochii noștri&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[93]&lt;/f&gt; &lt;J&gt;?&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,43,'&lt;pb/&gt;&lt;J&gt;De aceea vă spun că&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;Împărăția lui Dumnezeu va fi luată de la voi și va fi dată unui neam care va aduce roade.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,44,'&lt;J&gt;Cine va&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;cădea peste piatra aceasta va&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;fi zdrobit de ea, iar pe acela peste care va cădea ea îl va spulbera.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,21,45,'Când au auzit pildele Lui, preoții de seamă și fariseii au priceput că despre ei vorbea');
INSERT INTO &quot;verses&quot; VALUES (470,21,46,'și căutau să-L prindă, dar se temeau de mulțimi, pentru că ele&lt;f&gt;ⓝ&lt;/f&gt; Îl considerau profet.');
INSERT INTO &quot;verses&quot; VALUES (470,22,1,'&lt;pb/&gt;Isus le-a vorbit&lt;f&gt;ⓞ&lt;/f&gt; iarăși în pilde și a zis:');
INSERT INTO &quot;verses&quot; VALUES (470,22,2,'&lt;J&gt;„Împărăția cerurilor se aseamănă cu un rege care i-a făcut nuntă fiului său.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,3,'&lt;J&gt;I-a trimis pe robii lui să-i cheme pe cei invitați la nuntă, dar ei n-au vrut să vină.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,4,'&lt;J&gt;A trimis apoi alți robi și le-a zis: «Spuneți-le celor chemați: ‘Iată, ospățul meu este pregătit, taurii&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;și &lt;i&gt;vitele&lt;/i&gt; mele cele îngrășate au fost tăiate și toate sunt gata. Veniți la nuntă!’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,5,'&lt;J&gt;Dar ei, fără să le pese, au plecat: unul la ogorul lui, iar altul la negoțul lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,6,'&lt;J&gt;Ceilalți au pus mâna pe robii lui, i-au batjocorit și i-au omorât.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,7,'&lt;J&gt;[Când a auzit &lt;i&gt;aceasta&lt;/i&gt;,] regele s-a mâniat; a trimis oștile&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;sale, i-a nimicit pe ucigașii aceia și le-a pârjolit cetatea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,8,'&lt;J&gt;Apoi le-a zis robilor săi: «Nunta este pregătită, dar cei chemați n-au fost vrednici&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,9,'&lt;J&gt;Mergeți, așadar, la răscrucile drumurilor și chemați-i la nuntă pe toți aceia pe care-i veți găsi!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,10,'&lt;J&gt;Robii aceia au ieșit pe drumuri, i-au&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;strâns pe toți aceia pe care i-au găsit — și răi, și buni — iar camera de nuntă s-a umplut de oaspeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,11,'&lt;J&gt;Dar când a intrat regele să-i vadă pe oaspeți, a zărit acolo un om care&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;nu era îmbrăcat în haină de nuntă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,12,'&lt;J&gt;«Prietene», i-a zis el, «cum ai intrat aici fără să ai haină de nuntă?» &lt;i&gt;Omul&lt;/i&gt; acela a amuțit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,13,'&lt;J&gt;Atunci regele le-a zis slujitorilor: «Legați-l de mâini și de picioare, [luați-l] și aruncați-l în&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;întunericul de afară! Acolo va fi plânsul și scrâșnirea dinților.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,14,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;mulți sunt chemați, dar puțini &lt;i&gt;sunt&lt;/i&gt; aleși.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,15,'&lt;pb/&gt;Atunci&lt;f&gt;ⓦ&lt;/f&gt;, fariseii s-au dus și s-au sfătuit cum să-L prindă &lt;i&gt;pe Isus&lt;/i&gt; cu vorba.');
INSERT INTO &quot;verses&quot; VALUES (470,22,16,'I-au trimis la El pe ucenicii lor împreună cu irodienii și I-au zis: „Învățătorule, știm că ești adevărat și că-i înveți &lt;i&gt;pe oameni&lt;/i&gt; calea lui Dumnezeu după adevăr, fără să ții seama de nimeni, fiindcă nu cauți la fața oamenilor.');
INSERT INTO &quot;verses&quot; VALUES (470,22,17,'Spune-ne, ce crezi: este îngăduit să-i dăm bir cezarului sau nu?”');
INSERT INTO &quot;verses&quot; VALUES (470,22,18,'Isus, cunoscându-le răutatea, le-a răspuns: &lt;J&gt;„Pentru ce Mă ispitiți, fățarnicilor?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,19,'&lt;J&gt;Arătați-Mi banul pentru bir!”&lt;/J&gt; Și ei I-au adus un dinar.');
INSERT INTO &quot;verses&quot; VALUES (470,22,20,'El i-a întrebat: &lt;J&gt;„Chipul acesta și inscripția ale cui sunt?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,21,'„Ale cezarului”, I-au răspuns ei. Atunci El le-a zis: &lt;J&gt;„Dați-i&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;dar cezarului ce este al cezarului și lui Dumnezeu ce este al lui Dumnezeu!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,22,'Când au auzit ei, au rămas uimiți și, lăsându-L, au plecat.');
INSERT INTO &quot;verses&quot; VALUES (470,22,23,'&lt;pb/&gt;În aceeași&lt;f&gt;ⓨ&lt;/f&gt; zi, au venit la &lt;i&gt;Isus&lt;/i&gt; saducheii, care&lt;f&gt;ⓩ&lt;/f&gt; zic că nu este înviere, și L-au întrebat:');
INSERT INTO &quot;verses&quot; VALUES (470,22,24,'„Învățătorule, Moise a zis: «Dacă moare cineva fără să aibă copii, fratele lui să se însoare cu soția &lt;i&gt;văduvă&lt;/i&gt; și să-i ridice urmaș fratelui său.»&lt;f&gt;[94]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,25,'Erau la noi șapte frați. Primul s-a însurat, apoi a murit și, neavând urmași, i-a lăsat-o fratelui său pe soția lui.');
INSERT INTO &quot;verses&quot; VALUES (470,22,26,'La fel și al doilea, și al treilea, până la al șaptelea.');
INSERT INTO &quot;verses&quot; VALUES (470,22,27,'După ei toți, a murit și femeia.');
INSERT INTO &quot;verses&quot; VALUES (470,22,28,'La înviere, soția căruia dintre cei șapte va fi ea? Căci toți au avut-o &lt;i&gt;de soție&lt;/i&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (470,22,29,'Drept răspuns, Isus le-a zis: &lt;J&gt;„Vă rătăciți pentru că nu cunoașteți&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;nici Scripturile, nici puterea lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,30,'&lt;J&gt;Căci, la înviere, nici nu se vor însura, nici nu se vor mărita, ci vor fi&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;ca îngerii [lui Dumnezeu] în cer.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,31,'&lt;J&gt;Cât despre învierea morților, n-ați citit cele spuse vouă de către Dumnezeu, când zice:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,32,'&lt;e&gt;&lt;J&gt;Eu&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;&lt;J&gt;sunt Dumnezeul lui Avraam, Dumnezeul lui Isaac și Dumnezeul lui Iacov&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[95]&lt;/f&gt;&lt;J&gt;? El nu este Dumnezeul celor morți, ci al celor vii.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,33,'Mulțimile care-L ascultau erau&lt;f&gt;ⓓ&lt;/f&gt; uimite de învățătura Lui.');
INSERT INTO &quot;verses&quot; VALUES (470,22,34,'&lt;pb/&gt;Fariseii, auzind&lt;f&gt;ⓔ&lt;/f&gt; că &lt;i&gt;Isus&lt;/i&gt; le-a închis gura saducheilor, s-au adunat laolaltă');
INSERT INTO &quot;verses&quot; VALUES (470,22,35,'și unul dintre ei, un învățător al Legii&lt;f&gt;ⓕ&lt;/f&gt;, L-a întrebat, ca să-L ispitească:');
INSERT INTO &quot;verses&quot; VALUES (470,22,36,'„Învățătorule, care este cea mai mare poruncă din Lege?”');
INSERT INTO &quot;verses&quot; VALUES (470,22,37,'El i-a răspuns: &lt;J&gt;„&lt;e&gt;Să-L&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;&lt;J&gt;iubești pe Domnul Dumnezeul tău cu toată inima ta, cu tot sufletul tău și cu tot cugetul tău.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[96]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,38,'&lt;J&gt;Aceasta este cea mai mare și cea dintâi poruncă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,39,'&lt;J&gt;Iar a doua, asemenea ei, &lt;i&gt;este&lt;/i&gt;: &lt;e&gt;Să-l&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;&lt;J&gt;iubești pe aproapele tău ca pe tine însuți.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[97]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,40,'&lt;J&gt;De aceste&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;două porunci atârnă toată Legea și Profeții.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,41,'&lt;pb/&gt;Pe&lt;f&gt;ⓙ&lt;/f&gt; când erau fariseii adunați la un loc, Isus i-a întrebat:');
INSERT INTO &quot;verses&quot; VALUES (470,22,42,'&lt;J&gt;„Ce credeți voi despre Mesia&lt;/J&gt;&lt;f&gt;[98]&lt;/f&gt;&lt;J&gt;? Al cui fiu este?”&lt;/J&gt; „Al lui David”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (470,22,43,'Și &lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Atunci cum &lt;i&gt;se face că&lt;/i&gt; David, prin Duhul, Îl numește Domn, când zice:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,44,'&lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;Domnul&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;e&gt;&lt;J&gt;a zis Domnului Meu:&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;«Șezi la dreapta Mea&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;până voi pune pe vrăjmașii Tăi&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;sub picioarele Tale»&lt;/e&gt;&lt;/J&gt;&lt;f&gt;[99]&lt;/f&gt; &lt;J&gt;?&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,45,'&lt;pb/&gt;&lt;J&gt;Deci, dacă David Îl numește Domn, cum este fiul lui?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,22,46,'Nimeni&lt;f&gt;ⓛ&lt;/f&gt; n-a putut să-I răspundă vreun cuvânt și, din ziua aceea, n-a&lt;f&gt;ⓜ&lt;/f&gt; mai îndrăznit nimeni să-I pună alte întrebări.');
INSERT INTO &quot;verses&quot; VALUES (470,23,1,'&lt;pb/&gt;Atunci, Isus le-a vorbit mulțimilor și ucenicilor Săi');
INSERT INTO &quot;verses&quot; VALUES (470,23,2,'și le-a zis: &lt;J&gt;„Cărturarii&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;și fariseii stau pe scaunul lui Moise.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,3,'&lt;J&gt;Deci toate lucrurile pe care vi le spun ei păziți-le și faceți-le, dar după faptele lor să nu faceți! Căci ei&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;zic, dar nu fac.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,4,'&lt;J&gt;Leagă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;poveri grele și anevoie de purtat și le pun pe umerii oamenilor, dar ei nici cu un deget nu vor să le miște.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,5,'&lt;J&gt;Toate faptele lor&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;le fac pentru a fi văzuți de oameni: își fac filacterii mai late și își&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;măresc ciucurii &lt;i&gt;de la veșminte&lt;/i&gt;,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,6,'&lt;J&gt;le plac&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;locurile de cinste la ospețe, scaunele dintâi în sinagogi,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,7,'&lt;J&gt;saluturile prin piețe și &lt;i&gt;așteaptă&lt;/i&gt; să li se spună &lt;e&gt;rabbí&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[100]&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,8,'&lt;pb/&gt;&lt;J&gt;Dar voi&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;să nu vă lăsați numiți &lt;e&gt;rabbí&lt;/e&gt;. Fiindcă&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;Unul &lt;i&gt;singur&lt;/i&gt; este Învățătorul vostru [ — Hristos — ], iar voi toți sunteți frați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,9,'&lt;J&gt;Nici «tată» să nu numiți &lt;i&gt;pe nimeni&lt;/i&gt; pe pământ, pentru că Unul &lt;i&gt;singur&lt;/i&gt; este Tatăl vostru — Acela care este în ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,10,'&lt;J&gt;Nici «povățuitori» să nu vă numiți &lt;i&gt;între voi&lt;/i&gt;, căci Unul &lt;i&gt;singur&lt;/i&gt; vă este Povățuitor — Hristos.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,11,'&lt;J&gt;Cel&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;mai mare între voi să fie slujitorul vostru.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,12,'&lt;J&gt;Oricine&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;se va înălța va fi smerit, și oricine se va smeri va fi înălțat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,13,'&lt;pb/&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;de voi, cărturari și farisei fățarnici, pentru că închideți Împărăția cerurilor în fața oamenilor: nici voi nu intrați &lt;i&gt;în ea&lt;/i&gt;, nici pe cei care &lt;i&gt;vor&lt;/i&gt; să intre nu-i lăsați să intre! [&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,14,'&lt;J&gt;Vai de voi, cărturari și farisei fățarnici, pentru că&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;mâncați casele văduvelor, iar de ochii lumii faceți rugăciuni lungi; de aceea veți primi o mai mare osândă.]&lt;/J&gt;&lt;f&gt;[101]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,15,'&lt;J&gt;Vai de voi, cărturari și farisei fățarnici, pentru că străbateți marea și uscatul ca să faceți un prozelit, iar mai apoi faceți din el un fiu al gheenei de două ori mai rău decât voi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,16,'&lt;J&gt;Vai de voi, călăuze&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;oarbe, care ziceți: «Nu este nimic dacă jură cineva&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;pe Templu, dar dacă jură pe aurul Templului, este dator»!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,17,'&lt;J&gt;Nebuni și orbi! Ce este mai important: aurul sau&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;Templul, care sfințește aurul?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,18,'&lt;J&gt;Și «nu este nimic dacă jură cineva pe altar, dar dacă jură pe darul de pe el, este dator.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,19,'&lt;J&gt;[Nebuni și] orbi! Ce este mai important: darul sau altarul&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt;&lt;J&gt;, care sfințește darul?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,20,'&lt;J&gt;Așadar, cine jură pe altar jură pe el și pe tot ce este pe el;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,21,'&lt;J&gt;și cine jură pe Templu jură pe el și pe Cel care&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;locuiește în el;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,22,'&lt;J&gt;și cine jură pe cer jură pe tronul&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;lui Dumnezeu și pe Cel ce stă pe el.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,23,'&lt;pb/&gt;&lt;J&gt;Vai de voi, cărturari și farisei fățarnici, pentru că voi&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;dați zeciuială din mentă, mărar și chimen, dar lăsați deoparte&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;cele mai însemnate lucruri din Lege: dreptatea, mila și credința! Pe acestea trebuia să le faceți, și pe acelea să nu le lăsați nefăcute!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,24,'&lt;J&gt;Călăuze oarbe, care strecurați țânțarul, dar înghițiți cămila!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,25,'&lt;pb/&gt;&lt;J&gt;Vai de voi, cărturari și farisei fățarnici, pentru că&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;voi curățați partea din afară a paharului și a farfuriei, dar pe dinăuntru sunt pline de lăcomie și de nestăpânire!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,26,'&lt;J&gt;Fariseu orb, curăță mai întâi partea dinăuntru a paharului [și a farfuriei], pentru ca și partea din afară să fie curată!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,27,'&lt;pb/&gt;&lt;J&gt;Vai de voi, cărturari și farisei fățarnici, pentru că&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;vă asemănați cu niște morminte văruite, care pe din afară par frumoase, dar pe dinăuntru sunt pline de oasele morților și de toată necurăția!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,28,'&lt;J&gt;La fel și voi, pe din afară păreți drepți înaintea oamenilor, dar pe dinăuntru sunteți plini de fățărnicie și nelegiuire.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,29,'&lt;pb/&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;de voi, cărturari și farisei fățarnici, pentru că voi zidiți mormintele profeților, împodobiți criptele drepților&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,30,'&lt;J&gt;și ziceți: «Dacă am fi trăit noi în zilele părinților noștri, nu ne-am fi făcut părtași cu ei &lt;i&gt;la vărsarea&lt;/i&gt; sângelui profeților»!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,31,'&lt;J&gt;Mărturisiți astfel despre voi înșivă că sunteți&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;fiii celor ce i-au ucis pe profeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,32,'&lt;J&gt;Umpleți dar&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;măsura părinților voștri!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,33,'&lt;J&gt;Șerpi, pui&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;de vipere, cum veți scăpa de osânda gheenei?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,34,'&lt;J&gt;De aceea&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;, iată că vă trimit profeți, înțelepți și cărturari. Pe&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;unii dintre ei îi veți omorî și-i veți răstigni, pe&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;alții îi veți biciui în sinagogile voastre și-i veți prigoni din cetate în cetate,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,35,'&lt;J&gt;ca să&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;vină astfel asupra voastră tot sângele nevinovat care s-a vărsat pe pământ, de la&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;sângele dreptului Abel până la sângele&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;lui Zaharia, fiul lui Barachia, pe care l-ați omorât între Templu și altar.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,36,'&lt;J&gt;Adevărat vă spun că toate acestea vor veni peste generația aceasta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,37,'&lt;pb/&gt;&lt;J&gt;Ierusalime&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;, Ierusalime, care-i omori pe profeți și-i&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;ucizi cu pietre pe cei trimiși la tine, de câte ori n-am vrut să-i&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;adun pe copiii tăi cum își adună cloșca puii sub&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;aripi, dar voi n-ați vrut!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,38,'&lt;J&gt;Iată că vă rămâne casa pustie;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,23,39,'&lt;J&gt;căci vă spun că nu Mă veți mai vedea până când veți zice:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;«Binecuvântat&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;i&gt;&lt;e&gt;&lt;J&gt;este&lt;/i&gt; Cel ce vine în Numele Domnului!»&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[102]&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,1,'&lt;pb/&gt;Ieșind&lt;f&gt;ⓨ&lt;/f&gt; din Templu, Isus a plecat, iar ucenicii Lui s-au apropiat de El ca să-I arate clădirile Templului.');
INSERT INTO &quot;verses&quot; VALUES (470,24,2,'Dar &lt;i&gt;Isus&lt;/i&gt; le-a răspuns: &lt;J&gt;„Vedeți voi toate acestea? Adevărat vă spun că nu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;va rămâne aici piatră peste piatră care să nu fie dărâmată!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,3,'Când S-a așezat El pe Muntele Măslinilor, ucenicii&lt;f&gt;ⓐ&lt;/f&gt; Lui au venit la El deoparte și I-au zis: „Spune-ne&lt;f&gt;ⓑ&lt;/f&gt;, când se vor întâmpla &lt;i&gt;toate&lt;/i&gt; acestea și care este semnul venirii Tale și al sfârșitului veacului?”');
INSERT INTO &quot;verses&quot; VALUES (470,24,4,'Isus le-a răspuns: &lt;J&gt;„Luați seama&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;să nu vă înșele cineva.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,5,'&lt;J&gt;Vor&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;veni mulți în Numele Meu, zicând: «Eu sunt Hristosul&lt;/J&gt;&lt;f&gt;[103]&lt;/f&gt;&lt;J&gt;», și&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;îi vor înșela pe mulți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,6,'&lt;J&gt;Veți auzi despre războaie și zvonuri de războaie: vedeți să nu vă înspăimântați, &lt;i&gt;căci&lt;/i&gt; [toate] &lt;i&gt;acestea&lt;/i&gt; trebuie să se întâmple, dar încă nu va fi sfârșitul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,7,'&lt;J&gt;Un&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;popor se va ridica împotriva altui popor, și o împărăție — împotriva altei împărății; și pe alocuri vor fi cutremure de pământ, foamete [și molime].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,8,'&lt;J&gt;Dar toate acestea &lt;i&gt;sunt doar&lt;/i&gt; începutul durerilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,9,'&lt;pb/&gt;&lt;J&gt;Atunci&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;vă vor da să fiți chinuiți și vă vor omorî; veți fi urâți de toate neamurile din pricina Numelui Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,10,'&lt;J&gt;Și atunci mulți se vor&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;poticni, se vor vinde și se vor urî unii pe alții.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,11,'&lt;J&gt;Mulți&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;profeți mincinoși se vor ridica și-i vor&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;înșela pe mulți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,12,'&lt;J&gt;Și din pricina înmulțirii fărădelegii, dragostea &lt;i&gt;celor mai&lt;/i&gt; mulți se va răci.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,13,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;cine va răbda până la sfârșit va fi mântuit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,14,'&lt;J&gt;Evanghelia&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;aceasta a Împărăției va&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;fi propovăduită în toată lumea, ca mărturie pentru toate neamurile. Atunci va veni sfârșitul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,15,'&lt;pb/&gt;&lt;J&gt;De aceea&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;, când veți vedea &lt;e&gt;urâciunea pustiirii&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[104]&lt;/f&gt; &lt;J&gt;, despre care a vorbit profetul Daniel, așezată în &lt;e&gt;Locul Sfânt&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[105]&lt;/f&gt; — cine&lt;f&gt;ⓞ&lt;/f&gt; citește să înțeleagă —');
INSERT INTO &quot;verses&quot; VALUES (470,24,16,'&lt;J&gt;atunci cei din Iudeea să fugă în munți,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,17,'&lt;J&gt;cine va fi pe acoperiș să nu coboare &lt;i&gt;ca să mai&lt;/i&gt; ia ceva din casă&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,18,'&lt;J&gt;și cine &lt;i&gt;va fi&lt;/i&gt; la câmp să nu se întoarcă în urmă să-și ia haina.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,19,'&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;de femeile însărcinate și de cele care alăptează în zilele acelea!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,20,'&lt;pb/&gt;&lt;J&gt;Rugați-vă ca fuga voastră să nu fie iarna, nici într-o zi de sabat!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,21,'&lt;J&gt;Pentru că&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;atunci va fi &lt;e&gt;o strâmtorare&lt;/e&gt; mare, &lt;e&gt;cum n-a mai fost de la începutul lumii până acum&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[106]&lt;/f&gt; &lt;J&gt;și nici nu va mai fi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,22,'&lt;J&gt;Și dacă n-ar fi scurtate zilele acelea, n-ar scăpa nicio făptură; însă&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;, pentru cei aleși, zilele acelea vor fi scurtate.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,23,'&lt;pb/&gt;&lt;J&gt;Atunci&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;, dacă vă va spune cineva: «Iată, Hristosul&lt;/J&gt;&lt;f&gt;[107]&lt;/f&gt; &lt;J&gt;este aici!» sau «&lt;i&gt;Iată-l&lt;/i&gt; acolo!», să nu credeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,24,'&lt;J&gt;Căci se&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;vor ridica hristoși mincinoși și profeți mincinoși și vor face semne mari și minuni, ca să-i înșele, dacă este cu putință, chiar și pe cei aleși&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,25,'&lt;J&gt;Iată, v-am spus dinainte.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,26,'&lt;J&gt;Dacă vă vor zice: «Iată-l în pustiu», să nu vă duceți! «Iată-l în încăperi ascunse», să nu credeți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,27,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt;&lt;J&gt;, cum iese fulgerul de la răsărit și se vede până la apus, așa va fi și venirea Fiului Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,28,'&lt;J&gt;Unde&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;va fi stârvul, acolo se vor aduna vulturii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,29,'&lt;pb/&gt;&lt;J&gt;Îndată&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;după necazul din acele zile,&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;soarele se&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;&lt;J&gt;va întuneca,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;iar luna nu-și va mai da lumina;&lt;/e&gt;&lt;/J&gt;&lt;f&gt;[108]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;stelele vor cădea&lt;/e&gt;&lt;/J&gt;&lt;f&gt;[109]&lt;/f&gt; &lt;J&gt;din cer&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;și puterile cerurilor&lt;/J&gt;&lt;f&gt;[110]&lt;/f&gt; &lt;J&gt;vor fi clătinate.&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,30,'&lt;pb/&gt;&lt;J&gt;Atunci se va arăta pe cer semnul Fiului Omului; toate&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;neamurile pământului se vor tângui și-L&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;vor vedea &lt;e&gt;pe Fiul Omului venind pe norii cerului&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[111]&lt;/f&gt; &lt;J&gt;cu putere și slavă multă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,31,'&lt;J&gt;El&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;îi va trimite pe îngerii Săi cu o trâmbiță mare și-i vor aduna pe aleșii Lui din cele patru vânturi, de la un capăt la altul al cerurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,32,'&lt;pb/&gt;&lt;J&gt;Învățați de la smochin pilda &lt;i&gt;lui&lt;/J&gt;&lt;/i&gt; &lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;: când mlădița i se face fragedă și înfrunzește, știți că&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;vara este aproape.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,33,'&lt;J&gt;Tot așa și voi, când veți vedea toate acestea, să știți că &lt;i&gt;Fiul Omului&lt;/i&gt; este aproape, &lt;i&gt;este chiar&lt;/i&gt; la ușă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,34,'&lt;J&gt;Adevărat vă spun că nicidecum nu&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;va trece generația aceasta că se vor și împlini aceste lucruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,35,'&lt;J&gt;Cerul&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;și pământul vor trece, dar cuvintele Mele nu vor trece.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,36,'&lt;pb/&gt;&lt;J&gt;Însă despre&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;ziua aceea și ceasul acela nu știe nimeni: nici îngerii din ceruri, nici Fiul, ci numai&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;Tatăl.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,37,'&lt;J&gt;Cum &lt;i&gt;a fost&lt;/i&gt; în zilele lui Noe, la fel va fi și la venirea Fiului Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,38,'&lt;J&gt;Cum&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;era în zilele de dinainte de potop, când mâncau și beau, se însurau și se măritau, până în ziua când a intrat Noe în arcă,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,39,'&lt;J&gt;și n-au știut nimic până când a venit potopul și i-a luat pe toți, la fel va fi și la venirea Fiului Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,40,'&lt;J&gt;Atunci&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;din doi care vor fi la câmp unul va fi luat și altul va fi lăsat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,41,'&lt;J&gt;Din două &lt;i&gt;femei&lt;/i&gt; care vor măcina la moară, una va fi luată și alta va fi lăsată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,42,'&lt;pb/&gt;&lt;J&gt;Vegheați, așadar&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;, pentru că nu știți în ce zi vine Domnul vostru!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,43,'&lt;J&gt;Să știți&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;că, dacă stăpânul casei ar ști la ce strajă&lt;/J&gt;&lt;f&gt;[112]&lt;/f&gt; &lt;J&gt;vine hoțul, ar veghea și n-ar lăsa să-i fie spartă casa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,44,'&lt;J&gt;De aceea&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt;&lt;J&gt;, și voi fiți gata, căci Fiul Omului vine în ceasul în care nici nu vă gândiți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,45,'&lt;pb/&gt;&lt;J&gt;Cine este deci&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;robul credincios și chibzuit pe care l-a pus stăpânul său peste slugile sale, ca să le dea hrana la vremea cuvenită?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,46,'&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;de robul acela pe care stăpânul său, atunci când va veni, îl va găsi făcând astfel!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,47,'&lt;J&gt;Adevărat vă spun că&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;îl va pune peste toate avuțiile sale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,48,'&lt;J&gt;Dar dacă este un rob rău, care zice în inima lui: «Stăpânul meu întârzie»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,49,'&lt;J&gt;și începe să-i bată pe tovarășii lui, să mănânce și să bea cu bețivii,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,50,'&lt;J&gt;stăpânul robului aceluia va veni în ziua în care el nu se așteaptă și în ceasul pe care nu-l știe,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,24,51,'&lt;J&gt;îl va tăia în bucăți, și soarta lui va fi soarta fățarnicilor&lt;/J&gt;&lt;f&gt;[113]&lt;/f&gt;&lt;J&gt;; acolo&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;va fi plânsul și scrâșnirea dinților.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,1,'&lt;pb/&gt;&lt;J&gt;Atunci, Împărăția cerurilor se va asemăna cu zece fecioare care și-au luat candelele și au ieșit în întâmpinarea mirelui&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,2,'&lt;J&gt;Cinci&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;dintre ele erau nechibzuite, iar cinci — chibzuite.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,3,'&lt;J&gt;Cele nechibzuite, când și-au luat candelele, n-au luat cu ele ulei,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,4,'&lt;J&gt;dar cele chibzuite, împreună cu candelele, au luat și ulei în vase.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,5,'&lt;J&gt;Fiindcă mirele întârzia, au&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;ațipit toate și au adormit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,6,'&lt;J&gt;La miezul nopții, s-a&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;auzit un strigăt: «Iată mirele! Ieșiți-i în întâmpinare!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,7,'&lt;J&gt;Atunci, toate fecioarele s-au sculat și și-au&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;pregătit candelele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,8,'&lt;J&gt;Cele nechibzuite le-au zis celor chibzuite: «Dați-ne din uleiul vostru, căci ni se sting candelele!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,9,'&lt;J&gt;Cele chibzuite le-au răspuns: «Nu, ca nu cumva să nu ne ajungă nici nouă, nici vouă. Mai bine duceți-vă la cei ce vând &lt;i&gt;ulei&lt;/i&gt; și cumpărați-vă!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,10,'&lt;J&gt;Pe când erau ele plecate să cumpere, a venit mirele; cele care erau pregătite au intrat cu el la nuntă, și ușa s-a&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;închis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,11,'&lt;J&gt;Mai târziu au venit și celelalte fecioare și au zis: «Stăpâne&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt;&lt;J&gt;, stăpâne, deschide-ne!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,12,'&lt;J&gt;Dar el, drept răspuns, le-a zis: «Adevărat vă spun că nu&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;vă cunosc.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,13,'&lt;J&gt;Vegheați, așadar&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt;&lt;J&gt;, căci nu știți nici ziua, nici ceasul [în care va veni Fiul Omului].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,14,'&lt;pb/&gt;&lt;J&gt;Tot astfel&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;, un om care urma să plece în călătorie și-a chemat robii și le-a încredințat averea sa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,15,'&lt;J&gt;Unuia i-a dat cinci talanți, altuia — doi și altuia — unu, fiecăruia&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;după puterea lui, și a plecat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,16,'&lt;J&gt;Cel care primise cinci talanți s-a dus, i-a pus în negoț și a câștigat cu ei alți cinci [talanți].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,17,'&lt;J&gt;La fel și cel cu doi talanți: a câștigat [și el] alți doi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,18,'&lt;J&gt;Dar cel care primise un &lt;i&gt;talant&lt;/i&gt; s-a dus, a săpat &lt;i&gt;o groapă&lt;/i&gt; în pământ și a ascuns &lt;i&gt;acolo&lt;/i&gt; banii stăpânului său.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,19,'&lt;J&gt;După multă vreme, stăpânul robilor acelora s-a întors și le-a cerut socoteală.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,20,'&lt;J&gt;Cel care primise cinci talanți a venit și a adus alți cinci talanți, zicând: «Stăpâne, mi-ai încredințat cinci talanți; iată, am câștigat [cu ei] alți cinci talanți!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,21,'&lt;J&gt;Stăpânul său i-a zis: «Bine, rob bun și credincios! Ai fost credincios peste puține &lt;i&gt;lucruri&lt;/i&gt;, te voi pune peste&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;multe &lt;i&gt;lucruri&lt;/i&gt;; intră în bucuria&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;stăpânului tău!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,22,'&lt;J&gt;Cel cu doi talanți a venit și el și a zis: «Stăpâne, mi-ai încredințat doi talanți; iată, am câștigat [cu ei] alți doi talanți!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,23,'&lt;J&gt;Stăpânul său i-a zis: «Bine&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt;&lt;J&gt;, rob bun și credincios! Ai fost credincios peste puține &lt;i&gt;lucruri&lt;/i&gt;, te voi pune peste multe &lt;i&gt;lucruri&lt;/i&gt;; intră în bucuria stăpânului tău!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,24,'&lt;J&gt;A venit și cel care primise un talant și a zis: «Stăpâne, am știut că ești om aspru, care seceri de unde n-ai semănat și strângi de unde n-ai vânturat;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,25,'&lt;J&gt;pentru că mi-a fost frică, m-am dus și am ascuns talantul tău în pământ. Uite aici ce este al tău!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,26,'&lt;J&gt;Stăpânul său i-a răspuns: «Rob rău și leneș, ai știut că secer de unde n-am semănat și strâng de unde n-am vânturat!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,27,'&lt;J&gt;Atunci trebuia să-mi dai banii la zarafi și, la venirea mea, aș fi luat cu dobândă ce este al meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,28,'&lt;J&gt;Luați-i talantul și dați-i-l celui care are zece talanți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,29,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;celui ce are i se va da și va avea din belșug, dar celui ce nu are i se va lua și ce are.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,30,'&lt;J&gt;Iar pe robul acela netrebnic aruncați-l în&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;întunericul de afară: acolo va fi plânsul și scrâșnirea dinților.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,31,'&lt;pb/&gt;&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;va veni Fiul Omului în slava Sa cu toți [sfinții] îngeri, Se va așeza pe tronul slavei Sale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,32,'&lt;J&gt;Toate popoarele vor&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;fi adunate înaintea Lui, și El&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;îi va despărți pe unii de alții cum desparte păstorul oile de capre;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,33,'&lt;J&gt;și va pune oile la dreapta, iar caprele la stânga Lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,34,'&lt;J&gt;Atunci, Împăratul le va spune celor de la dreapta Lui: «Veniți, binecuvântații Tatălui Meu, și moșteniți&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;Împărăția care&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;v-a fost pregătită de la întemeierea lumii!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,35,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;am fost flămând și Mi-ați dat să mănânc; am fost însetat și Mi-ați dat să beau; am fost străin&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;și M-ați primit;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,36,'&lt;i&gt;&lt;J&gt;am fost&lt;/i&gt; gol&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;și M-ați îmbrăcat; &lt;i&gt;am fost&lt;/i&gt; bolnav și M-ați îngrijit; am fost în&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;temniță și ați venit pe la Mine.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,37,'&lt;J&gt;Atunci cei drepți Îi vor răspunde: «Doamne, când Te-am văzut noi flămând și Ți-am dat să mănânci sau însetat și Ți-am dat să bei?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,38,'&lt;J&gt;Când Te-am văzut noi străin și Te-am primit sau gol și Te-am îmbrăcat?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,39,'&lt;J&gt;Și când Te-am văzut noi bolnav sau în temniță și am venit pe la Tine?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,40,'&lt;J&gt;Împăratul le va răspunde: «Adevărat vă spun că, ori de câte ori&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;ați făcut aceste &lt;i&gt;lucruri&lt;/i&gt; unuia din acești foarte neînsemnați frați ai Mei, Mie Mi le-ați făcut.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,41,'&lt;J&gt;Apoi le va zice celor de la stânga Lui: «Plecați&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;de la Mine, blestemaților, în&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;focul cel veșnic, pregătit pentru Diavolul&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;și pentru îngerii lui!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,42,'&lt;J&gt;Căci am fost flămând, și nu Mi-ați dat să mănânc; &lt;i&gt;am fost&lt;/i&gt; însetat, și nu Mi-ați dat să beau;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,43,'&lt;J&gt;am fost străin, și nu M-ați primit; am fost gol, și nu M-ați îmbrăcat; am fost bolnav și în temniță, și nu ați venit pe la Mine.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,44,'&lt;J&gt;Atunci Îi vor răspunde și ei: «Doamne, când Te-am văzut noi flămând sau însetat sau străin sau gol sau bolnav sau în temniță și nu Ți-am slujit?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,45,'&lt;J&gt;El le va răspunde: «Adevărat vă spun că, ori de câte ori&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;n-ați făcut aceste &lt;i&gt;lucruri&lt;/i&gt; pentru unul dintre acești neînsemnați &lt;i&gt;frați ai Mei&lt;/i&gt;, Mie nu Mi le-ați făcut.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,25,46,'&lt;J&gt;Aceștia vor&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;merge în pedeapsa veșnică, iar cei drepți, în viața veșnică.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,1,'&lt;pb/&gt;După ce a terminat Isus &lt;i&gt;de zis&lt;/i&gt; toate aceste cuvinte, le-a spus ucenicilor Săi:');
INSERT INTO &quot;verses&quot; VALUES (470,26,2,'&lt;J&gt;„Știți&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;că peste două zile este Paștele, și Fiul Omului va fi dat să fie răstignit!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,3,'Atunci&lt;f&gt;ⓧ&lt;/f&gt;, preoții de seamă, [cărturarii] și bătrânii poporului s-au strâns la palatul marelui-preot numit Caiafa');
INSERT INTO &quot;verses&quot; VALUES (470,26,4,'și s-au sfătuit să-L prindă pe Isus prin viclenie și să-L omoare.');
INSERT INTO &quot;verses&quot; VALUES (470,26,5,'Dar ziceau: „Nu în timpul sărbătorii, ca să nu se facă tulburare în popor.”');
INSERT INTO &quot;verses&quot; VALUES (470,26,6,'&lt;pb/&gt;Pe când era&lt;f&gt;ⓨ&lt;/f&gt; &lt;i&gt;Isus&lt;/i&gt; în Betania&lt;f&gt;ⓩ&lt;/f&gt;, în casa lui Simon Leprosul,');
INSERT INTO &quot;verses&quot; VALUES (470,26,7,'s-a apropiat de El o femeie care avea un vas de alabastru cu mir de mare preț și l-a turnat pe capul Lui, pe când stătea El la masă.');
INSERT INTO &quot;verses&quot; VALUES (470,26,8,'Văzând aceasta, ucenicii&lt;f&gt;ⓐ&lt;/f&gt; s-au mâniat și au zis: „Pentru ce risipa aceasta?');
INSERT INTO &quot;verses&quot; VALUES (470,26,9,'Mirul acesta se putea vinde scump, iar &lt;i&gt;banii&lt;/i&gt; se puteau da săracilor.”');
INSERT INTO &quot;verses&quot; VALUES (470,26,10,'Când a băgat de seamă, Isus le-a zis: &lt;J&gt;„De ce supărați femeia? Ea a făcut un lucru frumos față de Mine;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,11,'&lt;J&gt;pe&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;săraci îi aveți întotdeauna cu voi, dar pe&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;Mine nu Mă aveți întotdeauna.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,12,'&lt;J&gt;Când a turnat acest mir pe trupul Meu, a făcut-o pentru îngroparea Mea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,13,'&lt;J&gt;Adevărat vă spun că, oriunde în lume va fi propovăduită Evanghelia aceasta, se va povesti și ce a făcut &lt;i&gt;femeia&lt;/i&gt; aceasta, în amintirea ei.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,14,'&lt;pb/&gt;Atunci&lt;f&gt;ⓓ&lt;/f&gt;, unul dintre cei doisprezece, numit Iuda Iscariotul&lt;f&gt;ⓔ&lt;/f&gt;, s-a dus la preoții de seamă');
INSERT INTO &quot;verses&quot; VALUES (470,26,15,'și le-a zis: „Ce-mi dați&lt;f&gt;ⓕ&lt;/f&gt; ca să Îl dau în mâinile voastre?” Ei &lt;e&gt;i-au oferit&lt;/e&gt;&lt;f&gt;[114]&lt;/f&gt; &lt;e&gt;treizeci de arginți&lt;/e&gt;&lt;f&gt;[115]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,26,16,'Din clipa aceea, &lt;i&gt;Iuda&lt;/i&gt; căuta un prilej potrivit ca să-L dea în mâinile lor.');
INSERT INTO &quot;verses&quot; VALUES (470,26,17,'&lt;pb/&gt;În&lt;f&gt;ⓖ&lt;/f&gt; prima &lt;i&gt;zi&lt;/i&gt; a &lt;i&gt;Sărbătorii&lt;/i&gt; Azimelor, ucenicii au venit la Isus și I-au zis: „Unde vrei să facem pregătiri ca să mănânci Paștele?”');
INSERT INTO &quot;verses&quot; VALUES (470,26,18,'El le-a răspuns: &lt;J&gt;„Duceți-vă în cetate, la cutare, și spuneți-i: «Învățătorul zice: ‘Timpul Meu este aproape; la tine voi face Paștele cu ucenicii Mei.’»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,19,'Ucenicii au făcut cum le poruncise Isus și au pregătit Paștele.');
INSERT INTO &quot;verses&quot; VALUES (470,26,20,'Când s-a înserat&lt;f&gt;ⓗ&lt;/f&gt;, &lt;i&gt;Isus&lt;/i&gt; S-a așezat la masă&lt;f&gt;[116]&lt;/f&gt; cu cei doisprezece [ucenici ai Săi].');
INSERT INTO &quot;verses&quot; VALUES (470,26,21,'Pe când mâncau, a zis: &lt;J&gt;„Adevărat vă spun că unul din voi Mă va vinde.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,22,'Ei s-au întristat mult și au început să-I spună fiecare în parte: „Nu cumva sunt eu, Doamne?”');
INSERT INTO &quot;verses&quot; VALUES (470,26,23,'El le-a răspuns: &lt;J&gt;„Cel&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;care a întins mâna cu Mine în blid, acela Mă va vinde.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,24,'&lt;J&gt;Fiul Omului negreșit Se duce, după cum&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;este scris despre El, dar vai&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;de omul acela prin care este vândut Fiul Omului! Ar fi fost mai bine pentru el dacă nu s-ar fi născut!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,25,'Iuda, care avea să-L vândă, a zis: „Nu cumva sunt eu, &lt;e&gt;Rabbí&lt;/e&gt;?” &lt;J&gt;„Tu însuți ai spus-o”&lt;/J&gt;, i-a zis &lt;i&gt;Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,26,26,'&lt;pb/&gt;Pe când&lt;f&gt;ⓛ&lt;/f&gt; mâncau, Isus a&lt;f&gt;ⓜ&lt;/f&gt; luat o pâine și, după ce a rostit binecuvântarea, a frânt-o și le-a dat-o ucenicilor, zicând: &lt;J&gt;„Luați, mâncați, acesta&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;este trupul Meu!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,27,'Apoi a luat un pahar și, după ce a adus mulțumire, li l-a dat zicând: &lt;J&gt;„Beți&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;toți din el,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,28,'&lt;J&gt;căci acesta&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;este sângele Meu, &lt;i&gt;sângele&lt;/i&gt; legământului [celui&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;nou], care se varsă pentru&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;mulți spre iertarea păcatelor!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,29,'&lt;J&gt;Vă spun&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;că, de acum &lt;i&gt;încolo&lt;/i&gt;, nu voi mai bea din acest rod al viței până&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;în ziua când îl voi bea cu voi nou în Împărăția Tatălui Meu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,30,'După ce&lt;f&gt;ⓤ&lt;/f&gt; au cântat cântările de laudă, au ieșit spre Muntele Măslinilor.');
INSERT INTO &quot;verses&quot; VALUES (470,26,31,'&lt;pb/&gt;Atunci, Isus le-a zis: &lt;J&gt;„În noaptea aceasta, toți&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;vă veți&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;poticni în Mine, pentru că este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Voi bate Păstorul,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și oile turmei se vor risipi.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[117]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,32,'&lt;pb/&gt;&lt;J&gt;Dar după ce voi învia, voi&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;merge înaintea voastră în Galileea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,33,'Petru a luat cuvântul și a zis: „Chiar dacă toți se vor poticni în Tine, eu niciodată nu mă voi poticni &lt;i&gt;în Tine&lt;/i&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (470,26,34,'Isus i-a zis: &lt;J&gt;„Adevărat&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;îți spun că tu, chiar în noaptea aceasta, înainte să cânte cocoșul, te vei lepăda de Mine de trei ori.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,35,'Petru I-a răspuns: „Chiar dacă ar trebui să mor împreună cu Tine, nu mă voi lepăda de Tine!” Și toți ucenicii au spus la fel.');
INSERT INTO &quot;verses&quot; VALUES (470,26,36,'&lt;pb/&gt;Atunci&lt;f&gt;ⓩ&lt;/f&gt; Isus a mers cu ucenicii într-un loc numit Ghetsimani și le-a zis: &lt;J&gt;„Așezați-vă aici până Mă voi duce acolo să Mă rog!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,37,'I-a luat cu Sine pe Petru și pe&lt;f&gt;ⓐ&lt;/f&gt; cei doi fii ai lui Zebedei și a început să Se întristeze și să Se mâhnească.');
INSERT INTO &quot;verses&quot; VALUES (470,26,38,'[Isus] le-a zis atunci: &lt;J&gt;„Sufletul Meu este cuprins de o întristare&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;de moarte; rămâneți aici și vegheați împreună cu Mine!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,39,'Apoi S-a depărtat puțin și a căzut cu fața la pământ, rugându-Se&lt;f&gt;ⓒ&lt;/f&gt; și zicând: &lt;J&gt;„Tată&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt;&lt;J&gt;, dacă este cu putință, îndepărtează&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;de la Mine paharul acesta! Totuși, nu&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;cum vreau Eu, ci cum vrei Tu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,40,'A venit la ucenici, i-a găsit dormind și i-a zis lui Petru: &lt;J&gt;„Așadar, nici măcar un ceas n-ați putut veghea împreună cu Mine?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,41,'&lt;J&gt;Vegheați&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;și rugați-vă să nu ajungeți în ispită&lt;/J&gt;&lt;f&gt;[118]&lt;/f&gt;&lt;J&gt;; duhul &lt;i&gt;este&lt;/i&gt; plin de râvnă, dar trupul &lt;i&gt;este&lt;/i&gt; neputincios.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,42,'A mers din nou, a doua oară, și S-a rugat zicând: &lt;J&gt;„Tată, dacă nu se poate să îndepărtezi de la Mine [paharul acesta] fără să-l beau, facă-se voia Ta!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,43,'Apoi a venit din nou și i-a găsit dormind, pentru că ochii le erau îngreunați.');
INSERT INTO &quot;verses&quot; VALUES (470,26,44,'I-a lăsat din nou, S-a depărtat și S-a rugat a treia oară, spunând aceleași cuvinte.');
INSERT INTO &quot;verses&quot; VALUES (470,26,45,'Apoi a venit la ucenici și le-a zis: &lt;J&gt;„De acum dormiți și odihniți-vă!… Iată, s-a apropiat ceasul când Fiul Omului va fi dat în mâinile păcătoșilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,46,'&lt;J&gt;Ridicați-vă, să mergem; iată, se apropie vânzătorul!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,47,'&lt;pb/&gt;Pe când&lt;f&gt;ⓗ&lt;/f&gt; vorbea El încă, iată că a venit Iuda, unul dintre cei doisprezece, și împreună cu el o gloată mare &lt;i&gt;de oameni&lt;/i&gt; cu săbii și cu ciomege, &lt;i&gt;trimiși&lt;/i&gt; de preoții de seamă și de bătrânii poporului.');
INSERT INTO &quot;verses&quot; VALUES (470,26,48,'Vânzătorul le dăduse un semn, zicând: „Cel pe care-l voi săruta eu, acela este; să-l prindeți!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,49,'S-a apropiat îndată de Isus și I-a zis: „Plecăciune, &lt;e&gt;Rabbí&lt;/e&gt;!” Și L-a&lt;f&gt;ⓘ&lt;/f&gt; sărutat.');
INSERT INTO &quot;verses&quot; VALUES (470,26,50,'Isus i-a zis: &lt;J&gt;„Prietene&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt;&lt;J&gt;, ce ai venit &lt;i&gt;să faci, fă&lt;/i&gt;!”&lt;/J&gt; Atunci, ei s-au apropiat, au pus mâna pe Isus și L-au prins.');
INSERT INTO &quot;verses&quot; VALUES (470,26,51,'Și iată că unul&lt;f&gt;ⓚ&lt;/f&gt; dintre cei ce erau cu Isus a întins mâna, și-a scos sabia, l-a lovit pe slujitorul marelui-preot și i-a tăiat urechea.');
INSERT INTO &quot;verses&quot; VALUES (470,26,52,'Atunci, Isus i-a zis: &lt;J&gt;„Pune-ți sabia la locul ei, căci&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;toți cei ce scot sabia de sabie vor pieri!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,53,'&lt;J&gt;Crezi că n-aș putea să-L rog pe Tatăl Meu să-mi pună chiar acum la îndemână peste&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;douăsprezece legiuni&lt;/J&gt;&lt;f&gt;[119]&lt;/f&gt; &lt;J&gt;de îngeri?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,54,'&lt;J&gt;Dar cum se vor mai împlini Scripturile, &lt;i&gt;care spun&lt;/i&gt; că&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;așa trebuie să fie?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,55,'În clipa aceea, Isus a zis gloatei: &lt;J&gt;„Ați ieșit ca după un tâlhar, cu săbii și cu ciomege, ca să Mă prindeți. În toate zilele eram [cu voi] în Templu învățându-i &lt;i&gt;pe oameni&lt;/i&gt;, și n-ați pus mâna pe Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,56,'&lt;J&gt;Dar toate acestea s-au petrecut ca să se împlinească Scripturile&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;profeților.”&lt;/J&gt; Atunci&lt;f&gt;ⓟ&lt;/f&gt;, toți ucenicii L-au părăsit și au fugit.');
INSERT INTO &quot;verses&quot; VALUES (470,26,57,'&lt;pb/&gt;Cei ce&lt;f&gt;ⓠ&lt;/f&gt; L-au prins pe Isus L-au dus la marele-preot Caiafa, unde se adunaseră cărturarii și bătrânii.');
INSERT INTO &quot;verses&quot; VALUES (470,26,58,'Petru L-a urmat de departe până înăuntru, în curtea marelui-preot; intrând, s-a așezat împreună cu servitorii, ca să vadă sfârșitul.');
INSERT INTO &quot;verses&quot; VALUES (470,26,59,'Preoții de seamă, [bătrânii] și tot Sinedriul căutau o mărturie mincinoasă împotriva lui Isus, ca să-L omoare,');
INSERT INTO &quot;verses&quot; VALUES (470,26,60,'dar n-au găsit &lt;i&gt;niciuna&lt;/i&gt;, chiar dacă veniseră mulți&lt;f&gt;ⓡ&lt;/f&gt; martori mincinoși. În cele din urmă, au venit doi&lt;f&gt;ⓢ&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,61,'care au spus: „El a zis: «Pot&lt;f&gt;ⓣ&lt;/f&gt; să dărâm Templul lui Dumnezeu și în trei zile să îl zidesc la loc.»”');
INSERT INTO &quot;verses&quot; VALUES (470,26,62,'Atunci marele-preot s-a ridicat&lt;f&gt;ⓤ&lt;/f&gt; &lt;i&gt;în picioare&lt;/i&gt; și I-a zis: „Nu răspunzi nimic? Ce mărturie aduc aceștia împotriva Ta?”');
INSERT INTO &quot;verses&quot; VALUES (470,26,63,'Dar Isus&lt;f&gt;ⓥ&lt;/f&gt; tăcea. Și marele-preot I-a zis: „Te&lt;f&gt;ⓦ&lt;/f&gt; conjur pe Dumnezeul cel viu să ne spui dacă Tu ești Hristosul, Fiul lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,64,'Isus i-a răspuns: &lt;J&gt;„Tu însuți ai zis-o!&lt;/J&gt;&lt;f&gt;[120]&lt;/f&gt; &lt;J&gt;Dar Eu vă mai spun că, de&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;acum &lt;i&gt;încolo&lt;/i&gt;, Îl veți vedea &lt;e&gt;pe Fiul Omului&lt;/e&gt; stând&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;la dreapta Puterii și &lt;e&gt;venind pe norii cerului&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[121]&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,26,65,'Atunci&lt;f&gt;ⓩ&lt;/f&gt;, marele-preot și-a rupt hainele și a zis: „A hulit! Ce nevoie mai avem de martori? Iată, acum ați auzit blasfemia!');
INSERT INTO &quot;verses&quot; VALUES (470,26,66,'Ce credeți?” Ei au răspuns: „Este vrednic de moarte&lt;f&gt;ⓐ&lt;/f&gt;!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,67,'Atunci&lt;f&gt;ⓑ&lt;/f&gt;, L-au scuipat în față, L-au&lt;f&gt;ⓒ&lt;/f&gt; bătut cu pumnii și L-au pălmuit,');
INSERT INTO &quot;verses&quot; VALUES (470,26,68,'zicând: „Profețește&lt;f&gt;ⓓ&lt;/f&gt; cine te-a lovit, hristosule!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,69,'&lt;pb/&gt;Petru&lt;f&gt;ⓔ&lt;/f&gt; însă stătea afară, în curte. O servitoare s-a apropiat de el și i-a zis: „Și tu erai cu Isus, galileeanul!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,70,'Dar el a tăgăduit înaintea tuturor, zicând: „Nu știu ce spui!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,71,'Când a ieșit în pridvor, l-a văzut alta și le-a zis celor de acolo: „Acesta era cu Isus din Nazaret!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,72,'El a tăgăduit din nou, cu jurământ: „Nu-l cunosc pe omul &lt;i&gt;acesta&lt;/i&gt;!”');
INSERT INTO &quot;verses&quot; VALUES (470,26,73,'La puțin timp, cei ce stăteau acolo s-au apropiat și i-au zis lui Petru: „Cu adevărat și tu ești &lt;i&gt;unul&lt;/i&gt; dintre ei, căci și vorba&lt;f&gt;ⓕ&lt;/f&gt; te dă de gol.”');
INSERT INTO &quot;verses&quot; VALUES (470,26,74,'Atunci, el&lt;f&gt;ⓖ&lt;/f&gt; a început să se blesteme și să se jure: „Nu-l cunosc pe omul &lt;i&gt;acesta&lt;/i&gt;!” Și îndată a cântat cocoșul.');
INSERT INTO &quot;verses&quot; VALUES (470,26,75,'Iar Petru și-a adus aminte ce îi spusese Isus: &lt;J&gt;„Înainte&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;să cânte cocoșul, te vei lepăda de Mine de trei ori.”&lt;/J&gt; A ieșit afară și a plâns cu amar.');
INSERT INTO &quot;verses&quot; VALUES (470,27,1,'&lt;pb/&gt;Când s-a făcut dimineață, toți&lt;f&gt;ⓘ&lt;/f&gt; preoții de seamă și bătrânii poporului au ținut sfat împotriva lui Isus, ca să-L omoare.');
INSERT INTO &quot;verses&quot; VALUES (470,27,2,'După ce L-au legat, L-au dus și L-au dat în mâna dregătorului&lt;f&gt;ⓙ&lt;/f&gt; Pilat [din Pont].');
INSERT INTO &quot;verses&quot; VALUES (470,27,3,'&lt;pb/&gt;Atunci, Iuda&lt;f&gt;ⓚ&lt;/f&gt;, cel care L-a vândut, văzând că &lt;i&gt;Isus&lt;/i&gt; a fost osândit, s-a căit, le-a înapoiat cei treizeci de arginți preoților de seamă și bătrânilor');
INSERT INTO &quot;verses&quot; VALUES (470,27,4,'și a zis: „Am păcătuit, căci am vândut sânge nevinovat!” „Ce ne pasă nouă? E treaba ta”, i-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (470,27,5,'&lt;i&gt;Iuda&lt;/i&gt; a aruncat arginții în Templu, apoi a ieșit, s-a&lt;f&gt;ⓛ&lt;/f&gt; dus și s-a spânzurat.');
INSERT INTO &quot;verses&quot; VALUES (470,27,6,'Preoții de seamă au luat arginții și au zis: „Nu este îngăduit să-i punem în vistieria Templului, fiindcă sunt preț de sânge.”');
INSERT INTO &quot;verses&quot; VALUES (470,27,7,'Și după ce s-au sfătuit, au cumpărat cu ei Ogorul Olarului, pentru înmormântarea străinilor.');
INSERT INTO &quot;verses&quot; VALUES (470,27,8,'De aceea ogorul&lt;f&gt;ⓜ&lt;/f&gt; acela se numește până astăzi Ogorul Sângelui.');
INSERT INTO &quot;verses&quot; VALUES (470,27,9,'Atunci s-a împlinit ce fusese vestit prin profetul Ieremia&lt;f&gt;[122]&lt;/f&gt;, care zice: „&lt;e&gt;Am luat cei treizeci de arginți, prețul celui prețuit, pe care îl hotărâseră&lt;/e&gt;&lt;f&gt;[123]&lt;/f&gt; &lt;i&gt;unii&lt;/i&gt; dintre fiii lui Israel,');
INSERT INTO &quot;verses&quot; VALUES (470,27,10,'și i-am dat pe Ogorul Olarului, după cum mi-a poruncit Domnul.”');
INSERT INTO &quot;verses&quot; VALUES (470,27,11,'&lt;pb/&gt;Isus a fost adus înaintea dregătorului, care L-a&lt;f&gt;ⓝ&lt;/f&gt; întrebat: „Ești tu Regele iudeilor?” &lt;J&gt;„Tu însuți o spui&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;, i-a răspuns Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,27,12,'Dar la învinuirile preoților de seamă și ale bătrânilor nu a răspuns nimic&lt;f&gt;ⓟ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,27,13,'Atunci, Pilat I-a zis: „Nu auzi&lt;f&gt;ⓠ&lt;/f&gt; câte mărturisesc ei împotriva ta?”');
INSERT INTO &quot;verses&quot; VALUES (470,27,14,'&lt;i&gt;Isus&lt;/i&gt; nu i-a răspuns la niciun cuvânt, așa că dregătorul se mira foarte mult.');
INSERT INTO &quot;verses&quot; VALUES (470,27,15,'&lt;pb/&gt;La&lt;f&gt;ⓡ&lt;/f&gt; fiecare sărbătoare, dregătorul obișnuia să elibereze mulțimii un întemnițat pe care-l cerea ea.');
INSERT INTO &quot;verses&quot; VALUES (470,27,16,'Pe atunci aveau un întemnițat vestit, pe nume Isus&lt;f&gt;[124]&lt;/f&gt; Barabba.');
INSERT INTO &quot;verses&quot; VALUES (470,27,17,'Când s-au adunat, Pilat le-a zis: „Pe cine vreți să vi-l eliberez: pe Isus Barabba sau pe Isus numit Hristos?”');
INSERT INTO &quot;verses&quot; VALUES (470,27,18,'Căci știa că din invidie Îl dăduseră în mâna lui.');
INSERT INTO &quot;verses&quot; VALUES (470,27,19,'Pe când stătea Pilat pe scaunul de judecată, soția lui a trimis să-i spună: „Să n-ai nimic de-a face cu dreptul acesta, căci azi am suferit mult în vis din pricina Lui.”');
INSERT INTO &quot;verses&quot; VALUES (470,27,20,'Însă preoții&lt;f&gt;ⓢ&lt;/f&gt; de seamă și bătrânii au înduplecat mulțimile să-l ceară pe Barabba, iar pe Isus să-L dea pieirii.');
INSERT INTO &quot;verses&quot; VALUES (470,27,21,'Dregătorul le-a răspuns: „Pe care dintre cei doi vreți să vi-l eliberez?” „Pe Barabba”, au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (470,27,22,'Pilat le-a zis: „Dar ce să fac cu Isus care se numește Hristos?” „Să fie răstignit”, i-au răspuns cu toții.');
INSERT INTO &quot;verses&quot; VALUES (470,27,23,'&lt;i&gt;Dregătorul&lt;/i&gt; a zis: „Dar ce rău a făcut?” Ei au început să strige și mai tare: „Să fie răstignit!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,24,'Văzând că nu o scoate la capăt, ci mai degrabă se face tulburare, Pilat a luat&lt;f&gt;ⓣ&lt;/f&gt; apă, s-a spălat pe mâini înaintea mulțimii și a zis: „Eu sunt nevinovat de sângele [dreptului] acestuia. E treaba voastră.”');
INSERT INTO &quot;verses&quot; VALUES (470,27,25,'Și tot poporul a răspuns: „Sângele&lt;f&gt;ⓤ&lt;/f&gt; lui &lt;i&gt;să fie&lt;/i&gt; asupra noastră și asupra copiilor noștri!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,26,'Atunci, Pilat l-a eliberat pe Barabba, iar pe Isus, după ce a pus să-L biciuiască&lt;f&gt;ⓥ&lt;/f&gt;, L-a dat să fie răstignit.');
INSERT INTO &quot;verses&quot; VALUES (470,27,27,'&lt;pb/&gt;Soldații&lt;f&gt;ⓦ&lt;/f&gt; dregătorului L-au dus pe Isus în pretoriu și au adunat în jurul Lui toată cohorta.');
INSERT INTO &quot;verses&quot; VALUES (470,27,28,'L-au dezbrăcat și L-au&lt;f&gt;ⓧ&lt;/f&gt; acoperit cu o mantie stacojie.');
INSERT INTO &quot;verses&quot; VALUES (470,27,29,'Au&lt;f&gt;ⓨ&lt;/f&gt; împletit o cunună de spini, I-au pus-o pe cap și &lt;i&gt;I-au pus&lt;/i&gt; o trestie în mâna dreaptă. Apoi îngenuncheau înaintea Lui și își băteau joc de El zicând: „Plecăciune, rege al iudeilor!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,30,'Îl scuipau&lt;f&gt;ⓩ&lt;/f&gt;, îi luau trestia și-L loveau peste cap.');
INSERT INTO &quot;verses&quot; VALUES (470,27,31,'După ce și-au bătut joc de El, L-au dezbrăcat de mantie, L-au îmbrăcat în hainele Lui și&lt;f&gt;ⓐ&lt;/f&gt; L-au dus să-L răstignească.');
INSERT INTO &quot;verses&quot; VALUES (470,27,32,'&lt;pb/&gt;Pe&lt;f&gt;ⓑ&lt;/f&gt; când ieșeau, au&lt;f&gt;ⓒ&lt;/f&gt; găsit un om din Cirene, numit Simon, pe care l-au silit să-I poarte crucea.');
INSERT INTO &quot;verses&quot; VALUES (470,27,33,'Când&lt;f&gt;ⓓ&lt;/f&gt; au ajuns la locul numit Golgota, care înseamnă „&lt;i&gt;Locul&lt;/i&gt; căpățânii”,');
INSERT INTO &quot;verses&quot; VALUES (470,27,34,'&lt;e&gt;I-au dat să bea&lt;/e&gt; vin&lt;f&gt;ⓔ&lt;/f&gt; amestecat &lt;e&gt;cu fiere&lt;/e&gt;&lt;f&gt;[125]&lt;/f&gt;; dar când l-a gustat, n-a vrut să bea.');
INSERT INTO &quot;verses&quot; VALUES (470,27,35,'După ce&lt;f&gt;ⓕ&lt;/f&gt; L-au răstignit, &lt;e&gt;au împărțit &lt;i&gt;între ei&lt;/i&gt; hainele&lt;/e&gt; Lui, &lt;e&gt;trăgând la sorți,&lt;/e&gt; [ca să se împlinească ce fusese spus prin profet: &lt;e&gt;Și-au împărțit hainele Mele între ei, și pentru veșmântul Meu au tras la sorți&lt;/e&gt;&lt;f&gt;[126]&lt;/f&gt;].');
INSERT INTO &quot;verses&quot; VALUES (470,27,36,'Apoi&lt;f&gt;ⓖ&lt;/f&gt; s-au așezat și-L păzeau acolo.');
INSERT INTO &quot;verses&quot; VALUES (470,27,37,'Și&lt;f&gt;ⓗ&lt;/f&gt; I-au scris deasupra capului vina: „Acesta este Isus, regele iudeilor.”');
INSERT INTO &quot;verses&quot; VALUES (470,27,38,'Împreună&lt;f&gt;ⓘ&lt;/f&gt; cu El au mai fost răstigniți doi tâlhari: unul la dreapta și altul la stânga.');
INSERT INTO &quot;verses&quot; VALUES (470,27,39,'&lt;pb/&gt;Trecătorii&lt;f&gt;ⓙ&lt;/f&gt; Îi aruncau blasfemii, &lt;e&gt;clătinau din cap&lt;/e&gt;&lt;f&gt;[127]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,27,40,'și ziceau: „Tu&lt;f&gt;ⓚ&lt;/f&gt;, care dărâmi Templul și-l zidești la loc în trei zile, izbăvește-te pe tine, dacă&lt;f&gt;ⓛ&lt;/f&gt; ești Fiul lui Dumnezeu, și coboară de pe cruce!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,41,'Preoții de seamă, împreună cu cărturarii și bătrânii, Îl luau în râs și ziceau:');
INSERT INTO &quot;verses&quot; VALUES (470,27,42,'„Pe alții i-a izbăvit, dar pe sine nu se poate izbăvi! Este regele lui Israel, să coboare acum de pe cruce și vom crede în el!');
INSERT INTO &quot;verses&quot; VALUES (470,27,43,'&lt;e&gt;S-a încrezut în Dumnezeu, să-l scape acum, dacă găsește plăcere în el.&lt;/e&gt;&lt;f&gt;[128]&lt;/f&gt; Doar a zis: &lt;J&gt;«Eu sunt Fiul lui Dumnezeu!»&lt;/J&gt;”');
INSERT INTO &quot;verses&quot; VALUES (470,27,44,'La fel Îl ocărau și tâlharii răstigniți împreună cu El.');
INSERT INTO &quot;verses&quot; VALUES (470,27,45,'&lt;pb/&gt;De la&lt;f&gt;ⓜ&lt;/f&gt; ceasul al șaselea&lt;f&gt;[129]&lt;/f&gt; s-a făcut întuneric în toată țara până la ceasul al nouălea.');
INSERT INTO &quot;verses&quot; VALUES (470,27,46,'Pe la ceasul al nouălea, Isus a&lt;f&gt;ⓝ&lt;/f&gt; strigat cu glas tare: &lt;pb/&gt;&lt;t&gt;&lt;J&gt;„&lt;e&gt;Elí, Elí, lemá sabahtáni?&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[130]&lt;/f&gt;,&lt;/t&gt; &lt;pb/&gt;care înseamnă: „Dumnezeul Meu, Dumnezeul Meu, de ce M-ai părăsit?”');
INSERT INTO &quot;verses&quot; VALUES (470,27,47,'Unii din cei ce stăteau acolo, când L-au auzit, au zis: „Îl cheamă pe Ilie!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,48,'Și îndată, unul dintre ei a alergat, a luat un burete, l-a umplut&lt;f&gt;ⓞ&lt;/f&gt; cu &lt;e&gt;oțet&lt;/e&gt;, l-a pus într-o trestie și &lt;e&gt;I l-a dat să bea&lt;/e&gt;&lt;f&gt;[131]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,27,49,'Dar ceilalți ziceau: „Lasă, să vedem dacă va veni Ilie să-l izbăvească!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,50,'Isus&lt;f&gt;ⓟ&lt;/f&gt; a strigat din nou cu glas tare, apoi Și-a dat duhul.');
INSERT INTO &quot;verses&quot; VALUES (470,27,51,'&lt;pb/&gt;Și iată, catapeteasma&lt;f&gt;ⓠ&lt;/f&gt; Templului s-a despicat în două, de sus până jos, pământul s-a cutremurat, stâncile s-au despicat,');
INSERT INTO &quot;verses&quot; VALUES (470,27,52,'mormintele s-au deschis și multe trupuri ale sfinților adormiți au înviat.');
INSERT INTO &quot;verses&quot; VALUES (470,27,53,'Ei au ieșit din morminte după învierea Lui, au intrat în sfânta cetate și li s-au arătat multora.');
INSERT INTO &quot;verses&quot; VALUES (470,27,54,'&lt;pb/&gt;Centurionul&lt;f&gt;ⓡ&lt;/f&gt; și cei care-L păzeau pe Isus împreună cu el, văzând cutremurul și cele petrecute, s-au înspăimântat foarte tare și au zis: „Cu adevărat, Acesta era Fiul lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,55,'Erau acolo și multe femei, care priveau de departe; ele&lt;f&gt;ⓢ&lt;/f&gt; Îl urmaseră pe Isus din Galileea, slujindu-I.');
INSERT INTO &quot;verses&quot; VALUES (470,27,56,'Între&lt;f&gt;ⓣ&lt;/f&gt; ele erau Maria Magdalena, Maria, mama lui Iacov și a lui Iosif, și mama fiilor lui Zebedei.');
INSERT INTO &quot;verses&quot; VALUES (470,27,57,'&lt;pb/&gt;Când&lt;f&gt;ⓤ&lt;/f&gt; s-a înserat, a venit un om bogat din Arimateea, numit Iosif, care era și el ucenic al lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (470,27,58,'El s-a dus la Pilat și i-a cerut trupul lui Isus. Pilat a poruncit să-i fie dat.');
INSERT INTO &quot;verses&quot; VALUES (470,27,59,'Și luând trupul, Iosif l-a înfășurat într-un giulgiu curat');
INSERT INTO &quot;verses&quot; VALUES (470,27,60,'și l-a&lt;f&gt;ⓥ&lt;/f&gt; pus într-un mormânt nou, al lui însuși, pe care-l săpase în stâncă. Apoi a rostogolit o piatră mare la intrarea mormântului și a plecat.');
INSERT INTO &quot;verses&quot; VALUES (470,27,61,'Maria Magdalena și cealaltă Marie erau acolo, stând jos în fața mormântului.');
INSERT INTO &quot;verses&quot; VALUES (470,27,62,'&lt;pb/&gt;În ziua care urmează după &lt;i&gt;ziua&lt;/i&gt; pregătirii, preoții de seamă și fariseii s-au adunat la Pilat');
INSERT INTO &quot;verses&quot; VALUES (470,27,63,'și i-au zis: „Stăpâne, ne-am amintit că înșelătorul acela, pe când trăia încă, a zis: &lt;J&gt;«După&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;trei zile voi învia.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,27,64,'Poruncește, așadar, ca mormântul să fie păzit bine până a treia zi, ca nu cumva să vină ucenicii lui [noaptea] să-l fure și să spună poporului: «A înviat din morți!» Înșelătoria de pe urmă ar fi mai rea decât cea dintâi.”');
INSERT INTO &quot;verses&quot; VALUES (470,27,65,'Pilat le-a zis: „Aveți strajă; mergeți și păziți cum știți!”');
INSERT INTO &quot;verses&quot; VALUES (470,27,66,'Ei au plecat și au întărit&lt;f&gt;ⓧ&lt;/f&gt; mormântul, pecetluind piatra și &lt;i&gt;punând&lt;/i&gt; strajă.');
INSERT INTO &quot;verses&quot; VALUES (470,28,1,'&lt;pb/&gt;La sfârșitul sabatului&lt;f&gt;ⓨ&lt;/f&gt;, când se lumina spre ziua întâi a săptămânii, Maria Magdalena și&lt;f&gt;ⓩ&lt;/f&gt; cealaltă Marie au venit să vadă mormântul.');
INSERT INTO &quot;verses&quot; VALUES (470,28,2,'Și iată că s-a făcut un cutremur mare, căci un înger&lt;f&gt;ⓐ&lt;/f&gt; al Domnului s-a coborât din cer, a venit și a rostogolit piatra [de la intrarea mormântului] și s-a așezat pe ea.');
INSERT INTO &quot;verses&quot; VALUES (470,28,3,'Înfățișarea&lt;f&gt;ⓑ&lt;/f&gt; lui era ca fulgerul, și îmbrăcămintea lui, albă ca zăpada.');
INSERT INTO &quot;verses&quot; VALUES (470,28,4,'Străjerii s-au cutremurat de frica lui și au rămas ca morți.');
INSERT INTO &quot;verses&quot; VALUES (470,28,5,'Dar îngerul a luat cuvântul și le-a zis femeilor: „Nu vă temeți, căci știu că Îl căutați pe Isus, care a fost răstignit!');
INSERT INTO &quot;verses&quot; VALUES (470,28,6,'Nu este aici; a înviat, după cum&lt;f&gt;ⓒ&lt;/f&gt; a spus. Veniți să vedeți locul unde zăcea [Domnul]!');
INSERT INTO &quot;verses&quot; VALUES (470,28,7,'Mergeți repede și spuneți-le ucenicilor Lui că a înviat dintre cei morți! Iată, El merge&lt;f&gt;ⓓ&lt;/f&gt; înaintea voastră în Galileea; acolo Îl veți vedea. Iată, v-am spus.”');
INSERT INTO &quot;verses&quot; VALUES (470,28,8,'&lt;pb/&gt;Ele au plecat în grabă de la mormânt, cu frică și cu mare bucurie, și au alergat să le dea de veste ucenicilor Lui.');
INSERT INTO &quot;verses&quot; VALUES (470,28,9,'Dar iată că le-a întâmpinat Isus&lt;f&gt;ⓔ&lt;/f&gt; și le-a zis: &lt;J&gt;„Bucurați-vă!”&lt;/J&gt; Apropiindu-se, ele I-au cuprins picioarele și I s-au închinat.');
INSERT INTO &quot;verses&quot; VALUES (470,28,10,'Atunci, Isus le-a zis: &lt;J&gt;„Nu vă temeți; mergeți și spuneți-le fraților&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;Mei să meargă în Galileea; acolo Mă vor vedea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,28,11,'&lt;pb/&gt;Pe când se duceau ele, unii dintre străjeri au mers în cetate și le-au dat preoților de seamă vestea &lt;i&gt;despre&lt;/i&gt; toate cele petrecute.');
INSERT INTO &quot;verses&quot; VALUES (470,28,12,'Aceștia s-au adunat împreună cu bătrânii, au ținut sfat și le-au dat soldaților mulți bani,');
INSERT INTO &quot;verses&quot; VALUES (470,28,13,'zicându-le: „Spuneți &lt;i&gt;așa&lt;/i&gt;: «Ucenicii lui au venit noaptea, pe când dormeam noi, și l-au furat»!');
INSERT INTO &quot;verses&quot; VALUES (470,28,14,'Și dacă vor ajunge acestea la urechile dregătorului, îl vom îndupleca noi și vă vom scăpa de grijă.”');
INSERT INTO &quot;verses&quot; VALUES (470,28,15,'Ei au luat banii și au făcut cum au fost învățați. Și s-a răspândit vorba aceasta printre iudei până în ziua de astăzi.');
INSERT INTO &quot;verses&quot; VALUES (470,28,16,'&lt;pb/&gt;Cei unsprezece ucenici au mers în Galileea, la muntele unde&lt;f&gt;ⓖ&lt;/f&gt; le poruncise Isus &lt;i&gt;să meargă&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (470,28,17,'Când L-au văzut, I s-au închinat, dar unii s-au îndoit.');
INSERT INTO &quot;verses&quot; VALUES (470,28,18,'Apropiindu-Se de ei, Isus le-a vorbit și le-a zis: &lt;J&gt;„Toată puterea Mi-a fost dată&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;în cer și pe pământ.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,28,19,'&lt;J&gt;Duceți-vă&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;și faceți ucenici&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;din toate neamurile, botezându-i în Numele Tatălui și al Fiului și al Sfântului Duh&lt;/J&gt;&lt;f&gt;[132]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (470,28,20,'&lt;J&gt;și învățându-i&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;să păzească tot ce v-am poruncit! Și iată că Eu sunt cu voi în toate zilele, până la sfârșitul veacului.”&lt;/J&gt; [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (480,1,1,'&lt;pb/&gt;Începutul Evangheliei lui Isus Hristos, Fiul&lt;f&gt;ⓐ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (480,1,2,'După cum este scris în profetul Isaia: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată, trimit pe solul Meu înaintea Feței Tale;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;el Îți va pregăti calea;&lt;/e&gt; &lt;f&gt;[1]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,3,'&lt;t&gt;&lt;e&gt;glasul celui ce strigă în pustiu:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;„Pregătiți calea Domnului,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;îndreptați cărările Lui!”&lt;/e&gt; &lt;f&gt;[2]&lt;/f&gt;,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,4,'&lt;pb/&gt;a venit Ioan&lt;f&gt;ⓑ&lt;/f&gt; botezând în pustiu și propovăduind botezul pocăinței spre iertarea păcatelor.');
INSERT INTO &quot;verses&quot; VALUES (480,1,5,'Și au început să iasă la el toți&lt;f&gt;ⓒ&lt;/f&gt; cei din ținutul Iudeei și toți locuitorii Ierusalimului; și mărturisindu-și păcatele, erau botezați de el în râul Iordan.');
INSERT INTO &quot;verses&quot; VALUES (480,1,6,'Ioan era îmbrăcat&lt;f&gt;ⓓ&lt;/f&gt; cu &lt;i&gt;o haină din&lt;/i&gt; păr de cămilă, era încins la mijloc cu un brâu de piele și se hrănea cu lăcuste&lt;f&gt;ⓔ&lt;/f&gt; și miere sălbatică.');
INSERT INTO &quot;verses&quot; VALUES (480,1,7,'Ioan propovăduia, zicând: „După mine vine&lt;f&gt;ⓕ&lt;/f&gt; Cel ce este mai puternic decât mine, căruia eu nu sunt vrednic să mă plec să-I dezleg cureaua încălțămintei.');
INSERT INTO &quot;verses&quot; VALUES (480,1,8,'Eu&lt;f&gt;ⓖ&lt;/f&gt; v-am botezat în apă, dar El vă va boteza în&lt;f&gt;ⓗ&lt;/f&gt; Duhul Sfânt.”');
INSERT INTO &quot;verses&quot; VALUES (480,1,9,'&lt;pb/&gt;În&lt;f&gt;ⓘ&lt;/f&gt; zilele acelea, a venit Isus din Nazaretul Galileei și a fost botezat în Iordan de către Ioan.');
INSERT INTO &quot;verses&quot; VALUES (480,1,10,'Și îndată, pe când ieșea&lt;f&gt;ⓙ&lt;/f&gt; &lt;i&gt;Isus&lt;/i&gt; din apă, &lt;i&gt;Ioan&lt;/i&gt; a văzut cerurile deschise și Duhul coborându-Se peste El în chip de porumbel.');
INSERT INTO &quot;verses&quot; VALUES (480,1,11,'Și un glas s-a auzit din ceruri: „Tu&lt;f&gt;ⓚ&lt;/f&gt; ești Fiul Meu preaiubit; în Tine Îmi găsesc toată plăcerea.”');
INSERT INTO &quot;verses&quot; VALUES (480,1,12,'&lt;pb/&gt;Îndată&lt;f&gt;ⓛ&lt;/f&gt;, Duhul L-a dus &lt;i&gt;pe Isus&lt;/i&gt; în pustiu.');
INSERT INTO &quot;verses&quot; VALUES (480,1,13,'Și a stat în pustiu patruzeci de zile, ca să fie ispitit de Satana. &lt;i&gt;Acolo&lt;/i&gt; era împreună cu fiarele sălbatice, și îngerii Îi&lt;f&gt;ⓜ&lt;/f&gt; slujeau.');
INSERT INTO &quot;verses&quot; VALUES (480,1,14,'&lt;pb/&gt;După&lt;f&gt;ⓝ&lt;/f&gt; ce a fost închis Ioan, Isus a venit în Galileea și propovăduia&lt;f&gt;ⓞ&lt;/f&gt; Evanghelia lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (480,1,15,'zicând: &lt;J&gt;„S-a&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;împlinit vremea și Împărăția lui Dumnezeu este aproape&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt;&lt;J&gt;. Pocăiți-vă și credeți în Evanghelie!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,16,'&lt;pb/&gt;Pe&lt;f&gt;ⓡ&lt;/f&gt; când trecea Isus pe țărmul Mării Galileei, i-a văzut pe Simon și pe Andrei, fratele lui Simon, aruncând năvodul în mare, căci erau pescari.');
INSERT INTO &quot;verses&quot; VALUES (480,1,17,'Isus le-a zis: &lt;J&gt;„Veniți după Mine și vă voi face pescari de oameni!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,18,'Iar ei îndată și-au lăsat&lt;f&gt;ⓢ&lt;/f&gt; mrejele și L-au urmat.');
INSERT INTO &quot;verses&quot; VALUES (480,1,19,'După ce a&lt;f&gt;ⓣ&lt;/f&gt; mers puțin mai departe, i-a văzut pe Iacov, fiul lui Zebedei, și pe Ioan, fratele lui, care erau și ei într-o barcă, dregându-și mrejele,');
INSERT INTO &quot;verses&quot; VALUES (480,1,20,'și îndată i-a chemat. Ei l-au lăsat pe Zebedei, tatăl lor, în barcă împreună cu simbriașii și au mers după El.');
INSERT INTO &quot;verses&quot; VALUES (480,1,21,'&lt;pb/&gt;S-au&lt;f&gt;ⓤ&lt;/f&gt; dus la Capernaum. În ziua sabatului, intrând în sinagogă, &lt;i&gt;Isus&lt;/i&gt; a început să dea învățătură,');
INSERT INTO &quot;verses&quot; VALUES (480,1,22,'iar &lt;i&gt;oamenii&lt;/i&gt; erau&lt;f&gt;ⓥ&lt;/f&gt; uimiți de învățătura Lui, căci îi învăța ca unul cu autoritate, nu cum &lt;i&gt;îi învățau&lt;/i&gt; cărturarii.');
INSERT INTO &quot;verses&quot; VALUES (480,1,23,'&lt;pb/&gt;Tocmai atunci era în&lt;f&gt;ⓦ&lt;/f&gt; sinagoga lor un om cu un duh necurat, care a început să strige:');
INSERT INTO &quot;verses&quot; VALUES (480,1,24,'„Ce&lt;f&gt;ⓧ&lt;/f&gt; avem noi a face cu Tine, Isuse din Nazaret? Ai venit să ne pierzi? Te știu cine ești: Sfântul lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (480,1,25,'Isus l-a&lt;f&gt;ⓨ&lt;/f&gt; certat și i-a spus: &lt;J&gt;„Taci și ieși din el!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,26,'Și duhul necurat a ieșit din el, scuturându-l&lt;f&gt;ⓩ&lt;/f&gt; și strigând cu putere.');
INSERT INTO &quot;verses&quot; VALUES (480,1,27,'Toți au rămas înmărmuriți și se întrebau unii pe alții: „Ce este aceasta? O învățătură nouă, cu autoritate! Chiar și duhurilor necurate le poruncește, iar ele I se supun!”');
INSERT INTO &quot;verses&quot; VALUES (480,1,28,'Și îndată I s-a dus vestea în toată Galileea.');
INSERT INTO &quot;verses&quot; VALUES (480,1,29,'&lt;pb/&gt;După ce&lt;f&gt;ⓐ&lt;/f&gt; au ieșit din sinagogă, au intrat în casa lui Simon și a lui Andrei, împreună cu Iacov și Ioan.');
INSERT INTO &quot;verses&quot; VALUES (480,1,30,'Soacra lui Simon zăcea cuprinsă de febră. Și îndată I-au vorbit lui Isus despre ea.');
INSERT INTO &quot;verses&quot; VALUES (480,1,31,'El a venit, a apucat-o de mână și a ridicat-o. &lt;i&gt;Atunci&lt;/i&gt; a lăsat-o febra și a început să le slujească.');
INSERT INTO &quot;verses&quot; VALUES (480,1,32,'Pe înserate&lt;f&gt;ⓑ&lt;/f&gt;, după asfințitul soarelui, au adus la El pe toți bolnavii și demonizații.');
INSERT INTO &quot;verses&quot; VALUES (480,1,33,'Și toată cetatea era adunată la ușă.');
INSERT INTO &quot;verses&quot; VALUES (480,1,34,'El i-a vindecat pe mulți care sufereau de felurite boli și a scos mulți demoni; și nu-i&lt;f&gt;ⓒ&lt;/f&gt; lăsa pe demoni să vorbească, pentru că știau cine este.');
INSERT INTO &quot;verses&quot; VALUES (480,1,35,'&lt;pb/&gt;Dis-de-dimineață&lt;f&gt;ⓓ&lt;/f&gt;, pe când era încă întuneric de tot, S-a sculat, a ieșit și S-a dus într-un loc pustiu; și Se ruga acolo.');
INSERT INTO &quot;verses&quot; VALUES (480,1,36,'Simon și cei care erau cu El s-au dus să-L caute');
INSERT INTO &quot;verses&quot; VALUES (480,1,37,'și, când L-au găsit, I-au zis: „Toți Te caută.”');
INSERT INTO &quot;verses&quot; VALUES (480,1,38,'El le-a răspuns: &lt;J&gt;„Să&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;mergem în altă parte, prin târgurile din împrejurimi, ca să propovăduiesc și acolo; căci pentru&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;aceasta am ieșit.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,39,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; S-a dus să propovăduiască&lt;f&gt;ⓖ&lt;/f&gt; în sinagogile lor din toată Galileea și să scoată demoni.');
INSERT INTO &quot;verses&quot; VALUES (480,1,40,'A&lt;f&gt;ⓗ&lt;/f&gt; venit la El un lepros, care s-a aruncat în genunchi înaintea Lui, rugându-L și zicând: „Dacă vrei, poți să mă curățești.”');
INSERT INTO &quot;verses&quot; VALUES (480,1,41,'Lui Isus I s-a făcut milă de el, a întins mâna, S-a atins de el și i-a spus: &lt;J&gt;„Vreau, fii curățit!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,42,'Îndată s-a dus lepra de pe el și a fost curățit.');
INSERT INTO &quot;verses&quot; VALUES (480,1,43,'&lt;i&gt;Isus&lt;/i&gt; l-a trimis de acolo, i-a poruncit cu tărie');
INSERT INTO &quot;verses&quot; VALUES (480,1,44,'și i-a zis: &lt;J&gt;„Vezi să nu spui nimănui nimic, ci mergi și arată-te preotului și adu pentru curățirea ta ce&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;a poruncit Moise, ca mărturie pentru ei!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,1,45,'Dar&lt;f&gt;ⓙ&lt;/f&gt; &lt;i&gt;omul&lt;/i&gt; acela, după ce a plecat, a început să dea de veste și să răspândească vorba, încât &lt;i&gt;Isus&lt;/i&gt; nu mai putea să intre pe față în nicio cetate, ci stătea afară, în locuri pustii, și&lt;f&gt;ⓚ&lt;/f&gt; veneau la El de pretutindeni.');
INSERT INTO &quot;verses&quot; VALUES (480,2,1,'&lt;pb/&gt;După câteva zile, Isus S-a întors&lt;f&gt;ⓛ&lt;/f&gt; în Capernaum. S-a auzit că este în casă');
INSERT INTO &quot;verses&quot; VALUES (480,2,2,'și s-au adunat [îndată] așa de mulți, încât nu mai încăpeau înaintea ușii; El le vestea Cuvântul.');
INSERT INTO &quot;verses&quot; VALUES (480,2,3,'Au venit &lt;i&gt;niște oameni&lt;/i&gt; care I-au adus un paralitic, purtat de patru inși.');
INSERT INTO &quot;verses&quot; VALUES (480,2,4,'Fiindcă nu puteau să-l ducă până la El din cauza mulțimii, au desfăcut acoperișul casei unde era &lt;i&gt;Isus&lt;/i&gt; și, după ce au făcut o spărtură, au coborât pe acolo patul&lt;f&gt;[3]&lt;/f&gt; în care zăcea paraliticul.');
INSERT INTO &quot;verses&quot; VALUES (480,2,5,'Văzându-le credința, Isus i-a zis paraliticului: &lt;J&gt;„Fiule, păcatele îți sunt iertate!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,6,'Niște cărturari care erau acolo se întrebau în inimile lor:');
INSERT INTO &quot;verses&quot; VALUES (480,2,7,'„Cum vorbește el astfel? Hulește! Cine&lt;f&gt;ⓜ&lt;/f&gt; poate ierta păcatele în afară de Unul singur — Dumnezeu?”');
INSERT INTO &quot;verses&quot; VALUES (480,2,8,'Îndată, Isus a cunoscut&lt;f&gt;ⓝ&lt;/f&gt; prin duhul Său că ei gândeau astfel și le-a zis: &lt;J&gt;„De ce aveți astfel de gânduri în inimile voastre?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,9,'&lt;J&gt;Ce&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;este mai ușor? A-i zice paraliticului: «Păcatele îți sunt iertate», sau a-i zice: «Scoală-te, ridică-ți patul și umblă»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,10,'&lt;J&gt;Dar, ca să știți că Fiul Omului are autoritate pe pământ să ierte păcatele,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,11,'&lt;J&gt;ție îți poruncesc”&lt;/J&gt;, i-a zis El paraliticului, &lt;J&gt;„scoală-te, ridică-ți patul și du-te acasă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,12,'Și îndată, paraliticul s-a sculat, și-a ridicat patul și a ieșit afară în fața tuturor; așa că toți au rămas uimiți și-L slăveau pe Dumnezeu, zicând: „Niciodată n-am mai văzut așa ceva!”');
INSERT INTO &quot;verses&quot; VALUES (480,2,13,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; &lt;f&gt;ⓟ&lt;/f&gt; a ieșit iarăși lângă mare. Toată mulțimea &lt;i&gt;de oameni&lt;/i&gt; venea la El, iar El îi învăța.');
INSERT INTO &quot;verses&quot; VALUES (480,2,14,'Când&lt;f&gt;ⓠ&lt;/f&gt; trecea pe acolo, l-a văzut pe Levi, fiul lui Alfeu, stând la vamă. Și i-a zis: &lt;J&gt;„Vino după Mine!”&lt;/J&gt; &lt;i&gt;Levi&lt;/i&gt; s-a sculat și a mers după El.');
INSERT INTO &quot;verses&quot; VALUES (480,2,15,'Pe&lt;f&gt;ⓡ&lt;/f&gt; când stătea Isus la masă în casa lui Levi, mulți vameși și păcătoși au stat și ei la masă cu El și cu ucenicii Lui; căci erau mulți și Îl urmau.');
INSERT INTO &quot;verses&quot; VALUES (480,2,16,'Când L-au văzut mâncând cu vameșii și cu păcătoșii, cărturarii din rândul fariseilor le-au zis ucenicilor Lui: „Cum de mănâncă el [și bea] cu vameșii și cu păcătoșii?”');
INSERT INTO &quot;verses&quot; VALUES (480,2,17,'Auzind &lt;i&gt;aceasta&lt;/i&gt;, Isus le-a zis: &lt;J&gt;„Nu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;cei sănătoși au nevoie de doctor, ci cei bolnavi. Eu n-am venit să-i chem [la pocăință] pe cei drepți, ci pe cei păcătoși.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,18,'&lt;pb/&gt;Ucenicii&lt;f&gt;ⓣ&lt;/f&gt; lui Ioan și fariseii obișnuiau să postească. Au venit și I-au zis lui Isus: „De ce ucenicii lui Ioan și ai fariseilor postesc, iar ucenicii Tăi nu postesc?”');
INSERT INTO &quot;verses&quot; VALUES (480,2,19,'Isus le-a răspuns: &lt;J&gt;„Oare pot posti nuntașii câtă vreme este mirele cu ei? Câtă vreme îl au pe mire cu ei nu pot posti.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,20,'&lt;J&gt;Vor veni zile când mirele va fi luat de la ei și atunci vor posti, în ziua aceea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,21,'&lt;J&gt;Nimeni nu coase un petic de țesătură nouă la o haină veche; altminteri, peticul cel nou trage din materialul vechi, și ruptura se face și mai mare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,22,'&lt;J&gt;Și nimeni nu pune vin nou în burdufuri vechi, căci vinul cel nou sparge burdufurile și se pierd astfel și vinul, și burdufurile; ci vinul nou &lt;i&gt;se pune&lt;/i&gt; în burdufuri noi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,23,'&lt;pb/&gt;S-a întâmplat că, într-o zi de sabat, Isus trecea&lt;f&gt;ⓤ&lt;/f&gt; prin lanurile de grâu. Ucenicii Lui, croindu-și drum, au început să smulgă&lt;f&gt;ⓥ&lt;/f&gt; spice.');
INSERT INTO &quot;verses&quot; VALUES (480,2,24,'Fariseii I-au zis: „Uite ce fac ei în sabat! Ce nu este îngăduit!”');
INSERT INTO &quot;verses&quot; VALUES (480,2,25,'Isus le-a răspuns: &lt;J&gt;„Oare n-ați citit niciodată ce&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;a făcut David când a fost în nevoie și când el și cei care erau împreună cu el au flămânzit?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,26,'&lt;J&gt;Cum a intrat în Casa lui Dumnezeu, pe vremea lui Abiatar&lt;/J&gt;&lt;f&gt;[4]&lt;/f&gt;&lt;J&gt;, marele-preot, și a mâncat pâinile pentru înfățișare&lt;/J&gt;&lt;f&gt;[5]&lt;/f&gt;&lt;J&gt;, pe care&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;nu este îngăduit să le mănânce decât preoții? Și cum a dat și celor care erau cu el?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,27,'Apoi le-a zis: &lt;J&gt;„Sabatul a fost făcut pentru om, și nu omul pentru sabat;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,2,28,'&lt;J&gt;așa că Fiul Omului este Domn&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;și al sabatului.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,1,'&lt;pb/&gt;Isus a&lt;f&gt;ⓩ&lt;/f&gt; intrat din nou în sinagogă. Acolo se afla un om cu mâna uscată.');
INSERT INTO &quot;verses&quot; VALUES (480,3,2,'Ei Îl pândeau &lt;i&gt;pe Isus să vadă&lt;/i&gt; dacă-l va vindeca în ziua sabatului, ca să-L poată învinui.');
INSERT INTO &quot;verses&quot; VALUES (480,3,3,'Și Isus i-a zis omului care avea mâna uscată: &lt;J&gt;„Ridică-te și stai în mijloc!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,4,'Apoi le-a zis: &lt;J&gt;„Este îngăduit în ziua sabatului să faci bine sau să faci rău? Să scapi o viață sau să o pierzi?”&lt;/J&gt; Dar ei tăceau.');
INSERT INTO &quot;verses&quot; VALUES (480,3,5,'Atunci, rotindu-Și privirile cu mânie peste ei și mâhnit de împietrirea inimii lor, i-a zis omului: &lt;J&gt;„Întinde mâna!”&lt;/J&gt; El a întins-o, și mâna i s-a vindecat.');
INSERT INTO &quot;verses&quot; VALUES (480,3,6,'Fariseii&lt;f&gt;ⓐ&lt;/f&gt; au ieșit afară și s-au sfătuit îndată cu irodienii&lt;f&gt;[6]&lt;/f&gt; cum să-L omoare.');
INSERT INTO &quot;verses&quot; VALUES (480,3,7,'&lt;pb/&gt;Isus S-a dus cu ucenicii Săi către mare. După El a mers o mare mulțime din Galileea, din Iudeea,');
INSERT INTO &quot;verses&quot; VALUES (480,3,8,'din Ierusalim, din Idumeea, de dincolo de Iordan și dimprejurul Tirului și Sidonului, mulțime care, auzind tot ce făcea, a venit la El.');
INSERT INTO &quot;verses&quot; VALUES (480,3,9,'Isus le-a poruncit ucenicilor să-I pregătească o barcă, pentru a nu fi îmbulzit de mulțime.');
INSERT INTO &quot;verses&quot; VALUES (480,3,10,'El îi vindeca pe mulți, așa că toți cei care aveau boli se înghesuiau spre El ca să-L atingă.');
INSERT INTO &quot;verses&quot; VALUES (480,3,11,'Duhurile&lt;f&gt;ⓑ&lt;/f&gt; necurate, când Îl vedeau, cădeau la pământ înaintea Lui și strigau: „Tu&lt;f&gt;ⓒ&lt;/f&gt; ești Fiul lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (480,3,12,'Dar El le&lt;f&gt;ⓓ&lt;/f&gt; poruncea cu asprime să nu-L dea pe față.');
INSERT INTO &quot;verses&quot; VALUES (480,3,13,'&lt;pb/&gt;Apoi&lt;f&gt;ⓔ&lt;/f&gt; &lt;i&gt;Isus&lt;/i&gt; S-a suit pe munte, a chemat la Sine pe cine a vrut El, iar ei au venit.');
INSERT INTO &quot;verses&quot; VALUES (480,3,14,'A rânduit dintre ei doisprezece, pe care i-a numit apostoli, ca să fie cu El, să-i trimită să propovăduiască');
INSERT INTO &quot;verses&quot; VALUES (480,3,15,'și să aibă putere [să vindece boli și] să scoată demoni.');
INSERT INTO &quot;verses&quot; VALUES (480,3,16,'Iată-i pe cei doisprezece pe care i-a rânduit: Simon, căruia i-a&lt;f&gt;ⓕ&lt;/f&gt; pus numele Petru,');
INSERT INTO &quot;verses&quot; VALUES (480,3,17,'Iacov, fiul lui Zebedei, și Ioan, fratele său, cărora le-a pus numele &lt;e&gt;Boanerghés&lt;/e&gt;, care înseamnă: „Fiii tunetului”,');
INSERT INTO &quot;verses&quot; VALUES (480,3,18,'Andrei, Filip, Bartolomeu, Matei, Toma, Iacov, fiul lui Alfeu, Tadeu, Simon Cananeul&lt;f&gt;[7]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,19,'și Iuda Iscariot, care L-a și vândut.');
INSERT INTO &quot;verses&quot; VALUES (480,3,20,'&lt;pb/&gt;A intrat într-o casă și s-a adunat din nou mulțimea, astfel încât&lt;f&gt;ⓖ&lt;/f&gt; nu mai puteau nici să mănânce.');
INSERT INTO &quot;verses&quot; VALUES (480,3,21,'Auzind ai Săi&lt;f&gt;[8]&lt;/f&gt; &lt;i&gt;toate acestea&lt;/i&gt;, au venit să pună mâna pe El, fiindcă&lt;f&gt;ⓗ&lt;/f&gt; spuneau că și-a ieșit din minți.');
INSERT INTO &quot;verses&quot; VALUES (480,3,22,'Iar cărturarii care se coborâseră din Ierusalim ziceau: „Îl are pe Beelzebul” și „Cu căpetenia demonilor scoate El demonii!”');
INSERT INTO &quot;verses&quot; VALUES (480,3,23,'Isus&lt;f&gt;ⓘ&lt;/f&gt; însă i-a chemat la El și le-a vorbit în pilde: &lt;J&gt;„Cum poate Satana să-l izgonească pe Satana?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,24,'&lt;J&gt;Dacă o împărăție este dezbinată împotriva ei însăși, împărăția aceea nu poate dăinui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,25,'&lt;J&gt;Și dacă o casă este dezbinată împotriva ei însăși, casa aceea nu poate dăinui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,26,'&lt;J&gt;Tot astfel, dacă Satana se ridică împotriva lui însuși și este dezbinat, nu poate dăinui, ci s-a isprăvit cu el.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,27,'&lt;J&gt;Nimeni&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;nu poate să intre în casa celui puternic și să-l jefuiască de bunuri dacă nu l-a legat mai întâi pe cel puternic; numai atunci îi va jefui casa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,28,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;vă spun că toate li se vor ierta fiilor oamenilor — păcatele și blasfemiile pe care le vor rosti —&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,29,'&lt;J&gt;dar oricine va huli împotriva Duhului Sfânt nu are iertare în veac, ci este vinovat de un păcat veșnic.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,30,'Aceasta pentru că ei ziceau: „Are un duh necurat.”');
INSERT INTO &quot;verses&quot; VALUES (480,3,31,'&lt;pb/&gt;Atunci&lt;f&gt;ⓛ&lt;/f&gt; mama și frații Lui au venit și, stând afară, au trimis să-L cheme.');
INSERT INTO &quot;verses&quot; VALUES (480,3,32,'O mulțime de oameni era în jurul Lui și I-au spus: „Iată, mama Ta, frații Tăi și surorile Tale sunt afară și Te caută.”');
INSERT INTO &quot;verses&quot; VALUES (480,3,33,'El le-a răspuns, zicând: &lt;J&gt;„Cine sunt mama Mea și frații Mei?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,34,'Apoi, rotindu-Și privirile peste cei care erau împrejurul Lui, a zis: &lt;J&gt;„Iată mama Mea și frații Mei!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,3,35,'&lt;J&gt;Căci oricine face voia lui Dumnezeu Îmi este frate, soră și mamă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; a început iarăși&lt;f&gt;ⓜ&lt;/f&gt; să învețe &lt;i&gt;mulțimile&lt;/i&gt; pe țărmul mării. Și s-a adunat la El o mulțime mare &lt;i&gt;de oameni&lt;/i&gt;, așa că S-a suit și a stat într-o barcă, pe mare; iar oamenii stăteau pe țărmul mării.');
INSERT INTO &quot;verses&quot; VALUES (480,4,2,'Apoi a început să-i învețe multe lucruri în pilde; și&lt;f&gt;ⓝ&lt;/f&gt; în învățătura pe care le-o dădea, le spunea:');
INSERT INTO &quot;verses&quot; VALUES (480,4,3,'&lt;J&gt;„Ascultați! Iată, semănătorul a ieșit să semene.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,4,'&lt;J&gt;Pe când semăna, o parte din sămânță a căzut lângă drum: au venit păsările și au mâncat-o.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,5,'&lt;J&gt;O altă parte a căzut pe loc pietros, unde n-avea mult pământ. A răsărit îndată, pentru că n-avea pământ adânc,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,6,'&lt;J&gt;dar când a răsărit soarele, s-a veștejit și, pentru că n-avea rădăcină, s-a uscat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,7,'&lt;J&gt;O altă parte a căzut între spini: spinii au crescut, au înăbușit-o și n-a dat rod.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,8,'&lt;J&gt;Alte &lt;i&gt;semințe&lt;/i&gt; au căzut pe pământ bun: au dat&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;rod, care se înălța și creștea; și au adus &lt;i&gt;rod&lt;/i&gt; una treizeci, alta șaizeci și alta o sută.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,9,'Apoi a zis: &lt;J&gt;„Cine are urechi de auzit să audă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,10,'&lt;pb/&gt;Când&lt;f&gt;ⓟ&lt;/f&gt; au rămas singuri, cei care erau pe lângă El împreună cu cei doisprezece L-au întrebat despre pilde.');
INSERT INTO &quot;verses&quot; VALUES (480,4,11,'&lt;J&gt;„Vouă”&lt;/J&gt;, le-a zis El, &lt;J&gt;„v-a fost dată taina Împărăției lui Dumnezeu, dar pentru cei de&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;afară toate lucrurile sunt &lt;i&gt;înfățișate&lt;/i&gt; în pilde,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,12,'&lt;J&gt;pentru ca&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;,&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;deși privesc, să privească și să nu vadă,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;deși aud, să audă și să nu înțeleagă,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;pentru ca nu cumva să se întoarcă și să fie iertați.&lt;/J&gt;&lt;/e&gt; &lt;J&gt;”&lt;/J&gt;&lt;f&gt;[9]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,13,'&lt;pb/&gt;El le-a mai zis: &lt;J&gt;„Nu înțelegeți pilda aceasta? Cum veți înțelege atunci &lt;i&gt;celelalte&lt;/i&gt; pilde?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,14,'&lt;J&gt;Semănătorul&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;seamănă Cuvântul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,15,'&lt;J&gt;Unii sunt cei de lângă drum: când se seamănă Cuvântul și îl ascultă, vine Satana îndată și ia Cuvântul semănat în ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,16,'&lt;J&gt;Alții sunt cei semănați în locurile pietroase: când aud Cuvântul, îl primesc degrabă cu bucurie,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,17,'&lt;J&gt;dar n-au rădăcină în ei, ci țin până la o vreme și, când vine un necaz sau o prigoană din cauza Cuvântului, se poticnesc îndată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,18,'&lt;J&gt;Alții sunt cei semănați între spini: ei sunt cei care aud Cuvântul,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,19,'&lt;J&gt;dar pătrund în ei grijile lumii, înșelăciunea&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;bogățiilor și alte pofte, care înăbușă Cuvântul și-l fac astfel neroditor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,20,'&lt;J&gt;Iar ceilalți sunt cei semănați în pământul cel bun: aceștia aud Cuvântul, îl primesc și rodesc: unul treizeci, altul șaizeci și altul o sută.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,21,'&lt;pb/&gt;El&lt;f&gt;ⓤ&lt;/f&gt; le-a mai zis: &lt;J&gt;„Oare lampa este adusă ca să fie pusă sub obroc sau sub pat? Nu ca să fie pusă în sfeșnic?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,22,'&lt;J&gt;Nimic&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;nu este ascuns decât ca să fie descoperit și nimic nu este tăinuit decât ca să iasă la iveală.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,23,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;are cineva urechi de auzit, să audă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,24,'El le-a mai zis: &lt;J&gt;„Luați seama la ce auziți! Cu&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;ce măsură măsurați, cu aceea vi se va măsura, ba chiar vi se va da mai mult.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,25,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;celui ce are i se va da; dar celui ce nu are i se va lua și ce are.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,26,'&lt;pb/&gt;El a mai zis: &lt;J&gt;„Împărăția&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;lui Dumnezeu este ca atunci când aruncă un om sămânța în pământ;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,27,'&lt;J&gt;fie că doarme, fie că se trezește, noapte și zi, sămânța încolțește și crește fără să știe el cum.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,28,'&lt;J&gt;Pământul rodește de la sine: întâi un fir &lt;i&gt;verde&lt;/i&gt;, apoi spic, după aceea grâu în spic.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,29,'&lt;J&gt;Când rodul este copt, omul pune&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;îndată secera, pentru că a venit vremea secerișului.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,30,'&lt;pb/&gt;El a mai zis: &lt;J&gt;„Cu&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;ce vom asemăna Împărăția lui Dumnezeu, sau prin ce pildă o vom înfățișa?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,31,'&lt;J&gt;Se aseamănă cu un grăunte de muștar, care, când este semănat în pământ, este cea mai mică dintre toate semințele de pe pământ.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,32,'&lt;J&gt;Dar după ce a fost semănat, crește și se face mai mare decât toate verdețurile &lt;i&gt;din grădină&lt;/i&gt;; și face ramuri mari, așa că păsările cerului își pot face cuiburi la umbra lui.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,33,'&lt;pb/&gt;Isus&lt;f&gt;ⓒ&lt;/f&gt; le vestea Cuvântul prin multe pilde de felul acesta, după cum erau ei în stare să priceapă.');
INSERT INTO &quot;verses&quot; VALUES (480,4,34,'Nu le vorbea deloc fără pilde, dar când era de o parte cu ucenicii, le explica acestora totul.');
INSERT INTO &quot;verses&quot; VALUES (480,4,35,'&lt;pb/&gt;În&lt;f&gt;ⓓ&lt;/f&gt; aceeași zi, seara, &lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Să trecem pe malul celălalt!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,36,'Lăsând mulțimea, &lt;i&gt;ucenicii&lt;/i&gt; L-au luat în barcă așa cum era. În apropiere mai erau și alte bărci.');
INSERT INTO &quot;verses&quot; VALUES (480,4,37,'S-a stârnit o mare furtună, care arunca valurile asupra bărcii, astfel încât aceasta aproape că se umpluse.');
INSERT INTO &quot;verses&quot; VALUES (480,4,38,'Iar El dormea în partea dinapoi, pe un căpătâi. &lt;i&gt;Ucenicii&lt;/i&gt; L-au trezit și I-au zis: „Învățătorule, nu-Ți pasă că pierim?”');
INSERT INTO &quot;verses&quot; VALUES (480,4,39,'Și sculându-Se, El a mustrat vântul și a zis mării: &lt;J&gt;„Taci! Potolește-te!”&lt;/J&gt; Vântul a stat și s-a făcut o liniște adâncă.');
INSERT INTO &quot;verses&quot; VALUES (480,4,40,'Apoi le-a zis: &lt;J&gt;„De ce sunteți fricoși? Tot nu aveți credință?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,4,41,'I-a cuprins o mare frică și ziceau unii către alții: „Cine este Acesta, de I se supun chiar și vântul și marea?”');
INSERT INTO &quot;verses&quot; VALUES (480,5,1,'&lt;pb/&gt;Au&lt;f&gt;ⓔ&lt;/f&gt; trecut pe țărmul celălalt al mării, în ținutul gherasenilor&lt;f&gt;[10]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,5,2,'Când a coborât Isus din barcă, L-a întâmpinat îndată un om care &lt;i&gt;ieșea&lt;/i&gt; din morminte și care &lt;i&gt;avea&lt;/i&gt; un duh necurat.');
INSERT INTO &quot;verses&quot; VALUES (480,5,3,'El își avea locuința în morminte și nu-l putea nimeni lega nici măcar cu lanțuri,');
INSERT INTO &quot;verses&quot; VALUES (480,5,4,'fiindcă de multe ori fusese legat cu cătușe și lanțuri, dar rupsese lanțurile și sfărâmase cătușele, și nimeni nu-l putea stăpâni.');
INSERT INTO &quot;verses&quot; VALUES (480,5,5,'Tot timpul, zi și noapte, stătea în morminte și pe munți, strigând și tăindu-se cu pietre.');
INSERT INTO &quot;verses&quot; VALUES (480,5,6,'Când L-a văzut pe Isus de departe, a alergat, I s-a închinat');
INSERT INTO &quot;verses&quot; VALUES (480,5,7,'și, strigând cu glas tare, a zis: „Ce am eu a face cu Tine, Isuse, Fiul Dumnezeului Preaînalt? Te implor, pentru Dumnezeu, să nu mă chinuiești!”');
INSERT INTO &quot;verses&quot; VALUES (480,5,8,'Căci &lt;i&gt;Isus&lt;/i&gt; îi zicea: &lt;J&gt;„Duh necurat, ieși din omul acesta!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,5,9,'Și l-a întrebat: &lt;J&gt;„Care-ți este numele?”&lt;/J&gt; „Numele meu este «Legiune»&lt;f&gt;[11]&lt;/f&gt;”, a răspuns el, „pentru că suntem mulți.”');
INSERT INTO &quot;verses&quot; VALUES (480,5,10,'Și Îl rugau stăruitor să nu-i trimită afară din ținutul &lt;i&gt;acela&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,5,11,'Acolo, lângă munte, era o turmă mare de porci care pășteau.');
INSERT INTO &quot;verses&quot; VALUES (480,5,12,'Și &lt;i&gt;demonii&lt;/i&gt; L-au rugat zicând: „Trimite-ne în porcii aceia, ca să intrăm în ei.”');
INSERT INTO &quot;verses&quot; VALUES (480,5,13,'Isus le-a dat voie. Și duhurile necurate au ieșit și au intrat în porci; turma, cam la două mii de capete, s-a repezit de pe stâncă în mare și s-a înecat.');
INSERT INTO &quot;verses&quot; VALUES (480,5,14,'&lt;pb/&gt;Porcarii au fugit și au dat de știre în cetate și prin satele &lt;i&gt;vecine&lt;/i&gt;, iar oamenii au venit să vadă ce s-a întâmplat.');
INSERT INTO &quot;verses&quot; VALUES (480,5,15,'Ajungând ei la Isus și văzându-l pe cel ce fusese demonizat că stă jos, îmbrăcat și sănătos la minte — pe el, care avusese legiunea — s-au înspăimântat.');
INSERT INTO &quot;verses&quot; VALUES (480,5,16,'Cei care văzuseră le-au povestit tot ce se petrecuse cu demonizatul și cu porcii.');
INSERT INTO &quot;verses&quot; VALUES (480,5,17,'Atunci au&lt;f&gt;ⓕ&lt;/f&gt; început să-L roage pe Isus să plece din ținutul lor.');
INSERT INTO &quot;verses&quot; VALUES (480,5,18,'&lt;pb/&gt;Pe când urca &lt;i&gt;Isus&lt;/i&gt; în barcă, cel&lt;f&gt;ⓖ&lt;/f&gt; care fusese demonizat Îl ruga să-l lase să rămână cu El.');
INSERT INTO &quot;verses&quot; VALUES (480,5,19,'Isus nu i-a dat voie, ci i-a zis: &lt;J&gt;„Du-te acasă la ai tăi și povestește-le tot ce ți-a făcut Domnul și cum a avut milă de tine!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,5,20,'El a plecat și a început să vestească prin Decapole tot ce făcuse Isus pentru el, și toți se minunau.');
INSERT INTO &quot;verses&quot; VALUES (480,5,21,'&lt;pb/&gt;După&lt;f&gt;ⓗ&lt;/f&gt; ce a trecut Isus iarăși pe celălalt țărm, s-a adunat o mare mulțime în jurul Lui. El stătea lângă mare.');
INSERT INTO &quot;verses&quot; VALUES (480,5,22,'Atunci&lt;f&gt;ⓘ&lt;/f&gt; a venit unul dintre mai-marii sinagogii, pe nume Iair, care, văzându-L pe Isus, a căzut la picioarele Lui');
INSERT INTO &quot;verses&quot; VALUES (480,5,23,'și Îl ruga stăruitor zicând: „Fetița mea este pe moarte; vino dar să-Ți pui mâinile peste ea, ca să fie vindecată și să trăiască!”');
INSERT INTO &quot;verses&quot; VALUES (480,5,24,'Isus a plecat împreună cu el. &lt;pb/&gt;După El mergea o mare mulțime, care Îl îmbulzea.');
INSERT INTO &quot;verses&quot; VALUES (480,5,25,'Acolo era o femeie care de doisprezece ani avea&lt;f&gt;ⓙ&lt;/f&gt; o scurgere de sânge.');
INSERT INTO &quot;verses&quot; VALUES (480,5,26,'Ea suferise mult de la mulți doctori, cheltuise tot ce avea, dar nimic nu-i fusese de ajutor, ba încă ajunsese și mai rău.');
INSERT INTO &quot;verses&quot; VALUES (480,5,27,'A auzit vorbindu-se despre Isus, a venit pe dinapoi prin mulțime și s-a atins de haina Lui,');
INSERT INTO &quot;verses&quot; VALUES (480,5,28,'căci își zicea: „Dacă aș putea să mă ating măcar de hainele Lui, mă voi vindeca.”');
INSERT INTO &quot;verses&quot; VALUES (480,5,29,'Scurgerea ei a încetat îndată și a simțit în trupul ei că s-a vindecat de boală.');
INSERT INTO &quot;verses&quot; VALUES (480,5,30,'Isus a cunoscut îndată că o putere&lt;f&gt;ⓚ&lt;/f&gt; ieșise din El și, întorcându-Se către mulțime, a zis: &lt;J&gt;„Cine s-a atins de hainele Mele?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,5,31,'Ucenicii I-au zis: „Vezi că mulțimea Te îmbulzește și mai întrebi: &lt;J&gt;«Cine s-a atins de Mine?»&lt;/J&gt;”');
INSERT INTO &quot;verses&quot; VALUES (480,5,32,'Dar El Se uita de jur împrejur să vadă pe cea care făcuse aceasta.');
INSERT INTO &quot;verses&quot; VALUES (480,5,33,'Femeia, înfricoșată și tremurând, căci știa ce se petrecuse cu ea, a venit, s-a aruncat la picioarele Lui și I-a spus tot adevărul.');
INSERT INTO &quot;verses&quot; VALUES (480,5,34,'Dar Isus i-a zis: &lt;J&gt;„Fiică, credința&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;ta te-a mântuit; mergi în pace și fii vindecată de boala ta!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,5,35,'&lt;pb/&gt;Pe când vorbea El&lt;f&gt;ⓜ&lt;/f&gt;, au venit unii de la mai-marele sinagogii și au zis: „Fiica ta a murit; de ce îl mai necăjești pe învățător?”');
INSERT INTO &quot;verses&quot; VALUES (480,5,36,'Dar Isus, neținând seama de cuvintele rostite, a zis mai-marelui sinagogii: &lt;J&gt;„Nu te teme; crede numai!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,5,37,'Și n-a îngăduit nimănui să-L însoțească, în afară de Petru, Iacov și Ioan, fratele lui Iacov.');
INSERT INTO &quot;verses&quot; VALUES (480,5,38,'Au ajuns la casa mai-marelui sinagogii, iar Isus a găsit zarvă și pe unii care plângeau și se tânguiau mult.');
INSERT INTO &quot;verses&quot; VALUES (480,5,39,'Intrând &lt;i&gt;în casă&lt;/i&gt;, El le-a zis: &lt;J&gt;„De ce faceți atâta zarvă și plângeți? Copila n-a murit, ci doarme&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,5,40,'Dar ei Îl luau în râs. Isus&lt;f&gt;ⓞ&lt;/f&gt;, după ce i-a scos afară pe toți, i-a luat cu El pe tatăl copilei, pe mama ei și pe cei care &lt;i&gt;erau&lt;/i&gt; cu El și a intrat unde era copila.');
INSERT INTO &quot;verses&quot; VALUES (480,5,41,'Apucând-o de mână, i-a zis: &lt;J&gt;„&lt;e&gt;Talitha kum&lt;/e&gt;”&lt;/J&gt;, care înseamnă: &lt;J&gt;„Fetițo, scoală-te, îți spun!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,5,42,'Îndată, fetița s-a sculat și a început să umble, căci era în vârstă de doisprezece ani. Atunci ei s-au minunat nespus.');
INSERT INTO &quot;verses&quot; VALUES (480,5,43,'Isus le-a&lt;f&gt;ⓟ&lt;/f&gt; poruncit cu tărie să nu spună nimănui lucrul acesta și le-a zis să-i dea &lt;i&gt;fetiței&lt;/i&gt; să mănânce.');
INSERT INTO &quot;verses&quot; VALUES (480,6,1,'&lt;pb/&gt;Isus a&lt;f&gt;ⓠ&lt;/f&gt; plecat de acolo și a venit în patria Lui, iar ucenicii Lui L-au urmat.');
INSERT INTO &quot;verses&quot; VALUES (480,6,2,'Când a venit ziua sabatului, a început să îi învețe pe oameni în sinagogă. Mulți, când Îl auzeau, se mirau și ziceau: „De&lt;f&gt;ⓡ&lt;/f&gt; unde are el aceste lucruri? Cine i-a dat înțelepciunea aceasta și cum se fac astfel de minuni prin mâinile lui?');
INSERT INTO &quot;verses&quot; VALUES (480,6,3,'Nu este acesta tâmplarul, feciorul Mariei, fratele&lt;f&gt;ⓢ&lt;/f&gt; lui Iacov, Iose, Iuda și Simon? Și nu sunt surorile lui aici, printre noi?” Și găseau&lt;f&gt;ⓣ&lt;/f&gt; o pricină de poticnire în El.');
INSERT INTO &quot;verses&quot; VALUES (480,6,4,'Dar Isus le-a zis: &lt;J&gt;„Un profet nu&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;este disprețuit decât în patria lui, între rudele lui și în casa lui.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,6,5,'N-a&lt;f&gt;ⓥ&lt;/f&gt; putut să facă nicio minune acolo, ci doar Și-a pus mâinile peste câțiva bolnavi și i-a vindecat.');
INSERT INTO &quot;verses&quot; VALUES (480,6,6,'Și Se&lt;f&gt;ⓦ&lt;/f&gt; mira de necredința lor. &lt;pb/&gt;Isus străbătea&lt;f&gt;ⓧ&lt;/f&gt; satele din împrejurimi învățându-i pe oameni.');
INSERT INTO &quot;verses&quot; VALUES (480,6,7,'&lt;pb/&gt;Atunci&lt;f&gt;ⓨ&lt;/f&gt; i-a chemat pe cei doisprezece și a început să-i trimită doi câte doi, dându-le autoritate asupra duhurilor necurate.');
INSERT INTO &quot;verses&quot; VALUES (480,6,8,'Le-a poruncit să nu ia cu ei pe drum nimic altceva decât un toiag: nici pâine, nici traistă, nici bani la brâu,');
INSERT INTO &quot;verses&quot; VALUES (480,6,9,'să&lt;f&gt;ⓩ&lt;/f&gt; se încalțe cu sandale și să nu se îmbrace cu două haine.');
INSERT INTO &quot;verses&quot; VALUES (480,6,10,'Apoi le-a zis: &lt;J&gt;„În orice casă&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;veți intra, să rămâneți acolo până veți pleca din locul acela.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,6,11,'&lt;J&gt;Și dacă&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;în vreun loc nu vă vor primi și nu vă vor asculta, să plecați de acolo și să scuturați&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;praful de sub picioarele voastre, ca mărturie pentru ei. [Adevărat vă spun că în ziua judecății va fi mai ușor pentru Sodoma și Gomora decât pentru cetatea aceea.]”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,6,12,'Ucenicii au plecat și au propovăduit pocăința.');
INSERT INTO &quot;verses&quot; VALUES (480,6,13,'Scoteau mulți demoni și&lt;f&gt;ⓓ&lt;/f&gt; ungeau cu untdelemn mulți bolnavi, vindecându-i.');
INSERT INTO &quot;verses&quot; VALUES (480,6,14,'&lt;pb/&gt;Regele Irod a auzit&lt;f&gt;ⓔ&lt;/f&gt; &lt;i&gt;de Isus&lt;/i&gt;, fiindcă Numele Lui ajunsese cunoscut, și zicea: „Ioan Botezătorul a înviat din morți și de aceea lucrează aceste puteri prin el!”');
INSERT INTO &quot;verses&quot; VALUES (480,6,15,'Alții&lt;f&gt;ⓕ&lt;/f&gt; ziceau că este Ilie, iar alții, că este un profet ca cei din vechime&lt;f&gt;[12]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,6,16,'Dar&lt;f&gt;ⓖ&lt;/f&gt; Irod, auzind &lt;i&gt;toate acestea&lt;/i&gt;, zicea: „Ioan, căruia i-am tăiat capul, a înviat!”');
INSERT INTO &quot;verses&quot; VALUES (480,6,17,'Căci Irod însuși trimisese &lt;i&gt;soldați&lt;/i&gt; să-l prindă pe Ioan și-l legase în temniță din cauza Irodiadei, soția fratelui său Filip, pe care o luase în căsătorie.');
INSERT INTO &quot;verses&quot; VALUES (480,6,18,'Ioan îi zicea lui Irod: „Nu-ți&lt;f&gt;ⓗ&lt;/f&gt; este îngăduit să trăiești cu soția fratelui tău!”');
INSERT INTO &quot;verses&quot; VALUES (480,6,19,'Irodiada îl ura pe Ioan și voia să-l omoare, dar nu putea,');
INSERT INTO &quot;verses&quot; VALUES (480,6,20,'fiindcă Irod se temea&lt;f&gt;ⓘ&lt;/f&gt; de el, știindu-l om drept și sfânt. Îl ocrotea și, când îl auzea, era în mare cumpănă, dar îl asculta cu plăcere.');
INSERT INTO &quot;verses&quot; VALUES (480,6,21,'&lt;pb/&gt;Totuși&lt;f&gt;ⓙ&lt;/f&gt;, a venit o zi cu bun prilej, atunci când, sărbătorindu-și ziua&lt;f&gt;ⓚ&lt;/f&gt; de naștere, Irod a dat un ospăț celor cu vază, căpeteniilor oștirii și fruntașilor Galileei.');
INSERT INTO &quot;verses&quot; VALUES (480,6,22,'Fata Irodiadei a intrat &lt;i&gt;la ospăț&lt;/i&gt;, a dansat și le-a fost pe plac lui Irod și celor care ședeau la masă cu el. Regele i-a zis fetei: „Cere-mi orice vrei și-ți voi da!”');
INSERT INTO &quot;verses&quot; VALUES (480,6,23,'Apoi a jurat cu tărie: „Orice&lt;f&gt;ⓛ&lt;/f&gt; îmi vei cere, îți voi da, chiar și jumătate din regatul meu!”');
INSERT INTO &quot;verses&quot; VALUES (480,6,24,'&lt;i&gt;Fata&lt;/i&gt; a ieșit și i-a zis mamei sale: „Ce să cer?” Aceasta i-a răspuns: „Capul lui Ioan Botezătorul!”');
INSERT INTO &quot;verses&quot; VALUES (480,6,25,'Ea a intrat în grabă la rege și i-a cerut, zicând: „Vreau să-mi dai îndată, pe o tavă, capul lui Ioan Botezătorul.”');
INSERT INTO &quot;verses&quot; VALUES (480,6,26,'Regele&lt;f&gt;ⓜ&lt;/f&gt; s-a întristat foarte mult, dar din pricina jurămintelor sale și a oaspeților, n-a vrut să-și încalce cuvântul.');
INSERT INTO &quot;verses&quot; VALUES (480,6,27,'Regele l-a trimis îndată pe călău, cu porunca de a aduce capul lui &lt;i&gt;Ioan&lt;/i&gt;. &lt;i&gt;Acela&lt;/i&gt; s-a dus, i-a tăiat capul lui Ioan în temniță,');
INSERT INTO &quot;verses&quot; VALUES (480,6,28,'l-a adus pe o tavă, i l-a dat fetei, iar fata i l-a dat mamei sale.');
INSERT INTO &quot;verses&quot; VALUES (480,6,29,'Ucenicii lui Ioan, când au auzit acest lucru, au venit, i-au luat trupul și l-au pus într-un mormânt.');
INSERT INTO &quot;verses&quot; VALUES (480,6,30,'&lt;pb/&gt;Apostolii&lt;f&gt;ⓝ&lt;/f&gt; s-au adunat la Isus și I-au spus tot ce făcuseră și îi învățaseră &lt;i&gt;pe oameni&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,6,31,'Isus&lt;f&gt;ⓞ&lt;/f&gt; le-a zis: &lt;J&gt;„Veniți doar voi, într-un loc pustiu, și odihniți-vă puțin!”&lt;/J&gt; Căci erau&lt;f&gt;ⓟ&lt;/f&gt; mulți care veneau și plecau, iar ei n-aveau timp nici să mănânce.');
INSERT INTO &quot;verses&quot; VALUES (480,6,32,'Au&lt;f&gt;ⓠ&lt;/f&gt; plecat cu barca spre un loc pustiu.');
INSERT INTO &quot;verses&quot; VALUES (480,6,33,'Oamenii i-au văzut plecând și i-au recunoscut; au alergat pe jos din toate cetățile și au ajuns înaintea lor &lt;i&gt;acolo&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,6,34,'Când&lt;f&gt;ⓡ&lt;/f&gt; a coborât &lt;i&gt;din barcă&lt;/i&gt;, &lt;i&gt;Isus&lt;/i&gt; a văzut o mulțime mare &lt;i&gt;de oameni&lt;/i&gt; și I s-a făcut milă de ei, pentru că erau &lt;e&gt;ca niște oi care nu au păstor&lt;/e&gt;&lt;f&gt;[13]&lt;/f&gt;, și a&lt;f&gt;ⓢ&lt;/f&gt; început să-i învețe multe.');
INSERT INTO &quot;verses&quot; VALUES (480,6,35,'Fiindcă&lt;f&gt;ⓣ&lt;/f&gt; se făcuse târziu, ucenicii s-au apropiat de El și I-au zis: „Locul acesta este pustiu și ora este înaintată.');
INSERT INTO &quot;verses&quot; VALUES (480,6,36,'Dă-le drumul să se ducă în cătunele și satele din împrejurimi, ca să-și cumpere ceva de mâncare!”');
INSERT INTO &quot;verses&quot; VALUES (480,6,37,'&lt;J&gt;„Dați-le voi să mănânce!”&lt;/J&gt;, le-a răspuns &lt;i&gt;Isus&lt;/i&gt;. Dar ei I-au zis: „Să&lt;f&gt;ⓤ&lt;/f&gt; ne ducem să cumpărăm pâini de două sute de dinari&lt;f&gt;[14]&lt;/f&gt; și să le dăm să mănânce?”');
INSERT INTO &quot;verses&quot; VALUES (480,6,38,'Și El i-a întrebat: &lt;J&gt;„Câte pâini aveți? Mergeți și vedeți!”&lt;/J&gt; După ce au aflat, au răspuns: „Cinci&lt;f&gt;ⓥ&lt;/f&gt; &lt;i&gt;pâini&lt;/i&gt; și doi pești.”');
INSERT INTO &quot;verses&quot; VALUES (480,6,39,'Atunci a poruncit să se așeze toți, grupuri-grupuri, pe iarba verde.');
INSERT INTO &quot;verses&quot; VALUES (480,6,40,'Și au stat jos în grupuri de câte o sută și de câte cincizeci.');
INSERT INTO &quot;verses&quot; VALUES (480,6,41,'El a luat cele cinci pâini și cei doi pești, Și-a ridicat ochii spre cer și a rostit binecuvântarea&lt;f&gt;ⓦ&lt;/f&gt;. Apoi a frânt pâinile și le-a dat ucenicilor, ca ei să le împartă mulțimii. Și cei doi pești i-a împărțit tuturor.');
INSERT INTO &quot;verses&quot; VALUES (480,6,42,'Au mâncat toți și s-au săturat,');
INSERT INTO &quot;verses&quot; VALUES (480,6,43,'și au ridicat douăsprezece coșuri pline cu bucățile rămase din pâine și din pești.');
INSERT INTO &quot;verses&quot; VALUES (480,6,44,'Cei care mâncaseră pâinile erau cinci mii de bărbați.');
INSERT INTO &quot;verses&quot; VALUES (480,6,45,'&lt;pb/&gt;Îndată&lt;f&gt;ⓧ&lt;/f&gt;, Isus i-a silit pe ucenicii Săi să intre în barcă și să treacă înaintea Lui de cealaltă parte, spre Betsaida, până ce El dă drumul mulțimii.');
INSERT INTO &quot;verses&quot; VALUES (480,6,46,'După ce și-a luat rămas-bun de la ei, S-a dus pe munte ca să Se roage.');
INSERT INTO &quot;verses&quot; VALUES (480,6,47,'Când&lt;f&gt;ⓨ&lt;/f&gt; s-a înserat, barca era în mijlocul mării, iar Isus era singur pe țărm.');
INSERT INTO &quot;verses&quot; VALUES (480,6,48,'I-a văzut pe ucenici necăjindu-se cu vâslitul, căci vântul le era împotrivă, și într-a patra strajă din noapte&lt;f&gt;[15]&lt;/f&gt;, a venit la ei umblând pe mare; și voia&lt;f&gt;ⓩ&lt;/f&gt; să treacă pe lângă ei.');
INSERT INTO &quot;verses&quot; VALUES (480,6,49,'Când L-au văzut ei umblând pe mare, au crezut că este o nălucă și au țipat,');
INSERT INTO &quot;verses&quot; VALUES (480,6,50,'pentru că toți L-au văzut și s-au înspăimântat. &lt;i&gt;Isus&lt;/i&gt; le-a vorbit îndată și le-a zis: &lt;J&gt;„Îndrăzniți, Eu sunt; nu vă temeți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,6,51,'Apoi S-a suit la ei în barcă, iar vântul s-a oprit. Ei au rămas nespus de uimiți,');
INSERT INTO &quot;verses&quot; VALUES (480,6,52,'căci nu&lt;f&gt;ⓐ&lt;/f&gt; înțeleseseră &lt;i&gt;minunea&lt;/i&gt; cu pâinile, fiindcă le era inima&lt;f&gt;ⓑ&lt;/f&gt; împietrită.');
INSERT INTO &quot;verses&quot; VALUES (480,6,53,'&lt;pb/&gt;După&lt;f&gt;ⓒ&lt;/f&gt; ce au trecut marea, au venit în ținutul Ghenezaretului și au tras la mal.');
INSERT INTO &quot;verses&quot; VALUES (480,6,54,'Când au coborât din barcă, oamenii L-au recunoscut îndată &lt;i&gt;pe Isus&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (480,6,55,'au alergat prin tot ținutul și au început să aducă bolnavi în paturi, pretutindeni unde se auzea că era El.');
INSERT INTO &quot;verses&quot; VALUES (480,6,56,'Oriunde intra, în sate, cetăți sau cătune, îi puneau pe bolnavi în piețe și-L rugau să le dea voie măcar&lt;f&gt;ⓓ&lt;/f&gt; de poala hainei Lui să se atingă. Și toți câți se atingeau de El erau vindecați.');
INSERT INTO &quot;verses&quot; VALUES (480,7,1,'&lt;pb/&gt;Fariseii și câțiva cărturari veniți din Ierusalim s-au&lt;f&gt;ⓔ&lt;/f&gt; adunat la Isus.');
INSERT INTO &quot;verses&quot; VALUES (480,7,2,'Ei i-au văzut pe unii dintre ucenicii Lui prânzind cu mâinile necurate, adică nespălate.');
INSERT INTO &quot;verses&quot; VALUES (480,7,3,'Însă fariseii și toți iudeii nu mănâncă fără să-și spele mâinile după rânduială, ținând tradiția din bătrâni.');
INSERT INTO &quot;verses&quot; VALUES (480,7,4,'Și, când vin de la piață, nu mănâncă decât după ce s-au îmbăiat. Și multe altele le-au primit ei ca să le păzească, &lt;i&gt;precum&lt;/i&gt; spălarea paharelor, a urcioarelor, a vaselor de aramă și a paturilor&lt;f&gt;[16]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,7,5,'Iar fariseii și cărturarii L-au întrebat&lt;f&gt;ⓕ&lt;/f&gt;: „Pentru ce nu trăiesc ucenicii tăi după tradiția din bătrâni, ci prânzesc cu mâinile necurate?”');
INSERT INTO &quot;verses&quot; VALUES (480,7,6,'Isus le-a răspuns: &lt;J&gt;„Bine a profețit Isaia despre voi, fățarnicilor, după cum este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Poporul&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;&lt;J&gt;acesta Mă cinstește cu buzele,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;dar inima lor este departe de Mine!&lt;/J&gt;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,7,'&lt;t&gt;&lt;e&gt;&lt;J&gt;În zadar Mi se închină ei,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;dând învățături care nu sunt decât porunci omenești.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[17]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,8,'&lt;pb/&gt;&lt;J&gt;Voi lepădați porunca lui Dumnezeu și țineți tradiția oamenilor [precum spălarea urcioarelor și a paharelor, și faceți multe alte lucruri ca acestea].”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,9,'Și le-a mai zis: &lt;J&gt;„Frumos dați voi la o parte porunca lui Dumnezeu ca să țineți tradiția voastră.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,10,'&lt;J&gt;Căci Moise a zis: &lt;e&gt;Să cinstești pe tatăl tău și pe mama ta&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[18]&lt;/f&gt; &lt;J&gt;și: &lt;e&gt;Cine blestemă pe tatăl lui sau pe mama lui să fie pedepsit cu moartea.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[19]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,11,'&lt;J&gt;Voi însă ziceți: «Un om poate spune tatălui său ori mamei sale: ‘Este &lt;e&gt;korban&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓗ&lt;/f&gt;&lt;J&gt;’ , adică dăruit lui Dumnezeu, ‘ceea ce îți pot da’»;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,12,'&lt;J&gt;și nu-l mai lăsați să facă nimic pentru tatăl sau mama lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,13,'&lt;J&gt;Ați desființat astfel Cuvântul lui Dumnezeu prin tradiția pe care o transmiteți. Și multe ca acestea mai faceți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,14,'&lt;pb/&gt;Și&lt;f&gt;ⓘ&lt;/f&gt; chemând din nou mulțimea la Sine, a zis: &lt;J&gt;„Ascultați-Mă toți și înțelegeți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,15,'&lt;J&gt;Nimic din ce este în afara omului nu poate să-l facă necurat, intrând în el. Dar ce iese din om, aceea îl face necurat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,16,'&lt;J&gt;[Dacă&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;are cineva urechi de auzit, să audă.”]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,17,'După ce&lt;f&gt;ⓚ&lt;/f&gt; a lăsat mulțimea și a intrat în casă, ucenicii Lui L-au întrebat cu privire la pilda aceasta.');
INSERT INTO &quot;verses&quot; VALUES (480,7,18,'El le-a zis: &lt;J&gt;„Și voi sunteți fără pricepere? Nu înțelegeți că nimic din ce intră în om din afară nu-l poate face necurat?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,19,'&lt;J&gt;Aceasta pentru că nu intră în inima lui, ci în pântec și apoi iese afară, în hazna”&lt;/J&gt;, curățind &lt;i&gt;astfel&lt;/i&gt; toate bucatele.');
INSERT INTO &quot;verses&quot; VALUES (480,7,20,'El le-a mai zis: &lt;J&gt;„Ce iese din om, aceea îl face necurat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,21,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;dinăuntru, din inima oamenilor, ies gândurile rele, desfrânările, hoțiile, uciderile,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,22,'&lt;J&gt;adulterele, lăcomiile, răutățile, înșelăciunea, nerușinarea, invidia&lt;/J&gt;&lt;f&gt;[20]&lt;/f&gt;&lt;J&gt;, blasfemia, trufia, nechibzuința.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,23,'&lt;J&gt;Toate aceste rele ies dinăuntru și îl fac pe om necurat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,24,'&lt;pb/&gt;Isus&lt;f&gt;ⓜ&lt;/f&gt; a plecat de acolo și S-a dus în ținutul Tirului [și al Sidonului]. A intrat într-o casă, dorind să nu știe nimeni &lt;i&gt;că este acolo&lt;/i&gt;, dar n-a putut să rămână ascuns.');
INSERT INTO &quot;verses&quot; VALUES (480,7,25,'Ci, îndată, o femeie a cărei fiică avea un duh necurat a auzit despre El și a venit și I-a căzut la picioare.');
INSERT INTO &quot;verses&quot; VALUES (480,7,26,'Femeia aceasta era vorbitoare de limbă greacă, de obârșie siro-feniciană. Ea L-a rugat să scoată demonul din fiica ei.');
INSERT INTO &quot;verses&quot; VALUES (480,7,27,'Isus i-a zis: &lt;J&gt;„Lasă mai întâi să se sature copiii, căci nu este bine să iei pâinea copiilor și s-o arunci la căței!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,28,'„Da, Doamne”, I-a răspuns ea, „dar și cățeii mănâncă sub masă din firimiturile copiilor!”');
INSERT INTO &quot;verses&quot; VALUES (480,7,29,'Atunci Isus i-a zis: &lt;J&gt;„Pentru cuvântul acesta, du-te; a ieșit demonul din fiica ta.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,30,'Când s-a dus femeia acasă, a găsit copila culcată în pat, iar demonul ieșise din ea.');
INSERT INTO &quot;verses&quot; VALUES (480,7,31,'&lt;pb/&gt;Isus&lt;f&gt;ⓝ&lt;/f&gt; a părăsit iarăși ținutul Tirului și a venit prin Sidon către Marea Galileei, în ținutul Decapole.');
INSERT INTO &quot;verses&quot; VALUES (480,7,32,'I-au&lt;f&gt;ⓞ&lt;/f&gt; adus un surd care era și gângav și L-au rugat să-Și pună mâna peste el.');
INSERT INTO &quot;verses&quot; VALUES (480,7,33,'El l-a luat deoparte din mulțime, apoi Și-a pus degetele în urechile lui și i-a atins limba cu scuipatul&lt;f&gt;ⓟ&lt;/f&gt; Său&lt;f&gt;[21]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,7,34,'Și-a ridicat&lt;f&gt;ⓠ&lt;/f&gt; ochii spre cer, a suspinat&lt;f&gt;ⓡ&lt;/f&gt; și a zis: &lt;e&gt;&lt;J&gt;„Effata”&lt;/J&gt;&lt;/e&gt;, care înseamnă: &lt;J&gt;„Deschide-te!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,7,35,'Îndată&lt;f&gt;ⓢ&lt;/f&gt; i s-au deschis urechile, i s-a dezlegat limba și vorbea foarte deslușit.');
INSERT INTO &quot;verses&quot; VALUES (480,7,36,'Isus le-a&lt;f&gt;ⓣ&lt;/f&gt; poruncit să nu spună nimănui, dar cu cât le poruncea &lt;i&gt;mai stăruitor&lt;/i&gt;, cu atât Îl vesteau mai mult.');
INSERT INTO &quot;verses&quot; VALUES (480,7,37,'Ei erau uimiți peste măsură și ziceau: „Cât de bine le face pe toate! Chiar și pe surzi îi face să audă și pe muți să vorbească!”');
INSERT INTO &quot;verses&quot; VALUES (480,8,1,'&lt;pb/&gt;În zilele acelea&lt;f&gt;ⓤ&lt;/f&gt;, strângându-se din nou o mare mulțime și neavând ce să mănânce, &lt;i&gt;Isus&lt;/i&gt; Și-a chemat ucenicii și le-a zis:');
INSERT INTO &quot;verses&quot; VALUES (480,8,2,'&lt;J&gt;„Mi-e milă de mulțimea aceasta, pentru că &lt;i&gt;acești oameni&lt;/i&gt; de trei zile stau lângă Mine și nu au ce mânca.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,3,'&lt;J&gt;Dacă le voi da drumul acasă flămânzi, vor leșina pe drum; iar unii dintre ei au venit de departe.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,4,'Ucenicii I-au răspuns: „Cum ar putea cineva să-i sature pe toți aceștia cu pâine aici, în pustiu?”');
INSERT INTO &quot;verses&quot; VALUES (480,8,5,'&lt;J&gt;„Câte pâini aveți?”&lt;/J&gt;, i-a întrebat&lt;f&gt;ⓥ&lt;/f&gt; &lt;i&gt;Isus&lt;/i&gt;. „Șapte”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (480,8,6,'Atunci a poruncit mulțimii să se așeze pe pământ și, luând cele șapte pâini, a adus mulțumire, le-a frânt și le-a dat ucenicilor Săi să le împartă, iar ei le-au împărțit mulțimii.');
INSERT INTO &quot;verses&quot; VALUES (480,8,7,'Mai aveau și câțiva pești. După ce i-a binecuvântat&lt;f&gt;ⓦ&lt;/f&gt;, a poruncit să-i împartă și pe aceia.');
INSERT INTO &quot;verses&quot; VALUES (480,8,8,'Au mâncat și s-au săturat și au ridicat șapte coșuri cu bucățile rămase.');
INSERT INTO &quot;verses&quot; VALUES (480,8,9,'Erau cam patru mii &lt;i&gt;de oameni&lt;/i&gt;. Apoi &lt;i&gt;Isus&lt;/i&gt; le-a dat drumul.');
INSERT INTO &quot;verses&quot; VALUES (480,8,10,'&lt;pb/&gt;Isus S-a urcat îndată&lt;f&gt;ⓧ&lt;/f&gt; în barcă împreună cu ucenicii Săi și a venit în părțile Dalmanutei.');
INSERT INTO &quot;verses&quot; VALUES (480,8,11,'Fariseii au venit&lt;f&gt;ⓨ&lt;/f&gt; și au început să discute aprins cu El. Și, ca să-L ispitească, I-au cerut un semn din cer.');
INSERT INTO &quot;verses&quot; VALUES (480,8,12,'&lt;i&gt;Isus&lt;/i&gt; a suspinat în duhul Său și a zis: &lt;J&gt;„De ce caută generația aceasta un semn? Adevărat vă spun că generației acesteia nu i se va da niciun semn!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,13,'Apoi i-a lăsat, S-a urcat din nou în barcă și a plecat spre celălalt țărm.');
INSERT INTO &quot;verses&quot; VALUES (480,8,14,'&lt;i&gt;Ucenicii&lt;/i&gt;&lt;f&gt;ⓩ&lt;/f&gt; uitaseră să ia pâini; cu ei, în barcă, n-aveau decât o pâine.');
INSERT INTO &quot;verses&quot; VALUES (480,8,15,'&lt;i&gt;Isus&lt;/i&gt; îi povățuia&lt;f&gt;ⓐ&lt;/f&gt;, zicându-le: &lt;J&gt;„Vedeți să vă păziți de aluatul fariseilor și de aluatul lui Irod!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,16,'&lt;i&gt;Ucenicii&lt;/i&gt; vorbeau între ei că nu au pâini&lt;f&gt;ⓑ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,8,17,'Știind aceasta, &lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„De ce vorbiți între voi că nu aveți pâini? Tot&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;nu pricepeți și tot nu înțelegeți? Aveți inima împietrită?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,18,'&lt;e&gt;&lt;J&gt;Aveți ochi, și nu vedeți? Aveți urechi, și nu auziți?&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[22]&lt;/f&gt; &lt;J&gt;Și nu vă amintiți nimic?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,19,'&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;am frânt cinci pâini la cei cinci mii, câte coșuri pline cu bucăți rămase ați ridicat?”&lt;/J&gt; „Douăsprezece”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (480,8,20,'&lt;J&gt;„Și când&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;i&gt;&lt;J&gt;am frânt&lt;/i&gt; șapte &lt;i&gt;pâini&lt;/i&gt; pentru cei patru mii, câte coșuri pline cu bucăți rămase ați ridicat?”&lt;/J&gt; „Șapte”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (480,8,21,'Și El le-a zis: &lt;J&gt;„Tot nu&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;înțelegeți?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,22,'&lt;pb/&gt;Au ajuns la Betsaida și I-au adus un orb, rugându-L să Se atingă de el.');
INSERT INTO &quot;verses&quot; VALUES (480,8,23,'&lt;i&gt;Isus&lt;/i&gt; l-a luat de mână și l-a scos din sat, apoi i-a pus scuipat&lt;f&gt;ⓖ&lt;/f&gt; pe ochi&lt;f&gt;[23]&lt;/f&gt;, Și-a pus mâinile peste el și l-a întrebat: &lt;J&gt;„Vezi ceva?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,24,'Ridicându-și privirea, el a zis: „Văd niște oameni ca niște copaci; îi văd umblând.”');
INSERT INTO &quot;verses&quot; VALUES (480,8,25,'Atunci Isus Și-a pus din nou mâinile pe ochii lui, iar el a deschis ochii&lt;f&gt;[24]&lt;/f&gt;, a fost vindecat și a început să vadă totul deslușit.');
INSERT INTO &quot;verses&quot; VALUES (480,8,26,'&lt;i&gt;Isus&lt;/i&gt; l-a trimis acasă, spunându-i: &lt;J&gt;„Să nu intri în sat [și nici&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;să nu spui cuiva]!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,27,'&lt;pb/&gt;Isus&lt;f&gt;ⓘ&lt;/f&gt; și ucenicii Săi au mers către satele Cezareei lui Filip. Pe drum, Isus i-a întrebat pe ucenici, zicându-le: &lt;J&gt;„Cine zic oamenii că sunt Eu?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,28,'Ei I-au răspuns: „Ioan&lt;f&gt;ⓙ&lt;/f&gt; Botezătorul; alții, Ilie; alții, unul din profeți.”');
INSERT INTO &quot;verses&quot; VALUES (480,8,29,'Atunci El i-a întrebat: &lt;J&gt;„Dar voi cine ziceți că sunt?”&lt;/J&gt; Petru i-a răspuns, zicându-I: „Tu&lt;f&gt;ⓚ&lt;/f&gt; ești Hristosul!”');
INSERT INTO &quot;verses&quot; VALUES (480,8,30,'Isus&lt;f&gt;ⓛ&lt;/f&gt; le-a poruncit cu tărie să nu spună nimănui despre El.');
INSERT INTO &quot;verses&quot; VALUES (480,8,31,'&lt;pb/&gt;Atunci a&lt;f&gt;ⓜ&lt;/f&gt; început să-i învețe că Fiul Omului trebuie să pătimească multe, să fie respins de bătrâni, de preoții de seamă și de cărturari, să fie omorât și după trei zile să învie.');
INSERT INTO &quot;verses&quot; VALUES (480,8,32,'Le spunea lucrurile acestea pe față. Însă Petru L-a luat deoparte și a început să-L mustre.');
INSERT INTO &quot;verses&quot; VALUES (480,8,33,'Dar &lt;i&gt;Isus&lt;/i&gt; S-a întors și S-a uitat la ucenici, l-a mustrat pe Petru și i-a zis: &lt;J&gt;„Înapoia Mea, Satano, fiindcă tu nu te gândești la lucrurile lui Dumnezeu, ci la cele ale oamenilor!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,34,'Și chemând apoi mulțimea împreună cu ucenicii Săi, le-a zis: &lt;J&gt;„Dacă vrea cineva&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;să vină după Mine, să se lepede de sine, să-și ia crucea și să Mă urmeze,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,35,'&lt;J&gt;căci cine&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;va vrea să-și scape viața o va pierde, dar cine își va pierde viața pentru Mine și pentru Evanghelie și-o va scăpa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,36,'&lt;J&gt;Și la ce i-ar folosi cuiva să câștige toată lumea dacă și-ar pierde sufletul?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,37,'&lt;J&gt;Sau ce ar putea da un om în schimb pentru sufletul său?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,8,38,'&lt;J&gt;Pentru că&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;de oricine se&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;va rușina de Mine și de cuvintele Mele, în această generație adulteră&lt;/J&gt;&lt;f&gt;[25]&lt;/f&gt; &lt;J&gt;și păcătoasă, Se va rușina și Fiul Omului când va veni în slava Tatălui Său împreună cu îngerii cei sfinți.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,1,'&lt;pb/&gt;Le-a mai zis: &lt;J&gt;„Adevărat&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;vă spun că sunt unii din cei ce stau aici care nu vor gusta moartea până nu vor vedea Împărăția&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;lui Dumnezeu venind cu putere.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,2,'&lt;pb/&gt;După&lt;f&gt;ⓣ&lt;/f&gt; șase zile, Isus i-a luat cu El pe Petru, pe Iacov și pe Ioan și i-a dus deoparte pe un munte înalt, numai pe ei. Și S-a schimbat la față înaintea lor;');
INSERT INTO &quot;verses&quot; VALUES (480,9,3,'hainele I s-au făcut strălucitoare și foarte albe&lt;f&gt;ⓤ&lt;/f&gt;, de un alb pe care niciun înălbitor de pe pământ nu-l poate da.');
INSERT INTO &quot;verses&quot; VALUES (480,9,4,'Și li s-au arătat Ilie și Moise, care stăteau de vorbă cu Isus.');
INSERT INTO &quot;verses&quot; VALUES (480,9,5,'Petru a luat cuvântul și I-a zis lui Isus: „&lt;e&gt;Rabbí&lt;/e&gt;&lt;f&gt;[26]&lt;/f&gt;, este bine să fim aici; să facem trei colibe: una pentru Tine, una pentru Moise și una pentru Ilie.”');
INSERT INTO &quot;verses&quot; VALUES (480,9,6,'De fapt, nu știa ce să spună, căci erau înspăimântați.');
INSERT INTO &quot;verses&quot; VALUES (480,9,7,'Și a venit un nor care i-a acoperit cu umbra lui, iar din nor s-a auzit un glas care zicea: „Acesta este Fiul Meu preaiubit: de El să ascultați!”');
INSERT INTO &quot;verses&quot; VALUES (480,9,8,'Și îndată, uitându-se în jur, n-au mai văzut pe nimeni decât pe Isus, singur cu ei.');
INSERT INTO &quot;verses&quot; VALUES (480,9,9,'Pe&lt;f&gt;ⓥ&lt;/f&gt; când coborau de pe munte, Isus le-a poruncit să nu povestească nimănui ce au văzut până când Fiul Omului va învia din morți.');
INSERT INTO &quot;verses&quot; VALUES (480,9,10,'Și au păstrat pentru ei cuvântul acesta, întrebându-se între ei ce înseamnă că „va învia din morți”.');
INSERT INTO &quot;verses&quot; VALUES (480,9,11,'&lt;i&gt;Ucenicii&lt;/i&gt; L-au întrebat, zicând: „De ce spun cărturarii că&lt;f&gt;ⓦ&lt;/f&gt; mai întâi trebuie să vină Ilie?”');
INSERT INTO &quot;verses&quot; VALUES (480,9,12,'Iar El le-a spus: &lt;J&gt;„Da, mai întâi vine Ilie ca să așeze toate &lt;i&gt;la locul lor&lt;/i&gt;; și cum de&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;stă scris despre Fiul Omului că va suferi multe și&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;va fi disprețuit?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,13,'&lt;J&gt;Eu vă spun că Ilie&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;a și venit, și i-au făcut tot ce au vrut, după cum este scris despre el.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,14,'&lt;pb/&gt;Când&lt;f&gt;ⓐ&lt;/f&gt; au ajuns la ucenici, au văzut o mare mulțime împrejurul lor și pe cărturari discutând aprins cu ei.');
INSERT INTO &quot;verses&quot; VALUES (480,9,15,'Și îndată, văzându-L, toată mulțimea a fost cuprinsă de freamăt și a alergat la El să-L întâmpine.');
INSERT INTO &quot;verses&quot; VALUES (480,9,16,'Iar El i-a întrebat: &lt;J&gt;„Despre ce vorbeați atât de aprins cu ei?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,17,'Și unul din mulțime I-a răspuns&lt;f&gt;ⓑ&lt;/f&gt;: „Învățătorule, am adus la Tine pe fiul meu, care are un duh mut.');
INSERT INTO &quot;verses&quot; VALUES (480,9,18,'Și ori de câte ori îl apucă, îl trântește la pământ, iar el face spume la gură, scrâșnește din dinți și înțepenește. Le-am spus ucenicilor Tăi să-l scoată, dar n-au putut.”');
INSERT INTO &quot;verses&quot; VALUES (480,9,19,'Iar El le-a răspuns: &lt;J&gt;„O, generație necredincioasă, până când voi mai fi cu voi? Până când vă voi mai răbda? Aduceți-l la Mine!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,20,'Și l-au adus la El. Și îndată, văzându-L&lt;f&gt;ⓒ&lt;/f&gt;, duhul l-a scuturat &lt;i&gt;pe copil,&lt;/i&gt; și &lt;i&gt;acesta&lt;/i&gt;, căzând la pământ, se zvârcolea și făcea spume la gură.');
INSERT INTO &quot;verses&quot; VALUES (480,9,21,'Și &lt;i&gt;Isus&lt;/i&gt; l-a întrebat pe tatăl lui: &lt;J&gt;„De câtă vreme i se întâmplă asta?”&lt;/J&gt; „Din copilărie”, a răspuns el.');
INSERT INTO &quot;verses&quot; VALUES (480,9,22,'„De multe ori l-a aruncat și în foc, și în apă, ca să-l omoare. Dar, dacă poți face ceva, fie-Ți milă de noi și ajută-ne!”');
INSERT INTO &quot;verses&quot; VALUES (480,9,23,'Isus i-a răspuns: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;poți!? Toate lucrurile sunt cu putință pentru cel ce crede!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,24,'Îndată, tatăl copilului a strigat: „Cred! Ajută necredinței mele!”');
INSERT INTO &quot;verses&quot; VALUES (480,9,25,'Când a văzut Isus că mulțimea vine în fugă, a mustrat duhul necurat, zicându-i: &lt;J&gt;„Duh mut și surd, îți poruncesc să ieși și să nu mai intri în el!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,26,'Și țipând și scuturându-l cu mare putere, &lt;i&gt;duhul&lt;/i&gt; a ieșit. &lt;i&gt;Copilul&lt;/i&gt; a rămas ca mort, încât mulți ziceau că a murit.');
INSERT INTO &quot;verses&quot; VALUES (480,9,27,'Dar Isus, apucându-l de mână, l-a ridicat, iar el s-a sculat în picioare.');
INSERT INTO &quot;verses&quot; VALUES (480,9,28,'Când&lt;f&gt;ⓔ&lt;/f&gt; a intrat Isus în casă, ucenicii Lui L-au luat deoparte și L-au întrebat: „Noi de ce n-am putut să-l scoatem?”');
INSERT INTO &quot;verses&quot; VALUES (480,9,29,'El le-a zis: &lt;J&gt;„Acest soi &lt;i&gt;de demoni&lt;/i&gt; nu poate fi scos cu nimic altceva decât cu rugăciune [și post].”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,30,'&lt;pb/&gt;Plecând din acel loc, au trecut prin Galileea, dar [Isus] nu voia să se știe că trecea &lt;i&gt;pe acolo&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,9,31,'Căci&lt;f&gt;ⓕ&lt;/f&gt; îi învăța pe ucenicii Săi și le zicea: &lt;J&gt;„Fiul Omului va fi dat în mâinile oamenilor; ei Îl vor omorî, și a treia zi după ce-L vor omorî, va învia.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,32,'Dar ei nu înțelegeau cuvântul acesta și se temeau să-L întrebe.');
INSERT INTO &quot;verses&quot; VALUES (480,9,33,'&lt;pb/&gt;Apoi&lt;f&gt;ⓖ&lt;/f&gt; au venit la Capernaum. Pe când era în casă, El i-a întrebat: &lt;J&gt;„Despre ce vorbeați unul cu altul pe drum?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,34,'Dar ei tăceau, pentru că pe drum avuseseră o dispută, &lt;i&gt;vrând să știe&lt;/i&gt; cine este mai mare.');
INSERT INTO &quot;verses&quot; VALUES (480,9,35,'Și așezându-Se, i-a chemat pe cei doisprezece și le-a zis: &lt;J&gt;„Dacă vrea cineva&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;să fie cel dintâi, să fie cel mai de pe urmă dintre toți și slujitorul tuturor!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,36,'Și luând&lt;f&gt;ⓘ&lt;/f&gt; un copilaș, l-a așezat în mijlocul lor, apoi l-a luat în brațe și le-a zis:');
INSERT INTO &quot;verses&quot; VALUES (480,9,37,'&lt;J&gt;„Oricine primește pe unul din acești copilași în Numele Meu pe Mine Mă primește, și oricine&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;Mă primește pe Mine nu pe Mine Mă primește, ci pe Cel ce M-a trimis.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,38,'&lt;pb/&gt;Ioan&lt;f&gt;ⓚ&lt;/f&gt; I-a zis: „Învățătorule, noi am văzut pe cineva scoțând demoni în Numele Tău și l-am oprit, pentru că nu ne urma.”');
INSERT INTO &quot;verses&quot; VALUES (480,9,39,'&lt;J&gt;„Nu-l opriți”&lt;/J&gt;, i-a răspuns Isus, &lt;J&gt;„căci&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;nimeni care face o minune în Numele Meu nu Mă poate vorbi de rău îndată după aceea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,40,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;nu este împotriva noastră este pentru noi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,41,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;oricine vă va da să beți un pahar cu apă în Numele Meu, pentru că sunteți ai lui Hristos, adevărat vă spun că nu-și va pierde răsplata.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,42,'&lt;pb/&gt;&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;dacă cineva îl va face să păcătuiască pe unul dintre acești micuți care cred în Mine, ar fi mai bine pentru omul acela să i se lege o piatră de moară de gât și să fie aruncat în mare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,43,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;mâna ta te face să cazi în păcat, tai-o; este mai bine pentru tine să intri ciung în viață decât să ai două mâini și să ajungi în gheenă, în focul care nu se stinge,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,44,'&lt;J&gt;[&lt;e&gt;unde viermele lor nu moare și focul nu se stinge.&lt;/e&gt; ]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,45,'&lt;J&gt;Iar dacă piciorul tău te face să cazi în păcat, taie-l; este mai bine pentru tine să intri în viață șchiop decât să ai două picioare și să fii aruncat în gheenă, [în focul care nu se stinge,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,46,'&lt;e&gt;&lt;J&gt;unde viermele lor nu moare și focul nu se stinge&lt;/J&gt;&lt;/e&gt;.&lt;J&gt;]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,47,'&lt;J&gt;Iar dacă ochiul tău te face să cazi în păcat, scoate-l; este mai bine pentru tine să intri în Împărăția lui Dumnezeu cu un singur ochi decât să ai doi ochi și să fii aruncat în gheena [de foc]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,48,'&lt;e&gt;&lt;J&gt;unde viermele lor nu moare și focul nu se stinge&lt;/e&gt;.&lt;/J&gt;&lt;f&gt;[27]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,49,'&lt;pb/&gt;&lt;J&gt;Pentru că orice &lt;i&gt;om&lt;/i&gt; va fi sărat cu foc [și orice&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;jertfă va fi sărată cu sare].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,9,50,'&lt;J&gt;Sarea este bună. Dar&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;dacă sarea ajunge nesărată, cum îi veți reda gustul? Să aveți în voi&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;sare și să trăiți&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;în pace unii cu alții!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,1,'&lt;pb/&gt;Isus a plecat&lt;f&gt;ⓤ&lt;/f&gt; de acolo și a venit în ținutul Iudeei, dincolo de Iordan. Mulțimile s-au adunat din nou la El și, după obiceiul Său, a început iarăși să-i învețe &lt;i&gt;pe oameni&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,10,2,'Au&lt;f&gt;ⓥ&lt;/f&gt; venit la El niște farisei și, ca să-L ispitească, L-au întrebat dacă unui bărbat îi este îngăduit să-și lase soția.');
INSERT INTO &quot;verses&quot; VALUES (480,10,3,'Drept răspuns, El le-a zis: &lt;J&gt;„Ce v-a poruncit Moise?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,4,'„Moise&lt;f&gt;ⓦ&lt;/f&gt;”, au zis ei, „a îngăduit &lt;i&gt;ca bărbatul&lt;/i&gt; &lt;e&gt;să scrie un act de despărțire și s-o lase&lt;/e&gt;&lt;f&gt;[28]&lt;/f&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (480,10,5,'Isus le-a zis: &lt;J&gt;„Din pricina împietririi inimii voastre v-a scris &lt;i&gt;Moise&lt;/i&gt; porunca aceasta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,6,'&lt;J&gt;Dar, la începutul creației, &lt;e&gt;parte bărbătească și parte femeiască i-a făcut.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[29]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,7,'&lt;e&gt;&lt;J&gt;De aceea&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;&lt;J&gt;va lăsa omul pe tatăl său și pe mama sa și se va lipi de nevasta sa,&lt;/J&gt;&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,8,'&lt;e&gt;&lt;J&gt;și cei doi vor fi un singur trup.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[30]&lt;/f&gt; &lt;J&gt;Așa că nu mai sunt doi, ci &lt;e&gt;un singur trup.&lt;/J&gt;&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,9,'&lt;J&gt;Așadar, ceea ce a unit Dumnezeu, omul să nu despartă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,10,'În casă, ucenicii L-au întrebat despre acest lucru.');
INSERT INTO &quot;verses&quot; VALUES (480,10,11,'El le-a zis: &lt;J&gt;„Oricine&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;își lasă soția și se căsătorește cu alta săvârșește adulter împotriva ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,12,'&lt;J&gt;Și dacă &lt;i&gt;femeia&lt;/i&gt; își lasă bărbatul și se căsătorește cu altul, săvârșește adulter.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,13,'&lt;pb/&gt;I-au&lt;f&gt;ⓩ&lt;/f&gt; adus niște copilași ca să Se atingă de ei. Dar ucenicii i-au certat [pe cei ce îi aduceau].');
INSERT INTO &quot;verses&quot; VALUES (480,10,14,'Când a văzut Isus acest lucru, S-a mâniat și le-a zis: &lt;J&gt;„Lăsați copiii să vină la Mine și nu-i opriți, căci Împărăția lui Dumnezeu este a celor&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;ca ei!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,15,'&lt;J&gt;Adevărat vă spun că cine&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;nu va primi Împărăția lui Dumnezeu ca un copil nu va intra în ea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,16,'Apoi, luându-i în brațe, i-a binecuvântat punându-Și mâinile peste ei.');
INSERT INTO &quot;verses&quot; VALUES (480,10,17,'&lt;pb/&gt;Când era gata să pornească la drum, a alergat la El un om, care a îngenuncheat înaintea Lui și L-a întrebat: „Bunule învățător, ce să fac ca să moștenesc viața veșnică?”');
INSERT INTO &quot;verses&quot; VALUES (480,10,18,'&lt;J&gt;„De ce Mă numești bun?”&lt;/J&gt;, i-a zis Isus. &lt;J&gt;„Nimeni nu este bun decât Unul singur: Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,19,'&lt;J&gt;Știi poruncile: &lt;e&gt;Să&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;&lt;J&gt;nu ucizi; să nu săvârșești adulter; să nu furi; să nu depui mărturie mincinoasă;&lt;/e&gt; să nu înșeli;&lt;/J&gt;&lt;f&gt;[31]&lt;/f&gt; &lt;e&gt;&lt;J&gt;să-i cinstești pe tatăl tău și pe mama ta.&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[32]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,20,'El I-a răspuns: „Învățătorule, pe toate acestea le-am păzit din tinerețea mea.”');
INSERT INTO &quot;verses&quot; VALUES (480,10,21,'Isus, uitându-Se cu dragoste la el, i-a zis: &lt;J&gt;„Un lucru îți &lt;i&gt;mai&lt;/i&gt; lipsește: du-te, vinde tot ce ai și dă-le săracilor, și vei avea o comoară&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;în cer. Apoi vino, [ia-ți crucea] și urmează-Mă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,22,'Întristat de aceste cuvinte, &lt;i&gt;omul&lt;/i&gt; acela a plecat mâhnit de tot, căci avea multe averi.');
INSERT INTO &quot;verses&quot; VALUES (480,10,23,'Isus S-a uitat&lt;f&gt;ⓔ&lt;/f&gt; în jur și le-a zis ucenicilor Săi: &lt;J&gt;„Cât de greu vor intra în Împărăția lui Dumnezeu cei ce au averi!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,24,'Ucenicii au rămas uimiți de cuvintele Lui. Isus a luat din nou cuvântul și le-a zis: &lt;J&gt;„Copii, cât de greu este [pentru cei ce&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;se încred în avuții] să intre în Împărăția lui Dumnezeu!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,25,'&lt;J&gt;Mai ușor este să treacă o cămilă prin urechea acului decât să intre un om bogat în Împărăția lui Dumnezeu!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,26,'&lt;i&gt;Ucenicii&lt;/i&gt; erau peste măsură de uluiți și ziceau unii către alții: „Atunci, cine poate să fie mântuit?”');
INSERT INTO &quot;verses&quot; VALUES (480,10,27,'Isus i-a țintuit cu privirea și le-a zis: &lt;J&gt;„La oameni este cu neputință, dar nu la Dumnezeu, căci la El toate sunt cu putință&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt;&lt;J&gt;!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,28,'&lt;pb/&gt;Petru&lt;f&gt;ⓗ&lt;/f&gt; a început să-I zică: „Iată, noi am lăsat totul și Te-am urmat.”');
INSERT INTO &quot;verses&quot; VALUES (480,10,29,'Isus i-a răspuns: &lt;J&gt;„Adevărat vă spun: nu este nimeni care să fi lăsat casă, frați, surori, mamă, tată, [soție,] copii sau ogoare pentru Mine și pentru Evanghelie&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,30,'&lt;J&gt;și să&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;nu primească însutit acum, în timpul acesta, case, frați, surori, mame, copii și ogoare, împreună cu prigoniri, iar în veacul care vine, viața veșnică.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,31,'&lt;J&gt;Mulți&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;dintre cei dintâi vor fi cei de pe urmă, și mulți dintre cei de pe urmă vor fi cei dintâi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,32,'&lt;pb/&gt;Se aflau&lt;f&gt;ⓚ&lt;/f&gt; pe drum, urcând spre Ierusalim, iar Isus mergea înaintea lor. &lt;i&gt;Ucenicii&lt;/i&gt; erau uimiți, iar cei care-L urmau erau înfricoșați. Luându-i&lt;f&gt;ⓛ&lt;/f&gt; iarăși deoparte pe cei doisprezece, &lt;i&gt;Isus&lt;/i&gt; a început să le vorbească despre cele ce aveau să I se întâmple:');
INSERT INTO &quot;verses&quot; VALUES (480,10,33,'&lt;J&gt;„Iată, urcăm la Ierusalim, și Fiul Omului va fi dat în mâinile marilor-preoți și ale cărturarilor. Ei Îl vor osândi la moarte și-L vor da în mâinile păgânilor,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,34,'&lt;J&gt;care își vor bate joc de El, Îl vor scuipa, Îl vor biciui și-L vor omorî; dar după trei zile va învia.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,35,'&lt;pb/&gt;Fiii lui Zebedei, Iacov și Ioan, au venit&lt;f&gt;ⓜ&lt;/f&gt; la El și I-au zis: „Învățătorule, am vrea să faci pentru noi ceea ce-Ți vom cere.”');
INSERT INTO &quot;verses&quot; VALUES (480,10,36,'El le-a zis: &lt;J&gt;„Ce vreți să fac pentru voi?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,37,'„Dă-ne”, I-au zis ei, „să stăm unul la dreapta și altul la stânga Ta &lt;i&gt;când vei fi&lt;/i&gt; în slava Ta!”');
INSERT INTO &quot;verses&quot; VALUES (480,10,38,'Isus le-a răspuns: &lt;J&gt;„Nu știți ce cereți! Puteți voi să beți paharul pe care-l beau Eu sau să fiți botezați cu botezul cu care voi fi botezat Eu?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,39,'„Putem”, I-au zis ei. Și Isus le-a răspuns: &lt;J&gt;„&lt;i&gt;Da,&lt;/i&gt; veți bea paharul pe care-l beau Eu și veți fi botezați cu botezul cu care voi fi botezat Eu,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,40,'&lt;J&gt;dar a sta la dreapta sau la stânga Mea nu Eu pot da, ci este pentru cei cărora le-a fost pregătit.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,41,'Auzind aceasta, cei zece&lt;f&gt;ⓝ&lt;/f&gt; au început să se mânie pe Iacov și pe Ioan.');
INSERT INTO &quot;verses&quot; VALUES (480,10,42,'Isus i-a chemat la El și le-a zis: &lt;J&gt;„Știți&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;că cei priviți drept conducători ai popoarelor domnesc peste ele, iar mai-marii lor stăpânesc asupra lor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,43,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;între voi să nu fie așa; cine va vrea să fie mare între voi să fie slujitorul vostru,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,44,'&lt;J&gt;și cine va vrea să fie cel dintâi între voi să fie robul tuturor!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,45,'&lt;J&gt;Căci Fiul&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Omului n-a venit ca să I se slujească, ci ca El să slujească și să-Și&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;dea viața ca răscumpărare pentru mulți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,10,46,'&lt;pb/&gt;Au&lt;f&gt;ⓢ&lt;/f&gt; ajuns la Ierihon. Și pe când ieșea &lt;i&gt;Isus&lt;/i&gt; din Ierihon împreună cu ucenicii Săi și cu o mulțime destul de numeroasă, fiul lui Timeu, Bartimeu, un cerșetor orb, ședea la marginea drumului [și cerșea].');
INSERT INTO &quot;verses&quot; VALUES (480,10,47,'Auzind că este Isus din Nazaret, a început să strige și să spună: „Isuse, Fiul lui David, ai milă de mine!”');
INSERT INTO &quot;verses&quot; VALUES (480,10,48,'Mulți îl certau ca să tacă, dar el și mai tare striga: „Fiul lui David, ai milă de mine!”');
INSERT INTO &quot;verses&quot; VALUES (480,10,49,'Isus S-a oprit și a zis: &lt;J&gt;„Chemați-l!”&lt;/J&gt; L-au chemat pe orb și i-au zis: „Îndrăznește, scoală-te, căci te cheamă!”');
INSERT INTO &quot;verses&quot; VALUES (480,10,50,'&lt;i&gt;Orbul&lt;/i&gt; și-a aruncat haina, a sărit în picioare și a venit la Isus.');
INSERT INTO &quot;verses&quot; VALUES (480,10,51,'Isus a luat cuvântul și i-a zis: &lt;J&gt;„Ce vrei să fac pentru tine?”&lt;/J&gt; „&lt;e&gt;Rabbuní&lt;/e&gt;”, I-a răspuns orbul, „&lt;i&gt;vreau&lt;/i&gt; să-mi recapăt vederea!”');
INSERT INTO &quot;verses&quot; VALUES (480,10,52,'Și Isus i-a zis: &lt;J&gt;„Du-te, credința&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;ta te-a mântuit!”&lt;/J&gt; Îndată el și-a recăpătat vederea și a mers după El pe drum.');
INSERT INTO &quot;verses&quot; VALUES (480,11,1,'&lt;pb/&gt;Când s-au apropiat&lt;f&gt;ⓤ&lt;/f&gt; de Ierusalim, la Betfaghe și Betania, lângă Muntele Măslinilor, Isus i-a trimis pe doi dintre ucenicii Săi');
INSERT INTO &quot;verses&quot; VALUES (480,11,2,'și le-a zis: &lt;J&gt;„Duceți-vă în satul dinaintea voastră! Îndată ce veți intra în el, veți găsi legat un măgăruș pe care niciun om n-a încălecat vreodată; dezlegați-l și aduceți-l la Mine!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,3,'&lt;J&gt;Dacă vă va întreba cineva: «Pentru ce faceți lucrul acesta?», să răspundeți: «Domnul are trebuință de el, și îndată îl va trimite înapoi aici.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,4,'&lt;i&gt;Ucenicii&lt;/i&gt; s-au dus, au găsit măgărușul legat afară, lângă o ușă, în drum, și l-au dezlegat.');
INSERT INTO &quot;verses&quot; VALUES (480,11,5,'Unii dintre cei ce stăteau acolo le-au zis: „Ce faceți? De ce dezlegați măgărușul?”');
INSERT INTO &quot;verses&quot; VALUES (480,11,6,'Ei le-au răspuns cum le spusese Isus. Și i-au lăsat să plece.');
INSERT INTO &quot;verses&quot; VALUES (480,11,7,'Au adus măgărușul la Isus, și-au aruncat hainele pe el și &lt;i&gt;Isus&lt;/i&gt; a încălecat pe el.');
INSERT INTO &quot;verses&quot; VALUES (480,11,8,'Mulți&lt;f&gt;ⓥ&lt;/f&gt; își așterneau hainele pe drum, iar alții presărau ramuri tăiate de pe câmp.');
INSERT INTO &quot;verses&quot; VALUES (480,11,9,'Cei ce mergeau înainte și cei ce veneau în urmă strigau: &lt;pb/&gt;&lt;t&gt;„&lt;e&gt;Osana!&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Binecuvântat este cel ce vine în Numele Domnului.&lt;/e&gt; &lt;f&gt;[33]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,10,'&lt;t&gt;Binecuvântată este Împărăția care vine, a părintelui nostru David!&lt;/t&gt; &lt;t&gt;Osana&lt;f&gt;ⓦ&lt;/f&gt; în înălțimi!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,11,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; &lt;f&gt;ⓧ&lt;/f&gt; a intrat în Ierusalim, în Templu. După ce S-a uitat la toate &lt;i&gt;lucrurile&lt;/i&gt; de jur împrejur, fiindcă era pe înserate, a ieșit spre Betania cu cei doisprezece.');
INSERT INTO &quot;verses&quot; VALUES (480,11,12,'&lt;pb/&gt;În ziua&lt;f&gt;ⓨ&lt;/f&gt; următoare, pe când ieșeau ei din Betania, lui &lt;i&gt;Isus&lt;/i&gt; I s-a făcut foame.');
INSERT INTO &quot;verses&quot; VALUES (480,11,13,'A zărit&lt;f&gt;ⓩ&lt;/f&gt; de departe un smochin care avea frunze și a venit &lt;i&gt;să vadă&lt;/i&gt; dacă va găsi ceva în el, dar ajungând lângă el, nu a găsit nimic decât frunze, căci nu era încă vremea smochinelor.');
INSERT INTO &quot;verses&quot; VALUES (480,11,14,'Atunci a zis smochinului: &lt;J&gt;„În veac să nu mai mănânce nimeni rod din tine!”&lt;/J&gt; Și ucenicii Lui au auzit &lt;i&gt;aceste vorbe&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,11,15,'&lt;pb/&gt;Au&lt;f&gt;ⓐ&lt;/f&gt; ajuns în Ierusalim și, după ce a intrat în Templu, Isus a început să-i scoată afară pe cei ce vindeau și cumpărau în Templu; a răsturnat mesele schimbătorilor de bani și scaunele celor ce vindeau porumbei.');
INSERT INTO &quot;verses&quot; VALUES (480,11,16,'Și nu îngăduia nimănui să care vreun lucru prin Templu.');
INSERT INTO &quot;verses&quot; VALUES (480,11,17,'Și-i învăța și zicea: &lt;J&gt;„Oare nu este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Casa Mea se va numi casă de rugăciune&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;pentru toate neamurile&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[34]&lt;/f&gt; &lt;J&gt;?&lt;/J&gt;&lt;/t&gt; &lt;pb/&gt;&lt;J&gt;Dar voi ați făcut din ea &lt;e&gt;o peșteră de tâlhari&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[35]&lt;/f&gt; &lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,18,'Preoții de seamă și cărturarii au auzit&lt;f&gt;ⓑ&lt;/f&gt; și se gândeau cum să-L omoare. Dar se temeau de El, pentru că toată&lt;f&gt;ⓒ&lt;/f&gt; mulțimea era uimită de învățătura Lui.');
INSERT INTO &quot;verses&quot; VALUES (480,11,19,'Și când s-a înserat, &lt;i&gt;Isus și ucenicii&lt;/i&gt; au ieșit din cetate.');
INSERT INTO &quot;verses&quot; VALUES (480,11,20,'&lt;pb/&gt;Dimineața&lt;f&gt;ⓓ&lt;/f&gt;, pe când treceau pe lângă smochin, ucenicii l-au văzut uscat din rădăcini.');
INSERT INTO &quot;verses&quot; VALUES (480,11,21,'Petru și-a adus aminte și I-a zis lui &lt;i&gt;Isus&lt;/i&gt;: „&lt;e&gt;Rabbí&lt;/e&gt;, iată, smochinul pe care l-ai blestemat s-a uscat!”');
INSERT INTO &quot;verses&quot; VALUES (480,11,22,'Drept răspuns, Isus le-a zis: &lt;J&gt;„Aveți credință în Dumnezeu!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,23,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;vă spun că, dacă va zice cineva acestui munte: «Ridică-te și aruncă-te în mare» și dacă nu se va îndoi în inima lui, ci va crede că se va împlini ceea ce zice, i se va face.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,24,'&lt;J&gt;De aceea vă spun: tot ce&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;veți cere, când vă rugați, să credeți că ați și primit, și vi se va da.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,25,'&lt;J&gt;Iar când stați &lt;i&gt;în picioare&lt;/i&gt; și vă rugați, să iertați&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;dacă aveți ceva împotriva cuiva, pentru ca și Tatăl vostru, care este în ceruri, să vă ierte greșelile voastre. [&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,26,'&lt;J&gt;Dar dacă&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;nu iertați, nici Tatăl vostru, care este în ceruri, nu vă va ierta greșelile voastre.”]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,27,'&lt;pb/&gt;S-au dus din nou la Ierusalim. Și&lt;f&gt;ⓘ&lt;/f&gt;, pe când Se afla &lt;i&gt;Isus&lt;/i&gt; în Templu, au venit la El preoții de seamă, cărturarii și bătrânii');
INSERT INTO &quot;verses&quot; VALUES (480,11,28,'și I-au zis: „Cu ce autoritate faci Tu aceste lucruri? Sau cine Ți-a dat autoritate să faci toate acestea?”');
INSERT INTO &quot;verses&quot; VALUES (480,11,29,'Isus le-a răspuns: &lt;J&gt;„Vă voi întreba un singur lucru. Răspundeți-Mi, și vă voi spune &lt;i&gt;și Eu&lt;/i&gt; cu ce autoritate fac toate acestea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,30,'&lt;J&gt;Botezul lui Ioan era din cer sau de la oameni? Răspundeți-Mi!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,11,31,'Ei însă vorbeau între ei, zicând: „Dacă spunem: «Din cer», va zice: «Atunci de ce n-ați crezut în el?»');
INSERT INTO &quot;verses&quot; VALUES (480,11,32,'Și dacă vom spune: «De la oameni…»”, se temeau de mulțime, căci toți&lt;f&gt;ⓙ&lt;/f&gt; credeau că Ioan fusese într-adevăr profet.');
INSERT INTO &quot;verses&quot; VALUES (480,11,33,'Atunci I-au răspuns lui Isus: „Nu știm.” Și Isus le-a zis: &lt;J&gt;„Nici Eu nu vă spun cu ce autoritate fac aceste lucruri.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,1,'&lt;pb/&gt;Isus a&lt;f&gt;ⓚ&lt;/f&gt; început să le vorbească în pilde. &lt;pb/&gt;&lt;J&gt;„Un om &lt;e&gt;a sădit o vie. A împrejmuit-o cu un gard, a săpat un teasc și a zidit un turn&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[36]&lt;/f&gt; &lt;J&gt;, a arendat-o unor lucrători și a plecat în călătorie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,2,'&lt;J&gt;La vremea potrivită, a trimis la lucrători un rob, ca să ia de la lucrători din roadele viei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,3,'&lt;J&gt;Dar aceștia, prinzându-l, l-au bătut și l-au trimis &lt;i&gt;înapoi&lt;/i&gt; cu mâinile goale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,4,'&lt;J&gt;Și din nou a trimis la ei pe un alt rob; pe acela l-au rănit la cap și l-au batjocorit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,5,'&lt;J&gt;A mai trimis un altul, pe care l-au omorât; apoi pe mulți alții: pe unii i-au bătut, iar pe alții i-au omorât.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,6,'&lt;J&gt;Mai avea pe cineva: pe fiul său iubit; în cele din urmă, l-a trimis și pe acesta la ei, zicându-și: «De fiul meu se vor rușina.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,7,'&lt;J&gt;Dar lucrătorii aceia au zis între ei: «Iată-l pe moștenitor; haideți să-l omorâm, și moștenirea va fi a noastră!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,8,'&lt;J&gt;Și, prinzându-l, l-au omorât și l-au aruncat afară din vie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,9,'&lt;J&gt;Acum, ce va face stăpânul viei? Va veni și-i va omorî pe lucrători, iar via o va da altora.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,10,'&lt;J&gt;N-ați citit scriptura aceasta:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Piatra pe care au lepădat-o zidarii&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;a ajuns în capul unghiului;&lt;/J&gt;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,11,'&lt;t&gt;&lt;e&gt;&lt;J&gt;De la Domnul s-a făcut aceasta,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și este minunat în ochii noștri&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[37]&lt;/f&gt; &lt;J&gt;?”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,12,'&lt;pb/&gt;Ei&lt;f&gt;ⓛ&lt;/f&gt; căutau să-L prindă, dar se temeau de mulțime. Pricepuseră că pentru ei spusese &lt;i&gt;El&lt;/i&gt; pilda aceasta. Și, lăsându-L, au plecat.');
INSERT INTO &quot;verses&quot; VALUES (480,12,13,'&lt;pb/&gt;Și&lt;f&gt;ⓜ&lt;/f&gt; i-au trimis la El pe unii dintre farisei și dintre irodieni ca să-L prindă cu vorba.');
INSERT INTO &quot;verses&quot; VALUES (480,12,14,'&lt;i&gt;Aceștia&lt;/i&gt; au venit și I-au zis: „Învățătorule, știm că spui adevărul și nu ții seama de nimeni, căci nu cauți la fața oamenilor, ci cu adevărat îi înveți calea lui Dumnezeu. Este îngăduit să dăm bir cezarului sau nu?');
INSERT INTO &quot;verses&quot; VALUES (480,12,15,'Să dăm sau să nu dăm?” &lt;i&gt;Isus&lt;/i&gt;, cunoscându-le fățărnicia, le-a răspuns: &lt;J&gt;„Pentru ce Mă ispitiți? Aduceți-Mi un dinar&lt;/J&gt;&lt;f&gt;[38]&lt;/f&gt; &lt;J&gt;ca să-l văd!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,16,'Iar ei I-au adus. &lt;i&gt;Isus&lt;/i&gt; i-a întrebat: &lt;J&gt;„Chipul acesta și inscripția ale cui sunt?”&lt;/J&gt; „Ale cezarului”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (480,12,17,'Atunci, Isus le-a zis: &lt;J&gt;„Dați-i cezarului ce este al cezarului și lui Dumnezeu ce este al lui Dumnezeu!”&lt;/J&gt; Și se mirau foarte mult de El.');
INSERT INTO &quot;verses&quot; VALUES (480,12,18,'&lt;pb/&gt;Saducheii&lt;f&gt;ⓝ&lt;/f&gt;, care&lt;f&gt;ⓞ&lt;/f&gt; zic că nu este înviere, au venit la &lt;i&gt;Isus&lt;/i&gt; și L-au întrebat:');
INSERT INTO &quot;verses&quot; VALUES (480,12,19,'„Învățătorule, Moise&lt;f&gt;ⓟ&lt;/f&gt; ne-a scris că, dacă moare cineva și-i rămâne femeia &lt;i&gt;văduvă&lt;/i&gt; fără să aibă copil, fratele lui să o ia de soție și să-i ridice acestuia urmași.');
INSERT INTO &quot;verses&quot; VALUES (480,12,20,'Erau șapte frați. Primul și-a luat soție și a murit fără să lase urmași.');
INSERT INTO &quot;verses&quot; VALUES (480,12,21,'Al doilea a luat-o &lt;i&gt;de soție&lt;/i&gt; și a murit fără să lase urmași. Tot așa și al treilea.');
INSERT INTO &quot;verses&quot; VALUES (480,12,22,'Și &lt;i&gt;niciunul din&lt;/i&gt; cei șapte n-a lăsat urmași. În cele din urmă, a murit și femeia.');
INSERT INTO &quot;verses&quot; VALUES (480,12,23,'La înviere, [când vor învia,] a căruia dintre ei va fi femeia? Căci toți șapte au avut-o de soție.”');
INSERT INTO &quot;verses&quot; VALUES (480,12,24,'Isus le-a zis: &lt;J&gt;„Oare nu vă rătăciți voi din pricină că nu cunoașteți Scripturile și nici puterea lui Dumnezeu?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,25,'&lt;J&gt;Căci atunci când vor învia din morți, nici nu se vor însura, nici nu se vor mărita, ci vor&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;fi ca îngerii în ceruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,26,'&lt;J&gt;Cât despre &lt;i&gt;faptul că&lt;/i&gt; morții învie, n-ați citit în cartea lui Moise, &lt;i&gt;în locul&lt;/i&gt; despre rug, când i-a spus Dumnezeu: &lt;e&gt;Eu sunt Dumnezeul lui Avraam, Dumnezeul lui Isaac și Dumnezeul lui Iacov&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[39]&lt;/f&gt;&lt;J&gt;?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,27,'&lt;J&gt;El nu este Dumnezeul celor morți, ci al celor vii. Tare vă mai rătăciți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,28,'&lt;pb/&gt;Unul&lt;f&gt;ⓡ&lt;/f&gt; din cărturari, care-i auzise vorbind, văzând că &lt;i&gt;Isus&lt;/i&gt; le răspunsese bine, s-a apropiat de El și L-a întrebat: „Care este cea dintâi dintre toate poruncile?”');
INSERT INTO &quot;verses&quot; VALUES (480,12,29,'Isus i-a răspuns: &lt;J&gt;„Cea dintâi este aceasta: &lt;e&gt;Ascultă,&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;&lt;J&gt;Israele! Domnul Dumnezeul nostru este singurul Domn!&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[40]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,30,'&lt;J&gt;și &lt;e&gt;să-L iubești pe Domnul Dumnezeul tău cu toată inima ta, cu tot sufletul tău, cu tot cugetul tău și cu toată puterea ta!&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[41]&lt;/f&gt;&lt;J&gt;[Iată porunca dintâi.]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,31,'&lt;J&gt;Iar a doua este aceasta: &lt;e&gt;Să-l&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;&lt;J&gt;iubești pe aproapele tău ca pe tine însuți!&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[42]&lt;/f&gt; &lt;J&gt;Nu este altă poruncă mai mare decât acestea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,32,'Cărturarul I-a zis: „Bine, învățătorule. Adevărat ai zis că &lt;i&gt;Dumnezeu&lt;/i&gt; &lt;e&gt;este unul singur&lt;/e&gt;, că &lt;e&gt;nu este altul în afară de El&lt;/e&gt;&lt;f&gt;[43]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,33,'și că &lt;e&gt;a-L iubi pe El din toată inima, din tot cugetul, din tot sufletul și cu toată puterea și a-l iubi pe aproapele ca pe tine însuți&lt;/e&gt;&lt;f&gt;[44]&lt;/f&gt; este&lt;f&gt;ⓤ&lt;/f&gt; mai mult decât toate arderile-de-tot și jertfele.”');
INSERT INTO &quot;verses&quot; VALUES (480,12,34,'Isus, văzând că a răspuns cu pricepere, i-a zis: &lt;J&gt;„Tu nu ești departe de Împărăția lui Dumnezeu.”&lt;/J&gt; Și&lt;f&gt;ⓥ&lt;/f&gt; nimeni nu îndrăznea să-I mai pună întrebări.');
INSERT INTO &quot;verses&quot; VALUES (480,12,35,'&lt;pb/&gt;Pe când&lt;f&gt;ⓦ&lt;/f&gt; dădea învățătură în Templu, Isus a zis: &lt;J&gt;„Cum spun cărturarii că Hristosul este fiul lui David?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,36,'&lt;J&gt;Însuși David a zis, prin&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Duhul Sfânt:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Domnul a zis Domnului meu:&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;«Șezi la dreapta Mea&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;până voi pune pe vrăjmașii Tăi&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;sub picioarele Tale!»&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[45]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,37,'&lt;pb/&gt;&lt;J&gt;Dacă însuși David Îl numește Domn, atunci cum este El fiul lui?”&lt;/J&gt; Și mulțimea Îl asculta cu plăcere.');
INSERT INTO &quot;verses&quot; VALUES (480,12,38,'&lt;pb/&gt;În învățătura Lui, &lt;i&gt;Isus&lt;/i&gt;&lt;f&gt;ⓨ&lt;/f&gt; zicea: &lt;J&gt;„Feriți-vă&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;de cărturarii cărora le place să umble în haine lungi și să fie&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;salutați prin piețe,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,39,'&lt;J&gt;să &lt;i&gt;aibă&lt;/i&gt; scaunele dintâi în sinagogi și locurile de cinste la ospețe;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,40,'&lt;J&gt;casele văduvelor le mănâncă&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;și fac rugăciuni lungi de ochii lumii. Aceștia vor primi o mai mare osândă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,41,'&lt;pb/&gt;Isus&lt;f&gt;ⓒ&lt;/f&gt; stătea jos în fața vistieriei Templului și Se uita cum arunca mulțimea bani în&lt;f&gt;ⓓ&lt;/f&gt; vistierie. Mulți care erau bogați aruncau mult.');
INSERT INTO &quot;verses&quot; VALUES (480,12,42,'A venit și o văduvă săracă și a aruncat doi bănuți&lt;f&gt;[46]&lt;/f&gt;, care fac un codrant&lt;f&gt;[47]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,12,43,'Isus i-a chemat pe ucenicii Săi și le-a zis: &lt;J&gt;„Adevărat vă spun că această&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;văduvă săracă a dat mai mult decât toți cei ce aruncă în vistierie,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,12,44,'&lt;J&gt;căci toți au aruncat din prisosul lor, dar ea, din sărăcia ei, a aruncat tot&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;ce avea, tot ce-i mai rămăsese ca să trăiască.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,1,'&lt;pb/&gt;Pe când ieșea&lt;f&gt;ⓖ&lt;/f&gt; El din Templu, unul dintre ucenicii Lui i-a zis: „Învățătorule, uite ce pietre și ce zidiri!”');
INSERT INTO &quot;verses&quot; VALUES (480,13,2,'Isus i-a răspuns: &lt;J&gt;„Vezi tu aceste zidiri mărețe? Nu&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;va rămâne aici piatră peste piatră care să nu fie dărâmată!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,3,'Și, pe când stătea pe Muntele Măslinilor, în fața Templului, Petru, Iacov, Ioan și Andrei L-au luat deoparte și L-au întrebat:');
INSERT INTO &quot;verses&quot; VALUES (480,13,4,'„Spune-ne&lt;f&gt;ⓘ&lt;/f&gt;, când se vor întâmpla toate acestea și care este semnul că urmează să se împlinească?”');
INSERT INTO &quot;verses&quot; VALUES (480,13,5,'Isus a început atunci să le spună: &lt;J&gt;„Luați seama&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;să nu vă înșele cineva!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,6,'&lt;J&gt;Vor veni mulți în Numele Meu, zicând: «Eu sunt &lt;i&gt;Hristosul!&lt;/i&gt;», și îi vor înșela pe mulți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,7,'&lt;J&gt;Când veți auzi despre războaie și zvonuri de războaie, să nu vă înspăimântați; &lt;i&gt;toate acestea&lt;/i&gt; trebuie să se întâmple, dar încă nu &lt;i&gt;va fi&lt;/i&gt; sfârșitul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,8,'&lt;J&gt;Un popor se va ridica împotriva altui popor, și o împărăție — împotriva altei împărății; pe alocuri vor fi cutremure de pământ, foamete [și tulburări]. Acestea&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;sunt începutul durerilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,9,'&lt;pb/&gt;&lt;J&gt;Cât despre voi, luați seama&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;: vă vor da pe mâna sinedriilor și vă vor bate în sinagogi, iar din pricina Mea veți fi duși înaintea dregătorilor și a regilor, ca mărturie pentru ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,10,'&lt;J&gt;Mai&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;întâi trebuie ca Evanghelia să fie propovăduită tuturor neamurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,11,'&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;vă vor duce să vă dea în mâinile lor, nu vă îngrijorați cu privire la ce veți vorbi, ci vorbiți ceea ce vi se va da în ceasul acela! Căci nu voi veți vorbi, ci&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;Duhul Sfânt.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,12,'&lt;J&gt;Frate&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;pe frate va da la moarte, și tată pe fiu; copiii se vor ridica împotriva părinților și-i vor omorî.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,13,'&lt;J&gt;Veți&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;fi urâți de toți din pricina Numelui Meu, dar cine&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;va răbda până la sfârșit va fi mântuit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,14,'&lt;pb/&gt;&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;veți vedea &lt;e&gt;urâciunea pustiirii&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[48]&lt;/f&gt; &lt;J&gt;stând&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;acolo unde nu se cade &lt;i&gt;să fie&lt;/i&gt; — cine citește să înțeleagă — atunci cei&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;din Iudeea să fugă în munți;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,15,'&lt;J&gt;cine &lt;i&gt;va fi&lt;/i&gt; pe acoperiș să nu coboare și să intre în casă, &lt;i&gt;cu gând să mai&lt;/i&gt; ia ceva din ea,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,16,'&lt;J&gt;iar cine &lt;i&gt;va fi&lt;/i&gt; la câmp să nu se întoarcă în urmă să-și ia haina.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,17,'&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;de femeile însărcinate și de cele care alăptează în zilele acelea!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,18,'&lt;J&gt;Rugați-vă să nu se întâmple iarna.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,19,'&lt;J&gt;Pentru că&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;în zilele acelea va fi &lt;e&gt;o strâmtorare cum n-a mai fost&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[49]&lt;/f&gt; &lt;J&gt;de la începutul creației pe care a zidit-o Dumnezeu până acum și nici nu va mai fi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,20,'&lt;J&gt;Și dacă n-ar scurta Domnul zilele acelea, n-ar scăpa nicio făptură, însă pentru cei pe care i-a ales, a scurtat zilele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,21,'&lt;J&gt;Iar dacă vă va zice cineva atunci&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt;&lt;J&gt;: «Iată, Hristosul este aici!» sau «Iată-L acolo!», să nu credeți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,22,'&lt;J&gt;Căci se vor scula hristoși mincinoși și profeți mincinoși, care vor face semne și minuni ca să-i înșele, dacă este cu putință, chiar și pe cei aleși.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,23,'&lt;J&gt;Dar voi luați seama&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt;&lt;J&gt;; vi le-am spus pe toate dinainte.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,24,'&lt;pb/&gt;&lt;J&gt;Dar în&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;zilele acelea, după strâmtorarea aceea,&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;soarele se va întuneca,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;iar luna nu-și va mai da lumina,&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[50]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,25,'&lt;t&gt;&lt;e&gt;&lt;J&gt;stelele vor cădea din cer&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[51]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;și puterile care sunt în ceruri vor fi clătinate.&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,26,'&lt;pb/&gt;&lt;J&gt;Apoi&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;Îl vor vedea &lt;e&gt;pe Fiul Omului venind pe nori&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[52]&lt;/f&gt; &lt;J&gt;cu mare putere și cu slavă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,27,'&lt;J&gt;Și atunci El va trimite îngerii ca să-i adune pe aleșii Săi din cele patru vânturi, de la capătul pământului până la capătul cerului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,28,'&lt;J&gt;Învățați&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;de la smochin pilda &lt;i&gt;lui&lt;/i&gt;: când mlădița i se face fragedă și înfrunzește, știți că vara este aproape.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,29,'&lt;J&gt;Tot așa, când veți vedea aceste lucruri împlinindu-se, să știți că &lt;i&gt;Fiul Omului&lt;/i&gt; este aproape, &lt;i&gt;este chiar&lt;/i&gt; la ușă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,30,'&lt;J&gt;Adevărat vă spun că nicidecum nu va trece generația aceasta că se vor și împlini aceste lucruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,31,'&lt;J&gt;Cerul și pământul vor trece, dar cuvintele&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;Mele nu vor trece.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,32,'&lt;pb/&gt;&lt;J&gt;Însă despre ziua aceea sau ceasul acela nu știe nimeni, nici îngerii din cer, nici Fiul, ci numai Tatăl.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,33,'&lt;J&gt;Luați seama&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt;&lt;J&gt;, vegheați [și rugați-vă], căci nu știți când este vremea &lt;i&gt;aceea&lt;/i&gt;!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,34,'&lt;J&gt;Ca&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;un om care, plecând în călătorie, își lasă casa și le dă robilor săi autoritate, și fiecăruia lucrarea lui, iar portarului i-a poruncit să vegheze.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,35,'&lt;J&gt;Vegheați&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;, așadar, pentru că nu știți când vine stăpânul casei — seara sau la miezul nopții sau la cântatul cocoșului sau în zori —&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,36,'&lt;J&gt;ca nu cumva, venind pe neașteptate, să vă găsească dormind.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,13,37,'&lt;J&gt;Ceea ce vă zic vouă, zic tuturor: «Vegheați!»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,1,'&lt;pb/&gt;După două&lt;f&gt;ⓖ&lt;/f&gt; zile erau Paștele și Sărbătoarea Azimelor. Preoții de seamă și cărturarii căutau cum să-L prindă &lt;i&gt;pe Isus&lt;/i&gt; cu viclenie ca să-L omoare.');
INSERT INTO &quot;verses&quot; VALUES (480,14,2,'Căci ziceau: „Nu în timpul sărbătorii, ca să nu se facă tulburare în popor!”');
INSERT INTO &quot;verses&quot; VALUES (480,14,3,'&lt;pb/&gt;Și&lt;f&gt;ⓗ&lt;/f&gt; fiind &lt;i&gt;Isus&lt;/i&gt; în Betania, în casa lui Simon Leprosul, pe când stătea la masă, a venit o femeie care avea un vas de alabastru cu mir de nard curat, de mare preț, și spărgând vasul, a turnat &lt;i&gt;mirul&lt;/i&gt; pe capul lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (480,14,4,'Dar erau unii care, mânioși, &lt;i&gt;își spuneau&lt;/i&gt; unii altora: „Pentru ce s-a făcut risipa aceasta de mir?');
INSERT INTO &quot;verses&quot; VALUES (480,14,5,'Mirul acesta s-ar fi putut vinde cu mai mult de trei sute de dinari, care s-ar fi putut da săracilor.” Și îi vorbeau aspru femeii.');
INSERT INTO &quot;verses&quot; VALUES (480,14,6,'Dar Isus le-a zis: &lt;J&gt;„Lăsați-o &lt;i&gt;în pace&lt;/i&gt;! De ce o necăjiți? Ea a făcut un lucru frumos față de Mine,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,7,'&lt;J&gt;căci pe&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;săraci îi aveți întotdeauna cu voi și le puteți face bine oricând voiți, dar pe Mine nu Mă aveți întotdeauna.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,8,'&lt;J&gt;Ea a făcut ce a putut; Mi-a uns trupul mai dinainte pentru îngropare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,9,'&lt;J&gt;Adevărat vă spun că, oriunde în lume va fi propovăduită Evanghelia [aceasta], se va povesti și ce a făcut femeia aceasta, în amintirea ei.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,10,'&lt;pb/&gt;Iuda&lt;f&gt;ⓙ&lt;/f&gt; Iscariot, unul dintre cei doisprezece, s-a dus la preoții de seamă să-L vândă &lt;i&gt;pe Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,14,11,'Când au auzit ei &lt;i&gt;lucrul acesta&lt;/i&gt;, s-au bucurat și i-au făgăduit că-i vor da bani. Iar el căuta un prilej potrivit ca să-L dea în mâinile lor.');
INSERT INTO &quot;verses&quot; VALUES (480,14,12,'&lt;pb/&gt;În&lt;f&gt;ⓚ&lt;/f&gt; ziua dintâi a &lt;i&gt;Sărbătorii&lt;/i&gt; Azimelor, când se jertfea &lt;i&gt;mielul de&lt;/i&gt; Paște, ucenicii lui Isus I-au zis: „Unde vrei să mergem să facem pregătiri ca să mănânci Paștele?”');
INSERT INTO &quot;verses&quot; VALUES (480,14,13,'El i-a trimis pe doi dintre ucenicii Săi și le-a zis: &lt;J&gt;„Duceți-vă în cetate și vă va ieși în cale un om ducând un urcior cu apă; mergeți după el&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,14,'&lt;J&gt;și, acolo unde va intra, să-i spuneți stăpânului casei: «Învățătorul zice: ‘Unde este odaia pentru Mine, în care să mănânc Paștele cu ucenicii Mei?’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,15,'&lt;J&gt;Și vă va arăta o cameră mare, sus, gata așternută; acolo să pregătiți pentru noi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,16,'Ucenicii au plecat, au intrat în cetate și au găsit așa cum le spusese El. Și au pregătit Paștele.');
INSERT INTO &quot;verses&quot; VALUES (480,14,17,'Când s-a înserat&lt;f&gt;ⓛ&lt;/f&gt;, &lt;i&gt;Isus&lt;/i&gt; a venit cu cei doisprezece.');
INSERT INTO &quot;verses&quot; VALUES (480,14,18,'Pe când stăteau la masă și mâncau, Isus a zis: &lt;J&gt;„Adevărat vă spun că unul din voi Mă va vinde, &lt;e&gt;unul care mănâncă împreună cu Mine&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[53]&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,19,'Ei au început să se întristeze și să-I zică, unul după altul: „Nu cumva &lt;i&gt;sunt&lt;/i&gt; eu?”');
INSERT INTO &quot;verses&quot; VALUES (480,14,20,'&lt;J&gt;„Este unul dintre voi doisprezece”&lt;/J&gt;, le-a răspuns El, &lt;J&gt;„care întinge cu Mine în blid.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,21,'&lt;J&gt;Fiul Omului&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;negreșit Se duce, după cum este scris despre El. Dar vai de omul acela prin care este vândut Fiul Omului! Ar fi fost mai bine pentru el dacă nu s-ar fi născut!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,22,'&lt;pb/&gt;Pe când&lt;f&gt;ⓝ&lt;/f&gt; mâncau, &lt;i&gt;Isus&lt;/i&gt; a luat o pâine și, după ce a rostit binecuvântarea, a frânt-o și le-a dat-o, zicând: &lt;J&gt;„Luați, [mâncați,] acesta este trupul Meu!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,23,'Apoi, luând un pahar, după ce a adus mulțumire, li l-a dat și au băut toți din el.');
INSERT INTO &quot;verses&quot; VALUES (480,14,24,'Și le-a zis: &lt;J&gt;„Acesta este sângele Meu, al legământului [celui nou], care se varsă pentru mulți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,25,'&lt;J&gt;Adevărat vă spun că nu voi mai bea din rodul viței până în ziua când îl voi bea nou în Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,26,'&lt;pb/&gt;După ce&lt;f&gt;ⓞ&lt;/f&gt; au cântat cântările de laudă, au ieșit spre Muntele Măslinilor.');
INSERT INTO &quot;verses&quot; VALUES (480,14,27,'Isus&lt;f&gt;ⓟ&lt;/f&gt; le-a zis: &lt;J&gt;„[În noaptea aceasta] toți vă veți poticni, pentru că este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Voi bate Păstorul,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și oile vor fi risipite.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[54]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,28,'&lt;pb/&gt;&lt;J&gt;Dar, după&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;ce voi învia, voi merge înaintea voastră în Galileea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,29,'Petru&lt;f&gt;ⓡ&lt;/f&gt; i-a zis: „Chiar dacă toți se vor poticni, eu nu &lt;i&gt;mă voi poticni&lt;/i&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (480,14,30,'Și Isus i-a zis: &lt;J&gt;„Adevărat îți spun că astăzi, chiar în noaptea aceasta, înainte să cânte cocoșul de două ori, te vei lepăda de Mine de trei ori.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,31,'Dar &lt;i&gt;Petru&lt;/i&gt; I-a zis și mai apăsat: „Chiar dacă ar trebui să mor împreună cu Tine, nu mă voi lepăda de Tine.” Și toți spuneau la fel.');
INSERT INTO &quot;verses&quot; VALUES (480,14,32,'&lt;pb/&gt;S-au&lt;f&gt;ⓢ&lt;/f&gt; dus &lt;i&gt;apoi&lt;/i&gt; într-un loc numit Ghetsimani. Și &lt;i&gt;Isus&lt;/i&gt; le-a zis ucenicilor Săi: &lt;J&gt;„Stați aici până Mă voi ruga!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,33,'I-a luat cu El pe Petru, pe Iacov și pe Ioan și a început să Se înfioare și să Se mâhnească.');
INSERT INTO &quot;verses&quot; VALUES (480,14,34,'Și le-a zis: &lt;J&gt;„Sufletul Meu este cuprins de o întristare&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;de moarte; rămâneți aici și vegheați!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,35,'Apoi, mergând puțin mai înainte, a căzut la pământ și Se ruga ca, dacă este cu putință, să treacă de la El ceasul acela.');
INSERT INTO &quot;verses&quot; VALUES (480,14,36,'El zicea: &lt;J&gt;„&lt;e&gt;Avva&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;(Tată), Ție toate&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;Îți sunt cu putință; îndepărtează de la Mine paharul acesta! Totuși&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt;&lt;J&gt;, &lt;i&gt;facă-se&lt;/i&gt; nu cum vreau Eu, ci cum &lt;i&gt;vrei&lt;/i&gt; Tu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,37,'Și a venit &lt;i&gt;la ucenici&lt;/i&gt;, pe care i-a găsit dormind. Și i-a zis lui Petru: &lt;J&gt;„Simone, dormi? Nici măcar un ceas n-ai putut să veghezi?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,38,'&lt;J&gt;Vegheați și rugați-vă să nu ajungeți în ispită&lt;/J&gt;&lt;f&gt;[55]&lt;/f&gt;&lt;J&gt;; duhul&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;este plin de râvnă, dar trupul este neputincios.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,39,'S-a dus iarăși și S-a rugat, zicând aceleași cuvinte.');
INSERT INTO &quot;verses&quot; VALUES (480,14,40,'Apoi a venit din nou și i-a găsit dormind, pentru că ochii le erau îngreunați; și nu știau ce să-I răspundă.');
INSERT INTO &quot;verses&quot; VALUES (480,14,41,'A venit a treia oară și le-a zis: &lt;J&gt;„De acum dormiți și odihniți-vă! Destul! A&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;sosit ceasul! Iată că Fiul Omului este dat în mâinile păcătoșilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,42,'&lt;J&gt;Ridicați-vă&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt;&lt;J&gt;, să mergem! Iată, se apropie vânzătorul!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,43,'&lt;pb/&gt;Și&lt;f&gt;ⓐ&lt;/f&gt; îndată, pe când încă vorbea, a venit Iuda, unul dintre cei doisprezece, și împreună cu el &lt;i&gt;au venit&lt;/i&gt; o mulțime &lt;i&gt;de oameni&lt;/i&gt; cu săbii și cu ciomege, &lt;i&gt;trimiși&lt;/i&gt; de preoții de seamă, de cărturari și de bătrâni.');
INSERT INTO &quot;verses&quot; VALUES (480,14,44,'Vânzătorul le dăduse un semn, zicând: „Cel pe care-l voi săruta eu, acela este; să-l prindeți!”');
INSERT INTO &quot;verses&quot; VALUES (480,14,45,'Și venind, s-a apropiat îndată de &lt;i&gt;Isus&lt;/i&gt;, I-a zis: „&lt;e&gt;Rabbí&lt;/e&gt;!” și L-a sărutat.');
INSERT INTO &quot;verses&quot; VALUES (480,14,46,'Atunci ei au pus mâna pe Isus și L-au prins.');
INSERT INTO &quot;verses&quot; VALUES (480,14,47,'Unul dintre cei de față și-a scos sabia, l-a lovit pe slujitorul marelui-preot și i-a tăiat urechea.');
INSERT INTO &quot;verses&quot; VALUES (480,14,48,'Isus&lt;f&gt;ⓑ&lt;/f&gt; le-a zis: &lt;J&gt;„Ați ieșit ca după un tâlhar, cu săbii și cu ciomege, ca să Mă prindeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,49,'&lt;J&gt;În toate zilele am fost cu voi în Templu dând învățătură, și n-ați pus mâna pe Mine. Dar &lt;i&gt;acestea sunt&lt;/i&gt; ca să&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;se împlinească Scripturile.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,50,'Atunci&lt;f&gt;ⓓ&lt;/f&gt;, toți L-au părăsit și au fugit.');
INSERT INTO &quot;verses&quot; VALUES (480,14,51,'După El mergea un tânăr înfășurat într-o pânză de in peste trupul gol. Au pus mâna pe el,');
INSERT INTO &quot;verses&quot; VALUES (480,14,52,'dar el și-a lăsat pânza și a fugit gol.');
INSERT INTO &quot;verses&quot; VALUES (480,14,53,'&lt;pb/&gt;Pe&lt;f&gt;ⓔ&lt;/f&gt; Isus L-au dus la marele-preot, unde s-au adunat toți preoții de seamă, bătrânii și cărturarii.');
INSERT INTO &quot;verses&quot; VALUES (480,14,54,'Petru L-a urmat de departe până înăuntru, în palatul marelui-preot, s-a așezat împreună cu servitorii și se încălzea la foc.');
INSERT INTO &quot;verses&quot; VALUES (480,14,55,'Preoții&lt;f&gt;ⓕ&lt;/f&gt; de seamă și tot Sinedriul căutau o mărturie împotriva lui Isus, ca să-L omoare, dar nu găseau,');
INSERT INTO &quot;verses&quot; VALUES (480,14,56,'pentru că mulți depuneau mărturie mincinoasă împotriva Lui, dar mărturiile &lt;i&gt;lor&lt;/i&gt; nu se potriveau.');
INSERT INTO &quot;verses&quot; VALUES (480,14,57,'Unii s-au sculat și au depus mărturie mincinoasă împotriva Lui, zicând:');
INSERT INTO &quot;verses&quot; VALUES (480,14,58,'„Noi l-am auzit zicând: «Eu&lt;f&gt;ⓖ&lt;/f&gt; voi dărâma Templul acesta făcut de mână omenească și în trei zile voi construi un altul, care nu va fi făcut de mână omenească.»”');
INSERT INTO &quot;verses&quot; VALUES (480,14,59,'Nici chiar în privința aceasta nu se potrivea mărturia lor.');
INSERT INTO &quot;verses&quot; VALUES (480,14,60,'Atunci&lt;f&gt;ⓗ&lt;/f&gt;, marele-preot s-a ridicat &lt;i&gt;în picioare&lt;/i&gt; în mijlocul &lt;i&gt;adunării&lt;/i&gt; și L-a întrebat pe Isus, zicându-I: „Nu răspunzi nimic? Ce mărturie aduc aceștia împotriva ta?”');
INSERT INTO &quot;verses&quot; VALUES (480,14,61,'Dar El&lt;f&gt;ⓘ&lt;/f&gt; tăcea și nu răspundea nimic. Marele-preot&lt;f&gt;ⓙ&lt;/f&gt; L-a întrebat iarăși și I-a zis: „Ești tu Hristosul, Fiul Celui Binecuvântat?”');
INSERT INTO &quot;verses&quot; VALUES (480,14,62,'Iar Isus i-a zis: &lt;J&gt;„Eu Sunt. Și-L&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;veți vedea pe &lt;e&gt;Fiul Omului&lt;/e&gt; stând la dreapta Puterii și &lt;e&gt;venind pe norii cerului.&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[56]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,14,63,'Atunci, marele-preot și-a rupt hainele și a zis: „Ce nevoie mai avem de martori?');
INSERT INTO &quot;verses&quot; VALUES (480,14,64,'Ați auzit blasfemia. Cum vi se pare?” Toți L-au osândit ca vrednic de moarte.');
INSERT INTO &quot;verses&quot; VALUES (480,14,65,'Și unii au început să-L scuipe, să-I acopere fața, să-L bată cu pumnii și să-I zică: „Profețește!”, iar servitorii L-au luat și L-au pălmuit.');
INSERT INTO &quot;verses&quot; VALUES (480,14,66,'&lt;pb/&gt;Pe&lt;f&gt;ⓛ&lt;/f&gt; când stătea Petru jos în curte, a venit una dintre slujnicele marelui-preot.');
INSERT INTO &quot;verses&quot; VALUES (480,14,67,'Când l-a văzut încălzindu-se, l-a privit țintă și i-a zis: „Și tu erai cu nazarineanul, cu Isus!”');
INSERT INTO &quot;verses&quot; VALUES (480,14,68,'El a tăgăduit și a zis: „Nu știu, nici nu înțeleg ce vrei să zici!” Apoi a ieșit afară, în locul din fața curții; și a cântat cocoșul.');
INSERT INTO &quot;verses&quot; VALUES (480,14,69,'Când&lt;f&gt;ⓜ&lt;/f&gt; l-a văzut slujnica, a început iarăși să spună celor ce stăteau acolo: „Acesta este dintre ei.”');
INSERT INTO &quot;verses&quot; VALUES (480,14,70,'Și el a tăgăduit din nou. După&lt;f&gt;ⓝ&lt;/f&gt; puțină vreme, cei ce stăteau acolo i-au zis iarăși lui Petru: „Cu adevărat ești dintre ei, căci&lt;f&gt;ⓞ&lt;/f&gt; ești galileean [și vorba ta seamănă cu a lor].”');
INSERT INTO &quot;verses&quot; VALUES (480,14,71,'Atunci, el a început să se blesteme și să se jure: „Nu-l cunosc pe omul acesta despre care vorbiți!”');
INSERT INTO &quot;verses&quot; VALUES (480,14,72,'Îndată&lt;f&gt;ⓟ&lt;/f&gt; a cântat cocoșul a doua oară. Și Petru și-a adus aminte de ceea ce îi spusese Isus: &lt;J&gt;„Înainte să cânte cocoșul de două ori, te vei lepăda de Mine de trei ori.”&lt;/J&gt; Și a izbucnit în plâns.');
INSERT INTO &quot;verses&quot; VALUES (480,15,1,'&lt;pb/&gt;Și îndată, dimineața&lt;f&gt;ⓠ&lt;/f&gt;, după ce au ținut sfat, preoții de seamă, împreună cu bătrânii, cărturarii și tot Sinedriul, L-au legat pe Isus, L-au dus și L-au dat în mâinile lui Pilat.');
INSERT INTO &quot;verses&quot; VALUES (480,15,2,'Pilat&lt;f&gt;ⓡ&lt;/f&gt; L-a întrebat: „Ești tu regele iudeilor?” &lt;i&gt;Isus&lt;/i&gt; i-a răspuns: &lt;J&gt;„Tu însuți o spui.”&lt;/J&gt;&lt;f&gt;[57]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,15,3,'Preoții de seamă Îl învinuiau de multe.');
INSERT INTO &quot;verses&quot; VALUES (480,15,4,'Pilat&lt;f&gt;ⓢ&lt;/f&gt; L-a întrebat din nou: „Nu răspunzi nimic? Uite câte învinuiri îți aduc ei!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,5,'Dar Isus&lt;f&gt;ⓣ&lt;/f&gt; n-a mai dat niciun răspuns, așa că Pilat era uimit.');
INSERT INTO &quot;verses&quot; VALUES (480,15,6,'&lt;pb/&gt;La&lt;f&gt;ⓤ&lt;/f&gt; fiecare sărbătoare, &lt;i&gt;Pilat&lt;/i&gt; le elibera un întemnițat pe care-l cereau ei.');
INSERT INTO &quot;verses&quot; VALUES (480,15,7,'În temniță era unul numit Barabba, închis împreună cu niște răzvrătiți care săvârșiseră un omor în timpul unei răscoale.');
INSERT INTO &quot;verses&quot; VALUES (480,15,8,'Mulțimea s-a suit și a început să-i ceară &lt;i&gt;lui Pilat&lt;/i&gt; să le facă potrivit obiceiului.');
INSERT INTO &quot;verses&quot; VALUES (480,15,9,'Pilat le-a răspuns: „Vreți să vi-l eliberez pe regele iudeilor?”');
INSERT INTO &quot;verses&quot; VALUES (480,15,10,'Căci știa că din invidie Îl dăduseră preoții de seamă în mâna lui.');
INSERT INTO &quot;verses&quot; VALUES (480,15,11,'Dar aceștia au ațâțat&lt;f&gt;ⓥ&lt;/f&gt; mulțimea ca mai degrabă să-l elibereze pe Barabba.');
INSERT INTO &quot;verses&quot; VALUES (480,15,12,'Pilat a luat din nou cuvântul și le-a zis: „Atunci, ce vreți să fac cu cel pe care-l numiți regele iudeilor?”');
INSERT INTO &quot;verses&quot; VALUES (480,15,13,'Iar ei au strigat din nou: „Răstignește-l!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,14,'„Dar ce rău a făcut?”, le-a zis Pilat. Însă ei au început să strige și mai tare: „Răstignește-l!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,15,'Pilat&lt;f&gt;ⓦ&lt;/f&gt;, vrând să facă pe placul mulțimii, l-a eliberat pe Barabba, iar pe Isus, după ce a pus să-L biciuiască, L-a dat să fie răstignit.');
INSERT INTO &quot;verses&quot; VALUES (480,15,16,'&lt;pb/&gt;Soldații&lt;f&gt;ⓧ&lt;/f&gt; L-au adus pe Isus în palat, adică în pretoriu, și au adunat toată cohorta.');
INSERT INTO &quot;verses&quot; VALUES (480,15,17,'L-au îmbrăcat într-o &lt;i&gt;haină&lt;/i&gt; purpurie, au împletit o cunună de spini și I-au pus-o pe cap.');
INSERT INTO &quot;verses&quot; VALUES (480,15,18,'Apoi au început să-L salute și să-I zică: „Plecăciune, rege al iudeilor!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,19,'Și-L loveau în cap cu o trestie, Îl scuipau, îngenuncheau și I se închinau.');
INSERT INTO &quot;verses&quot; VALUES (480,15,20,'După ce și-au bătut joc de El, L-au dezbrăcat de &lt;i&gt;haina&lt;/i&gt; purpurie, L-au îmbrăcat în hainele Lui și L-au dus afară ca să-L răstignească.');
INSERT INTO &quot;verses&quot; VALUES (480,15,21,'&lt;pb/&gt;L-au&lt;f&gt;ⓨ&lt;/f&gt; silit pe un trecător care venea de la câmp, Simon din Cirene, tatăl lui Alexandru și al lui Rufus, să poarte crucea &lt;i&gt;lui Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (480,15,22,'Și&lt;f&gt;ⓩ&lt;/f&gt; L-au adus pe Isus la locul numit Golgota, care tradus înseamnă: „Locul căpățânii”.');
INSERT INTO &quot;verses&quot; VALUES (480,15,23,'I-au&lt;f&gt;ⓐ&lt;/f&gt; dat să bea vin amestecat cu smirnă, dar El nu l-a luat.');
INSERT INTO &quot;verses&quot; VALUES (480,15,24,'După ce L-au răstignit, &lt;e&gt;și-au&lt;/e&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;împărțit hainele Lui și au tras la sorți pentru ele&lt;/e&gt;&lt;f&gt;[58]&lt;/f&gt;, &lt;i&gt;ca să știe&lt;/i&gt; cine ce să ia.');
INSERT INTO &quot;verses&quot; VALUES (480,15,25,'Era&lt;f&gt;ⓒ&lt;/f&gt; ceasul al treilea&lt;f&gt;[59]&lt;/f&gt; când L-au răstignit.');
INSERT INTO &quot;verses&quot; VALUES (480,15,26,'Deasupra Lui Îi era scrisă&lt;f&gt;ⓓ&lt;/f&gt; vina: „Regele iudeilor”.');
INSERT INTO &quot;verses&quot; VALUES (480,15,27,'Împreună&lt;f&gt;ⓔ&lt;/f&gt; cu El au răstignit și doi tâlhari, unul la dreapta, și altul la stânga Lui. [');
INSERT INTO &quot;verses&quot; VALUES (480,15,28,'Astfel s-a împlinit scriptura care zice: &lt;e&gt;A&lt;/e&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;fost pus în numărul celor fără de lege.&lt;/e&gt;&lt;f&gt;[60]&lt;/f&gt;]');
INSERT INTO &quot;verses&quot; VALUES (480,15,29,'&lt;pb/&gt;Trecătorii&lt;f&gt;ⓖ&lt;/f&gt; rosteau blasfemii împotriva Lui, &lt;e&gt;clătinau din cap&lt;/e&gt;&lt;f&gt;[61]&lt;/f&gt; și ziceau: „Huo! Tu, care dărâmi Templul și-l zidești în trei zile,');
INSERT INTO &quot;verses&quot; VALUES (480,15,30,'izbăvește-te pe tine coborând de pe cruce!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,31,'Tot astfel și preoții de seamă, împreună cu cărturarii, Îl luau în râs și ziceau unii către alții: „Pe alții i-a izbăvit, pe sine nu se poate izbăvi!');
INSERT INTO &quot;verses&quot; VALUES (480,15,32,'Hristos, Regele lui Israel, să coboare acum de pe cruce, ca să vedem și să credem!” &lt;i&gt;Chiar&lt;/i&gt; și cei&lt;f&gt;ⓗ&lt;/f&gt; răstigniți împreună cu El își băteau joc de El.');
INSERT INTO &quot;verses&quot; VALUES (480,15,33,'&lt;pb/&gt;La ceasul&lt;f&gt;ⓘ&lt;/f&gt; al șaselea&lt;f&gt;[62]&lt;/f&gt;, s-a făcut întuneric în toată țara, până la ceasul al nouălea.');
INSERT INTO &quot;verses&quot; VALUES (480,15,34,'Și la ceasul al nouălea, Isus a strigat cu glas puternic: &lt;pb/&gt;&lt;t&gt;&lt;J&gt;„&lt;e&gt;Eloí&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;, &lt;e&gt;Eloí, lemá sabahtáni?&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[63]&lt;/f&gt;,&lt;/t&gt; &lt;pb/&gt;care, tradus, înseamnă: „Dumnezeul Meu, Dumnezeul Meu, pentru ce M-ai părăsit?”');
INSERT INTO &quot;verses&quot; VALUES (480,15,35,'Unii dintre cei ce erau de față, când L-au auzit, au zis: „Iată că îl cheamă pe Ilie!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,36,'Și unul dintre ei a alergat&lt;f&gt;ⓚ&lt;/f&gt; și a umplut un burete cu &lt;e&gt;oțet&lt;/e&gt;, &lt;i&gt;apoi&lt;/i&gt; l-a pus într-o trestie și &lt;e&gt;I l-a dat să bea&lt;/e&gt;&lt;f&gt;[64]&lt;/f&gt;, zicând: „Lăsați, să vedem dacă va veni Ilie să-l dea jos &lt;i&gt;de pe cruce&lt;/i&gt;!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,37,'Dar&lt;f&gt;ⓛ&lt;/f&gt; Isus a scos un strigăt puternic și Și-a dat duhul.');
INSERT INTO &quot;verses&quot; VALUES (480,15,38,'Atunci catapeteasma&lt;f&gt;ⓜ&lt;/f&gt; Templului s-a despicat în două, de sus până jos.');
INSERT INTO &quot;verses&quot; VALUES (480,15,39,'Văzând cum&lt;f&gt;ⓝ&lt;/f&gt; Și-a dat duhul, centurionul care stătea în fața lui Isus a zis: „Cu adevărat, Omul acesta era Fiul lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (480,15,40,'Acolo&lt;f&gt;ⓞ&lt;/f&gt; erau și niște femei care priveau de la depărtare&lt;f&gt;ⓟ&lt;/f&gt;. Printre ele &lt;i&gt;erau&lt;/i&gt; Maria Magdalena, Maria, mama lui Iacov cel mic și a lui Iosif, și Salome,');
INSERT INTO &quot;verses&quot; VALUES (480,15,41,'care Îl urmau&lt;f&gt;ⓠ&lt;/f&gt; și Îi slujeau de când era El în Galileea; și multe altele care se suiseră împreună cu El la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (480,15,42,'&lt;pb/&gt;Când&lt;f&gt;ⓡ&lt;/f&gt; s-a înserat, fiindcă era ziua pregătirii, adică ajunul sabatului,');
INSERT INTO &quot;verses&quot; VALUES (480,15,43,'a venit Iosif din Arimateea, un membru de vază al Sinedriului, care aștepta&lt;f&gt;ⓢ&lt;/f&gt; și el Împărăția lui Dumnezeu. El a îndrăznit să intre la Pilat și să-i ceară trupul lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (480,15,44,'Pilat s-a mirat că murise așa de curând. L-a chemat pe centurion și l-a întrebat dacă a murit de mult.');
INSERT INTO &quot;verses&quot; VALUES (480,15,45,'După ce s-a încredințat de la centurion, i-a dat lui Iosif trupul.');
INSERT INTO &quot;verses&quot; VALUES (480,15,46,'Și&lt;f&gt;ⓣ&lt;/f&gt; [Iosif] a cumpărat un giulgiu, a coborât &lt;i&gt;trupul de pe cruce&lt;/i&gt;, l-a înfășurat în giulgiu și l-a pus într-un mormânt săpat în stâncă. Apoi a rostogolit o piatră la intrarea mormântului.');
INSERT INTO &quot;verses&quot; VALUES (480,15,47,'Iar Maria Magdalena și Maria, mama lui Iose, se uitau unde-L puneau.');
INSERT INTO &quot;verses&quot; VALUES (480,16,1,'&lt;pb/&gt;După ce a trecut&lt;f&gt;ⓤ&lt;/f&gt; sabatul, Maria Magdalena, Maria, mama lui Iacov, și Salome au cumpărat&lt;f&gt;ⓥ&lt;/f&gt; miresme ca să meargă să-I ungă trupul lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (480,16,2,'În&lt;f&gt;ⓦ&lt;/f&gt; ziua dintâi a săptămânii, s-au dus la mormânt dis-de-dimineață, pe când răsărea soarele.');
INSERT INTO &quot;verses&quot; VALUES (480,16,3,'&lt;i&gt;Femeile&lt;/i&gt; ziceau una către alta: „Cine ne va rostogoli piatra de la intrarea mormântului?”');
INSERT INTO &quot;verses&quot; VALUES (480,16,4,'Și când și-au ridicat ochii, au văzut că piatra, care era foarte mare, fusese rostogolită.');
INSERT INTO &quot;verses&quot; VALUES (480,16,5,'Au&lt;f&gt;ⓧ&lt;/f&gt; intrat în mormânt, au văzut un tânăr așezat în partea dreaptă, îmbrăcat într-un veșmânt alb, și s-au înspăimântat.');
INSERT INTO &quot;verses&quot; VALUES (480,16,6,'Dar el&lt;f&gt;ⓨ&lt;/f&gt; le-a zis: „Nu vă temeți! Îl căutați pe Isus Nazarineanul, care a fost răstignit. A înviat, nu este aici; iată locul unde Îl puseseră.');
INSERT INTO &quot;verses&quot; VALUES (480,16,7,'Dar duceți-vă și spuneți-le ucenicilor Lui și lui Petru că &lt;i&gt;El&lt;/i&gt; merge înaintea voastră în Galileea; acolo Îl veți vedea, după cum&lt;f&gt;ⓩ&lt;/f&gt; v-a spus.”');
INSERT INTO &quot;verses&quot; VALUES (480,16,8,'Ele au ieșit și au fugit de la mormânt, pentru că erau cuprinse de spaimă și de uimire. Și&lt;f&gt;ⓐ&lt;/f&gt; n-au spus nimănui nimic, pentru că se temeau. &lt;pb/&gt;[[');
INSERT INTO &quot;verses&quot; VALUES (480,16,9,'După ce a înviat în dimineața zilei dintâi a săptămânii, &lt;i&gt;Isus&lt;/i&gt; i S-a&lt;f&gt;ⓑ&lt;/f&gt; arătat mai întâi Mariei Magdalena, din&lt;f&gt;ⓒ&lt;/f&gt; care scosese șapte demoni.');
INSERT INTO &quot;verses&quot; VALUES (480,16,10,'Ea&lt;f&gt;ⓓ&lt;/f&gt; s-a dus și le-a povestit celor care fuseseră cu El, care plângeau și se tânguiau.');
INSERT INTO &quot;verses&quot; VALUES (480,16,11,'Când au auzit ei&lt;f&gt;ⓔ&lt;/f&gt; că este viu și că ea L-a văzut, n-au crezut-o.');
INSERT INTO &quot;verses&quot; VALUES (480,16,12,'După aceea, li S-a arătat, sub o altă înfățișare, la&lt;f&gt;ⓕ&lt;/f&gt; doi dintre ei pe drum, pe când se duceau la țară.');
INSERT INTO &quot;verses&quot; VALUES (480,16,13,'Și aceștia s-au dus și le-au povestit celorlalți, dar nici pe ei nu i-au crezut.');
INSERT INTO &quot;verses&quot; VALUES (480,16,14,'În cele din urmă&lt;f&gt;ⓖ&lt;/f&gt;, li S-a arătat celor unsprezece, pe când stăteau la masă, și i-a mustrat pentru necredința și împietrirea inimii lor, pentru că nu i-au crezut pe cei care-L văzuseră înviat.');
INSERT INTO &quot;verses&quot; VALUES (480,16,15,'&lt;pb/&gt;Apoi&lt;f&gt;ⓗ&lt;/f&gt; le-a zis: &lt;J&gt;„Mergeți în toată lumea și&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;vestiți Evanghelia la orice făptură!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,16,16,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;va crede și se va boteza va fi mântuit, iar&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;cine nu va crede va fi osândit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,16,17,'&lt;i&gt;&lt;J&gt;Iată&lt;/i&gt; semnele care îi vor însoți pe cei ce vor crede: în&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;Numele Meu vor scoate demoni, vor vorbi în&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;limbi noi,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,16,18,'&lt;J&gt;vor lua în mână șerpi&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;, dacă vor bea ceva aducător de moarte, nu-i va vătăma și își&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vor pune mâinile peste cei bolnavi, iar aceștia se vor vindeca.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (480,16,19,'&lt;pb/&gt;După&lt;f&gt;ⓟ&lt;/f&gt; ce le-a vorbit, Domnul Isus a fost luat&lt;f&gt;ⓠ&lt;/f&gt; la cer și S-a&lt;f&gt;ⓡ&lt;/f&gt; așezat la dreapta lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (480,16,20,'Iar ei au plecat și au propovăduit pretutindeni. Domnul lucra împreună cu ei și&lt;f&gt;ⓢ&lt;/f&gt; întărea Cuvântul prin semnele care-l însoțeau. Amin!]]&lt;f&gt;[65]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,1,'&lt;pb/&gt;Fiindcă mulți au încercat să alcătuiască o istorisire despre faptele petrecute printre noi,');
INSERT INTO &quot;verses&quot; VALUES (490,1,2,'după cum&lt;f&gt;ⓐ&lt;/f&gt; ni le-au încredințat cei care le-au văzut de la&lt;f&gt;ⓑ&lt;/f&gt; început cu ochii lor și au ajuns slujitori ai Cuvântului,');
INSERT INTO &quot;verses&quot; VALUES (490,1,3,'am socotit&lt;f&gt;ⓒ&lt;/f&gt; și eu nimerit ca, după ce le-am urmărit pe toate cu migală de la început, să ți le scriu pe rând&lt;f&gt;ⓓ&lt;/f&gt;, preaalesule&lt;f&gt;ⓔ&lt;/f&gt; Teofile,');
INSERT INTO &quot;verses&quot; VALUES (490,1,4,'ca&lt;f&gt;ⓕ&lt;/f&gt; să fii încredințat de temeinicia învățăturilor pe care le-ai primit.');
INSERT INTO &quot;verses&quot; VALUES (490,1,5,'&lt;pb/&gt;În&lt;f&gt;ⓖ&lt;/f&gt; zilele lui Irod, regele Iudeei, era un preot pe nume Zaharia, din&lt;f&gt;ⓗ&lt;/f&gt; ceata preoțească a lui Abia, iar soția lui era dintre fetele lui Aaron și se numea Elisabeta.');
INSERT INTO &quot;verses&quot; VALUES (490,1,6,'Amândoi erau drepți&lt;f&gt;ⓘ&lt;/f&gt; înaintea lui Dumnezeu, trăind fără prihană, după toate poruncile și rânduielile Domnului.');
INSERT INTO &quot;verses&quot; VALUES (490,1,7,'Dar nu aveau copii, pentru că Elisabeta era stearpă; și amândoi erau înaintați în vârstă.');
INSERT INTO &quot;verses&quot; VALUES (490,1,8,'&lt;pb/&gt;Pe când slujea &lt;i&gt;Zaharia&lt;/i&gt; înaintea lui Dumnezeu, la rândul cetei lui,');
INSERT INTO &quot;verses&quot; VALUES (490,1,9,'după obiceiul preoției, a ieșit la&lt;f&gt;ⓙ&lt;/f&gt; sorți să intre să tămâieze&lt;f&gt;ⓚ&lt;/f&gt; în Templul Domnului.');
INSERT INTO &quot;verses&quot; VALUES (490,1,10,'Și o mulțime de popor se ruga afară la&lt;f&gt;ⓛ&lt;/f&gt; ceasul tămâierii.');
INSERT INTO &quot;verses&quot; VALUES (490,1,11,'Atunci, i s-a arătat un înger al Domnului, care stătea în picioare în partea dreaptă a altarului&lt;f&gt;ⓜ&lt;/f&gt; tămâierii.');
INSERT INTO &quot;verses&quot; VALUES (490,1,12,'Când l-a văzut, Zaharia s-a&lt;f&gt;ⓝ&lt;/f&gt; tulburat și a fost cuprins de spaimă.');
INSERT INTO &quot;verses&quot; VALUES (490,1,13,'Dar îngerul i-a spus: &lt;pb/&gt;&lt;t&gt;„Nu te teme, Zaharia,&lt;/t&gt; &lt;t&gt;căci rugăciunea ta a fost ascultată,&lt;/t&gt; &lt;t&gt;și soția ta, Elisabeta, îți va naște un fiu,&lt;/t&gt; &lt;t&gt;căruia îi vei&lt;f&gt;ⓞ&lt;/f&gt; pune numele Ioan!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,14,'&lt;t&gt;El îți va fi bucurie și veselie,&lt;/t&gt; &lt;t&gt;și mulți&lt;f&gt;ⓟ&lt;/f&gt; se vor bucura de nașterea lui.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,15,'&lt;t&gt;Va fi mare înaintea Domnului,&lt;/t&gt; &lt;t&gt;&lt;e&gt;vin și băutură amețitoare nu&lt;/e&gt; &lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;va bea&lt;/e&gt;&lt;f&gt;[1]&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;ci va fi plin de Duhul Sfânt&lt;/t&gt; &lt;t&gt;încă&lt;f&gt;ⓡ&lt;/f&gt; din pântecele mamei lui,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,16,'&lt;t&gt;și pe mulți dintre fiii lui Israel îi va întoarce&lt;/t&gt; &lt;t&gt;la Domnul Dumnezeul lor.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,17,'&lt;t&gt;El va&lt;f&gt;ⓢ&lt;/f&gt; merge înaintea Lui,&lt;/t&gt; &lt;t&gt;în duhul și puterea lui Ilie,&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca să întoarcă inimile părinților spre copii&lt;/e&gt; &lt;f&gt;[2]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;și pe cei neascultători la înțelepciunea celor drepți,&lt;/t&gt; &lt;t&gt;ca să-I rânduiască Domnului un popor pregătit.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,18,'&lt;pb/&gt;Zaharia i-a zis îngerului: „După&lt;f&gt;ⓣ&lt;/f&gt; ce voi cunoaște lucrul acesta, căci eu sunt bătrân și soția mea este înaintată în vârstă?”');
INSERT INTO &quot;verses&quot; VALUES (490,1,19,'Drept răspuns, îngerul i-a zis: „Eu sunt Gabriel&lt;f&gt;ⓤ&lt;/f&gt;, cel care stă înaintea lui Dumnezeu; am fost trimis să-ți vorbesc și să-ți aduc această veste bună.');
INSERT INTO &quot;verses&quot; VALUES (490,1,20,'Iată, vei&lt;f&gt;ⓥ&lt;/f&gt; fi mut și nu vei putea vorbi până în ziua când se vor întâmpla aceste lucruri, pentru că n-ai crezut cuvintele mele, care se vor împlini la timpul lor.”');
INSERT INTO &quot;verses&quot; VALUES (490,1,21,'&lt;pb/&gt;Poporul îl aștepta pe Zaharia și se mira că el zăbovește în Templu.');
INSERT INTO &quot;verses&quot; VALUES (490,1,22,'Când a ieșit, nu putea să le vorbească, iar ei au înțeles că avusese o vedenie în Templu. El le tot făcea semne căci rămăsese mut.');
INSERT INTO &quot;verses&quot; VALUES (490,1,23,'Când i s-au împlinit zilele&lt;f&gt;ⓦ&lt;/f&gt; de slujbă, a plecat acasă.');
INSERT INTO &quot;verses&quot; VALUES (490,1,24,'După un timp, Elisabeta, soția lui, a rămas însărcinată și s-a ascuns vreme de cinci luni, căci zicea:');
INSERT INTO &quot;verses&quot; VALUES (490,1,25,'„Aceasta a făcut-o Domnul pentru mine, în zilele când Și-a aruncat privirea asupra mea, ca să-mi&lt;f&gt;ⓧ&lt;/f&gt; șteargă ocara dintre oameni.”');
INSERT INTO &quot;verses&quot; VALUES (490,1,26,'&lt;pb/&gt;În luna a șasea, îngerul Gabriel a fost trimis de Dumnezeu într-o cetate din Galileea numită Nazaret,');
INSERT INTO &quot;verses&quot; VALUES (490,1,27,'la o fecioară logodită&lt;f&gt;ⓨ&lt;/f&gt; cu un bărbat pe nume Iosif, din casa lui David. Numele fecioarei era Maria.');
INSERT INTO &quot;verses&quot; VALUES (490,1,28,'[Îngerul] a intrat la ea și i-a zis: „Plecăciune&lt;f&gt;[3]&lt;/f&gt; ție&lt;f&gt;ⓩ&lt;/f&gt;, căreia ți s-a făcut har! Domnul&lt;f&gt;ⓐ&lt;/f&gt; este cu tine! [Binecuvântată ești tu între femei!]”');
INSERT INTO &quot;verses&quot; VALUES (490,1,29,'Tulburată&lt;f&gt;ⓑ&lt;/f&gt; foarte mult de cuvântul acesta, ea se întreba ce fel de salut este acesta.');
INSERT INTO &quot;verses&quot; VALUES (490,1,30,'Îngerul i-a zis: &lt;pb/&gt;&lt;t&gt;„Nu te teme, Maria, căci ai găsit har înaintea lui Dumnezeu!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,31,'&lt;t&gt;Iată&lt;f&gt;ⓒ&lt;/f&gt;, vei zămisli și vei naște un Fiu,&lt;/t&gt; &lt;t&gt;căruia Îi vei pune numele&lt;f&gt;ⓓ&lt;/f&gt; Isus.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,32,'&lt;t&gt;El va fi mare și&lt;f&gt;ⓔ&lt;/f&gt; va fi numit Fiul Celui Preaînalt;&lt;/t&gt; &lt;t&gt;și&lt;f&gt;ⓕ&lt;/f&gt; Domnul Dumnezeu Îi va da tronul lui David, părintele Său.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,33,'&lt;t&gt;El va&lt;f&gt;ⓖ&lt;/f&gt; domni peste casa lui Iacov în veci,&lt;/t&gt; &lt;t&gt;și împărăția Lui nu va avea sfârșit.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,34,'&lt;pb/&gt;Maria i-a zis îngerului: „Cum se va întâmpla asta, fiindcă eu nu știu de bărbat?”');
INSERT INTO &quot;verses&quot; VALUES (490,1,35,'Îngerul i-a răspuns zicând: &lt;pb/&gt;&lt;t&gt;„Duhul Sfânt&lt;f&gt;ⓗ&lt;/f&gt; Se va coborî peste tine,&lt;/t&gt; &lt;t&gt;și puterea Celui Preaînalt te va umbri.&lt;/t&gt; &lt;t&gt;De aceea, &lt;i&gt;copilul&lt;/i&gt; sfânt care se va naște va fi numit Fiul&lt;f&gt;ⓘ&lt;/f&gt; lui Dumnezeu.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,36,'&lt;pb/&gt;Iată, Elisabeta, ruda ta, a zămislit și ea un fiu la bătrânețe și este în a șasea lună, ea, care era numită stearpă.');
INSERT INTO &quot;verses&quot; VALUES (490,1,37,'Căci&lt;f&gt;ⓙ&lt;/f&gt; &lt;e&gt;la Dumnezeu niciun lucru nu este cu neputință&lt;/e&gt;&lt;f&gt;[4]&lt;/f&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (490,1,38,'Maria a zis: „Iată roaba Domnului; facă-mi-se după cuvântul tău!” Și îngerul a plecat de la ea.');
INSERT INTO &quot;verses&quot; VALUES (490,1,39,'&lt;pb/&gt;În&lt;f&gt;ⓚ&lt;/f&gt; zilele acelea, Maria s-a sculat și a plecat în grabă spre ținutul muntos, într-o cetate din Iuda.');
INSERT INTO &quot;verses&quot; VALUES (490,1,40,'A intrat în casa lui Zaharia și a salutat-o pe Elisabeta.');
INSERT INTO &quot;verses&quot; VALUES (490,1,41,'De îndată ce a auzit Elisabeta salutul Mariei, i-a tresăltat pruncul în pântec, iar Elisabeta s-a umplut de Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (490,1,42,'Ea a strigat cu glas tare și a zis: &lt;pb/&gt;&lt;t&gt;„Binecuvântată&lt;f&gt;ⓛ&lt;/f&gt; ești tu între femei&lt;/t&gt; &lt;t&gt;și binecuvântat este rodul pântecului tău!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,43,'&lt;pb/&gt;Cum de mi-a fost dat să vină la mine mama Domnului meu?');
INSERT INTO &quot;verses&quot; VALUES (490,1,44,'Fiindcă, iată, cum a ajuns salutul tău la urechile mele, mi-a tresăltat pruncul în pântec de bucurie.');
INSERT INTO &quot;verses&quot; VALUES (490,1,45,'Ferice de cea care a crezut că se vor împlini cele ce i-au fost spuse de Domnul!”');
INSERT INTO &quot;verses&quot; VALUES (490,1,46,'&lt;pb/&gt;Și Maria a zis: &lt;pb/&gt;&lt;t&gt;„Sufletul meu Îl preamărește&lt;f&gt;ⓜ&lt;/f&gt; pe Domnul&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,47,'&lt;t&gt;și mi se bucură duhul în Dumnezeu, Mântuitorul meu,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,48,'&lt;t&gt;pentru că &lt;e&gt;a privit&lt;/e&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;către starea umilă a roabei Sale&lt;/e&gt;&lt;f&gt;[5]&lt;/f&gt;.&lt;/t&gt; &lt;t&gt;Iată, de acum toate generațiile mă&lt;f&gt;ⓞ&lt;/f&gt; vor ferici,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,49,'&lt;t&gt;pentru că Cel Puternic a făcut lucruri mărețe pentru mine&lt;f&gt;ⓟ&lt;/f&gt;.&lt;/t&gt; &lt;t&gt;Numele Lui este sfânt&lt;f&gt;ⓠ&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,50,'&lt;t&gt;și îndurarea&lt;f&gt;ⓡ&lt;/f&gt; Lui &lt;i&gt;rămâne&lt;/i&gt; din generație în generație peste cei ce se tem de El.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,51,'&lt;t&gt;El a făcut &lt;i&gt;lucrări&lt;/i&gt; puternice&lt;f&gt;ⓢ&lt;/f&gt; cu brațul Lui:&lt;/t&gt; &lt;t&gt;i-a risipit&lt;f&gt;ⓣ&lt;/f&gt; pe cei ce se mândreau în cugetul inimii lor,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,52,'&lt;t&gt;i-a răsturnat&lt;f&gt;ⓤ&lt;/f&gt; pe cei puternici de pe tronurile lor&lt;/t&gt; &lt;t&gt;și i-a înălțat pe cei smeriți;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,53,'&lt;t&gt;pe&lt;f&gt;ⓥ&lt;/f&gt; cei flămânzi i-a umplut de bunătăți,&lt;/t&gt; &lt;t&gt;iar pe cei bogați i-a dat afară cu mâinile goale;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,54,'&lt;t&gt;a venit în ajutorul robului Său Israel,&lt;/t&gt; &lt;t&gt;căci Și-a amintit&lt;f&gt;ⓦ&lt;/f&gt; de mila &lt;i&gt;Sa&lt;/i&gt; —&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,55,'&lt;t&gt;după cum&lt;f&gt;ⓧ&lt;/f&gt; le-a vorbit părinților noștri —&lt;/t&gt; &lt;t&gt;față de Avraam și urmașii lui în veac.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,56,'&lt;pb/&gt;Maria a rămas împreună cu &lt;i&gt;Elisabeta&lt;/i&gt; cam trei luni, &lt;i&gt;apoi&lt;/i&gt; s-a întors acasă.');
INSERT INTO &quot;verses&quot; VALUES (490,1,57,'&lt;pb/&gt;Elisabetei i s-a împlinit vremea să nască și a născut un fiu.');
INSERT INTO &quot;verses&quot; VALUES (490,1,58,'Vecinii și rudele ei au auzit că Domnul Și-a arătat marea Lui îndurare față de ea și se&lt;f&gt;ⓨ&lt;/f&gt; bucurau împreună cu ea.');
INSERT INTO &quot;verses&quot; VALUES (490,1,59,'În&lt;f&gt;ⓩ&lt;/f&gt; ziua a opta, au venit să-l circumcidă pe prunc și voiau să-i pună numele Zaharia, după numele tatălui său.');
INSERT INTO &quot;verses&quot; VALUES (490,1,60,'Dar mama lui a răspuns zicând: „Nu&lt;f&gt;ⓐ&lt;/f&gt;, ci se va numi Ioan!”');
INSERT INTO &quot;verses&quot; VALUES (490,1,61,'Ei i-au zis: „Nimeni dintre rudele tale nu poartă acest nume!”');
INSERT INTO &quot;verses&quot; VALUES (490,1,62,'Atunci l-au întrebat pe tatăl său prin semne cum ar vrea să-i pună numele.');
INSERT INTO &quot;verses&quot; VALUES (490,1,63,'&lt;i&gt;Zaharia&lt;/i&gt; a cerut o tăbliță și a scris astfel: „Numele lui este Ioan&lt;f&gt;ⓑ&lt;/f&gt;.” Și toți s-au mirat.');
INSERT INTO &quot;verses&quot; VALUES (490,1,64,'Dintr-odată&lt;f&gt;ⓒ&lt;/f&gt;, i s-a deschis gura și i &lt;i&gt;s-a dezlegat&lt;/i&gt; limba și a început să vorbească, binecuvântându-L pe Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,1,65,'Pe toți vecinii lui i-a cuprins teama și în tot ținutul muntos&lt;f&gt;ⓓ&lt;/f&gt; al Iudeei se vorbea despre toate lucrurile acestea.');
INSERT INTO &quot;verses&quot; VALUES (490,1,66,'Toți cei ce le auzeau le păstrau&lt;f&gt;ⓔ&lt;/f&gt; în inima lor și ziceau: „Oare ce va fi copilul acesta?” Și mâna&lt;f&gt;ⓕ&lt;/f&gt; Domnului era cu el.');
INSERT INTO &quot;verses&quot; VALUES (490,1,67,'&lt;pb/&gt;Zaharia, tatăl lui, s-a&lt;f&gt;ⓖ&lt;/f&gt; umplut de Duhul Sfânt, a profețit și a zis:');
INSERT INTO &quot;verses&quot; VALUES (490,1,68,'&lt;pb/&gt;&lt;t&gt;„Binecuvântat&lt;f&gt;ⓗ&lt;/f&gt; este Domnul Dumnezeul lui Israel&lt;/t&gt; &lt;t&gt;pentru că&lt;f&gt;ⓘ&lt;/f&gt; l-a cercetat și l-a răscumpărat pe poporul Său&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,69,'&lt;t&gt;și&lt;f&gt;ⓙ&lt;/f&gt; &lt;i&gt;pentru că&lt;/i&gt; ne-a ridicat un Mântuitor puternic&lt;f&gt;[6]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;în casa robului Său David,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,70,'&lt;t&gt;după cum&lt;f&gt;ⓚ&lt;/f&gt; vestise prin gura sfinților Săi profeți din vechime:&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,71,'&lt;t&gt;mântuire de vrăjmașii noștri și din mâna tuturor celor ce ne urăsc!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,72,'&lt;t&gt;Astfel&lt;f&gt;ⓛ&lt;/f&gt; Se îndură El de părinții noștri&lt;/t&gt; &lt;t&gt;și Își aduce aminte de legământul &lt;i&gt;Său&lt;/i&gt; cel sfânt:&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,73,'&lt;t&gt;jurământul&lt;f&gt;ⓜ&lt;/f&gt; cu care S-a jurat părintelui nostru Avraam&lt;/t&gt; &lt;t&gt;că,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,74,'&lt;t&gt;după ce ne va izbăvi din mâna vrăjmașilor,&lt;/t&gt; &lt;t&gt;ne va lăsa să-I slujim&lt;f&gt;ⓝ&lt;/f&gt; fără frică&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,75,'&lt;t&gt;în&lt;f&gt;ⓞ&lt;/f&gt; sfințenie și dreptate,&lt;/t&gt; &lt;t&gt;înaintea Lui, în toate zilele noastre.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,76,'&lt;t&gt;Și tu, copile, vei fi numit profet al Celui Preaînalt,&lt;/t&gt; &lt;t&gt;căci&lt;f&gt;ⓟ&lt;/f&gt; vei merge înaintea Domnului ca să-I pregătești căile&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,77,'&lt;t&gt;și să-i dai poporului Său cunoștința mântuirii,&lt;/t&gt; &lt;t&gt;spre&lt;f&gt;ⓠ&lt;/f&gt; iertarea păcatelor lui.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,78,'&lt;t&gt;Prin mila cea mare a Dumnezeului nostru,&lt;/t&gt; &lt;t&gt;ne va cerceta Soarele din înălțime,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,79,'&lt;t&gt;ca &lt;e&gt;să-i lumineze&lt;/e&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;e&gt;pe cei ce zac în întuneric și în umbra morții&lt;/e&gt;&lt;f&gt;[7]&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;și să ne îndrume pașii pe calea păcii!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,1,80,'&lt;pb/&gt;Iar copilul&lt;f&gt;ⓢ&lt;/f&gt; creștea și se întărea în duh, și a&lt;f&gt;ⓣ&lt;/f&gt; stat în pustiu până în ziua arătării sale înaintea lui Israel.');
INSERT INTO &quot;verses&quot; VALUES (490,2,1,'&lt;pb/&gt;În zilele acelea, a ieșit poruncă de la Cezar Augustus să se înscrie&lt;f&gt;[8]&lt;/f&gt; toată lumea.');
INSERT INTO &quot;verses&quot; VALUES (490,2,2,'Recensământul acesta&lt;f&gt;ⓤ&lt;/f&gt; s-a făcut pentru prima dată pe vremea când Quirinius era guvernator al Siriei.');
INSERT INTO &quot;verses&quot; VALUES (490,2,3,'Toți mergeau să se înscrie, fiecare în cetatea lui.');
INSERT INTO &quot;verses&quot; VALUES (490,2,4,'Iosif s-a suit și el din Galileea, din cetatea Nazaret, în Iudeea, în cetatea&lt;f&gt;ⓥ&lt;/f&gt; lui David, numită Betleem — fiindcă&lt;f&gt;ⓦ&lt;/f&gt; era din casa și din seminția lui David —');
INSERT INTO &quot;verses&quot; VALUES (490,2,5,'ca să se înscrie împreună cu Maria, logodnica&lt;f&gt;ⓧ&lt;/f&gt; lui, care era însărcinată.');
INSERT INTO &quot;verses&quot; VALUES (490,2,6,'Pe când erau ei acolo, s-a împlinit vremea ca ea să nască.');
INSERT INTO &quot;verses&quot; VALUES (490,2,7,'Și L-a&lt;f&gt;ⓨ&lt;/f&gt; născut pe Fiul ei întâi născut, L-a înfășat și L-a culcat într-o iesle, pentru că la han nu mai era loc pentru ei.');
INSERT INTO &quot;verses&quot; VALUES (490,2,8,'&lt;pb/&gt;În ținutul acela erau niște păstori care stăteau afară, în câmp, păzindu-și turma în timpul nopții.');
INSERT INTO &quot;verses&quot; VALUES (490,2,9,'Și [iată că] un înger al Domnului li s-a arătat și slava Domnului a strălucit împrejurul lor. Ei&lt;f&gt;ⓩ&lt;/f&gt; au fost cuprinși de o mare spaimă,');
INSERT INTO &quot;verses&quot; VALUES (490,2,10,'dar îngerul le-a zis: „Nu vă temeți! Iată, vă aduc o veste bună, care va fi o mare bucurie pentru&lt;f&gt;ⓐ&lt;/f&gt; tot poporul:');
INSERT INTO &quot;verses&quot; VALUES (490,2,11,'astăzi&lt;f&gt;ⓑ&lt;/f&gt;, în cetatea lui David, vi S-a născut un Mântuitor&lt;f&gt;ⓒ&lt;/f&gt;, care&lt;f&gt;ⓓ&lt;/f&gt; este Hristos, Domnul.');
INSERT INTO &quot;verses&quot; VALUES (490,2,12,'Acesta vă &lt;i&gt;va fi&lt;/i&gt; semnul: veți găsi un copilaș înfășat și culcat într-o iesle.”');
INSERT INTO &quot;verses&quot; VALUES (490,2,13,'Și&lt;f&gt;ⓔ&lt;/f&gt; deodată, alături de înger a venit mulțime de oștire cerească, lăudându-L pe Dumnezeu și zicând:');
INSERT INTO &quot;verses&quot; VALUES (490,2,14,'&lt;pb/&gt;&lt;t&gt;„Slavă&lt;f&gt;ⓕ&lt;/f&gt; lui Dumnezeu în înălțimi&lt;/t&gt; &lt;t&gt;și pace pe pământ&lt;f&gt;ⓖ&lt;/f&gt; între oamenii plăcuți&lt;f&gt;ⓗ&lt;/f&gt; Lui!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,2,15,'&lt;pb/&gt;După ce au plecat îngerii de la ei către cer, păstorii și-au zis unii altora: „Să mergem până la Betleem și să vedem ce s-a întâmplat și ce ne-a făcut cunoscut Domnul!”');
INSERT INTO &quot;verses&quot; VALUES (490,2,16,'Au plecat în grabă și i-au găsit pe Maria, pe Iosif și pe copilaș culcat în iesle.');
INSERT INTO &quot;verses&quot; VALUES (490,2,17,'Când L-au văzut, au povestit ce li se spusese despre copil.');
INSERT INTO &quot;verses&quot; VALUES (490,2,18,'Toți cei ce i-au auzit s-au mirat de cele spuse de ei.');
INSERT INTO &quot;verses&quot; VALUES (490,2,19,'Maria&lt;f&gt;ⓘ&lt;/f&gt; păstra toate cuvintele acestea, cântărindu-le în inima ei.');
INSERT INTO &quot;verses&quot; VALUES (490,2,20,'Păstorii s-au întors, slăvindu-L și lăudându-L pe Dumnezeu pentru toate câte auziseră și văzuseră și care &lt;i&gt;erau&lt;/i&gt; întocmai cum li se spusese.');
INSERT INTO &quot;verses&quot; VALUES (490,2,21,'&lt;pb/&gt;Când&lt;f&gt;ⓙ&lt;/f&gt; a venit ziua a opta, în care trebuia circumcis &lt;i&gt;copilul&lt;/i&gt;, I-au pus numele Isus&lt;f&gt;ⓚ&lt;/f&gt;, care i-a fost dat de înger înainte de zămislirea Sa.');
INSERT INTO &quot;verses&quot; VALUES (490,2,22,'&lt;pb/&gt;Iar când s-au împlinit zilele&lt;f&gt;ⓛ&lt;/f&gt; curățirii lor, după Legea lui Moise, L-au adus la Ierusalim ca să-L înfățișeze înaintea Domnului,');
INSERT INTO &quot;verses&quot; VALUES (490,2,23,'după cum este scris în Legea Domnului: &lt;e&gt;Orice întâi născut de parte bărbătească va fi socotit sfânt pentru Domnul&lt;/e&gt;&lt;f&gt;[9]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (490,2,24,'și ca să aducă jertfă, după cum se spune în Legea Domnului: &lt;e&gt;o pereche de turturele sau doi pui de porumbel&lt;/e&gt;&lt;f&gt;[10]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,2,25,'&lt;pb/&gt;Și iată că în Ierusalim era un om pe nume Simeon. Omul acesta, &lt;i&gt;care era&lt;/i&gt; drept și temător de Dumnezeu, aștepta&lt;f&gt;ⓜ&lt;/f&gt; mângâierea lui Israel, și Duhul Sfânt era asupra lui.');
INSERT INTO &quot;verses&quot; VALUES (490,2,26,'El fusese înștiințat de Duhul Sfânt că nu va muri înainte de a-L vedea&lt;f&gt;ⓝ&lt;/f&gt; pe Unsul&lt;f&gt;[11]&lt;/f&gt; Domnului.');
INSERT INTO &quot;verses&quot; VALUES (490,2,27,'Și a venit în Templu îndemnat de&lt;f&gt;ⓞ&lt;/f&gt; Duhul. Iar când părinții L-au adus pe copilul Isus pentru a-I face după rânduiala din Lege,');
INSERT INTO &quot;verses&quot; VALUES (490,2,28,'&lt;i&gt;Simeon&lt;/i&gt; L-a luat în brațe, L-a binecuvântat pe Dumnezeu și a zis:');
INSERT INTO &quot;verses&quot; VALUES (490,2,29,'&lt;pb/&gt;&lt;t&gt;„Acum&lt;f&gt;ⓟ&lt;/f&gt;, dă drumul în pace robului Tău, Stăpâne,&lt;/t&gt; &lt;t&gt;după cuvântul Tău,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,2,30,'&lt;t&gt;căci ochii mei au văzut&lt;f&gt;ⓠ&lt;/f&gt; mântuirea Ta,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,2,31,'&lt;t&gt;pe care ai pregătit-o în fața tuturor popoarelor,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,2,32,'&lt;t&gt;lumină&lt;f&gt;ⓡ&lt;/f&gt; &lt;i&gt;care aduce&lt;/i&gt; descoperire&lt;f&gt;[12]&lt;/f&gt; neamurilor&lt;/t&gt; &lt;t&gt;și slavă poporului Tău, Israel.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,2,33,'&lt;pb/&gt;Tatăl și mama Lui se minunau de cele ce se spuneau cu privire la El.');
INSERT INTO &quot;verses&quot; VALUES (490,2,34,'Simeon i-a binecuvântat și i-a zis Mariei, mama Lui: „Iată, acest &lt;i&gt;Copil&lt;/i&gt; este menit să ducă la căderea&lt;f&gt;ⓢ&lt;/f&gt; și ridicarea multora din Israel și ca un semn&lt;f&gt;ⓣ&lt;/f&gt; &lt;i&gt;care va stârni&lt;/i&gt; împotrivire');
INSERT INTO &quot;verses&quot; VALUES (490,2,35,'— chiar&lt;f&gt;ⓤ&lt;/f&gt; sufletul tău va fi străpuns de o sabie — ca să fie dezvăluite gândurile multor inimi.”');
INSERT INTO &quot;verses&quot; VALUES (490,2,36,'&lt;pb/&gt;Mai era acolo și o profetesă, Ana, fiica lui Fanuel, din seminția lui Așer. Ea era foarte înaintată în vârstă și trăise cu bărbatul ei șapte ani după fecioria ei.');
INSERT INTO &quot;verses&quot; VALUES (490,2,37,'Fiind văduvă și în vârstă de optzeci și patru de ani, nu se depărta de Templu și zi&lt;f&gt;ⓥ&lt;/f&gt; și noapte slujea cu post și rugăciuni.');
INSERT INTO &quot;verses&quot; VALUES (490,2,38,'A venit și ea în același ceas și-I mulțumea lui Dumnezeu, vorbind despre &lt;i&gt;Isus&lt;/i&gt; tuturor celor ce așteptau&lt;f&gt;ⓦ&lt;/f&gt; răscumpărarea Ierusalimului.');
INSERT INTO &quot;verses&quot; VALUES (490,2,39,'&lt;pb/&gt;După ce au împlinit rânduiala cerută de Legea Domnului, s-au întors în Galileea, în cetatea lor, Nazaret.');
INSERT INTO &quot;verses&quot; VALUES (490,2,40,'Iar&lt;f&gt;ⓧ&lt;/f&gt; Copilul creștea și Se întărea, era tot mai plin de înțelepciune, și harul lui Dumnezeu era peste El.');
INSERT INTO &quot;verses&quot; VALUES (490,2,41,'&lt;pb/&gt;În&lt;f&gt;ⓨ&lt;/f&gt; fiecare an, părinții lui Isus mergeau la Ierusalim cu ocazia sărbătorii Paștelui.');
INSERT INTO &quot;verses&quot; VALUES (490,2,42,'Când era El în vârstă de doisprezece ani, s-au suit [la Ierusalim], după obiceiul sărbătorii.');
INSERT INTO &quot;verses&quot; VALUES (490,2,43,'După ce au trecut &lt;i&gt;acele&lt;/i&gt; zile, pe când se întorceau &lt;i&gt;acasă&lt;/i&gt;, băiatul Isus a rămas în Ierusalim, iar părinții Lui n-au știut.');
INSERT INTO &quot;verses&quot; VALUES (490,2,44,'Au crezut că este cu tovarășii lor de călătorie și au mers cale de o zi; apoi L-au căutat printre rudele și cunoscuții lor,');
INSERT INTO &quot;verses&quot; VALUES (490,2,45,'dar nu L-au găsit, așa că s-au întors la Ierusalim să-L caute.');
INSERT INTO &quot;verses&quot; VALUES (490,2,46,'După trei zile L-au găsit în Templu, așezat în mijlocul învățătorilor, ascultându-i și punându-le întrebări.');
INSERT INTO &quot;verses&quot; VALUES (490,2,47,'Toți care-L auzeau erau uimiți&lt;f&gt;ⓩ&lt;/f&gt; de priceperea și răspunsurile Lui.');
INSERT INTO &quot;verses&quot; VALUES (490,2,48,'Când L-au văzut &lt;i&gt;părinții Lui&lt;/i&gt;, au rămas înmărmuriți, iar mama Lui I-a zis: „Copile, de ce ne-ai făcut una ca asta? Iată că tatăl Tău și cu mine Te-am căutat îngrijorați.”');
INSERT INTO &quot;verses&quot; VALUES (490,2,49,'El le-a zis: &lt;J&gt;„De ce M-ați căutat? Nu știați că trebuie să fiu în&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;casa Tatălui Meu?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,2,50,'Dar ei&lt;f&gt;ⓑ&lt;/f&gt; n-au înțeles ce le-a spus.');
INSERT INTO &quot;verses&quot; VALUES (490,2,51,'Apoi, S-a coborât împreună cu ei, a venit la Nazaret și le era supus. Mama Sa păstra&lt;f&gt;ⓒ&lt;/f&gt; toate cuvintele acestea în inima ei.');
INSERT INTO &quot;verses&quot; VALUES (490,2,52,'Și Isus &lt;e&gt;creștea&lt;/e&gt;&lt;f&gt;ⓓ&lt;/f&gt; în înțelepciune, în statură și în har &lt;e&gt;înaintea lui Dumnezeu și a oamenilor&lt;/e&gt;&lt;f&gt;[13]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,3,1,'&lt;pb/&gt;În anul al cincisprezecelea al domniei lui Tiberiu Cezar, pe când Pilat din Pont era procuratorul Iudeei, Irod — tetrarh al Galileei, Filip, fratele lui — tetrarh al provinciilor Ituria și Trahonitis, iar Lisanias — tetrarh al Abilenei,');
INSERT INTO &quot;verses&quot; VALUES (490,3,2,'în&lt;f&gt;ⓔ&lt;/f&gt; zilele marilor-preoți Ana și Caiafa, cuvântul lui Dumnezeu a venit la Ioan, fiul lui Zaharia, în pustiu.');
INSERT INTO &quot;verses&quot; VALUES (490,3,3,'Și&lt;f&gt;ⓕ&lt;/f&gt; el a mers prin tot ținutul din împrejurimile Iordanului, propovăduind botezul pocăinței spre&lt;f&gt;ⓖ&lt;/f&gt; iertarea păcatelor,');
INSERT INTO &quot;verses&quot; VALUES (490,3,4,'după cum este scris în cartea cuvintelor profetului Isaia: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Glasul&lt;/e&gt; &lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;celui ce strigă în pustiu:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;„Pregătiți calea Domnului,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;îndreptați cărările Lui!&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,3,5,'&lt;t&gt;&lt;e&gt;Orice vale să se umple&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și orice munte sau deal să se plece;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;i&gt;căile&lt;/i&gt; strâmbe să se îndrepte&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și cele pietroase să se facă drumuri netede,&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,3,6,'&lt;t&gt;&lt;e&gt;pentru ca orice&lt;/e&gt; &lt;f&gt;ⓘ&lt;/f&gt; &lt;e&gt;făptură să vadă mântuirea lui Dumnezeu.”&lt;/e&gt;&lt;f&gt;[14]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,3,7,'&lt;pb/&gt;&lt;i&gt;Ioan&lt;/i&gt; le zicea mulțimilor care veneau să fie botezate de el: „Pui&lt;f&gt;ⓙ&lt;/f&gt; de vipere, cine v-a învățat să fugiți de mânia care vine?');
INSERT INTO &quot;verses&quot; VALUES (490,3,8,'Faceți roade vrednice de pocăință și nu începeți să ziceți în sinea voastră: «Îl avem ca tată pe Avraam»! Vă spun că Dumnezeu din pietrele acestea poate să-i ridice copii lui Avraam.');
INSERT INTO &quot;verses&quot; VALUES (490,3,9,'Securea așteaptă deja la rădăcina pomilor. Orice pom&lt;f&gt;ⓚ&lt;/f&gt; care nu face rod bun va fi tăiat și aruncat în foc.”');
INSERT INTO &quot;verses&quot; VALUES (490,3,10,'&lt;pb/&gt;Mulțimile îl întrebau zicând: „Atunci&lt;f&gt;ⓛ&lt;/f&gt; ce să facem?”');
INSERT INTO &quot;verses&quot; VALUES (490,3,11,'Drept răspuns, el le zicea: „Cine&lt;f&gt;ⓜ&lt;/f&gt; are două haine să împartă cu cine nu are, și cine are mâncare să facă la fel.”');
INSERT INTO &quot;verses&quot; VALUES (490,3,12,'Au&lt;f&gt;ⓝ&lt;/f&gt; venit și niște vameși să fie botezați și i-au zis: „Învățătorule, noi ce să facem?”');
INSERT INTO &quot;verses&quot; VALUES (490,3,13,'El le-a zis: „Să nu&lt;f&gt;ⓞ&lt;/f&gt; luați mai mult decât v-a fost poruncit.”');
INSERT INTO &quot;verses&quot; VALUES (490,3,14,'Niște soldați îl întrebau și ei zicând: „Dar noi ce să facem?” El le-a spus: „Să nu stoarceți bani de la nimeni, nici&lt;f&gt;ⓟ&lt;/f&gt; să nu învinuiți pe nedrept, ci mulțumiți-vă cu soldele voastre!”');
INSERT INTO &quot;verses&quot; VALUES (490,3,15,'&lt;pb/&gt;Fiindcă poporul era în așteptare și toți se întrebau în sinea lor cu privire la Ioan dacă nu cumva el este Hristosul,');
INSERT INTO &quot;verses&quot; VALUES (490,3,16,'Ioan le-a răspuns tuturor zicând: „Eu&lt;f&gt;ⓠ&lt;/f&gt; vă botez în apă; dar vine Unul mai puternic decât mine, căruia eu nu sunt vrednic să-I dezleg cureaua încălțămintei. El vă va boteza în Duhul Sfânt și în foc.');
INSERT INTO &quot;verses&quot; VALUES (490,3,17,'Acela are lopata &lt;i&gt;de vânturat&lt;/i&gt; în mână ca să-Și curețe aria și&lt;f&gt;ⓡ&lt;/f&gt; să-Și adune grâul în grânar; pleava însă o va arde în focul care nu se stinge.”');
INSERT INTO &quot;verses&quot; VALUES (490,3,18,'&lt;pb/&gt;Cu aceste &lt;i&gt;cuvinte&lt;/i&gt; și cu multe altele propovăduia Ioan și dădea îndemnuri poporului.');
INSERT INTO &quot;verses&quot; VALUES (490,3,19,'Dar&lt;f&gt;ⓢ&lt;/f&gt; tetrarhul Irod, care era mustrat de &lt;i&gt;Ioan&lt;/i&gt; pentru Irodiada, soția fratelui său [Filip], și pentru toate relele pe care le făcuse,');
INSERT INTO &quot;verses&quot; VALUES (490,3,20,'a mai adăugat la ele și pe acela că l-a închis pe Ioan în temniță.');
INSERT INTO &quot;verses&quot; VALUES (490,3,21,'&lt;pb/&gt;După ce a fost botezat tot poporul, a fost botezat și Isus&lt;f&gt;ⓣ&lt;/f&gt;; și, pe când Se ruga, s-a deschis cerul,');
INSERT INTO &quot;verses&quot; VALUES (490,3,22,'și Duhul Sfânt a coborât peste El în chip trupesc, ca un porumbel. Și un glas s-a auzit din cer: „Tu ești Fiul Meu preaiubit, în Tine Îmi găsesc toată plăcerea!”');
INSERT INTO &quot;verses&quot; VALUES (490,3,23,'&lt;pb/&gt;Când Și-a început &lt;i&gt;lucrarea&lt;/i&gt;, Isus avea în jur de&lt;f&gt;ⓤ&lt;/f&gt; treizeci de ani; și era, după cum se credea, fiul lui Iosif, fiul lui Eli,');
INSERT INTO &quot;verses&quot; VALUES (490,3,24,'fiul lui Matat, fiul lui Levi, fiul lui Melhi, fiul lui Ianai, fiul lui Iosif,');
INSERT INTO &quot;verses&quot; VALUES (490,3,25,'fiul lui Matatia, fiul lui Amos, fiul lui Naum, fiul lui Hesli, fiul lui Nagai,');
INSERT INTO &quot;verses&quot; VALUES (490,3,26,'fiul lui Maat, fiul lui Matatia, fiul lui Semein, fiul lui Ioseh, fiul lui Ioda,');
INSERT INTO &quot;verses&quot; VALUES (490,3,27,'fiul lui Ioanan, fiul lui Resa, fiul lui Zorobabel, fiul lui Salatiel, fiul lui Neri,');
INSERT INTO &quot;verses&quot; VALUES (490,3,28,'fiul lui Melhi, fiul lui Adi, fiul lui Cosam, fiul lui Elmadam, fiul lui Er,');
INSERT INTO &quot;verses&quot; VALUES (490,3,29,'fiul lui Isus, fiul lui Eliezer, fiul lui Iorim, fiul lui Matat, fiul lui Levi,');
INSERT INTO &quot;verses&quot; VALUES (490,3,30,'fiul lui Simeon, fiul lui Iuda, fiul lui Iosif, fiul lui Ionam, fiul lui Eliachim,');
INSERT INTO &quot;verses&quot; VALUES (490,3,31,'fiul lui Melea, fiul lui Mena, fiul lui Matata, fiul lui Natan&lt;f&gt;ⓥ&lt;/f&gt;, fiul lui&lt;f&gt;ⓦ&lt;/f&gt; David,');
INSERT INTO &quot;verses&quot; VALUES (490,3,32,'fiul&lt;f&gt;ⓧ&lt;/f&gt; lui Ișai, fiul lui Iobed, fiul lui Boaz, fiul lui Sala&lt;f&gt;[15]&lt;/f&gt;, fiul lui Naason,');
INSERT INTO &quot;verses&quot; VALUES (490,3,33,'fiul lui Aminadab, fiul lui Admin, fiul lui Arni, fiul lui Esrom, fiul lui Fares, fiul lui Iuda,');
INSERT INTO &quot;verses&quot; VALUES (490,3,34,'fiul lui Iacov, fiul lui Isaac, fiul lui Avraam, fiul&lt;f&gt;ⓨ&lt;/f&gt; lui Tara&lt;f&gt;[16]&lt;/f&gt;, fiul lui Nahor,');
INSERT INTO &quot;verses&quot; VALUES (490,3,35,'fiul lui Seruh, fiul lui Ragau, fiul lui Falec, fiul lui Eber, fiul lui Sala,');
INSERT INTO &quot;verses&quot; VALUES (490,3,36,'fiul&lt;f&gt;ⓩ&lt;/f&gt; lui Cainan, fiul lui Arfaxad, fiul&lt;f&gt;ⓐ&lt;/f&gt; lui Sem, fiul lui Noe, fiul lui Lameh,');
INSERT INTO &quot;verses&quot; VALUES (490,3,37,'fiul lui Matusala, fiul lui Enoh, fiul lui Iared, fiul lui Maleleel, fiul lui Cainan,');
INSERT INTO &quot;verses&quot; VALUES (490,3,38,'fiul lui Enos, fiul lui Set, fiul lui Adam, fiul&lt;f&gt;ⓑ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,4,1,'&lt;pb/&gt;Isus&lt;f&gt;ⓒ&lt;/f&gt;, plin de Duhul Sfânt, S-a întors de la Iordan și a fost dus&lt;f&gt;ⓓ&lt;/f&gt; de Duhul în pustiu,');
INSERT INTO &quot;verses&quot; VALUES (490,4,2,'&lt;i&gt;unde&lt;/i&gt; a fost ispitit de Diavol timp de patruzeci de zile. N-a mâncat nimic&lt;f&gt;ⓔ&lt;/f&gt; în acele zile; la sfârșitul lor, a flămânzit.');
INSERT INTO &quot;verses&quot; VALUES (490,4,3,'Diavolul I-a zis: „Dacă ești Fiul lui Dumnezeu, poruncește acestei pietre să se facă pâine!”');
INSERT INTO &quot;verses&quot; VALUES (490,4,4,'Isus i-a răspuns: &lt;J&gt;„Este scris: &lt;e&gt;Omul nu va trăi numai cu pâine, [ci cu orice cuvânt care iese din gura lui Dumnezeu].&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[17]&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,5,'[Diavolul] L-a urcat [pe un munte înalt], I-a arătat într-o clipă toate împărățiile lumii');
INSERT INTO &quot;verses&quot; VALUES (490,4,6,'și I-a zis: „Ție Îți voi da slava lor și toată stăpânirea aceasta; căci mie mi-a fost dată&lt;f&gt;ⓕ&lt;/f&gt; și o dau cui vreau.');
INSERT INTO &quot;verses&quot; VALUES (490,4,7,'Așadar, dacă Te vei închina în fața mea, toată va fi a Ta.”');
INSERT INTO &quot;verses&quot; VALUES (490,4,8,'Drept răspuns, Isus i-a zis: &lt;J&gt;„[Înapoia Mea, Satano!] Este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Domnului Dumnezeului tău să te închini&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și numai Lui să-I slujești.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[18]&lt;/f&gt; &lt;J&gt;”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,9,'&lt;pb/&gt;&lt;i&gt;Diavolul&lt;/i&gt; &lt;f&gt;ⓖ&lt;/f&gt; L-a dus apoi în Ierusalim, L-a pus pe acoperișul Templului și I-a zis: „Dacă ești Fiul lui Dumnezeu, aruncă-Te jos de aici;');
INSERT INTO &quot;verses&quot; VALUES (490,4,10,'căci este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;El va porunci îngerilor Săi&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;să te păzească&lt;/e&gt; &lt;f&gt;[19]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,11,'&lt;pb/&gt;și &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Ei te vor purta pe mâini,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca nu cumva să-ți lovești piciorul de vreo piatră.&lt;/e&gt; &lt;f&gt;[20]&lt;/f&gt;”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,12,'&lt;pb/&gt;Isus i-a răspuns zicând: &lt;J&gt;„S-a spus: &lt;e&gt;Să nu-L ispitești pe Domnul Dumnezeul tău.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[21]&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,13,'După ce L-a ispitit în toate aceste feluri, Diavolul a plecat de la El pentru&lt;f&gt;ⓗ&lt;/f&gt; o vreme.');
INSERT INTO &quot;verses&quot; VALUES (490,4,14,'&lt;pb/&gt;Isus&lt;f&gt;ⓘ&lt;/f&gt;, plin de&lt;f&gt;ⓙ&lt;/f&gt; puterea Duhului, S-a întors în Galileea&lt;f&gt;ⓚ&lt;/f&gt;, iar vestea despre El s-a răspândit peste tot în împrejurimi.');
INSERT INTO &quot;verses&quot; VALUES (490,4,15,'El îi învăța &lt;i&gt;pe oameni&lt;/i&gt; în sinagogile lor și era lăudat de toți.');
INSERT INTO &quot;verses&quot; VALUES (490,4,16,'&lt;pb/&gt;A mers în Nazaret&lt;f&gt;ⓛ&lt;/f&gt;, unde fusese crescut. După &lt;f&gt;ⓜ&lt;/f&gt; obiceiul Său, în sabat a intrat în sinagogă și S-a ridicat să citească.');
INSERT INTO &quot;verses&quot; VALUES (490,4,17,'I s-a dat cartea profetului Isaia, iar El a deschis-o și a găsit locul unde era scris:');
INSERT INTO &quot;verses&quot; VALUES (490,4,18,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;„Duhul Domnului este peste Mine,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;pentru că M-a uns&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;să le aduc săracilor o veste bună,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;M-a trimis [să-i tămăduiesc pe cei cu inima zdrobită,]&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;să le propovăduiesc prinșilor de război eliberarea&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și orbilor căpătarea vederii,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;să-i eliberez pe cei asupriți&lt;/J&gt;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,19,'&lt;t&gt;&lt;e&gt;&lt;J&gt;și să vestesc anul bunăvoinței Domnului.”&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[22]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,20,'&lt;pb/&gt;Apoi, a înfășurat sulul cărții, i l-a dat înapoi îngrijitorului și S-a așezat. Privirile tuturor celor din sinagogă erau ațintite asupra Lui.');
INSERT INTO &quot;verses&quot; VALUES (490,4,21,'Și a început să le spună: &lt;J&gt;„Astăzi, Scriptura aceasta s-a împlinit în auzul vostru.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,22,'Și toți Îl vorbeau de bine, se&lt;f&gt;ⓝ&lt;/f&gt; mirau de cuvintele cu har care-I ieșeau din gură și ziceau: „Oare&lt;f&gt;ⓞ&lt;/f&gt; nu este acesta fiul lui Iosif?”');
INSERT INTO &quot;verses&quot; VALUES (490,4,23,'&lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Fără îndoială, Îmi veți spune zicala aceea: «‘Doctore, vindecă-te pe tine însuți!’ Ce am auzit că ai făcut în Capernaum&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;, fă și aici, în patria&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Ta!»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,24,'Și a mai zis: &lt;J&gt;„Adevărat vă spun că niciun profet&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;nu este bine primit în patria lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,25,'&lt;J&gt;Ba încă, adevărat vă spun că erau multe&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;văduve în Israel în zilele lui Ilie, când a fost încuiat cerul timp de trei ani și șase luni, astfel că a fost o mare foamete în toată țara,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,26,'&lt;J&gt;și totuși Ilie n-a fost trimis la niciuna dintre ele, ci la o văduvă din Sarepta Sidonului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,27,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;mulți leproși erau în Israel pe vremea profetului Elisei, dar niciunul dintre ei n-a fost curățit, ci numai Naaman, sirianul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,28,'Auzind acestea, toți cei din sinagogă s-au umplut de mânie,');
INSERT INTO &quot;verses&quot; VALUES (490,4,29,'s-au ridicat, L-au scos din cetate și L-au dus până pe sprânceana muntelui pe care era zidită cetatea lor, ca să-L arunce &lt;i&gt;de acolo&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,4,30,'Dar &lt;i&gt;Isus&lt;/i&gt; a trecut&lt;f&gt;ⓤ&lt;/f&gt; prin mijlocul lor și a plecat.');
INSERT INTO &quot;verses&quot; VALUES (490,4,31,'&lt;pb/&gt;S-a&lt;f&gt;ⓥ&lt;/f&gt; coborât în Capernaum, cetate din Galileea, și acolo îi învăța &lt;i&gt;pe oameni&lt;/i&gt; în sabat.');
INSERT INTO &quot;verses&quot; VALUES (490,4,32,'Ei erau uimiți de învățătura Lui, căci&lt;f&gt;ⓦ&lt;/f&gt; cuvântul Lui avea putere.');
INSERT INTO &quot;verses&quot; VALUES (490,4,33,'În&lt;f&gt;ⓧ&lt;/f&gt; sinagogă era un om care avea un demon, un duh necurat. El a strigat cu glas tare:');
INSERT INTO &quot;verses&quot; VALUES (490,4,34,'„Ah! Ce avem noi a face cu Tine, Isuse din Nazaret? Ai venit să ne pierzi? Te&lt;f&gt;ⓨ&lt;/f&gt; știu cine ești: Sfântul&lt;f&gt;ⓩ&lt;/f&gt; lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (490,4,35,'Isus l-a certat și i-a zis: &lt;J&gt;„Taci și ieși din el!”&lt;/J&gt; Demonul l-a trântit în mijlocul &lt;i&gt;adunării&lt;/i&gt; și a ieșit afară din el, fără a-l vătăma.');
INSERT INTO &quot;verses&quot; VALUES (490,4,36,'Toți au fost cuprinși de spaimă, vorbeau între ei și ziceau: „Ce &lt;i&gt;fel de&lt;/i&gt; cuvânt are acesta de poruncește cu autoritate și putere duhurilor necurate, și ele ies afară?”');
INSERT INTO &quot;verses&quot; VALUES (490,4,37,'Și s-a dus vestea despre El peste tot în împrejurimi.');
INSERT INTO &quot;verses&quot; VALUES (490,4,38,'&lt;pb/&gt;După ce&lt;f&gt;ⓐ&lt;/f&gt; a ieșit din sinagogă, a intrat în casa lui Simon. Soacra lui Simon era cuprinsă de febră mare și I-au făcut o rugăminte cu privire la ea.');
INSERT INTO &quot;verses&quot; VALUES (490,4,39,'El S-a aplecat către ea, a certat febra și &lt;i&gt;aceasta&lt;/i&gt; a lăsat-o. Îndată ea s-a ridicat și a început să le slujească.');
INSERT INTO &quot;verses&quot; VALUES (490,4,40,'La&lt;f&gt;ⓑ&lt;/f&gt; asfințitul soarelui, toți cei ce aveau bolnavi atinși de felurite neputințe i-au adus la El. El Și-a pus mâinile peste fiecare dintre ei și i-a vindecat.');
INSERT INTO &quot;verses&quot; VALUES (490,4,41,'Din&lt;f&gt;ⓒ&lt;/f&gt; mulți ieșeau și demoni, care strigau și ziceau: „Tu ești [Hristosul,] Fiul lui Dumnezeu!” Dar El&lt;f&gt;ⓓ&lt;/f&gt; îi mustra și nu-i lăsa să vorbească, fiindcă știau că El este Hristosul.');
INSERT INTO &quot;verses&quot; VALUES (490,4,42,'&lt;pb/&gt;Când&lt;f&gt;ⓔ&lt;/f&gt; s-a luminat de ziuă, &lt;i&gt;Isus&lt;/i&gt; a ieșit și a plecat într-un loc pustiu. Mulțimile Îl căutau și, când au ajuns la El, voiau să-L oprească să nu plece de la ei.');
INSERT INTO &quot;verses&quot; VALUES (490,4,43,'Dar El le-a zis: &lt;J&gt;„Trebuie să vestesc Împărăția lui Dumnezeu și în alte cetăți, fiindcă pentru aceasta am fost trimis.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,4,44,'Și&lt;f&gt;ⓕ&lt;/f&gt; propovăduia în sinagogile din Iudeea.&lt;f&gt;[23]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,1,'&lt;pb/&gt;Mulțimea se îmbulzea în jurul lui &lt;i&gt;Isus&lt;/i&gt; ca să audă Cuvântul lui Dumnezeu. El era&lt;f&gt;ⓖ&lt;/f&gt; pe malul lacului Ghenezaret');
INSERT INTO &quot;verses&quot; VALUES (490,5,2,'și a văzut două bărci la marginea lacului. Pescarii coborâseră din ele să-și spele mrejele.');
INSERT INTO &quot;verses&quot; VALUES (490,5,3,'S-a suit într-una dintre aceste bărci, care era a lui Simon, și l-a rugat s-o depărteze puțin de țărm. Apoi S-a așezat și, din barcă, învăța mulțimile.');
INSERT INTO &quot;verses&quot; VALUES (490,5,4,'Când a terminat de vorbit, i-a zis lui Simon: &lt;J&gt;„Depărteaz-o&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;la adânc și aruncați-vă mrejele pentru pescuit!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,5,'Drept răspuns, Simon I-a zis: „Învățătorule, toată noaptea ne-am trudit și n-am prins nimic, dar la cuvântul Tău voi arunca mrejele!”');
INSERT INTO &quot;verses&quot; VALUES (490,5,6,'Și, făcând aceasta, au prins o mulțime atât de mare de pești, încât mrejele începuseră să se rupă.');
INSERT INTO &quot;verses&quot; VALUES (490,5,7,'Le-au făcut semn tovarășilor lor din cealaltă barcă să vină să-i ajute. Au venit și au umplut amândouă bărcile, încât erau gata să se scufunde.');
INSERT INTO &quot;verses&quot; VALUES (490,5,8,'Văzând &lt;i&gt;aceasta&lt;/i&gt;, Simon Petru a căzut la picioarele lui Isus și I-a zis: „Doamne, pleacă&lt;f&gt;ⓘ&lt;/f&gt; de la mine, căci sunt un om păcătos!”');
INSERT INTO &quot;verses&quot; VALUES (490,5,9,'Fiindcă îi apucase spaima pe el și pe toți cei care erau cu el din pricina pescuirii pe care o făcuseră.');
INSERT INTO &quot;verses&quot; VALUES (490,5,10,'Tot așa pe Iacov și pe Ioan, fiii lui Zebedei, tovarășii lui Simon. Dar Isus i-a zis lui Simon: &lt;J&gt;„Nu te teme; de acum&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;încolo, vei fi pescar de oameni.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,11,'Ei au adus bărcile la mal, au lăsat&lt;f&gt;ⓚ&lt;/f&gt; totul și L-au urmat.');
INSERT INTO &quot;verses&quot; VALUES (490,5,12,'&lt;pb/&gt;Pe când Se afla Isus&lt;f&gt;ⓛ&lt;/f&gt; într-una dintre cetăți, iată că un bărbat plin de lepră, văzându-L, a căzut cu fața la pământ și L-a rugat zicând: „Doamne, dacă vrei, poți să mă curățești!”');
INSERT INTO &quot;verses&quot; VALUES (490,5,13,'&lt;i&gt;Isus&lt;/i&gt; a întins mâna și l-a atins, zicând: &lt;J&gt;„Da, vreau, fii curățit!”&lt;/J&gt; Și îndată s-a dus lepra de pe el.');
INSERT INTO &quot;verses&quot; VALUES (490,5,14,'I-a poruncit să nu spună nimănui&lt;f&gt;ⓜ&lt;/f&gt;, &lt;i&gt;apoi i-a zis&lt;/i&gt;: &lt;J&gt;„Mergi și arată-te preotului și adu &lt;i&gt;jertfă&lt;/i&gt; pentru curățirea ta, după cum&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;a poruncit Moise, ca mărturie pentru ei!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,15,'Vestea despre El se răspândea tot mai mult și mulțimi mari &lt;i&gt;de oameni&lt;/i&gt; se&lt;f&gt;ⓞ&lt;/f&gt; adunau ca să-L asculte și să se vindece de boli.');
INSERT INTO &quot;verses&quot; VALUES (490,5,16,'Și&lt;f&gt;ⓟ&lt;/f&gt; El Se retrăgea în locuri pustii și Se ruga.');
INSERT INTO &quot;verses&quot; VALUES (490,5,17,'&lt;pb/&gt;Într-o zi, pe când Isus îi învăța &lt;i&gt;pe oameni&lt;/i&gt;, s-au așezat acolo și niște farisei și învățători ai Legii, care veniseră din toate satele Galileei și Iudeei și din Ierusalim; iar puterea Domnului era cu El, ca să vindece.');
INSERT INTO &quot;verses&quot; VALUES (490,5,18,'Și&lt;f&gt;ⓠ&lt;/f&gt; iată că niște bărbați purtau un om paralizat pe un pat și încercau să-l ducă &lt;i&gt;înăuntru&lt;/i&gt;, ca să-l pună înaintea Lui.');
INSERT INTO &quot;verses&quot; VALUES (490,5,19,'Fiindcă n-au găsit pe unde să-l ducă &lt;i&gt;înăuntru&lt;/i&gt; din cauza mulțimii, s-au urcat pe acoperișul &lt;i&gt;casei&lt;/i&gt; și l-au coborât cu patul printre țigle, în mijloc, înaintea lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (490,5,20,'Văzându-le credința, El a zis: &lt;J&gt;„Omule, păcatele îți sunt iertate!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,21,'Cărturarii și fariseii au&lt;f&gt;ⓡ&lt;/f&gt; început să se întrebe în sinea lor: „Cine&lt;f&gt;ⓢ&lt;/f&gt; se crede acesta, de rostește blasfemii? Cine poate ierta păcatele în afară de Unul singur — Dumnezeu?”');
INSERT INTO &quot;verses&quot; VALUES (490,5,22,'Isus le-a cunoscut gândurile și le-a răspuns zicând: &lt;J&gt;„De ce gândiți astfel în inimile voastre?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,23,'&lt;J&gt;Ce este mai ușor? A zice: «Păcatele îți sunt iertate» sau a zice: «Scoală-te și umblă»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,24,'&lt;J&gt;Dar, ca să știți că Fiul Omului are autoritate pe pământ să ierte păcatele, ție îți poruncesc”&lt;/J&gt;, i-a zis El paraliticului, &lt;J&gt;„scoală-te, ridică-ți patul și du-te acasă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,25,'Și îndată, paraliticul s-a sculat în fața lor, a ridicat &lt;i&gt;patul&lt;/i&gt; pe care zăcea și a mers acasă, slăvindu-L pe Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,5,26,'Toți au rămas uimiți și Îl slăveau pe Dumnezeu; cuprinși de teamă, ziceau: „Astăzi am văzut lucruri nemaipomenite.”');
INSERT INTO &quot;verses&quot; VALUES (490,5,27,'&lt;pb/&gt;După&lt;f&gt;ⓣ&lt;/f&gt; aceea, Isus a ieșit și l-a văzut pe un vameș pe nume Levi stând la vamă și i-a zis: &lt;J&gt;„Vino după Mine!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,28,'El a lăsat totul, s-a ridicat și L-a urmat.');
INSERT INTO &quot;verses&quot; VALUES (490,5,29,'Levi a dat&lt;f&gt;ⓤ&lt;/f&gt; pentru &lt;i&gt;Isus&lt;/i&gt; un ospăț mare în casa lui și o mulțime de vameși și alți &lt;i&gt;oaspeți&lt;/i&gt; au stat la masă cu ei.');
INSERT INTO &quot;verses&quot; VALUES (490,5,30,'Fariseii și cărturarii cârteau și le ziceau ucenicilor Lui: „De ce mâncați și beți cu vameșii și cu păcătoșii?”');
INSERT INTO &quot;verses&quot; VALUES (490,5,31,'Isus le-a răspuns: &lt;J&gt;„Nu cei sănătoși au nevoie de doctor, ci cei bolnavi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,32,'&lt;J&gt;N-am&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;venit să-i chem la pocăință pe cei drepți, ci pe cei păcătoși.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,33,'&lt;pb/&gt;Ei I-au zis: „Ucenicii&lt;f&gt;ⓦ&lt;/f&gt; lui Ioan postesc des și fac rugăciuni, la fel și ai fariseilor, pe când ai tăi mănâncă și beau.”');
INSERT INTO &quot;verses&quot; VALUES (490,5,34,'El le-a răspuns: &lt;J&gt;„Oare puteți să-i faceți pe nuntași să țină post câtă vreme este mirele cu ei?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,35,'&lt;J&gt;Vor veni zile când mirele va fi luat de la ei, și vor posti atunci, în acele zile.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,36,'&lt;pb/&gt;Le-a spus&lt;f&gt;ⓧ&lt;/f&gt; și o pildă: &lt;J&gt;„Nimeni nu rupe un petic dintr-o haină nouă ca să-l pună la o haină veche; altminteri, și pe cea nouă o rupe și nici celei vechi nu i se va potrivi peticul din cea nouă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,37,'&lt;J&gt;Și nimeni nu pune vin nou în burdufuri vechi; altminteri, vinul cel nou sparge burdufurile, se varsă, iar burdufurile se pierd;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,38,'&lt;J&gt;ci vinul nou trebuie pus în burdufuri noi [și se păstrează amândouă].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,5,39,'&lt;J&gt;Și nimeni, după ce a băut &lt;i&gt;vin&lt;/i&gt; vechi, nu vrea &lt;i&gt;vin&lt;/i&gt; nou, căci spune: «Cel vechi este [mai] bun.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,1,'&lt;pb/&gt;S-a întâmplat că, într-o&lt;f&gt;ⓨ&lt;/f&gt; zi de sabat, &lt;i&gt;Isus&lt;/i&gt; trecea prin lanurile de grâu. Ucenicii Lui smulgeau spice, le frecau în palme și le mâncau.');
INSERT INTO &quot;verses&quot; VALUES (490,6,2,'Unii dintre farisei au zis: „De ce faceți ce&lt;f&gt;ⓩ&lt;/f&gt; nu este îngăduit în ziua sabatului?”');
INSERT INTO &quot;verses&quot; VALUES (490,6,3,'Isus le-a răspuns și le-a zis: &lt;J&gt;„Oare n-ați citit ce&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;a făcut David când el și cei ce erau împreună cu el au flămânzit?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,4,'&lt;J&gt;Cum a intrat în Casa lui Dumnezeu, a luat pâinile pentru înfățișare&lt;/J&gt;&lt;f&gt;[24]&lt;/f&gt; &lt;J&gt;— pe care&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;nu este îngăduit să le mănânce decât preoții — a mâncat din ele și le-a dat și celor ce erau cu el?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,5,'Și le zicea: &lt;J&gt;„Fiul Omului este Domn al sabatului.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,6,'&lt;pb/&gt;Într-o&lt;f&gt;ⓒ&lt;/f&gt; altă zi de sabat, &lt;i&gt;Isus&lt;/i&gt; a intrat în sinagogă și îi învăța &lt;i&gt;pe oameni&lt;/i&gt;. Acolo se afla un om a cărui mână dreaptă era uscată.');
INSERT INTO &quot;verses&quot; VALUES (490,6,7,'Cărturarii și fariseii Îl pândeau &lt;i&gt;pe Isus să vadă&lt;/i&gt; dacă va vindeca în ziua sabatului, ca să-I găsească &lt;i&gt;pricină&lt;/i&gt; de învinuire.');
INSERT INTO &quot;verses&quot; VALUES (490,6,8,'Dar El le știa gândurile; și i-a zis omului care avea mâna uscată: &lt;J&gt;„Ridică-te și stai în mijloc!”&lt;/J&gt; El s-a ridicat.');
INSERT INTO &quot;verses&quot; VALUES (490,6,9,'Și Isus le-a zis: &lt;J&gt;„Vă întreb: este îngăduit în ziua sabatului să faci bine sau să faci rău? Să scapi o viață sau să o pierzi?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,10,'&lt;i&gt;Atunci&lt;/i&gt; Și-a rotit privirile peste ei toți și i-a zis omului: &lt;J&gt;„Întinde-ți mâna!”&lt;/J&gt; El a făcut întocmai, și mâna s-a făcut sănătoasă [ca și cealaltă].');
INSERT INTO &quot;verses&quot; VALUES (490,6,11,'Ei s-au umplut de mânie și s-au sfătuit ce să-I facă lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (490,6,12,'&lt;pb/&gt;În&lt;f&gt;ⓓ&lt;/f&gt; zilele acelea, &lt;i&gt;Isus&lt;/i&gt; a mers pe munte să Se roage și a petrecut toată noaptea în rugăciune către Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,6,13,'Când s-a făcut ziuă, i-a chemat pe ucenicii Săi și&lt;f&gt;ⓔ&lt;/f&gt; a ales dintre ei doisprezece, pe care i-a numit apostoli:');
INSERT INTO &quot;verses&quot; VALUES (490,6,14,'Simon, pe&lt;f&gt;ⓕ&lt;/f&gt; care l-a numit și Petru, Andrei, fratele lui, Iacov, Ioan, Filip, Bartolomeu,');
INSERT INTO &quot;verses&quot; VALUES (490,6,15,'Matei, Toma, Iacov, &lt;i&gt;fiul&lt;/i&gt; lui Alfeu, Simon, numit Zelotul,');
INSERT INTO &quot;verses&quot; VALUES (490,6,16,'Iuda, &lt;i&gt;fiul&lt;/i&gt;&lt;f&gt;ⓖ&lt;/f&gt; lui Iacov, și Iuda Iscariot, care L-a și vândut.');
INSERT INTO &quot;verses&quot; VALUES (490,6,17,'&lt;pb/&gt;A coborât împreună cu ei și S-a oprit pe o câmpie, unde se afla o mare mulțime de ucenici de-ai Lui și&lt;f&gt;ⓗ&lt;/f&gt; mulți oameni din toată Iudeea, din Ierusalim și din ținutul de coastă al Tirului și Sidonului.');
INSERT INTO &quot;verses&quot; VALUES (490,6,18,'Ei veniseră să-L asculte și să fie vindecați de boli. Cei chinuiți de duhuri necurate erau vindecați');
INSERT INTO &quot;verses&quot; VALUES (490,6,19,'și toată mulțimea căuta&lt;f&gt;ⓘ&lt;/f&gt; să-L atingă, pentru că din El ieșea&lt;f&gt;ⓙ&lt;/f&gt; o putere care-i vindeca pe toți.');
INSERT INTO &quot;verses&quot; VALUES (490,6,20,'&lt;pb/&gt;Atunci, Isus Și-a ridicat ochii spre ucenicii Săi și a zis: &lt;pb/&gt;&lt;t&gt;&lt;J&gt;„Ferice&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;de &lt;i&gt;voi,&lt;/i&gt; care &lt;i&gt;sunteți&lt;/i&gt; săraci,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;căci a voastră este Împărăția lui Dumnezeu!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,21,'&lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;de &lt;i&gt;voi,&lt;/i&gt; care flămânziți acum,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;căci vă veți sătura!&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;de &lt;i&gt;voi,&lt;/i&gt; care plângeți acum,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;căci veți râde!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,22,'&lt;t&gt;&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;i&gt;&lt;J&gt;va fi&lt;/i&gt; de voi când oamenii vă vor urî, vă&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vor izgoni, vă vor batjocori și vor lepăda numele vostru ca pe ceva rău&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;din pricina Fiului Omului!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,23,'&lt;pb/&gt;&lt;J&gt;Bucurați-vă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;în ziua aceea și tresăltați de veselie, căci, iată, răsplata voastră este mare în cer; fiindcă tot așa&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;le făceau părinții lor și profeților!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,24,'&lt;pb/&gt;&lt;t&gt;&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;vai de voi, bogaților,&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;pentru că voi vă&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;primiți &lt;i&gt;acum&lt;/i&gt; mângâierea!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,25,'&lt;t&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;de voi, care vă îmbuibați acum,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;căci veți flămânzi!&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;[de voi], care râdeți acum,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;căci veți jeli și veți plânge!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,26,'&lt;t&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;de voi când toți oamenii vă vor vorbi de bine!&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;Căci tot așa le făceau și părinții lor profeților mincinoși!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,27,'&lt;pb/&gt;&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Eu vă spun vouă, care &lt;i&gt;Mă&lt;/i&gt; ascultați: iubiți-i pe vrăjmașii voștri, faceți-le bine celor ce vă urăsc,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,28,'&lt;J&gt;binecuvântați-i pe cei ce vă blestemă, rugați-vă&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;pentru cei ce se poartă rău cu voi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,29,'&lt;J&gt;Celui ce&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;te lovește peste un obraz dă-i-l și pe celălalt, iar&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;pe cel care-ți ia haina nu-l împiedica să-ți ia și cămașa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,30,'&lt;J&gt;Oricui&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;îți cere dă-i, și celui ce-ți ia cu sila ce este al tău nu i-l cere înapoi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,31,'&lt;pb/&gt;&lt;J&gt;Cum&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;vreți să vă facă vouă oamenii, tot așa faceți-le și voi lor!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,32,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;îi iubiți pe cei ce vă iubesc, ce merit aveți? Și păcătoșii îi iubesc pe cei care-i iubesc pe ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,33,'&lt;J&gt;Dacă le faceți bine celor ce vă fac bine, ce merit aveți? Și păcătoșii fac la fel.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,34,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;dacă le dați cu împrumut celor de la care așteptați să primiți înapoi, ce merit aveți? Și păcătoșii le dau cu împrumut păcătoșilor ca să ia înapoi tot atât.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,35,'&lt;J&gt;Voi însă iubiți-i&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;pe vrăjmașii voștri, faceți bine și dați&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;cu împrumut fără să așteptați ceva în schimb! Răsplata voastră va fi mare și veți&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;fi &lt;i&gt;astfel&lt;/i&gt; fiii Celui Preaînalt; căci El este bun și cu cei nerecunoscători și răi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,36,'&lt;pb/&gt;&lt;J&gt;Fiți [deci]&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;milostivi, după cum și Tatăl vostru este milostiv!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,37,'&lt;pb/&gt;&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;judecați, și nu veți fi judecați; nu osândiți, și nu veți fi osândiți; iertați, și vi se va ierta!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,38,'&lt;J&gt;Dați&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt;&lt;J&gt;, și vi se va da; ba încă vi se va turna în poală&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;o măsură bună, îndesată, clătinată, care va da pe dinafară. Căci cu&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;ce măsură măsurați, cu aceea vi se va măsura.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,39,'&lt;pb/&gt;Le-a spus și o pildă: &lt;J&gt;„Oare&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;poate un orb să-l călăuzească pe un alt orb? Nu vor cădea amândoi în groapă?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,40,'&lt;J&gt;Ucenicul nu&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;este mai presus de învățător, iar &lt;i&gt;ucenicul&lt;/i&gt; desăvârșit va fi ca învățătorul lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,41,'&lt;pb/&gt;&lt;J&gt;De&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;ce vezi tu paiul din ochiul fratelui tău, și nu te uiți cu băgare de seamă la bârna din ochiul tău?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,42,'&lt;J&gt;[Sau] cum poți să-i zici fratelui tău: «Frate, lasă-mă să scot paiul din ochiul tău» și, când colo, tu nu vezi bârna din ochiul tău? Fățarnicule, scoate&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;întâi bârna din ochiul tău, și atunci vei vedea deslușit să scoți paiul din ochiul fratelui tău!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,43,'&lt;pb/&gt;&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;există pom bun care să facă rod rău, nici pom rău care să facă rod bun.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,44,'&lt;J&gt;Orice&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;pom se cunoaște după rodul lui. Nu se adună smochine din mărăcini și nici struguri nu se culeg din spini.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,45,'&lt;J&gt;Omul&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;bun scoate ce este bun din vistieria bună a inimii lui, iar omul rău scoate ce este rău din [vistieria] rea [a inimii lui]. Căci din&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;prisosul inimii vorbește gura.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,46,'&lt;pb/&gt;&lt;J&gt;De&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;ce-Mi ziceți: «Doamne, Doamne!» și nu faceți ce spun Eu?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,47,'&lt;J&gt;Vă voi arăta cu cine se aseamănă orice &lt;i&gt;om&lt;/J&gt;&lt;/i&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;care vine la Mine, aude cuvintele Mele și le înfăptuiește:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,48,'&lt;J&gt;se aseamănă cu un om care, zidind o casă, a săpat adânc și a pus temelia pe stâncă. A venit un puhoi de ape și s-a năpustit șuvoiul peste casa aceea, dar n-a putut s-o clatine, pentru că a fost bine zidită.&lt;/J&gt;&lt;f&gt;[25]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,6,49,'&lt;J&gt;Dar cine aude și nu face se aseamănă cu un om care a zidit o casă pe pământ, fără temelie. S-a năpustit șuvoiul asupra ei, ea s-a surpat îndată, iar prăbușirea acelei case a fost mare.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,1,'&lt;pb/&gt;După ce a sfârșit de rostit toate &lt;i&gt;aceste&lt;/i&gt; cuvinte ale Sale în auzul poporului, Isus a&lt;f&gt;ⓧ&lt;/f&gt; intrat în Capernaum.');
INSERT INTO &quot;verses&quot; VALUES (490,7,2,'Robul unui centurion, la care &lt;i&gt;acesta&lt;/i&gt; ținea foarte mult, era bolnav, pe moarte.');
INSERT INTO &quot;verses&quot; VALUES (490,7,3,'Auzind despre Isus, &lt;i&gt;centurionul&lt;/i&gt; a trimis la El câțiva bătrâni ai iudeilor, ca să-L roage să vină și să-i vindece robul.');
INSERT INTO &quot;verses&quot; VALUES (490,7,4,'Aceștia au venit la Isus și L-au rugat stăruitor, zicând: „Merită să-i faci acest &lt;i&gt;bine&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (490,7,5,'căci iubește neamul nostru, iar sinagoga el ne-a zidit-o.”');
INSERT INTO &quot;verses&quot; VALUES (490,7,6,'Isus a plecat cu ei, dar, când a ajuns aproape de casă, centurionul a trimis la El &lt;i&gt;niște&lt;/i&gt; prieteni să-I spună: „Doamne, nu Te osteni, pentru că nu sunt vrednic să intri sub acoperișul meu!');
INSERT INTO &quot;verses&quot; VALUES (490,7,7,'De aceea nici nu m-am socotit vrednic să vin eu însumi la Tine. Ci spune un cuvânt, și slujitorul meu va fi vindecat.');
INSERT INTO &quot;verses&quot; VALUES (490,7,8,'Căci și eu sunt om &lt;i&gt;aflat&lt;/i&gt; sub autoritate și am sub mine soldați; iar &lt;i&gt;când&lt;/i&gt; îi zic unuia: «Du-te!», se duce, &lt;i&gt;când îi zic&lt;/i&gt; altuia: «Vino!», vine, și &lt;i&gt;când îi zic&lt;/i&gt; robului meu: «Fă asta!», face.”');
INSERT INTO &quot;verses&quot; VALUES (490,7,9,'La auzul acestor vorbe, Isus S-a mirat de el și, întorcându-Se către mulțimea care-L urma, a zis: &lt;J&gt;„Vă spun că nici în Israel n-am găsit o asemenea credință!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,10,'Când s-au întors acasă, cei trimiși l-au găsit sănătos pe robul [care fusese bolnav].');
INSERT INTO &quot;verses&quot; VALUES (490,7,11,'&lt;pb/&gt;După aceea, &lt;i&gt;Isus&lt;/i&gt; S-a dus într-o cetate numită Nain. Împreună cu El mergeau ucenicii Lui și o mulțime numeroasă.');
INSERT INTO &quot;verses&quot; VALUES (490,7,12,'Când S-a apropiat de poarta cetății, iată că duceau la groapă un mort, singurul fiu al mamei sale, care era văduvă; și împreună cu ea era o mulțime destul de mare din cetate.');
INSERT INTO &quot;verses&quot; VALUES (490,7,13,'Când a văzut-o, Domnului I s-a făcut milă de ea și i-a zis: &lt;J&gt;„Nu plânge!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,14,'Apoi S-a apropiat și S-a atins de raclă, iar cei care o duceau s-au oprit. El a zis: &lt;J&gt;„Tinere, scoală-te&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt;&lt;J&gt;, îți spun!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,15,'Mortul s-a ridicat în capul oaselor și a început să vorbească, iar &lt;i&gt;Isus&lt;/i&gt; l-a dat mamei lui.');
INSERT INTO &quot;verses&quot; VALUES (490,7,16,'Toți&lt;f&gt;ⓩ&lt;/f&gt; au fost cuprinși de frică și-L slăveau pe Dumnezeu, zicând: „Un mare profet s-a&lt;f&gt;ⓐ&lt;/f&gt; ridicat între noi” și „Dumnezeu&lt;f&gt;ⓑ&lt;/f&gt; a cercetat poporul Său.”');
INSERT INTO &quot;verses&quot; VALUES (490,7,17,'Cuvântul acesta despre El s-a răspândit în toată Iudeea și în tot ținutul dimprejur.');
INSERT INTO &quot;verses&quot; VALUES (490,7,18,'&lt;pb/&gt;Ucenicii lui Ioan&lt;f&gt;ⓒ&lt;/f&gt; l-au înștiințat despre toate acestea. Ioan i-a chemat pe doi dintre ei');
INSERT INTO &quot;verses&quot; VALUES (490,7,19,'și i-a trimis la Domnul să-I spună: „Tu ești Cel care vine sau să așteptăm pe altul?”');
INSERT INTO &quot;verses&quot; VALUES (490,7,20,'Când au ajuns la El, oamenii I-au zis: „Ioan Botezătorul ne-a trimis la Tine să-Ți spunem: «Tu ești Cel care vine sau să așteptăm pe altul?»”');
INSERT INTO &quot;verses&quot; VALUES (490,7,21,'Chiar în ceasul acela &lt;i&gt;Isus&lt;/i&gt; îi vindeca pe mulți de boli, de neputințe și de duhuri rele și multor orbi le dăruia vederea.');
INSERT INTO &quot;verses&quot; VALUES (490,7,22,'Și&lt;f&gt;ⓓ&lt;/f&gt; le-a răspuns zicând: &lt;J&gt;„Mergeți și spuneți-i lui Ioan ce vedeți și auziți:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;orbii își capătă vederea&lt;/e&gt;, &lt;e&gt;șchiopii umblă&lt;/e&gt;,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;leproșii sunt curățiți, &lt;e&gt;surzii aud&lt;/e&gt;,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;morții învie, iar &lt;e&gt;săracilor&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓔ&lt;/f&gt; &lt;e&gt;&lt;J&gt;li se aduc vești bune&lt;/e&gt;.&lt;/J&gt;&lt;f&gt;[26]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,23,'&lt;pb/&gt;&lt;J&gt;Ferice de cel care nu se poticnește din pricina Mea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,24,'&lt;pb/&gt;După ce&lt;f&gt;ⓕ&lt;/f&gt; au plecat trimișii lui Ioan, &lt;i&gt;Isus&lt;/i&gt; a început să le vorbească mulțimilor despre Ioan: &lt;J&gt;„Ce ați ieșit să vedeți în pustiu? O trestie clătinată de vânt?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,25,'&lt;J&gt;Atunci ce ați ieșit să vedeți? Un om îmbrăcat în haine moi? Iată că cei care &lt;i&gt;poartă&lt;/i&gt; haine scumpe și care trăiesc în desfătări sunt în casele regilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,26,'&lt;J&gt;Atunci ce ați ieșit să vedeți? Un profet? Da, vă spun, chiar mai mult decât un profet.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,27,'&lt;J&gt;El este acela despre care s-a scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Iată, trimit înaintea Feței Tale pe solul Meu,&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;care va pregăti calea înaintea Ta.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[27]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,28,'&lt;pb/&gt;&lt;J&gt;Vă spun că, dintre cei născuți din femei, nu este niciunul mai mare decât Ioan [Botezătorul]. Totuși cel mai mic în Împărăția lui Dumnezeu este mai mare decât el.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,29,'&lt;J&gt;Și toți oamenii, când au auzit, chiar și vameșii, au recunoscut dreptatea lui Dumnezeu, primind&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;botezul lui Ioan,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,30,'&lt;J&gt;dar fariseii și învățătorii Legii au respins planul&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;lui Dumnezeu pentru ei, neprimind botezul lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,31,'&lt;J&gt;Cu&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;cine-i voi asemăna așadar pe oamenii din generația acesta și cu cine se aseamănă ei?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,32,'&lt;J&gt;Sunt asemenea unor copii care stau în piață și strigă unii către alții:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;«V-am cântat din fluier, și n-ați jucat;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;v-am cântat de jale, și n-ați plâns.»&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,33,'&lt;pb/&gt;&lt;J&gt;Căci a venit Ioan&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;Botezătorul, nemâncând pâine și nebând vin, iar voi ziceți: «Are demon!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,34,'&lt;J&gt;A venit Fiul Omului mâncând și bând, iar voi ziceți: «Iată un mâncău și un bețiv, prieten al vameșilor și păcătoșilor.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,35,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;înțelepciunea a fost găsită dreaptă de toți copiii ei.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,36,'&lt;pb/&gt;Unul dintre&lt;f&gt;ⓛ&lt;/f&gt; farisei L-a poftit să mănânce &lt;i&gt;acasă&lt;/i&gt; la el. &lt;i&gt;Isus&lt;/i&gt; a intrat în casa fariseului și S-a așezat la masă.');
INSERT INTO &quot;verses&quot; VALUES (490,7,37,'Și iată că o femeie din cetate, o păcătoasă, aflând că era la masă în casa fariseului, a adus un vas de alabastru cu mir.');
INSERT INTO &quot;verses&quot; VALUES (490,7,38,'Și, cum stătea în spate, lângă picioarele lui &lt;i&gt;Isus&lt;/i&gt;, și plângea, a început să-I stropească picioarele cu lacrimi și să I le șteargă cu părul capului ei; Îi săruta picioarele și le ungea cu mir.');
INSERT INTO &quot;verses&quot; VALUES (490,7,39,'Când a văzut aceasta, fariseul care-L chemase a zis în sinea lui: „Acesta&lt;f&gt;ⓜ&lt;/f&gt;, dacă ar fi profet, ar ști cine și ce fel de femeie este cea care-L atinge, &lt;i&gt;ar ști&lt;/i&gt; că este o păcătoasă!”');
INSERT INTO &quot;verses&quot; VALUES (490,7,40,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Simone, am ceva să-ți spun!”&lt;/J&gt; „Spune, Învățătorule”, I-a zis el.');
INSERT INTO &quot;verses&quot; VALUES (490,7,41,'&lt;J&gt;„Un cămătar&lt;/J&gt;&lt;f&gt;[28]&lt;/f&gt; &lt;J&gt;avea doi datornici: unul îi era dator cu cinci sute de dinari&lt;/J&gt;&lt;f&gt;[29]&lt;/f&gt;&lt;J&gt;, iar celălalt cu cincizeci.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,42,'&lt;J&gt;Fiindcă n-aveau cu ce plăti, i-a iertat pe amândoi. [Spune,] care dintre ei îl va iubi mai mult?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,43,'Simon I-a răspuns: „Socotesc că acela căruia i-a iertat mai mult.” Iar El i-a zis: &lt;J&gt;„Drept ai judecat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,44,'Apoi S-a întors spre femeie și i-a zis lui Simon: &lt;J&gt;„O vezi pe femeia aceasta? Am intrat în casa ta și nu Mi-ai dat apă pentru picioare, dar ea Mi-a stropit picioarele cu lacrimi și Mi le-a șters cu părul [capului] ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,45,'&lt;J&gt;Sărutare nu Mi-ai dat, dar ea, de când am intrat, n-a încetat să-Mi sărute picioarele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,46,'&lt;J&gt;Capul nu Mi l-ai uns cu untdelemn&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;, dar ea Mi-a uns picioarele cu mir.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,47,'&lt;J&gt;De aceea&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;îți spun: păcatele ei cele multe sunt iertate, căci a iubit mult. Dar cui i se iartă puțin, iubește puțin.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,48,'Apoi i-a zis &lt;i&gt;femeii&lt;/i&gt;: &lt;J&gt;„Păcatele&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;îți sunt iertate!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,7,49,'Iar comesenii au început să zică în sinea lor: „Cine&lt;f&gt;ⓠ&lt;/f&gt; este acesta care iartă și păcatele?”');
INSERT INTO &quot;verses&quot; VALUES (490,7,50,'Dar &lt;i&gt;Isus&lt;/i&gt; i-a zis femeii: &lt;J&gt;„Credința&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;ta te-a mântuit; mergi în pace!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,1,'&lt;pb/&gt;Curând după aceea, &lt;i&gt;Isus&lt;/i&gt; umbla din cetate în cetate și din sat în sat propovăduind și ducând vestea bună a Împărăției lui Dumnezeu. Cu El erau cei doisprezece');
INSERT INTO &quot;verses&quot; VALUES (490,8,2,'și niște&lt;f&gt;ⓢ&lt;/f&gt; femei care fuseseră vindecate de duhuri rele și de boli: Maria, numită Magdalena, din&lt;f&gt;ⓣ&lt;/f&gt; care ieșiseră șapte demoni,');
INSERT INTO &quot;verses&quot; VALUES (490,8,3,'Ioana, soția lui Huza, administrator al lui Irod, Susana și multe altele, care le slujeau cu ce aveau.');
INSERT INTO &quot;verses&quot; VALUES (490,8,4,'&lt;pb/&gt;Când&lt;f&gt;ⓤ&lt;/f&gt; s-a strâns o mare mulțime și au venit la El &lt;i&gt;oameni&lt;/i&gt; de prin cetăți, &lt;i&gt;Isus&lt;/i&gt; a spus o pildă:');
INSERT INTO &quot;verses&quot; VALUES (490,8,5,'&lt;J&gt;„Semănătorul a ieșit să-și semene sămânța. Pe când semăna el, o parte din sămânță a căzut lângă drum: a fost călcată în picioare și păsările cerului au mâncat-o.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,6,'&lt;J&gt;O altă parte a căzut pe piatră și, după ce a răsărit, s-a uscat, pentru că nu avea umezeală.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,7,'&lt;J&gt;O altă parte a căzut în mijlocul spinilor: spinii au crescut odată cu ea și au înăbușit-o.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,8,'&lt;J&gt;O altă parte a căzut pe pământ bun, a crescut și a făcut rod însutit.”&lt;/J&gt; După ce a spus acestea, &lt;i&gt;Isus&lt;/i&gt; a strigat: &lt;J&gt;„Cine are urechi de auzit să audă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,9,'&lt;pb/&gt;Ucenicii&lt;f&gt;ⓥ&lt;/f&gt; Lui L-au întrebat ce înseamnă pilda aceasta.');
INSERT INTO &quot;verses&quot; VALUES (490,8,10,'El le-a zis: &lt;J&gt;„Vouă vi s-a dat să cunoașteți tainele Împărăției lui Dumnezeu, dar celorlalți &lt;i&gt;li se vorbește&lt;/i&gt; în pilde, pentru ca,&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;deși privesc, să nu vadă&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;și, deși aud, să nu înțeleagă.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[30]&lt;/f&gt; &lt;J&gt;”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,11,'&lt;pb/&gt;&lt;J&gt;Acesta este &lt;i&gt;înțelesul&lt;/i&gt; pildei&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;: „Sămânța este Cuvântul lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,12,'&lt;J&gt;Cei de lângă drum sunt cei care au auzit; apoi vine Diavolul și ia Cuvântul din inima lor, ca nu cumva, crezând, să fie mântuiți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,13,'&lt;J&gt;Cei de pe piatră sunt aceia care, atunci când aud, primesc Cuvântul cu bucurie; dar n-au rădăcină, ci cred până la o vreme, iar la vremea încercării cad.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,14,'&lt;i&gt;&lt;J&gt;Sămânța&lt;/i&gt; căzută între spini sunt cei care au auzit, dar sunt înăbușiți pe drum de griji, bogăție și de plăcerile vieții acesteia și nu ajung până la coacere.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,15,'&lt;J&gt;Iar &lt;i&gt;sămânța&lt;/i&gt; ce &lt;i&gt;a căzut&lt;/i&gt; pe pământ bun sunt aceia care, după ce au auzit Cuvântul cu o inimă bună și curată, îl păstrează și aduc rod în răbdare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,16,'&lt;pb/&gt;&lt;J&gt;Nimeni&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;, după ce a aprins o lampă, n-o acoperă cu un vas, nici nu o pune sub pat, ci o pune într-un sfeșnic, pentru ca cei care intră să vadă lumina.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,17,'&lt;J&gt;Fiindcă&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;nu este nimic acoperit care nu va fi descoperit, nimic tăinuit care nu va fi cunoscut și nu va ieși la iveală.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,18,'&lt;J&gt;Luați aminte, așadar, cum ascultați; căci&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;celui ce are i se va da, dar celui ce nu are i se va lua și ce i se pare că are.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,19,'&lt;pb/&gt;Au venit la El mama&lt;f&gt;ⓐ&lt;/f&gt; și frații Lui, dar nu puteau să ajungă la El din pricina mulțimii.');
INSERT INTO &quot;verses&quot; VALUES (490,8,20,'Cineva I-a spus: „Mama Ta și frații Tăi stau afară și vor să Te vadă.”');
INSERT INTO &quot;verses&quot; VALUES (490,8,21,'Însă El a răspuns: &lt;J&gt;„Mama Mea și frații Mei sunt cei ce ascultă Cuvântul lui Dumnezeu și-l împlinesc.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,22,'&lt;pb/&gt;Într-o&lt;f&gt;ⓑ&lt;/f&gt; zi, Isus S-a urcat într-o barcă împreună cu ucenicii Lui și le-a zis: &lt;J&gt;„Să trecem de cealaltă parte a lacului.”&lt;/J&gt; Și au ieșit în larg.');
INSERT INTO &quot;verses&quot; VALUES (490,8,23,'Pe când vâsleau ei, &lt;i&gt;Isus&lt;/i&gt; a adormit. Pe lac s-a iscat o furtună cu vânt &lt;i&gt;puternic&lt;/i&gt;, au început să ia apă și erau în primejdie.');
INSERT INTO &quot;verses&quot; VALUES (490,8,24,'Au&lt;f&gt;ⓒ&lt;/f&gt; venit la El, L-au trezit și I-au zis: „Stăpâne, Stăpâne, pierim!” &lt;i&gt;Isus&lt;/i&gt; S-a trezit și a certat vântul și valurile, care s-au potolit; și s-a făcut liniște.');
INSERT INTO &quot;verses&quot; VALUES (490,8,25,'Apoi le-a zis &lt;i&gt;ucenicilor&lt;/i&gt;: &lt;J&gt;„Unde vă este credința?”&lt;/J&gt; Plini de spaimă, ei s-au minunat și ziceau unii către alții: „Cine este oare Acesta, care poruncește și vânturilor, și apei, iar &lt;i&gt;ele&lt;/i&gt; I se supun?”');
INSERT INTO &quot;verses&quot; VALUES (490,8,26,'&lt;pb/&gt;Au tras la țărm în ținutul gherasenilor&lt;f&gt;[31]&lt;/f&gt;, care este în fața Galileei.');
INSERT INTO &quot;verses&quot; VALUES (490,8,27,'Când a coborât &lt;i&gt;Isus&lt;/i&gt; pe țărm, L-a întâmpinat un om din cetate care avea demoni. De multă vreme nu purta haine și nu locuia într-o casă, ci în morminte.');
INSERT INTO &quot;verses&quot; VALUES (490,8,28,'Când L-a văzut pe Isus, a căzut înaintea Lui urlând și a strigat tare: „Ce am eu a face cu Tine, Isuse, Fiul Dumnezeului Preaînalt? Te rog să nu mă chinuiești!”');
INSERT INTO &quot;verses&quot; VALUES (490,8,29,'Căci &lt;i&gt;Isus&lt;/i&gt; îi poruncise duhului necurat să iasă din om. (Pentru că de multe ori &lt;i&gt;duhul&lt;/i&gt; pusese stăpânire pe el și, ca să-l păzească, îl legau cu lanțuri și cu obezi, dar el rupea legăturile și era gonit de demon în locuri pustii.)');
INSERT INTO &quot;verses&quot; VALUES (490,8,30,'Isus l-a întrebat: &lt;J&gt;„Care-ți este numele?”&lt;/J&gt; „Legiune&lt;f&gt;[32]&lt;/f&gt;”, a răspuns el; pentru că intraseră mulți demoni în el.');
INSERT INTO &quot;verses&quot; VALUES (490,8,31,'Și &lt;i&gt;demonii&lt;/i&gt; Îl rugau stăruitor &lt;i&gt;pe Isus&lt;/i&gt; să nu le poruncească să plece în&lt;f&gt;ⓓ&lt;/f&gt; Adânc.');
INSERT INTO &quot;verses&quot; VALUES (490,8,32,'&lt;pb/&gt;Acolo, pe munte, era o turmă mare de porci care pășteau. Și &lt;i&gt;demonii&lt;/i&gt; L-au rugat să le dea voie să intre în ei. Și El le-a dat voie.');
INSERT INTO &quot;verses&quot; VALUES (490,8,33,'Demonii au ieșit din om, au intrat în porci, și turma s-a repezit de pe stâncă în lac și s-a înecat.');
INSERT INTO &quot;verses&quot; VALUES (490,8,34,'&lt;pb/&gt;Când au văzut cele întâmplate, porcarii au fugit și au dat de veste în cetate și prin sate.');
INSERT INTO &quot;verses&quot; VALUES (490,8,35,'&lt;i&gt;Oamenii&lt;/i&gt; au ieșit să vadă cele întâmplate. Au venit la Isus și l-au găsit pe omul din care ieșiseră demonii șezând la picioarele Lui, îmbrăcat și sănătos la minte; și s-au înspăimântat.');
INSERT INTO &quot;verses&quot; VALUES (490,8,36,'Cei care văzuseră le-au povestit cum fusese vindecat demonizatul.');
INSERT INTO &quot;verses&quot; VALUES (490,8,37,'Toată&lt;f&gt;ⓔ&lt;/f&gt; mulțimea din ținutul gherasenilor L-a&lt;f&gt;ⓕ&lt;/f&gt; rugat &lt;i&gt;pe Isus&lt;/i&gt; să plece de la ei, pentru că îi apucase o mare frică. Iar El S-a urcat în barcă și S-a întors.');
INSERT INTO &quot;verses&quot; VALUES (490,8,38,'Omul&lt;f&gt;ⓖ&lt;/f&gt; din care ieșiseră demonii Îl ruga să-i dea voie să rămână cu El. Dar [Isus] i-a dat drumul, zicând:');
INSERT INTO &quot;verses&quot; VALUES (490,8,39,'&lt;J&gt;„Întoarce-te acasă și povestește tot ce a făcut Dumnezeu pentru tine!”&lt;/J&gt; El a plecat și a vestit în toată cetatea tot ce făcuse Isus pentru el.');
INSERT INTO &quot;verses&quot; VALUES (490,8,40,'&lt;pb/&gt;Când S-a întors Isus, mulțimea L-a primit cu bucurie, căci toți Îl așteptau.');
INSERT INTO &quot;verses&quot; VALUES (490,8,41,'Și&lt;f&gt;ⓗ&lt;/f&gt; iată că a venit un om pe nume Iair, care era un conducător al sinagogii. El a căzut la picioarele lui Isus și L-a rugat să vină în casa lui,');
INSERT INTO &quot;verses&quot; VALUES (490,8,42,'pentru că avea o singură fiică, de vreo doisprezece ani, care trăgea să moară. &lt;pb/&gt;Pe când se ducea El &lt;i&gt;într-acolo&lt;/i&gt;, mulțimile &lt;i&gt;au început&lt;/i&gt; să-L îmbulzească.');
INSERT INTO &quot;verses&quot; VALUES (490,8,43,'Și&lt;f&gt;ⓘ&lt;/f&gt; era o femeie care de doisprezece ani avea o scurgere de sânge și care își cheltuise tot ce avea cu doctorii fără să o fi putut vindeca vreunul.');
INSERT INTO &quot;verses&quot; VALUES (490,8,44,'Ea s-a apropiat din spate și I-a atins poala hainei. Îndată, scurgerea de sânge s-a oprit.');
INSERT INTO &quot;verses&quot; VALUES (490,8,45,'Și Isus a zis: &lt;J&gt;„Cine s-a atins de Mine?”&lt;/J&gt; Fiindcă toți tăgăduiau, Petru [și cei ce erau cu El] au zis: „Învățătorule, mulțimile Te îmbulzesc și Te împresoară, [și mai întrebi: &lt;J&gt;«Cine s-a atins de Mine?»&lt;/J&gt;”]');
INSERT INTO &quot;verses&quot; VALUES (490,8,46,'Dar Isus a răspuns: &lt;J&gt;„S-a atins cineva de Mine, căci am simțit că a ieșit din Mine o putere&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,47,'Când a văzut femeia că nu se poate ascunde, a venit tremurând, a căzut la picioarele Lui și a povestit în fața întregului popor din ce pricină se atinsese de El și cum fusese vindecată pe loc.');
INSERT INTO &quot;verses&quot; VALUES (490,8,48,'Dar Isus i-a zis: &lt;J&gt;„[Îndrăznește,] fiică! Credința ta te-a mântuit. Mergi în pace!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,49,'&lt;pb/&gt;Pe&lt;f&gt;ⓚ&lt;/f&gt; când vorbea El, a venit cineva din casa mai-marelui sinagogii și i-a spus &lt;i&gt;lui Iair&lt;/i&gt;: „Fiica ta a murit; nu-L mai necăji pe Învățător!”');
INSERT INTO &quot;verses&quot; VALUES (490,8,50,'Dar Isus, când a auzit, i-a zis: &lt;J&gt;„Nu te teme; crede numai, și va fi vindecată!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,51,'Când a ajuns acasă, n-a lăsat pe nimeni să intre cu El, în afară de Petru, Ioan, Iacov, tatăl fetei și mama ei.');
INSERT INTO &quot;verses&quot; VALUES (490,8,52,'Toți plângeau și o boceau. Atunci, Isus a zis: &lt;J&gt;„Nu plângeți, căci &lt;i&gt;fetița&lt;/i&gt; n-a murit, ci&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;doarme!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,53,'Ei însă Îl luau în râs, căci știau că murise.');
INSERT INTO &quot;verses&quot; VALUES (490,8,54,'Dar El, [după ce i-a scos pe toți afară,] a apucat-o de mână și a strigat-o, zicând: &lt;J&gt;„Fetițo, scoală-te&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt;&lt;J&gt;!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,8,55,'Și duhul ei s-a întors, iar ea s-a sculat îndată. &lt;i&gt;Isus&lt;/i&gt; a poruncit să-i dea &lt;i&gt;fetiței&lt;/i&gt; să mănânce.');
INSERT INTO &quot;verses&quot; VALUES (490,8,56,'Părinții ei au rămas uimiți. Dar El&lt;f&gt;ⓝ&lt;/f&gt; le-a poruncit să nu spună nimănui cele întâmplate.');
INSERT INTO &quot;verses&quot; VALUES (490,9,1,'&lt;pb/&gt;Chemându-i pe cei doisprezece, &lt;i&gt;Isus&lt;/i&gt;&lt;f&gt;ⓞ&lt;/f&gt; le-a dat autoritate peste toți demonii și putere să vindece boli.');
INSERT INTO &quot;verses&quot; VALUES (490,9,2,'I-a&lt;f&gt;ⓟ&lt;/f&gt; trimis să propovăduiască Împărăția lui Dumnezeu și să-i vindece pe cei bolnavi.');
INSERT INTO &quot;verses&quot; VALUES (490,9,3,'Și le-a&lt;f&gt;ⓠ&lt;/f&gt; zis: &lt;J&gt;„Să nu luați nimic &lt;i&gt;cu voi&lt;/i&gt; pentru drum: nici toiag, nici traistă, nici pâine, nici bani; și nici două haine să nu vă luați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,4,'&lt;J&gt;În&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;orice casă veți intra, să rămâneți acolo până veți pleca din &lt;i&gt;locul&lt;/i&gt; acela.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,5,'&lt;J&gt;Iar&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;dacă nu vă vor primi, să ieșiți din cetatea aceea și să scuturați&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;praful de pe picioarele voastre, ca mărturie împotriva lor.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,6,'Ei&lt;f&gt;ⓤ&lt;/f&gt; au plecat și au mers din sat în sat, propovăduind și făcând vindecări pretutindeni.');
INSERT INTO &quot;verses&quot; VALUES (490,9,7,'&lt;pb/&gt;Tetrarhul&lt;f&gt;ⓥ&lt;/f&gt; Irod a auzit despre toate cele săvârșite [de El] și era nedumerit de ceea ce se spunea: după unii, Ioan ar fi înviat din morți;');
INSERT INTO &quot;verses&quot; VALUES (490,9,8,'după alții, se arătase Ilie; iar după alții, înviase unul dintre profeții din vechime.');
INSERT INTO &quot;verses&quot; VALUES (490,9,9,'Dar Irod zicea: „Lui Ioan i-am tăiat capul; cine este cel despre care aud toate acestea?” Și&lt;f&gt;ⓦ&lt;/f&gt; căuta să-L vadă.');
INSERT INTO &quot;verses&quot; VALUES (490,9,10,'&lt;pb/&gt;După ce&lt;f&gt;ⓧ&lt;/f&gt; s-au întors, apostolii I-au povestit &lt;i&gt;lui Isus&lt;/i&gt; tot ce făcuseră. El&lt;f&gt;ⓨ&lt;/f&gt; i-a luat și S-a retras deoparte, în cetatea numită Betsaida.');
INSERT INTO &quot;verses&quot; VALUES (490,9,11,'Mulțimile, când au aflat, au mers după El. &lt;i&gt;Isus&lt;/i&gt; le-a primit, le-a vorbit despre Împărăția lui Dumnezeu și i-a vindecat pe cei ce aveau nevoie de vindecare.');
INSERT INTO &quot;verses&quot; VALUES (490,9,12,'Fiindcă&lt;f&gt;ⓩ&lt;/f&gt; ziua era pe sfârșite, cei doisprezece s-au apropiat și I-au zis: „Dă drumul mulțimilor, să se ducă în satele și cătunele din împrejurimi să găzduiască și să-și caute de mâncare, pentru că aici suntem într-un loc pustiu!”');
INSERT INTO &quot;verses&quot; VALUES (490,9,13,'&lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Dați-le voi să mănânce!”&lt;/J&gt; Dar ei I-au răspuns: „Noi nu avem decât cinci pâini și doi pești; doar dacă ne-am duce noi să cumpărăm mâncare pentru tot poporul acesta!”&lt;f&gt;[33]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,14,'Erau cam cinci mii de bărbați. &lt;i&gt;Isus&lt;/i&gt; le-a zis ucenicilor Săi: &lt;J&gt;„Puneți-i să se așeze în grupuri de câte cincizeci!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,15,'Au făcut întocmai: i-au pus să se așeze pe toți.');
INSERT INTO &quot;verses&quot; VALUES (490,9,16,'&lt;i&gt;Isus&lt;/i&gt; a luat cele cinci pâini și cei doi pești, Și-a ridicat ochii spre cer, a rostit binecuvântarea, apoi le-a frânt și le-a dat ucenicilor să le împartă mulțimilor.');
INSERT INTO &quot;verses&quot; VALUES (490,9,17,'Au mâncat și s-au săturat toți, și au ridicat douăsprezece coșuri pline cu bucățile rămase.');
INSERT INTO &quot;verses&quot; VALUES (490,9,18,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; Se ruga&lt;f&gt;ⓐ&lt;/f&gt; singur deoparte, iar ucenicii erau cu El; și i-a întrebat, zicând: &lt;J&gt;„Cine zic mulțimile că sunt Eu?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,19,'Ei I-au răspuns: „&lt;i&gt;Unii&lt;/i&gt;, Ioan&lt;f&gt;ⓑ&lt;/f&gt; Botezătorul; alții, Ilie; alții — că a înviat un profet dintre cei din vechime.”');
INSERT INTO &quot;verses&quot; VALUES (490,9,20,'&lt;J&gt;„Dar voi”&lt;/J&gt;, i-a întrebat El, &lt;J&gt;„cine ziceți că sunt?”&lt;/J&gt; „Hristosul lui Dumnezeu!”, I-a răspuns Petru&lt;f&gt;ⓒ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,9,21,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; &lt;f&gt;ⓓ&lt;/f&gt; le-a poruncit cu asprime să nu spună nimănui lucrul acesta,');
INSERT INTO &quot;verses&quot; VALUES (490,9,22,'căci le zisese că Fiul&lt;f&gt;ⓔ&lt;/f&gt; Omului trebuie să pătimească multe, să fie respins de bătrâni, de preoții de seamă și de cărturari, să fie omorât și a treia zi să învie.');
INSERT INTO &quot;verses&quot; VALUES (490,9,23,'&lt;pb/&gt;Apoi&lt;f&gt;ⓕ&lt;/f&gt; le-a zis tuturor: &lt;J&gt;„Dacă vrea cineva să vină după Mine, să se lepede de sine, să-și ia crucea în fiecare zi și să Mă urmeze.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,24,'&lt;J&gt;Căci oricine va vrea să-și scape viața o va pierde, dar oricine își va pierde viața pentru Mine o va scăpa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,25,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;ce ar folosi unui om să câștige toată lumea dacă s-ar nimici ori s-ar pierde pe sine?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,26,'&lt;J&gt;Pentru că&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;dacă se va rușina cineva de Mine și de cuvintele Mele, se va rușina și Fiul Omului de el când va veni în slava Sa și a Tatălui și a sfinților îngeri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,27,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;vă spun că sunt unii dintre cei ce stau aici care nu vor gusta moartea până nu vor vedea Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,28,'&lt;pb/&gt;Cam&lt;f&gt;ⓙ&lt;/f&gt; la opt zile după cuvintele acestea, &lt;i&gt;Isus&lt;/i&gt; i-a luat cu El pe Petru, pe Ioan și pe Iacov și a urcat pe munte să Se roage.');
INSERT INTO &quot;verses&quot; VALUES (490,9,29,'Pe când Se ruga, I s-a schimbat înfățișarea chipului, iar îmbrăcămintea I &lt;i&gt;s-a făcut&lt;/i&gt; de un alb strălucitor.');
INSERT INTO &quot;verses&quot; VALUES (490,9,30,'Și iată că doi bărbați vorbeau cu El: erau Moise și Ilie,');
INSERT INTO &quot;verses&quot; VALUES (490,9,31,'care se arătaseră în slavă și vorbeau despre plecarea&lt;f&gt;[34]&lt;/f&gt; Lui, ce avea să se împlinească în Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (490,9,32,'Petru și cei ce erau cu el erau doborâți&lt;f&gt;ⓚ&lt;/f&gt; de somn. Când s-au trezit, au văzut slava lui &lt;i&gt;Isus&lt;/i&gt; și pe cei doi bărbați care erau cu El.');
INSERT INTO &quot;verses&quot; VALUES (490,9,33,'Când să se despartă &lt;i&gt;aceștia&lt;/i&gt; de El, Petru I-a zis lui Isus: „Stăpâne, este bine să fim aici; să facem trei colibe: una pentru Tine, una pentru Moise și una pentru Ilie.” Nu știa ce spune.');
INSERT INTO &quot;verses&quot; VALUES (490,9,34,'Pe când spunea el aceste &lt;i&gt;cuvinte&lt;/i&gt;, a venit un nor și i-a acoperit cu umbra lui; &lt;i&gt;ucenicii&lt;/i&gt; s-au înspăimântat când au intrat ei în nor.');
INSERT INTO &quot;verses&quot; VALUES (490,9,35,'Și din nor s-a auzit un glas care zicea: „Acesta&lt;f&gt;ⓛ&lt;/f&gt; este Fiul Meu cel ales&lt;f&gt;[35]&lt;/f&gt;: de El să ascultați&lt;f&gt;ⓜ&lt;/f&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (490,9,36,'Când s-a auzit glasul acela, Isus a rămas singur. &lt;i&gt;Ucenicii&lt;/i&gt;&lt;f&gt;ⓝ&lt;/f&gt; au tăcut și, în acele zile, n-au spus nimănui nimic despre cele ce văzuseră.');
INSERT INTO &quot;verses&quot; VALUES (490,9,37,'&lt;pb/&gt;A&lt;f&gt;ⓞ&lt;/f&gt; doua zi, când au coborât de pe munte, o mulțime mare L-a întâmpinat &lt;i&gt;pe Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,9,38,'Și iată că un om din mulțime a strigat: „Învățătorule, Te rog, uită-Te la fiul meu, căci este singurul meu copil!');
INSERT INTO &quot;verses&quot; VALUES (490,9,39,'Îl apucă un duh și, deodată, &lt;i&gt;băiatul&lt;/i&gt; răcnește, se zvârcolește cu spume &lt;i&gt;la gură&lt;/i&gt;, și &lt;i&gt;demonul&lt;/i&gt; cu greu pleacă de la el, lăsându-l zdrobit.');
INSERT INTO &quot;verses&quot; VALUES (490,9,40,'I-am rugat pe ucenicii Tăi să scoată &lt;i&gt;demonul din&lt;/i&gt; el, dar n-au putut.”');
INSERT INTO &quot;verses&quot; VALUES (490,9,41,'Isus a răspuns: &lt;J&gt;„O, generație necredincioasă și pornită la rău, până când voi mai fi cu voi și vă voi răbda? Adu-l aici pe fiul tău!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,42,'Chiar când venea &lt;i&gt;băiatul&lt;/i&gt;, demonul l-a trântit și l-a zguduit. Dar Isus a mustrat duhul necurat, l-a vindecat pe băiat și l-a dat înapoi tatălui său.');
INSERT INTO &quot;verses&quot; VALUES (490,9,43,'Toți au rămas uimiți de măreția lui Dumnezeu. &lt;pb/&gt;Pe când toți se mirau de tot ce făcea &lt;i&gt;Isus&lt;/i&gt;, El le-a zis ucenicilor Săi:');
INSERT INTO &quot;verses&quot; VALUES (490,9,44,'&lt;J&gt;„Luați bine aminte&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;la cuvintele acestea: Fiul Omului va fi dat în mâinile oamenilor!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,45,'Dar&lt;f&gt;ⓠ&lt;/f&gt; ei nu înțelegeau cuvântul acesta, căci era ascuns pentru ei, ca să nu-l priceapă; și se temeau să-I pună întrebări cu privire la ceea ce spusese.');
INSERT INTO &quot;verses&quot; VALUES (490,9,46,'&lt;pb/&gt;Apoi&lt;f&gt;ⓡ&lt;/f&gt; le-a venit gândul să știe care dintre ei ar fi mai mare.');
INSERT INTO &quot;verses&quot; VALUES (490,9,47,'Isus, care le-a cunoscut gândul inimii, a luat un copilaș, l-a pus să stea lângă El');
INSERT INTO &quot;verses&quot; VALUES (490,9,48,'și le-a zis: &lt;J&gt;„Oricine&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;îl primește pe acest copilaș în Numele Meu pe Mine Mă primește; și oricine Mă primește pe Mine Îl primește pe Cel ce M-a trimis. Fiindcă&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;cel care este mai mic între voi toți, acela este mare.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,49,'&lt;pb/&gt;Ioan a răspuns&lt;f&gt;ⓤ&lt;/f&gt;: „Stăpâne, noi am văzut pe cineva scoțând demoni în Numele Tău și l-am oprit, pentru că nu &lt;i&gt;Te&lt;/i&gt; urmează împreună cu noi.”');
INSERT INTO &quot;verses&quot; VALUES (490,9,50,'Isus i-a răspuns: &lt;J&gt;„Nu-l opriți, fiindcă cine&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;nu este împotriva voastră este pentru voi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,51,'&lt;pb/&gt;Când s-a împlinit vremea înălțării Sale&lt;f&gt;ⓦ&lt;/f&gt;, Isus S-a îndreptat cu hotărâre&lt;f&gt;[36]&lt;/f&gt; spre Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (490,9,52,'A trimis soli înaintea Sa, care au mers și au intrat într-un sat al samaritenilor, ca să facă pregătiri pentru El.');
INSERT INTO &quot;verses&quot; VALUES (490,9,53,'Dar&lt;f&gt;ⓧ&lt;/f&gt; aceștia nu L-au primit, pentru că Se îndrepta spre Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (490,9,54,'Văzând aceasta, Iacov și Ioan, ucenicii Săi, au zis: „Doamne, vrei să chemăm &lt;e&gt;să se coboare foc din cer și să-i mistuie&lt;/e&gt;&lt;f&gt;[37]&lt;/f&gt;, [cum a făcut Ilie]?”');
INSERT INTO &quot;verses&quot; VALUES (490,9,55,'&lt;i&gt;Isus&lt;/i&gt; S-a întors spre ei și i-a mustrat. [El a zis: &lt;J&gt;„Nu știți de ce duh sunteți mânați!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,56,'&lt;J&gt;Căci Fiul&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;Omului n-a venit să nimicească sufletele oamenilor, ci să le mântuiască.”&lt;/J&gt;] Și au plecat în alt sat.');
INSERT INTO &quot;verses&quot; VALUES (490,9,57,'&lt;pb/&gt;Pe&lt;f&gt;ⓩ&lt;/f&gt; când erau pe drum, cineva I-a zis: „[Doamne,] Te voi urma oriunde vei merge.”');
INSERT INTO &quot;verses&quot; VALUES (490,9,58,'Isus i-a răspuns: &lt;J&gt;„Vulpile au vizuini și păsările cerului au cuiburi, dar Fiul Omului n-are unde să-Și plece capul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,59,'A zis apoi către altul&lt;f&gt;ⓐ&lt;/f&gt;: &lt;J&gt;„Urmează-mă!”&lt;/J&gt; Acesta I-a răspuns: „Doamne, îngăduie-mi să mă duc mai întâi să-l îngrop pe tatăl meu!”');
INSERT INTO &quot;verses&quot; VALUES (490,9,60,'Dar &lt;i&gt;Isus&lt;/i&gt; i-a zis: &lt;J&gt;„Lasă morții să-și îngroape morții, iar tu mergi și vestește Împărăția lui Dumnezeu!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,9,61,'Altul a zis: „Doamne, Te&lt;f&gt;ⓑ&lt;/f&gt; voi urma, dar îngăduie-mi mai întâi să-mi iau rămas-bun de la cei din casa mea!”');
INSERT INTO &quot;verses&quot; VALUES (490,9,62,'Isus i-a răspuns: &lt;J&gt;„Oricine pune mâna pe plug și privește înapoi nu este potrivit pentru Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,1,'&lt;pb/&gt;După aceea, Domnul a mai ales șaptezeci&lt;f&gt;[38]&lt;/f&gt; și i-a trimis&lt;f&gt;ⓒ&lt;/f&gt; doi câte doi înaintea Lui, în toate cetățile și în toate locurile pe unde avea El să treacă.');
INSERT INTO &quot;verses&quot; VALUES (490,10,2,'Și le-a zis: &lt;J&gt;„De secerat este mult&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt;&lt;J&gt;, dar lucrători puțini sunt! Rugați-L deci&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;pe Stăpânul secerișului să scoată lucrători la secerișul Lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,3,'&lt;J&gt;Mergeți! Iată&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;, vă trimit ca pe niște miei în mijlocul lupilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,4,'&lt;J&gt;Să&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;nu luați cu voi nici pungă, nici traistă, nici sandale și să nu salutați pe nimeni&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;pe drum.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,5,'&lt;J&gt;În&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;orice casă veți intra, să ziceți întâi: «Pace casei acesteia!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,6,'&lt;J&gt;Și dacă va fi acolo un fiu al păcii, pacea voastră va rămâne peste el; altminteri, ea se va întoarce la voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,7,'&lt;J&gt;Să&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;rămâneți în casa aceea, să mâncați&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;și să beți de la ei, căci vrednic este lucrătorul&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;de plata lui. Să nu vă mutați din casă în casă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,8,'&lt;J&gt;În orice cetate veți intra și vă vor primi, să mâncați ce vi se va pune înainte,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,9,'&lt;J&gt;să-i&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;vindecați pe bolnavii de acolo și să le ziceți: «Împărăția lui Dumnezeu s-a&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;apropiat de voi.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,10,'&lt;J&gt;Dar în orice cetate veți intra și nu vă vor primi, să ieșiți pe străzile ei și să ziceți:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,11,'&lt;J&gt;«Chiar și&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;praful din cetatea voastră, care s-a prins pe picioarele noastre, îl scuturăm împotriva voastră. Să știți însă că Împărăția lui Dumnezeu s-a apropiat de voi.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,12,'&lt;J&gt;Vă spun că, în ziua aceea,&lt;/J&gt;&lt;f&gt;[39]&lt;/f&gt; &lt;J&gt;va fi mai&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;ușor pentru Sodoma decât pentru cetatea aceea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,13,'&lt;pb/&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;de tine, Horazine! Vai de tine, Betsaido! Căci&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;, dacă s-ar fi făcut în Tir și în Sidon minunile care au fost făcute în voi, de mult s-ar fi pocăit în sac și cenușă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,14,'&lt;J&gt;De aceea, la judecată, va fi mai ușor pentru Tir și Sidon decât pentru voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,15,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;tu, Capernaume, &lt;e&gt;vei fi oare înălțat până la cer&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[40]&lt;/f&gt;&lt;J&gt;? &lt;i&gt;Nu, ci&lt;/i&gt; &lt;e&gt;până în Locuința Morților vei fi coborât&lt;/e&gt;!&lt;/J&gt;&lt;f&gt;[41]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,16,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;vă ascultă pe voi Mă ascultă pe Mine și&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;cine vă respinge pe voi Mă respinge pe Mine; iar&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;cine Mă respinge pe Mine Îl respinge pe Cel care M-a trimis.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,17,'&lt;pb/&gt;Cei șaptezeci s-au&lt;f&gt;ⓦ&lt;/f&gt; întors cu bucurie și au zis: „Doamne, chiar și demonii ni se supun în Numele Tău!”');
INSERT INTO &quot;verses&quot; VALUES (490,10,18,'Isus le-a zis: &lt;J&gt;„L-am&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;văzut pe Satana căzând din cer ca un fulger.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,19,'&lt;J&gt;Iată, v-am&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;dat putere să călcați peste șerpi și scorpioni și peste toată puterea vrăjmașului; și de nimic nu veți fi vătămați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,20,'&lt;J&gt;Totuși, să nu vă bucurați de faptul că duhurile vi se supun, ci bucurați-vă că numele&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;voastre sunt scrise în ceruri!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,21,'&lt;pb/&gt;În&lt;f&gt;ⓐ&lt;/f&gt; ceasul acela, Isus S-a bucurat în Duhul Sfânt și a zis: &lt;J&gt;„Te laud, Tată, Domn al cerului și al pământului, pentru că ai ascuns aceste &lt;i&gt;lucruri&lt;/i&gt; de cei înțelepți și pricepuți și le-ai descoperit pruncilor! Da, Tată, pentru că așa ai găsit Tu cu cale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,22,'&lt;J&gt;Toate&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;Mi-au fost încredințate de Tatăl Meu; și&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;nimeni nu știe cine este Fiul în afară de Tatăl și nici cine este Tatăl în afară de Fiul și de cel căruia vrea Fiul să i-L descopere.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,23,'Apoi S-a întors către ucenici și le-a spus deoparte: &lt;J&gt;„Ferice&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;de ochii care văd ce vedeți voi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,24,'&lt;J&gt;Căci vă spun că&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;mulți profeți și regi au vrut să vadă ce vedeți voi, și nu au văzut, și să audă ce auziți voi, și nu au auzit.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,25,'&lt;pb/&gt;Și, iată, un învățător&lt;f&gt;ⓕ&lt;/f&gt; al Legii s-a ridicat să-L ispitească &lt;i&gt;pe Isus&lt;/i&gt; și I-a zis: „Învățătorule, ce să fac ca să moștenesc viața veșnică?”');
INSERT INTO &quot;verses&quot; VALUES (490,10,26,'&lt;i&gt;Isus&lt;/i&gt; i-a zis: &lt;J&gt;„Ce este scris în Lege? Cum citești în ea?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,27,'El a răspuns, zicând: „&lt;e&gt;Să-L iubești pe Domnul, Dumnezeul tău, cu toată inima ta, cu tot sufletul tău, cu toată puterea ta și cu tot cugetul tău&lt;/e&gt;&lt;f&gt;[42]&lt;/f&gt; &lt;e&gt;și pe aproapele tău ca pe tine însuți.&lt;/e&gt;&lt;f&gt;[43]&lt;/f&gt;”');
INSERT INTO &quot;verses&quot; VALUES (490,10,28,'&lt;J&gt;„Drept ai răspuns”&lt;/J&gt;, i-a zis Isus; &lt;J&gt;„fă așa și vei&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;trăi!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,29,'Dar el, vrând să&lt;f&gt;ⓗ&lt;/f&gt; se îndreptățească, I-a zis lui Isus: „Și cine este aproapele meu?”');
INSERT INTO &quot;verses&quot; VALUES (490,10,30,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Un om se cobora din Ierusalim spre Ierihon și a căzut între tâlhari. Aceștia l-au dezbrăcat, l-au bătut zdravăn și au plecat, lăsându-l aproape mort.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,31,'&lt;J&gt;Din întâmplare, se cobora pe acel drum un preot, care, văzându-l, a&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;trecut pe partea cealaltă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,32,'&lt;J&gt;La fel și un levit care mergea pe acolo, când l-a văzut, a trecut pe partea cealaltă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,33,'&lt;J&gt;Dar un samaritean&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;aflat în călătorie a venit spre el și, văzându-l, i s-a făcut milă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,34,'&lt;J&gt;S-a apropiat, i-a legat rănile și a turnat &lt;i&gt;pe ele&lt;/i&gt; untdelemn și vin. Apoi l-a pus pe animalul lui de povară, l-a dus la un han și l-a îngrijit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,35,'&lt;J&gt;A doua zi, [când a plecat,] a scos doi dinari, i-a dat hangiului și i-a zis: «Să-l îngrijești, iar ce vei cheltui în plus îți voi da eu, la întoarcere.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,36,'&lt;J&gt;Care dintre aceștia trei ți se pare că a fost aproapele celui căzut între tâlhari?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,37,'„Cel care și-a făcut milă de el”, a răspuns acela. Isus i-a zis: &lt;J&gt;„Du-te și fă &lt;i&gt;și tu&lt;/i&gt; la fel!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,38,'&lt;pb/&gt;Pe când erau pe drum, &lt;i&gt;Isus&lt;/i&gt; a intrat într-un sat. O femeie pe nume Marta&lt;f&gt;ⓚ&lt;/f&gt; L-a primit &lt;i&gt;în casa ei&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,10,39,'Ea avea o soră numită Maria, care&lt;f&gt;ⓛ&lt;/f&gt; s-a așezat&lt;f&gt;ⓜ&lt;/f&gt; la picioarele Domnului și asculta cuvântul Lui.');
INSERT INTO &quot;verses&quot; VALUES (490,10,40,'Marta era împărțită cu multă slujire. Ea a venit repede la &lt;i&gt;Isus&lt;/i&gt; și I-a zis: „Doamne, nu-Ți pasă că sora mea m-a lăsat să slujesc singură? Zi-i deci să mă ajute!”');
INSERT INTO &quot;verses&quot; VALUES (490,10,41,'Domnul i-a răspuns, zicând: &lt;J&gt;„Marto, Marto, te îngrijorezi și te frămânți pentru multe,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,10,42,'&lt;J&gt;dar de un&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;singur &lt;i&gt;lucru&lt;/i&gt; este nevoie. Maria a ales partea cea bună, care nu i se va lua.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; Se ruga într-un anumit loc, iar când a încetat, unul dintre ucenicii Lui I-a zis: „Doamne, învață-ne să ne rugăm, cum i-a învățat și Ioan pe ucenicii lui!”');
INSERT INTO &quot;verses&quot; VALUES (490,11,2,'&lt;pb/&gt;El le-a zis: &lt;J&gt;„Când vă rugați, să ziceți:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;«Tatăl&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;[nostru care ești în ceruri],&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;sfințească-se Numele Tău,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;vie Împărăția Ta,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;[facă-se voia Ta precum în cer, așa și pe pământ]!&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,3,'&lt;t&gt;&lt;J&gt;Pâinea noastră cea de toate zilele dă-ne-o nouă în fiecare zi&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,4,'&lt;t&gt;&lt;J&gt;și ne iartă nouă păcatele noastre&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;fiindcă și noi iertăm oricui ne este dator;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;și nu ne duce în ispită,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;[ci ne izbăvește de Cel Rău]!»”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,5,'&lt;pb/&gt;&lt;i&gt;El&lt;/i&gt; le-a mai zis: &lt;J&gt;„&lt;i&gt;Dacă&lt;/i&gt; unul dintre voi are un prieten și merge la el în miez de noapte și-i zice: «Prietene, împrumută-mi trei pâini,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,6,'&lt;J&gt;căci a venit la mine de pe drum un prieten al meu și n-am ce să-i pun înainte»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,7,'&lt;J&gt;și dacă dinăuntru &lt;i&gt;prietenul&lt;/i&gt; îi răspunde: «Nu mă tulbura! Acum ușa este încuiată, copiii mei sunt cu mine în pat: nu pot să mă ridic să-ți dau»,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,8,'&lt;J&gt;vă spun: chiar&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;dacă nu se va ridica să-i dea fiindcă-i este prieten, &lt;i&gt;totuși&lt;/i&gt; pentru îndrăzneala&lt;/J&gt;&lt;f&gt;[44]&lt;/f&gt; &lt;J&gt;lui se va ridica să-i dea ce-i trebuie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,9,'&lt;J&gt;De aceea și&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Eu vă spun: cereți, și vi se va da; căutați, și veți găsi; bateți, și vi se va deschide!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,10,'&lt;J&gt;Căci oricine cere primește; cine caută găsește, iar celui ce bate i se deschide.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,11,'&lt;J&gt;Cine dintre voi este tatăl care, [dacă-i&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;cere fiul său pâine, să-i dea o piatră,] dacă-i cere fiul un pește, să-i dea un șarpe în loc de pește&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,12,'&lt;J&gt;sau, dacă-i cere un ou, să-i dea un scorpion?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,13,'&lt;J&gt;Deci, dacă voi, care sunteți răi, știți să le dați daruri bune copiilor voștri, cu atât mai mult Tatăl [vostru] din cer le va da Duh Sfânt celor care I-L cer!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,14,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; &lt;f&gt;ⓢ&lt;/f&gt; a scos &lt;i&gt;dintr-un om&lt;/i&gt; un duh de muțenie&lt;f&gt;[45]&lt;/f&gt;. După ce a ieșit demonul, mutul a vorbit, iar mulțimile au rămas uimite.');
INSERT INTO &quot;verses&quot; VALUES (490,11,15,'Unii dintre ei ziceau: „Cu Beelzebul, căpetenia demonilor, scoate El&lt;f&gt;ⓣ&lt;/f&gt; demonii.”');
INSERT INTO &quot;verses&quot; VALUES (490,11,16,'Alții, ca să-L ispitească, Îi cereau&lt;f&gt;ⓤ&lt;/f&gt; un semn din cer.');
INSERT INTO &quot;verses&quot; VALUES (490,11,17,'Dar El&lt;f&gt;ⓥ&lt;/f&gt;, cunoscându-le&lt;f&gt;ⓦ&lt;/f&gt; gândurile, le-a zis: &lt;J&gt;„Orice împărăție dezbinată împotriva ei înseși este pustiită și casă peste casă va cădea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,18,'&lt;J&gt;Deci, dacă Satana este dezbinat împotriva lui însuși, cum va dăinui împărăția lui? Fiindcă ziceți că Eu scot demonii cu ajutorul lui Beelzebul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,19,'&lt;J&gt;Și dacă Eu scot demonii cu ajutorul lui Beelzebul, fiii voștri cu cine-i scot? De aceea, ei vor fi judecătorii voștri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,20,'&lt;J&gt;Dar, dacă Eu scot demonii cu&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;degetul lui Dumnezeu, atunci Împărăția lui Dumnezeu a ajuns la voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,21,'&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;cel puternic și bine înarmat își păzește curtea, avutul lui este în siguranță.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,22,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt;&lt;J&gt;, dacă vine unul mai puternic decât el și-l răpune, atunci îi ia toate armele în care se încredea și împarte prăzile.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,23,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;nu este cu Mine este împotriva Mea și cine nu adună cu Mine risipește.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,24,'&lt;pb/&gt;&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;a ieșit din om, duhul necurat umblă prin locuri fără apă, căutând odihnă. Fiindcă n-o găsește, zice: «Mă voi întoarce în casa mea, de unde am ieșit.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,25,'&lt;J&gt;Și când ajunge, o găsește măturată și împodobită.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,26,'&lt;J&gt;Atunci se duce, ia alte șapte duhuri, mai rele decât el, intră &lt;i&gt;în casă&lt;/i&gt; și își fac locuința acolo; iar starea din urmă&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;a acelui om devine mai rea decât cea dintâi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,27,'&lt;pb/&gt;Pe când spunea El aceste &lt;i&gt;cuvinte&lt;/i&gt;, o femeie din mulțime și-a ridicat glasul și I-a zis: „Ferice&lt;f&gt;ⓓ&lt;/f&gt; de pântecele care Te-a purtat și de sânul care te-a alăptat!”');
INSERT INTO &quot;verses&quot; VALUES (490,11,28,'Iar El a răspuns: &lt;J&gt;„Ferice&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;mai degrabă de cei ce ascultă Cuvântul lui Dumnezeu și-L păzesc!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,29,'&lt;pb/&gt;Când&lt;f&gt;ⓕ&lt;/f&gt; mulțimile se înghesuiau &lt;i&gt;în jurul Lui&lt;/i&gt;, &lt;i&gt;Isus&lt;/i&gt; a început să vorbească: &lt;J&gt;„Generația aceasta este o generație rea; ea cere un semn, dar nu i se va da &lt;i&gt;alt&lt;/i&gt; semn decât semnul [profetului] Iona.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,30,'&lt;J&gt;Căci, după cum Iona&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;a fost un semn pentru cei din Ninive, tot așa Fiul Omului va fi &lt;i&gt;un semn&lt;/i&gt; pentru generația aceasta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,31,'&lt;J&gt;Regina&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;de la miazăzi se va ridica la judecată împotriva bărbaților din generația aceasta și-i va osândi; pentru că ea a venit de la marginile pământului ca să asculte înțelepciunea lui Solomon; și iată că aici &lt;i&gt;este&lt;/i&gt; Unul mai mare decât Solomon.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,32,'&lt;J&gt;Bărbații din Ninive se vor ridica la judecată împotriva generației acesteia și o vor osândi, pentru că ei&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;s-au pocăit la propovăduirea lui Iona; și iată că aici &lt;i&gt;este&lt;/i&gt; Unul mai mare decât Iona.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,33,'&lt;pb/&gt;&lt;J&gt;Nimeni&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;, după ce a aprins o candelă, n-o pune într-un loc ascuns sau sub obroc, ci &lt;i&gt;o pune&lt;/i&gt; în sfeșnic, pentru ca cei care intră să vadă lumina.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,34,'&lt;J&gt;Ochiul tău este candela&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;trupului. Când ochiul tău este sănătos, tot trupul tău este luminat; dar când &lt;i&gt;ochiul tău&lt;/i&gt; este rău, și trupul tău este întunecat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,35,'&lt;J&gt;Ia seama deci ca nu cumva lumina din tine să fie întuneric!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,36,'&lt;J&gt;Dacă trupul tău este luminat în întregime, neavând nicio parte întunecată, va fi la fel de plin de lumină ca atunci când te luminează o candelă cu lumina ei.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,37,'&lt;pb/&gt;Pe când vorbea &lt;i&gt;Isus&lt;/i&gt;, un fariseu L-a rugat să prânzească la el. El a intrat și S-a așezat la masă.');
INSERT INTO &quot;verses&quot; VALUES (490,11,38,'Fariseul&lt;f&gt;ⓛ&lt;/f&gt; s-a mirat văzând că Isus nu S-a spălat înainte de masă.');
INSERT INTO &quot;verses&quot; VALUES (490,11,39,'Dar&lt;f&gt;ⓜ&lt;/f&gt; Domnul i-a zis: &lt;J&gt;„Voi, fariseii, curățați partea de afară a paharului și a vasului, dar interiorul&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;vostru este plin de lăcomie și răutate.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,40,'&lt;J&gt;Nesăbuiților, oare Acela care a făcut partea din afară n-a făcut-o și pe cea dinăuntru?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,41,'&lt;J&gt;Dați&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;milostenie din cele dinăuntru, și atunci toate vă vor fi curate!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,42,'&lt;pb/&gt;&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;vai de voi, fariseilor, pentru că voi dați zeciuială din mentă, din rută și din toate legumele, dar lăsați deoparte dreptatea și dragostea de Dumnezeu! Pe acestea trebuia să le faceți, și pe acelea să nu le lăsați deoparte.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,43,'&lt;pb/&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;de voi, fariseilor, pentru că vă plac scaunele dintâi în sinagogi și saluturile prin piețe!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,44,'&lt;pb/&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;de voi, [cărturari și farisei fățarnici,] pentru că&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;sunteți ca mormintele care nu se văd și pe care oamenii pășesc fără să știe!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,45,'&lt;pb/&gt;Unul dintre învățătorii Legii I-a răspuns, zicând: „Învățătorule, spunând aceste lucruri, ne jignești și pe noi!”');
INSERT INTO &quot;verses&quot; VALUES (490,11,46,'&lt;pb/&gt;&lt;J&gt;„Vai și de voi, învățători ai Legii”&lt;/J&gt;, a răspuns &lt;i&gt;Isus&lt;/i&gt;, &lt;J&gt;„pentru că&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;puneți pe oameni poveri grele, iar voi nici măcar cu un deget nu le atingeți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,47,'&lt;pb/&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;de voi, pentru că zidiți mormintele profeților pe care i-au ucis părinții voștri!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,48,'&lt;J&gt;Prin aceasta mărturisiți că încuviințați faptele părinților voștri, căci ei i-au ucis &lt;i&gt;pe profeți&lt;/i&gt;, iar voi le zidiți [mormintele].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,49,'&lt;J&gt;De aceea și înțelepciunea lui Dumnezeu a zis: «Le voi trimite&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;profeți și apostoli; pe unii dintre ei îi vor ucide și îi vor prigoni,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,50,'&lt;J&gt;ca să se ceară de la generația aceasta sângele tuturor profeților care a fost vărsat de la întemeierea lumii:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,51,'&lt;J&gt;de la&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;sângele lui Abel până la sângele&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;lui Zaharia, ucis între altar și sanctuar.» Da, vă spun, se va cere de la această generație.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,52,'&lt;pb/&gt;&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;de voi, învățători ai Legii, pentru că ați pus mâna pe cheia cunoștinței; &lt;i&gt;nu doar că&lt;/i&gt; voi înșivă n-ați intrat, ci i-ați și împiedicat pe cei care voiau să intre!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,11,53,'&lt;pb/&gt;După ce a ieșit de acolo, cărturarii și fariseii au început să-L dușmănească înverșunat, &lt;i&gt;cu gând&lt;/i&gt; să-L iscodească în multe feluri;');
INSERT INTO &quot;verses&quot; VALUES (490,11,54,'I-au întins astfel curse, ca&lt;f&gt;ⓩ&lt;/f&gt; să-L prindă cu vreo vorbă [și să-L poată învinui].');
INSERT INTO &quot;verses&quot; VALUES (490,12,1,'&lt;pb/&gt;Când&lt;f&gt;ⓐ&lt;/f&gt; s-a adunat mulțimea cu miile, încât &lt;i&gt;oamenii&lt;/i&gt; se călcau unii pe alții, &lt;i&gt;Isus&lt;/i&gt; a început să le vorbească mai întâi ucenicilor Săi: &lt;J&gt;„Feriți-vă&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;de aluatul fariseilor, care este fățărnicia!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,2,'&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;este nimic acoperit care nu va fi descoperit și nimic tăinuit care nu va fi cunoscut.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,3,'&lt;J&gt;De aceea, orice ați spus la întuneric va fi auzit la lumină; și orice ați vorbit la ureche în odăițe va fi vestit de pe acoperișuri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,4,'&lt;pb/&gt;&lt;J&gt;Vă&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;spun vouă, prietenii&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;Mei, nu vă temeți de cei ce ucid trupul, iar după aceea nu mai pot face nimic!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,5,'&lt;J&gt;Vă voi arăta însă de cine să vă temeți: temeți-vă de Acela care, după ce a ucis, are putere să arunce în gheenă; da, vă spun, de El să vă temeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,6,'&lt;J&gt;Nu se vând oare cinci vrăbii pe doi bani&lt;/J&gt;&lt;f&gt;[46]&lt;/f&gt;&lt;J&gt;? Totuși, niciuna dintre ele nu este uitată înaintea lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,7,'&lt;J&gt;Până și perii capului, toți vă sunt numărați. Nu vă temeți! Voi sunteți mai prețioși decât multe vrăbii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,8,'&lt;pb/&gt;&lt;J&gt;Vă spun&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;: pe oricine Mă va mărturisi înaintea oamenilor îl va mărturisi și Fiul Omului înaintea îngerilor lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,9,'&lt;J&gt;Dar cine Mă va tăgădui înaintea oamenilor va fi tăgăduit și el înaintea îngerilor lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,10,'&lt;J&gt;Oricine&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;va vorbi împotriva Fiului Omului va fi iertat, dar cine va huli împotriva Duhului Sfânt nu va fi iertat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,11,'&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;vă vor duce înaintea sinagogilor, a dregătorilor și a autorităților, să nu vă îngrijorați cum sau ce veți răspunde în apărarea voastră, nici ce veți vorbi,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,12,'&lt;J&gt;fiindcă Duhul Sfânt vă va învăța chiar în ceasul acela ce trebuie să spuneți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,13,'&lt;pb/&gt;Cineva din mulțime I-a zis &lt;i&gt;lui Isus&lt;/i&gt;: „Învățătorule, spune-i fratelui meu să împartă cu mine moștenirea!”');
INSERT INTO &quot;verses&quot; VALUES (490,12,14,'&lt;J&gt;„Omule&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;, i-a răspuns &lt;i&gt;Isus&lt;/i&gt;, &lt;J&gt;„cine M-a pus judecător sau împărțitor peste voi?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,15,'Apoi le-a zis: &lt;J&gt;„Luați seama&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;și păziți-vă de orice lăcomie, fiindcă oricât de bogat ar fi cineva, viața lui nu stă în averile lui!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,16,'&lt;pb/&gt;Și le-a spus o pildă: &lt;J&gt;„Ogorul unui om bogat a dat recoltă îmbelșugată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,17,'&lt;J&gt;El se gândea în sinea lui, zicând: «Ce voi face, fiindcă nu mai am unde să-mi adun roadele?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,18,'&lt;J&gt;Și a mai zis: «Iată ce voi face: îmi voi dărâma hambarele, voi zidi &lt;i&gt;altele&lt;/i&gt; mai mari și acolo îmi voi strânge tot grâul și toate bunătățile mele;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,19,'&lt;J&gt;și-i voi zice sufletului meu: ‘Suflete&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt;&lt;J&gt;, ai multe bunătăți, strânse pentru mulți ani. Odihnește-te, mănâncă, bea și veselește-te!’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,20,'&lt;J&gt;Dar Dumnezeu i-a zis: «Nebunule, chiar în noaptea aceasta ți se va&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;cere sufletul&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt;&lt;J&gt;! Cele pe care le-ai pregătit ale cui vor fi?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,21,'&lt;J&gt;La fel este și cel care adună comori pentru sine și&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;nu se îmbogățește față de Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,22,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; le-a zis ucenicilor săi: &lt;J&gt;„De aceea vă spun: nu&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vă îngrijorați cu privire la viață (ce veți mânca), nici cu privire la trup (cu ce vă veți îmbrăca).&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,23,'&lt;J&gt;Viața este mai mult decât hrana, și trupul — &lt;i&gt;mai mult&lt;/i&gt; decât îmbrăcămintea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,24,'&lt;J&gt;Uitați-vă la corbi: ei nu seamănă, nici nu seceră, n-au nici cămară, nici hambar, și totuși Dumnezeu&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;îi hrănește! Cu cât mai prețioși sunteți voi decât păsările!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,25,'&lt;J&gt;Cine dintre voi, chiar îngrijorându-se, poate să adauge un cot la statura lui?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,26,'&lt;J&gt;Deci, dacă nu puteți face nici cel mai mic &lt;i&gt;lucru&lt;/i&gt;, de ce vă îngrijorați de celelalte?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,27,'&lt;J&gt;Uitați-vă cum cresc crinii: ei nu trudesc, nici nu torc. Vă spun însă că nici chiar Solomon, în toată slava lui, nu s-a îmbrăcat ca unul dintre ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,28,'&lt;J&gt;Iar dacă astfel îmbracă Dumnezeu iarba de pe câmp, care astăzi este, dar mâine va fi aruncată în cuptor, cu cât mai mult &lt;i&gt;vă va îmbrăca&lt;/i&gt; pe voi, puțin credincioșilor!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,29,'&lt;J&gt;Voi să nu căutați și să nu vă frământați cu privire la ce veți mânca și ce veți bea!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,30,'&lt;J&gt;Pe toate acestea neamurile lumii le râvnesc. Tatăl vostru știe că aveți nevoie de ele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,31,'&lt;J&gt;Căutați&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;mai întâi Împărăția lui [Dumnezeu], și [toate] acestea vi se vor da pe deasupra!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,32,'&lt;J&gt;Nu te teme, turmă mică, fiindcă&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;Tatăl vostru vă dă cu plăcere Împărăția!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,33,'&lt;J&gt;Vindeți&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;ce aveți și dați milostenie! Faceți-vă&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;i&gt;&lt;J&gt;rost de&lt;/i&gt; pungi care nu se învechesc, comoară nesfârșită în ceruri, unde hoțul nu se apropie și molia nu roade!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,34,'&lt;J&gt;Pentru că, unde este comoara voastră, acolo va fi și inima voastră.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,35,'&lt;pb/&gt;&lt;e&gt;&lt;J&gt;Mijlocul&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;&lt;J&gt;să vă fie încins&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[47]&lt;/f&gt; &lt;J&gt;și făcliile&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt;&lt;J&gt;, aprinse.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,36,'&lt;i&gt;&lt;J&gt;Să fiți&lt;/i&gt; ca niște oameni care-l așteaptă pe stăpânul lor să se întoarcă de la nuntă, ca să-i deschidă îndată când va veni și va bate &lt;i&gt;la ușă&lt;/i&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,37,'&lt;J&gt;Ferice&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;de robii aceia pe care stăpânul îi va găsi veghind când va veni! Adevărat vă spun că el se va încinge, îi va așeza la masă și va veni să-i servească.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,38,'&lt;J&gt;Fie că vine la a doua strajă &lt;i&gt;din noapte&lt;/i&gt;, fie că vine la a treia strajă, dacă-i va găsi așa, ferice de [robii] aceia!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,39,'&lt;J&gt;Să&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;știți că, dacă ar ști stăpânul casei la ce ceas vine hoțul, [ar veghea și] n-ar îngădui să-i fie spartă casa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,40,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;voi fiți gata, căci Fiul Omului vine în ceasul în care nici nu vă gândiți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,41,'&lt;pb/&gt;„Doamne”, I-a zis Petru, „pentru noi spui pilda aceasta sau pentru toți?”');
INSERT INTO &quot;verses&quot; VALUES (490,12,42,'Și Domnul a zis: &lt;J&gt;„Cine&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;este ispravnicul credincios și chibzuit pe care-l va pune stăpânul său peste slujitorii din casa lui, ca să le dea partea de hrană la vremea cuvenită?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,43,'&lt;J&gt;Ferice de robul acela pe care stăpânul său, când va veni, îl va găsi făcând astfel!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,44,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;vă spun că îl va pune peste toate avuțiile sale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,45,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt;&lt;J&gt;, dacă robul acela zice în inima lui: «Stăpânul meu întârzie să vină» și începe să-i bată pe slujitori și pe slujnice, să mănânce, să bea și să se îmbete,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,46,'&lt;J&gt;stăpânul robului aceluia va veni în ziua în care el nu se așteaptă și în ceasul pe care nu-l știe; îl va tăia în bucăți, și soarta lui va fi soarta necredincioșilor&lt;/J&gt;&lt;f&gt;[48]&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,47,'&lt;J&gt;Robul&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;acela care a știut voia stăpânului său și nu s-a pregătit, nici n-a lucrat după voia lui va primi multe lovituri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,48,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;cel care n-a știut-o și a făcut lucruri ce merită lovituri va primi puține lovituri. Oricui i s-a dat mult, mult i se va cere, și cui i s-a încredințat mult, mai mult i se va cere.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,49,'&lt;pb/&gt;&lt;J&gt;Am venit să arunc foc&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;pe pământ, și cât aș vrea să fie aprins deja!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,50,'&lt;J&gt;Am un botez cu&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;care trebuie să fiu botezat și cât sunt de apăsat până se va împlini!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,51,'&lt;J&gt;Credeți&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;că am venit să aduc pace pe pământ? Vă spun: nu, ci&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;mai degrabă dezbinare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,52,'&lt;J&gt;De acum înainte&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt;&lt;J&gt;, cinci dacă vor fi într-o casă, vor fi dezbinați: trei împotriva a doi și doi împotriva a trei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,53,'&lt;pb/&gt;&lt;t&gt;&lt;J&gt;Tatăl va fi dezbinat împotriva fiului,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;și &lt;e&gt;fiul împotriva tatălui;&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;mama, împotriva fiicei,&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[49]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;și fiica, împotriva mamei;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;soacra, împotriva nurorii sale,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;și &lt;e&gt;nora, împotriva soacrei&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[50]&lt;/f&gt; &lt;J&gt;.”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,54,'&lt;pb/&gt;El le-a mai zis mulțimilor: &lt;J&gt;„Când&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;vedeți un nor ridicându-se la apus, îndată ziceți: «Vine ploaia.» Și așa se întâmplă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,55,'&lt;J&gt;Și când vântul suflă de la miazăzi, ziceți: «Va fi arșiță.» Și așa se întâmplă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,56,'&lt;J&gt;Fățarnicilor, chipul pământului și al cerului îl puteți recunoaște, dar vremea aceasta cum de nu puteți s-o înțelegeți?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,57,'&lt;pb/&gt;&lt;J&gt;Și de ce nu judecați voi înșivă ce este drept?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,58,'&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;mergi cu potrivnicul tău înaintea stăpânitorului, pe drum&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;dă-ți silința să te împaci cu el, ca nu cumva să te târască înaintea judecătorului, judecătorul să te dea pe mâna aprodului, iar aprodul să te arunce în temniță!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,12,59,'&lt;J&gt;Îți spun că nu vei ieși de acolo până nu vei da înapoi și ultimul bănuț&lt;/J&gt;&lt;f&gt;[51]&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,1,'&lt;pb/&gt;În vremea aceea, au venit unii și I-au povestit &lt;i&gt;lui Isus&lt;/i&gt; despre galileenii al căror sânge îl amestecase Pilat cu jertfele lor.');
INSERT INTO &quot;verses&quot; VALUES (490,13,2,'[Isus] le-a răspuns, zicând: &lt;J&gt;„Credeți că galileenii aceștia au fost mai păcătoși decât toți ceilalți galileeni pentru că au pătimit astfel?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,3,'&lt;J&gt;Eu vă spun: nu, ci, dacă nu vă pocăiți, toți veți pieri la fel!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,4,'&lt;J&gt;Sau cei optsprezece peste care a căzut turnul din Siloam și i-a omorât, credeți că au fost mai vinovați decât toți ceilalți oameni care locuiau în Ierusalim?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,5,'&lt;J&gt;Eu vă spun: nu, ci, dacă nu vă pocăiți, toți veți pieri la fel!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,6,'&lt;pb/&gt;El a spus pilda aceasta: &lt;J&gt;„Un&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;om avea un smochin sădit în vie. A venit să caute rod în el, dar n-a găsit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,7,'&lt;J&gt;Atunci i-a zis lucrătorului: «Iată, &lt;i&gt;sunt&lt;/i&gt; trei ani de când vin și caut rod în smochinul acesta, dar nu găsesc. Taie-l! De ce să mai secătuiască pământul?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,8,'&lt;J&gt;Dar &lt;i&gt;lucrătorul&lt;/i&gt; i-a răspuns: «Stăpâne, mai lasă-l și anul acesta; îl voi săpa împrejur și-i voi pune gunoi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,9,'&lt;J&gt;Poate că anul viitor va rodi; dacă nu, îl vei tăia.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,10,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; dădea învățătură într-o sinagogă în ziua sabatului.');
INSERT INTO &quot;verses&quot; VALUES (490,13,11,'Și iată că &lt;i&gt;acolo era&lt;/i&gt; o femeie care de optsprezece ani avea un duh de neputință: era gârbovă și nu putea nicidecum să se îndrepte.');
INSERT INTO &quot;verses&quot; VALUES (490,13,12,'Văzând-o, Isus a chemat-o și i-a zis: &lt;J&gt;„Femeie, ești dezlegată de neputința ta!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,13,'Și-a&lt;f&gt;ⓝ&lt;/f&gt; pus mâinile peste ea, iar &lt;i&gt;femeia&lt;/i&gt; s-a îndreptat îndată și-L slăvea pe Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,13,14,'Dar mai-marele sinagogii, mânios că Isus vindecase în ziua sabatului, a zis mulțimii: „Sunt șase&lt;f&gt;ⓞ&lt;/f&gt; zile în care trebuie să se lucreze; în acestea să veniți ca să fiți vindecați, nu&lt;f&gt;ⓟ&lt;/f&gt; în ziua sabatului!”');
INSERT INTO &quot;verses&quot; VALUES (490,13,15,'&lt;J&gt;„Fățarnicilor”&lt;/J&gt;, i-a răspuns Domnul, &lt;J&gt;„în ziua sabatului oare nu-și dezleagă fiecare&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;dintre voi boul sau măgarul de la iesle și-l duce să-l adape?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,16,'&lt;J&gt;Iar &lt;i&gt;femeia&lt;/i&gt; aceasta, care&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;este o fiică a lui Avraam și pe care Satana, iată, de optsprezece ani o ținea legată, nu trebuia să fie dezlegată de legătura aceasta în ziua sabatului?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,17,'Când a vorbit El astfel, toți potrivnicii Lui au rămas rușinați, iar mulțimea se bucura de toate &lt;i&gt;faptele&lt;/i&gt; slăvite pe care le săvârșea El.');
INSERT INTO &quot;verses&quot; VALUES (490,13,18,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; &lt;f&gt;ⓢ&lt;/f&gt; a zis: &lt;J&gt;„Cu ce se aseamănă Împărăția lui Dumnezeu și cu ce o voi asemăna?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,19,'&lt;J&gt;Se aseamănă cu un grăunte de muștar pe care l-a luat un om și l-a aruncat în grădina sa; el a crescut, s-a făcut un copac [mare] și păsările cerului și-au făcut cuiburi în ramurile lui.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,20,'&lt;pb/&gt;El a mai zis: &lt;J&gt;„Cu ce voi asemăna Împărăția lui Dumnezeu?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,21,'&lt;J&gt;Se aseamănă cu aluatul pe care l-a luat o femeie și l-a pus în trei măsuri&lt;/J&gt;&lt;f&gt;[52]&lt;/f&gt; &lt;J&gt;de făină de grâu, până a dospit totul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,22,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; &lt;f&gt;ⓣ&lt;/f&gt; trecea prin cetăți și prin sate, dând învățătură în drum spre Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (490,13,23,'Cineva I-a zis: „Doamne, sunt oare puțini cei ce se mântuiesc?” El a răspuns:');
INSERT INTO &quot;verses&quot; VALUES (490,13,24,'&lt;J&gt;„Străduiți-vă&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;să intrați pe ușa cea strâmtă, căci — vă spun — mulți&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;vor căuta să intre, și nu vor putea!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,25,'&lt;J&gt;Odată ce&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;Stăpânul casei Se va ridica și va&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;încuia ușa, iar voi, de afară, veți începe să bateți la ușă, zicând: «Doamne&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt;&lt;J&gt;, [Doamne,] deschide-ne!», drept răspuns, El vă va zice: «Nu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;știu de unde sunteți!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,26,'&lt;J&gt;Atunci veți începe să spuneți: «Noi am mâncat și am băut în fața Ta și pe străzile noastre ai dat învățături!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,27,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;El va răspunde zicând: «Nu știu de unde sunteți; &lt;e&gt;plecați&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;&lt;J&gt;de la Mine, voi toți care lucrați fărădelege!&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[53]&lt;/f&gt;&lt;J&gt;»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,28,'&lt;J&gt;Va&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;fi plânsul și scrâșnirea dinților când&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;îi veți vedea pe Avraam, pe Isaac, pe Iacov și pe toți profeții în Împărăția lui Dumnezeu, iar voi veți fi scoși afară.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,29,'&lt;J&gt;Vor veni de la răsărit și de la apus, de la miazănoapte și de la miazăzi și vor sta la masă în Împărăția lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,30,'&lt;J&gt;Și iată&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt;&lt;J&gt;: sunt unii dintre cei de pe urmă care vor fi cei dintâi și sunt unii dintre cei dintâi care vor fi cei de pe urmă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,31,'&lt;pb/&gt;În ceasul acela, au venit niște farisei și I-au zis: „Ieși și pleacă de aici, căci Irod vrea să te omoare!”');
INSERT INTO &quot;verses&quot; VALUES (490,13,32,'El le-a răspuns &lt;J&gt;„Mergeți și spuneți-i vulpii aceleia: «Iată, scot demoni și săvârșesc vindecări astăzi și mâine, iar a treia &lt;i&gt;zi&lt;/i&gt; voi&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;sfârși&lt;/J&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,13,33,'&lt;J&gt;Dar astăzi, mâine și-n ziua următoare trebuie să umblu, fiindcă nu se poate ca un profet să moară în afara Ierusalimului.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,34,'&lt;J&gt;Ierusalime&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt;&lt;J&gt;, Ierusalime, care-i omori pe profeți și-i ucizi cu pietre pe cei trimiși la tine, de câte ori n-am vrut să-i adun pe copiii tăi cum își adună cloșca puii sub aripi, dar voi n-ați vrut!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,13,35,'&lt;J&gt;Iată că&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;vă rămâne casa [pustie], dar vă spun că nu Mă veți mai vedea până când veți zice:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;«Binecuvântat&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓘ&lt;/f&gt; &lt;e&gt;&lt;J&gt;este Cel ce vine în Numele Domnului!»&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[54]&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,1,'&lt;pb/&gt;Într-o zi de sabat, &lt;i&gt;Isus&lt;/i&gt; a intrat în casa unuia dintre mai-marii fariseilor ca să mănânce, iar fariseii Îl urmăreau cu atenție.');
INSERT INTO &quot;verses&quot; VALUES (490,14,2,'Și iată că în fața Lui era un om bolnav de hidropizie&lt;f&gt;[55]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,14,3,'Isus le-a zis învățătorilor Legii și fariseilor: &lt;J&gt;„Este îngăduit&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;a vindeca în ziua sabatului sau nu?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,4,'Însă ei tăceau. &lt;i&gt;Isus&lt;/i&gt; a pus mâna peste el, l-a vindecat și l-a lăsat să plece.');
INSERT INTO &quot;verses&quot; VALUES (490,14,5,'Apoi le-a zis: &lt;J&gt;„Care&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;dintre voi, dacă-i cade fiul sau boul în fântână, nu-l va scoate îndată, chiar în ziua sabatului?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,6,'Și n-au putut să-I răspundă nimic la aceste &lt;i&gt;cuvinte&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,14,7,'&lt;pb/&gt;Apoi, văzând că cei chemați &lt;i&gt;la masă&lt;/i&gt; își alegeau locurile de cinste, le-a spus o pildă:');
INSERT INTO &quot;verses&quot; VALUES (490,14,8,'&lt;J&gt;„Când ești chemat de cineva la nuntă, nu te așeza la masă pe locul de cinste, ca nu cumva să fi fost chemat și unul mai însemnat decât tine,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,9,'&lt;J&gt;iar cel ce v-a chemat și pe tine, și pe el să vină să-ți spună: «Dă-i acestuia locul!» Atunci, cu rușine, te vei duce și te vei așeza pe ultimul loc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,10,'&lt;J&gt;Ci&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt;&lt;J&gt;, când ești chemat, mergi și așază-te pe ultimul loc, pentru ca, atunci când va veni cel ce te-a chemat, să-ți spună: «Prietene, urcă mai sus!» Aceasta îți va face cinste înaintea tuturor comesenilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,11,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;oricine se înalță va fi smerit și oricine se smerește va fi înălțat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,12,'&lt;pb/&gt;Iar celui ce-L poftise i-a zis: &lt;J&gt;„Când dai un prânz sau o cină, nu-i chema pe prietenii tăi, nici pe frații tăi, nici pe rudele tale, nici pe vecinii bogați, ca nu cumva să te cheme și ei la rândul lor, și &lt;i&gt;aceasta&lt;/i&gt; să-ți fie răsplata!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,13,'&lt;J&gt;Ci, când dai un ospăț, cheamă-i pe săraci&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;, pe neputincioși, pe șchiopi și pe orbi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,14,'&lt;J&gt;Și va fi ferice de tine, fiindcă ei n-au cum să-ți răsplătească, dar vei fi răsplătit la învierea celor drepți.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,15,'&lt;pb/&gt;Auzind acestea, unul dintre comeseni I-a zis: „Ferice&lt;f&gt;ⓞ&lt;/f&gt; de acela care va sta la masă în Împărăția lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (490,14,16,'Și&lt;f&gt;ⓟ&lt;/f&gt; &lt;i&gt;Isus&lt;/i&gt; i-a răspuns: &lt;J&gt;„Un om a dat un ospăț mare și i-a chemat pe mulți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,17,'&lt;J&gt;La ceasul ospățului, l-a&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;trimis pe robul său să le spună celor chemați: «Veniți, căci toate sunt pregătite!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,18,'&lt;J&gt;Dar ei, unul după altul, au început să se scuze. Primul i-a zis: «Am cumpărat un ogor și trebuie să mă duc să-l văd; iartă-mă, te rog!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,19,'&lt;J&gt;Altul i-a zis: «Am cumpărat cinci perechi de boi și mă duc să-i încerc: iartă-mă, te rog!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,20,'&lt;J&gt;Altul i-a zis: «Tocmai m-am însurat și de aceea nu pot veni.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,21,'&lt;J&gt;Când s-a întors, robul i-a adus stăpânului vestea despre &lt;i&gt;toate&lt;/i&gt; acestea. Atunci, stăpânul casei s-a mâniat și i-a zis robului său: «Mergi degrabă pe străzile și ulițele cetății și adu-i aici pe săraci, pe neputincioși, pe orbi și pe șchiopi!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,22,'&lt;J&gt;Robul a zis apoi: «Stăpâne, s-a făcut cum ai poruncit și încă mai este loc.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,23,'&lt;J&gt;Stăpânul i-a răspuns robului: «Ieși la drumuri și la garduri și silește-i să intre, ca să mi se umple casa!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,24,'&lt;J&gt;Căci vă spun că niciunul&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;dintre cei chemați nu va gusta din ospățul meu!»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,25,'&lt;pb/&gt;Împreună cu &lt;i&gt;Isus&lt;/i&gt; mergeau mulțimi mari, iar El S-a întors și le-a zis:');
INSERT INTO &quot;verses&quot; VALUES (490,14,26,'&lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;vine cineva la Mine și&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;nu-și urăște tatăl, mama, soția, copiii, frații, surorile și&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;chiar viața, nu poate fi ucenicul Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,27,'&lt;J&gt;Și cine&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;nu-și poartă crucea și nu vine după Mine nu poate fi ucenicul Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,28,'&lt;pb/&gt;&lt;J&gt;Care&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;din voi, vrând să zidească un turn, nu stă mai întâi să-și facă socoteala cheltuielilor, &lt;i&gt;să vadă&lt;/i&gt; dacă are cu ce să-l termine,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,29,'&lt;J&gt;ca nu cumva, după ce i-a pus temelia, să nu-l poată termina și toți care-l văd să înceapă să-și bată joc de el,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,30,'&lt;J&gt;zicând: «Omul acesta a început să zidească, dar n-a putut termina»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,31,'&lt;J&gt;Sau ce rege, când merge la război împotriva altui rege, nu stă mai întâi să țină sfat dacă va putea merge cu zece mii &lt;i&gt;de oameni&lt;/i&gt; înaintea celui ce vine împotriva lui cu douăzeci de mii?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,32,'&lt;J&gt;Altminteri, pe când celălalt este încă departe, îi trimite o solie să ceară pace.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,33,'&lt;J&gt;Tot așa, cine nu se leapădă de tot ce are nu poate fi ucenicul Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,34,'&lt;pb/&gt;&lt;J&gt;Sarea este bună&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;, dar dacă sarea își pierde gustul, cum și-l va recăpăta?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,14,35,'&lt;J&gt;Atunci nu mai este bună nici pentru pământ, nici pentru gunoi, ci este aruncată afară. Cine are urechi de auzit să audă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,1,'&lt;pb/&gt;Toți vameșii și păcătoșii se apropiau&lt;f&gt;ⓨ&lt;/f&gt; de &lt;i&gt;Isus&lt;/i&gt; ca să-L asculte.');
INSERT INTO &quot;verses&quot; VALUES (490,15,2,'Dar fariseii și cărturarii cârteau, zicând: „Acesta îi primește pe păcătoși și&lt;f&gt;ⓩ&lt;/f&gt; mănâncă împreună cu ei.”');
INSERT INTO &quot;verses&quot; VALUES (490,15,3,'Însă El le-a spus pilda aceasta:');
INSERT INTO &quot;verses&quot; VALUES (490,15,4,'&lt;J&gt;„Care&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;din voi, dacă are o sută de oi și pierde una dintre ele, nu le lasă pe celelalte nouăzeci și nouă în pustiu și nu umblă după cea&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;pierdută până când o găsește?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,5,'&lt;J&gt;După ce o găsește, o pune bucuros pe umeri&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,6,'&lt;J&gt;și, când se întoarce acasă, își cheamă prietenii și vecinii, zicându-le: «Bucurați-vă împreună cu mine, căci mi-am găsit oaia pierdută!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,7,'&lt;J&gt;Tot la fel, vă spun că este mai multă bucurie în cer&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;pentru un singur păcătos care se pocăiește decât pentru nouăzeci și nouă de &lt;i&gt;oameni&lt;/i&gt; drepți, care n-au nevoie de pocăință.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,8,'&lt;pb/&gt;&lt;J&gt;Sau care femeie, dacă are zece drahme&lt;/J&gt;&lt;f&gt;[56]&lt;/f&gt; &lt;J&gt;și pierde una dintre ele, nu aprinde o lampă, nu mătură casa și nu caută cu grijă până când o găsește?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,9,'&lt;J&gt;După ce o găsește, își cheamă prietenele și vecinele, zicând: «Bucurați-vă împreună cu mine, căci mi-am găsit drahma pe care o pierdusem!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,10,'&lt;J&gt;La fel — vă spun — este bucurie înaintea îngerilor lui Dumnezeu pentru un singur păcătos care se pocăiește.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,11,'&lt;pb/&gt;El a mai zis: &lt;J&gt;„Un om avea doi fii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,12,'&lt;J&gt;Cel mai tânăr i-a spus tatălui său: «Tată, dă-mi partea de avere ce mi se cuvine!» Și &lt;i&gt;tatăl&lt;/i&gt; le-a împărțit averea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,13,'&lt;J&gt;Nu după multe zile, fiul mai tânăr și-a strâns toate &lt;i&gt;lucrurile&lt;/i&gt; și a plecat într-o țară îndepărtată și acolo și-a risipit averea&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;trăind în destrăbălare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,14,'&lt;J&gt;După ce a cheltuit totul, a venit o foamete cumplită peste țara aceea, iar el a început să ducă lipsă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,15,'&lt;J&gt;Atunci a mers și s-a pripășit pe lângă unul dintre locuitorii acelei țări, care l-a trimis pe câmpurile lui să-i pască porcii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,16,'&lt;J&gt;Mult și-ar fi dorit el să se sature cu roșcovele pe care le mâncau porcii, dar nu i le dădea nimeni.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,17,'&lt;J&gt;Atunci, venindu-și în fire, și-a zis: «Câți argați ai tatălui meu au pâine din belșug, iar eu mor de foame aici!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,18,'&lt;J&gt;Mă voi ridica, voi merge la tatăl meu și-i voi spune: ‘Tată, am păcătuit împotriva Cerului și înaintea ta;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,19,'&lt;J&gt;nu mai sunt vrednic să mă numesc fiul tău; fă-mă ca pe unul dintre argații tăi!’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,20,'&lt;J&gt;Și s-a ridicat și a plecat la tatăl său. Pe când era încă&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;departe, tatăl său l-a văzut și i s-a făcut milă de el; a alergat, l-a îmbrățișat și l-a sărutat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,21,'&lt;J&gt;Fiul i-a zis: «Tată, am păcătuit împotriva Cerului și&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;înaintea ta; nu mai sunt vrednic să mă numesc fiul tău.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,22,'&lt;J&gt;Dar tatăl le-a zis robilor săi: «Aduceți repede haina cea mai aleasă și îmbrăcați-l; puneți-i un inel în deget și încălțăminte în picioare!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,23,'&lt;J&gt;Aduceți vițelul cel îngrășat și tăiați-l! Să mâncăm și să ne veselim,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,24,'&lt;J&gt;căci&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;acest fiu al meu era mort și a revenit la viață; era pierdut și a fost găsit.» Și au început să se veselească.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,25,'&lt;pb/&gt;&lt;J&gt;Fiul cel mare era pe câmp. Când a venit și s-a apropiat de casă, a auzit muzică și jocuri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,26,'&lt;J&gt;L-a chemat pe unul dintre robi și l-a întrebat ce înseamnă acestea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,27,'&lt;i&gt;&lt;J&gt;Robul&lt;/i&gt; acela i-a răspuns: «Fratele tău s-a întors, iar tatăl tău a tăiat vițelul cel îngrășat, pentru că l-a găsit sănătos.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,28,'&lt;J&gt;El s-a mâniat și nu voia să intre. Tatăl său a ieșit și a început să-l roage stăruitor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,29,'&lt;J&gt;Dar el, drept răspuns, i-a zis tatălui său: «Iată, eu de atâția ani îți slujesc ca un rob și niciodată nu ți-am încălcat porunca; iar mie niciodată nu mi-ai dat măcar un ied ca să mă veselesc cu prietenii mei!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,30,'&lt;J&gt;Dar când a venit acest fiu al tău, care ți-a mâncat averea cu desfrânatele, ai tăiat pentru el vițelul cel îngrășat!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,31,'&lt;i&gt;&lt;J&gt;Tatăl&lt;/i&gt; i-a spus: «Fiule, tu ești totdeauna cu mine și tot ce am eu este al tău.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,15,32,'&lt;J&gt;Dar trebuia să ne veselim și să ne bucurăm, pentru că&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;acest frate al tău era mort și a revenit la viață, era pierdut și a fost găsit.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; le-a spus ucenicilor: &lt;J&gt;„Un om bogat avea un ispravnic care a fost pârât la el că-i risipește avuțiile.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,2,'&lt;J&gt;El l-a chemat și i-a zis: «Ce aud despre tine? Dă-mi socotelile isprăvniciei tale, căci nu mai poți rămâne în slujbă!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,3,'&lt;J&gt;Ispravnicul și-a zis: «Ce voi face dacă stăpânul îmi ia slujba? Să sap, nu pot; să cerșesc, mi-e rușine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,4,'&lt;J&gt;Știu ce voi face pentru ca, atunci când voi fi scos din slujbă, ei să mă primească în casele lor.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,5,'&lt;J&gt;I-a chemat pe datornicii stăpânului său, unul câte unul, și i-a zis celui dintâi: «Cât îi datorezi stăpânului meu?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,6,'&lt;J&gt;«O sută de măsuri&lt;/J&gt;&lt;f&gt;[57]&lt;/f&gt; &lt;J&gt;de untdelemn», a răspuns el. Și i-a zis: «Ia-ți înscrisul, așază-te și scrie degrabă cincizeci!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,7,'&lt;J&gt;Apoi i-a zis altuia: «Dar tu cât îi datorezi?» «O sută de măsuri&lt;/J&gt;&lt;f&gt;[58]&lt;/f&gt; &lt;J&gt;de grâu», a răspuns el. Și i-a zis: «Ia-ți înscrisul și scrie optzeci!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,8,'&lt;J&gt;Stăpânul l-a lăudat pe ispravnicul nedrept pentru că lucrase cu iscusință. Căci fiii veacului acestuia sunt mai iscusiți decât fiii&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;luminii &lt;i&gt;în relațiile&lt;/i&gt; cu semenii lor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,9,'&lt;J&gt;Eu vă spun: Faceți-vă&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;prieteni cu ajutorul avuțiilor&lt;/J&gt;&lt;f&gt;[59]&lt;/f&gt; &lt;J&gt;nedrepte, pentru ca, atunci când ele se vor sfârși, să fiți primiți în corturile veșnice.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,10,'&lt;pb/&gt;&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;este credincios în cele mai mici &lt;i&gt;lucruri&lt;/i&gt; este credincios și în cele mari, iar cine este nedrept în cele mai mici &lt;i&gt;lucruri&lt;/i&gt; este nedrept și în cele mari.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,11,'&lt;J&gt;Dacă în avuția nedreaptă n-ați fost credincioși, cine o să v-o încredințeze pe cea adevărată?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,12,'&lt;J&gt;Și, dacă în lucrul altuia n-ați fost credincioși, cine o să vă dea ce-i al vostru?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,13,'&lt;J&gt;Niciun&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;rob nu poate sluji la doi stăpâni, căci ori îl va urî pe unul și-l va iubi pe celălalt, ori va ține la unul și-l va disprețui pe celălalt. Nu puteți sluji și lui Dumnezeu, și avuției.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,14,'&lt;pb/&gt;Fariseii, care erau iubitori de bani&lt;f&gt;ⓜ&lt;/f&gt;, ascultau toate aceste &lt;i&gt;cuvinte&lt;/i&gt; și Îl luau în râs.');
INSERT INTO &quot;verses&quot; VALUES (490,16,15,'&lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Voi vă arătați&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;drepți înaintea oamenilor, dar Dumnezeu&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vă cunoaște inimile. Ce&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;i&gt;&lt;J&gt;este&lt;/i&gt; înălțat la oameni &lt;i&gt;este&lt;/i&gt; o urâciune înaintea lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,16,'&lt;J&gt;Legea&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;și Profeții &lt;i&gt;sunt&lt;/i&gt; până la Ioan; de atunci, Împărăția lui Dumnezeu este vestită și toți dau năvală către ea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,17,'&lt;J&gt;Este&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;mai lesne să treacă cerul și pământul decât să cadă o frântură de literă din Lege.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,18,'&lt;J&gt;Oricine&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;își lasă soția și se căsătorește cu alta săvârșește adulter și cine se căsătorește cu cea lăsată de bărbat săvârșește adulter.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,19,'&lt;pb/&gt;&lt;J&gt;Era &lt;i&gt;odată&lt;/i&gt; un om bogat, care se îmbrăca în purpură și in subțire și petrecea în fiecare zi cu mare fast.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,20,'&lt;J&gt;Iar un sărac numit Lazăr zăcea la poarta lui, plin de bube,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,21,'&lt;J&gt;și ar fi dorit să se sature cu [resturile] ce cădeau de la masa bogatului. Ba mai veneau și câinii să-i lingă bubele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,22,'&lt;J&gt;S-a întâmplat că săracul a murit și a fost dus de îngeri în sânul lui Avraam. A murit și bogatul și a fost îngropat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,23,'&lt;J&gt;Pe când era el în Locuința Morților, în chinuri, și-a ridicat privirile și l-a văzut de departe pe Avraam, iar pe Lazăr în sânul lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,24,'&lt;J&gt;Atunci a strigat: «Părinte Avraam, fie-ți milă de mine și trimite-l pe Lazăr să-și înmoaie vârful degetului în apă și să-mi răcorească&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;limba, căci&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;grozav mă chinuiesc în văpaia asta!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,25,'&lt;J&gt;Avraam i-a răspuns: «Fiule, adu-ți&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;aminte că în viață tu ai avut parte de lucrurile bune, iar Lazăr de cele rele; acum, aici, el este mângâiat, iar tu ești chinuit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,26,'&lt;J&gt;Pe lângă toate acestea, între noi și voi este o prăpastie mare, pentru ca cei ce ar vrea să meargă de aici la voi să nu poată, și nici de acolo la noi să nu treacă.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,27,'&lt;i&gt;&lt;J&gt;Bogatul&lt;/i&gt; a zis: «Atunci, părinte [Avraam], te rog să-l trimiți în casa tatălui meu,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,28,'&lt;J&gt;unde am cinci frați, ca să-i avertizeze și să nu ajungă și ei în acest loc de chin.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,29,'&lt;J&gt;Avraam a răspuns: «Îi au&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;pe Moise și pe profeți; să asculte de ei!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,30,'&lt;J&gt;«Nu, părinte Avraam», a zis el, «ci, dacă va merge la ei cineva din morți, se vor pocăi.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,16,31,'&lt;i&gt;&lt;J&gt;Avraam&lt;/i&gt; i-a răspuns: «Dacă nu ascultă de Moise și de profeți, nu vor fi convinși nici dacă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;ar învia cineva din morți.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; le-a zis ucenicilor Săi: &lt;J&gt;„Este&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;cu neputință să nu vină prilejuri de păcătuire, dar vai de cel prin care vin!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,2,'&lt;J&gt;Ar fi mai bine pentru el să i se lege de gât o piatră de moară și să fie aruncat în mare decât să-l facă pe unul dintre acești micuți să păcătuiască.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,3,'&lt;J&gt;Luați seama la voi înșivă!&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;fratele tău păcătuiește [împotriva ta], mustră-l&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt;&lt;J&gt;, iar dacă se pocăiește, iartă-l!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,4,'&lt;J&gt;Și chiar dacă păcătuiește împotriva ta de șapte ori pe zi și de șapte ori [pe zi] se întoarce la tine și zice: «Mă căiesc», iartă-l!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,5,'&lt;pb/&gt;Apostolii I-au zis Domnului: „Mărește-ne credința!”');
INSERT INTO &quot;verses&quot; VALUES (490,17,6,'Și&lt;f&gt;ⓑ&lt;/f&gt; Domnul a zis: &lt;J&gt;„Dacă ați avea credință cât un grăunte de muștar, ați zice acestui dud: «Dezrădăcinează-te și sădește-te în mare» și v-ar asculta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,7,'&lt;pb/&gt;&lt;J&gt;Care din voi, dacă are un rob ce ară sau paște oile, îi va zice, când &lt;i&gt;acesta&lt;/i&gt; vine de la câmp: «Vino degrabă și așază-te la masă»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,8,'&lt;J&gt;Nu-i va zice oare: «Pregătește-mi&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;cina, încinge-te să-mi slujești cât timp voi mânca și voi bea eu; după aceea, vei mânca și vei bea și tu»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,9,'&lt;J&gt;Îi va mulțumi el robului pentru că a făcut ce i se poruncise? [Nu cred.]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,10,'&lt;J&gt;La fel și voi, când veți face tot ce vi s-a poruncit, să ziceți: «Suntem robi&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;nevrednici;&lt;/J&gt;&lt;f&gt;[60]&lt;/f&gt; &lt;J&gt;am făcut ce eram datori să facem.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,11,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; mergea&lt;f&gt;ⓔ&lt;/f&gt; spre Ierusalim și a trecut prin &lt;i&gt;ținutul dintre&lt;/i&gt; Samaria și Galileea.');
INSERT INTO &quot;verses&quot; VALUES (490,17,12,'Pe când intra într-un sat, L-au întâmpinat zece leproși. Ei&lt;f&gt;ⓕ&lt;/f&gt; s-au oprit departe');
INSERT INTO &quot;verses&quot; VALUES (490,17,13,'și au strigat, zicând: „Isuse, stăpâne, ai milă de noi!”');
INSERT INTO &quot;verses&quot; VALUES (490,17,14,'Văzându-i, &lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Mergeți&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;și arătați-vă preoților!”&lt;/J&gt; Și pe când se duceau ei, au fost curățiți.');
INSERT INTO &quot;verses&quot; VALUES (490,17,15,'Unul dintre ei, când s-a văzut vindecat, s-a întors, slăvindu-L pe Dumnezeu cu glas tare.');
INSERT INTO &quot;verses&quot; VALUES (490,17,16,'A căzut la picioarele lui &lt;i&gt;Isus&lt;/i&gt; și I-a mulțumit. Era samaritean.');
INSERT INTO &quot;verses&quot; VALUES (490,17,17,'Isus a zis: &lt;J&gt;„N-au fost curățiți toți zece? Ceilalți nouă unde &lt;i&gt;sunt&lt;/i&gt;?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,18,'&lt;J&gt;Nu s-a găsit să se întoarcă și să dea slavă lui Dumnezeu decât străinul acesta?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,19,'Apoi&lt;f&gt;ⓗ&lt;/f&gt; i-a zis: &lt;J&gt;„Ridică-te și du-te; credința ta te-a mântuit!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,20,'&lt;pb/&gt;Fiind întrebat de farisei când va veni Împărăția lui Dumnezeu, &lt;i&gt;Isus&lt;/i&gt; le-a răspuns: &lt;J&gt;„Împărăția lui Dumnezeu nu vine cu &lt;i&gt;semne&lt;/i&gt; bătătoare la ochi.&lt;/J&gt;&lt;f&gt;[61]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,21,'&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;se va zice: «Iat-o aici» sau «[Iat-o] acolo». Căci Împărăția&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;lui Dumnezeu este între voi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,22,'&lt;pb/&gt;Apoi le-a spus ucenicilor: &lt;J&gt;„Vor&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;veni zile când veți dori să vedeți una dintre zilele Fiului Omului, și nu veți vedea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,23,'&lt;J&gt;Vă&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;vor zice: «Iată-l acolo», «Iată-l aici». Să nu mergeți și să nu-i urmați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,24,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt;&lt;J&gt;, după cum fulgerul strălucește și luminează cerul de la un capăt la altul, așa va fi și Fiul Omului în ziua Sa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,25,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;mai întâi trebuie să sufere mult și să fie respins de generația aceasta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,26,'&lt;J&gt;Cum&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;a fost în zilele lui Noe, la fel va fi și în zilele Fiului Omului:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,27,'&lt;i&gt;&lt;J&gt;atunci&lt;/i&gt; mâncau, beau, se însurau și se măritau până în ziua când a intrat Noe în arcă, a venit potopul și i-a nimicit pe toți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,28,'&lt;J&gt;Cum&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;a fost în zilele lui Lot — &lt;i&gt;când&lt;/i&gt; mâncau, beau, cumpărau, vindeau, sădeau și zideau,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,29,'&lt;J&gt;dar&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt;&lt;J&gt;, în ziua când a ieșit Lot din Sodoma, a plouat din cer foc și pucioasă și i-a nimicit pe toți —&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,30,'&lt;J&gt;la fel va fi și în ziua când Se&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;va arăta Fiul Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,31,'&lt;J&gt;În acea zi, cine&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;va fi pe acoperiș și &lt;i&gt;își va avea&lt;/i&gt; lucrurile în casă, să nu coboare să le ia! Cine va fi pe câmp, tot la fel, să nu se întoarcă înapoi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,32,'&lt;J&gt;Amintiți-vă&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;de soția lui Lot!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,33,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;va căuta să-și scape viața o va pierde și cine o va pierde o va păstra.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,34,'&lt;J&gt;Vă spun&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt;&lt;J&gt;: în noaptea aceea, din doi care vor fi într-un pat, unul va fi luat și celălalt va fi lăsat;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,35,'&lt;J&gt;din două care vor măcina împreună, una va fi luată și cealaltă va fi lăsată; [&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,36,'&lt;J&gt;din doi care vor fi la câmp, unul va fi luat și celălalt va fi lăsat.]”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,17,37,'Ucenicii L-au întrebat: „Unde, Doamne?” Iar El le-a răspuns: &lt;J&gt;„Unde&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;va fi trupul, acolo se vor aduna și vulturii.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,1,'&lt;pb/&gt;Isus le-a spus o pildă despre &lt;i&gt;faptul&lt;/i&gt; că trebuie să se roage&lt;f&gt;ⓧ&lt;/f&gt; mereu și să nu renunțe.');
INSERT INTO &quot;verses&quot; VALUES (490,18,2,'El le-a zis: &lt;J&gt;„Într-o cetate era un judecător care de Dumnezeu nu se temea, iar de oameni nu se rușina.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,3,'&lt;J&gt;În cetatea aceea era și o văduvă, care tot venea la el și-i zicea: «Fă-mi dreptate față de potrivnicul meu!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,4,'&lt;J&gt;O vreme el nu a vrut, dar, în cele din urmă, și-a zis în sinea lui: «Cu toate că de Dumnezeu nu mă tem, iar de oameni nu mă rușinez,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,5,'&lt;J&gt;totuși, pentru că&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;văduva aceasta mă tot supără, îi voi face dreptate, ca să nu mă necăjească la nesfârșit.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,6,'Domnul a adăugat: &lt;J&gt;„Ați auzit ce zice judecătorul nedrept.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,7,'&lt;J&gt;Și Dumnezeu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;nu va face &lt;i&gt;oare&lt;/i&gt; dreptate aleșilor Lui, care strigă zi și noapte către El, deși zăbovește față de ei?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,8,'&lt;J&gt;Vă spun că degrabă&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;le va face dreptate. Dar când va veni Fiul Omului, va găsi oare credință pe pământ?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,9,'&lt;pb/&gt;Pentru unii, care&lt;f&gt;ⓑ&lt;/f&gt; credeau despre ei înșiși că sunt drepți și-i disprețuiau pe ceilalți, a rostit pilda aceasta:');
INSERT INTO &quot;verses&quot; VALUES (490,18,10,'&lt;J&gt;„Doi oameni s-au suit la Templu să se roage; unul era fariseu, iar celălalt, vameș.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,11,'&lt;J&gt;Fariseul stătea&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;în picioare și se ruga astfel în sinea lui: «Dumnezeule&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt;&lt;J&gt;, Îți mulțumesc că nu sunt ca ceilalți oameni: hapsân, nedrept, adulter, sau chiar ca vameșul acesta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,12,'&lt;J&gt;Eu postesc de două ori pe săptămână și dau zeciuială din tot ce câștig.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,13,'&lt;J&gt;Însă vameșul stătea la o oarecare depărtare și nu îndrăznea nici măcar să-și ridice ochii spre cer, ci se bătea în piept&lt;/J&gt;&lt;f&gt;[62]&lt;/f&gt;&lt;J&gt;, zicând: «Dumnezeule, ai milă de mine, păcătosul!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,14,'&lt;J&gt;Vă spun că &lt;i&gt;omul&lt;/i&gt; acesta s-a coborât acasă îndreptățit, nu celălalt. Căci&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;oricine se înalță va fi smerit, și oricine se smerește va fi înălțat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,15,'&lt;pb/&gt;I-au&lt;f&gt;ⓕ&lt;/f&gt; adus și copilași, ca să Se atingă de ei. Ucenicii, văzând &lt;i&gt;aceasta&lt;/i&gt;, îi certau.');
INSERT INTO &quot;verses&quot; VALUES (490,18,16,'Dar Isus i-a chemat, zicând: &lt;J&gt;„Lăsați copiii să vină la Mine și nu-i opriți, căci Împărăția lui Dumnezeu este a&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;celor ca ei!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,17,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;vă spun: cine nu va primi Împărăția lui Dumnezeu ca un copil nu va intra în ea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,18,'&lt;pb/&gt;Un&lt;f&gt;ⓘ&lt;/f&gt; anume conducător L-a întrebat &lt;i&gt;pe Isus&lt;/i&gt;, zicând: „Bunule Învățător, ce să fac ca să moștenesc viața veșnică?”');
INSERT INTO &quot;verses&quot; VALUES (490,18,19,'&lt;J&gt;„De ce Mă numești «bun»?”&lt;/J&gt;, i-a zis Isus. &lt;J&gt;„Nimeni nu este bun decât Unul singur: Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,20,'&lt;J&gt;Știi poruncile: &lt;e&gt;«Să&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;e&gt;&lt;J&gt;nu săvârșești adulter, să nu ucizi, să nu furi, să nu depui mărturie mincinoasă, să-i cinstești&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;e&gt;&lt;J&gt;pe tatăl tău și pe mama ta.»&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[63]&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,21,'El a zis: „Pe toate acestea le-am păzit din tinerețea [mea].”');
INSERT INTO &quot;verses&quot; VALUES (490,18,22,'Când a auzit, Isus i-a răspuns: &lt;J&gt;„Un lucru îți mai lipsește: vinde&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;tot ce ai și dă-le săracilor, și vei avea o comoară în ceruri! Apoi vino și urmează-Mă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,23,'Când a auzit el aceste &lt;i&gt;cuvinte&lt;/i&gt;, s-a întristat nepus, căci era foarte bogat.');
INSERT INTO &quot;verses&quot; VALUES (490,18,24,'&lt;pb/&gt;Când l-a văzut Isus atât de trist, a zis: &lt;J&gt;„Cât&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;de greu vor intra în Împărăția lui Dumnezeu cei ce au averi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,25,'&lt;J&gt;Fiindcă mai ușor este să treacă o cămilă prin urechea acului decât să intre un om bogat în Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,26,'Cei ce-L ascultau au zis: „Atunci, cine poate să fie mântuit?”');
INSERT INTO &quot;verses&quot; VALUES (490,18,27,'&lt;i&gt;Isus&lt;/i&gt; a răspuns: &lt;J&gt;„Ce&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;e cu neputință la oameni &lt;i&gt;este&lt;/i&gt; cu putință la Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,28,'Atunci&lt;f&gt;ⓞ&lt;/f&gt;, Petru a zis: „Iată, noi am lăsat totul&lt;f&gt;[64]&lt;/f&gt; și Te-am urmat!”');
INSERT INTO &quot;verses&quot; VALUES (490,18,29,'Dar &lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Adevărat vă spun că nu este nimeni&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;care să fi lăsat casă, soție, frați, părinți sau copii pentru Împărăția lui Dumnezeu&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,30,'&lt;J&gt;și să&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;nu primească mult mai mult în veacul acesta, iar în veacul care vine, viața veșnică.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,31,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; i-a luat&lt;f&gt;ⓡ&lt;/f&gt; deoparte pe cei doisprezece și le-a zis: &lt;J&gt;„Iată, urcăm la Ierusalim și se va împlini tot&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;ce a fost scris prin profeți despre Fiul Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,32,'&lt;J&gt;Va&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;fi dat în mâinile păgânilor și va fi batjocorit, ocărât și scuipat,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,33,'&lt;J&gt;iar după ce-L vor biciui, Îl vor omorî; dar a treia zi va învia.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,34,'Ei&lt;f&gt;ⓤ&lt;/f&gt; n-au înțeles nimic, căci lucrul acesta era ascuns de ei și nu pricepeau cele spuse.');
INSERT INTO &quot;verses&quot; VALUES (490,18,35,'&lt;pb/&gt;Pe&lt;f&gt;ⓥ&lt;/f&gt; când Se apropia &lt;i&gt;Isus&lt;/i&gt; de Ierihon, un orb ședea lângă drum și cerșea.');
INSERT INTO &quot;verses&quot; VALUES (490,18,36,'Auzind mulțimea trecând, a întrebat ce este.');
INSERT INTO &quot;verses&quot; VALUES (490,18,37,'I-au dat de veste că trece Isus Nazarineanul.');
INSERT INTO &quot;verses&quot; VALUES (490,18,38,'Și el a strigat, zicând: „Isuse, Fiul lui David, ai milă de mine!”');
INSERT INTO &quot;verses&quot; VALUES (490,18,39,'Cei ce mergeau înainte îl certau să tacă, dar el și mai tare striga: „Fiul lui David, ai milă de mine!”');
INSERT INTO &quot;verses&quot; VALUES (490,18,40,'Isus S-a oprit și a poruncit să-l aducă la El. Când s-a apropiat, l-a întrebat:');
INSERT INTO &quot;verses&quot; VALUES (490,18,41,'&lt;J&gt;„Ce vrei să fac pentru tine?”&lt;/J&gt; „Doamne”, a răspuns el, „să-mi recapăt vederea!”');
INSERT INTO &quot;verses&quot; VALUES (490,18,42,'Și Isus i-a zis: &lt;J&gt;„Recapătă-ți vederea! Credința&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;ta te-a mântuit.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,18,43,'Îndată, el și-a recăpătat vederea și a mers&lt;f&gt;ⓧ&lt;/f&gt; după &lt;i&gt;Isus&lt;/i&gt;, slăvindu-L pe Dumnezeu. Tot poporul, văzând &lt;i&gt;aceasta&lt;/i&gt;, I-a dat laudă lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,19,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; a intrat în Ierihon și străbătea &lt;i&gt;cetatea&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,19,2,'Și iată că &lt;i&gt;acolo&lt;/i&gt; era un om bogat, pe nume Zacheu, mai-marele vameșilor.');
INSERT INTO &quot;verses&quot; VALUES (490,19,3,'El căuta să vadă care este Isus, dar nu putea de mulțime, căci era mic de statură.');
INSERT INTO &quot;verses&quot; VALUES (490,19,4,'A alergat înainte și s-a suit într-un sicomor ca să-L vadă, fiindcă pe acolo avea să treacă.');
INSERT INTO &quot;verses&quot; VALUES (490,19,5,'Isus, când a ajuns la locul acela, Și-a ridicat ochii și i-a zis: &lt;J&gt;„Zacheu, coboară iute, căci astăzi trebuie să rămân în casa ta!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,6,'&lt;i&gt;Zacheu&lt;/i&gt; a coborât repede și L-a primit cu bucurie.');
INSERT INTO &quot;verses&quot; VALUES (490,19,7,'Când au văzut, toți au început să cârtească, zicând: „A&lt;f&gt;ⓨ&lt;/f&gt; intrat să găzduiască la un om păcătos!”');
INSERT INTO &quot;verses&quot; VALUES (490,19,8,'Dar Zacheu a stat în picioare și I-a zis Domnului: „Iată, Doamne, jumătate din avuția mea o dau săracilor și, dacă am înșelat&lt;f&gt;ⓩ&lt;/f&gt; pe cineva cu ceva, îi dau înapoi&lt;f&gt;ⓐ&lt;/f&gt; împătrit.”');
INSERT INTO &quot;verses&quot; VALUES (490,19,9,'Isus a zis despre el: &lt;J&gt;„Astăzi a ajuns mântuirea în această casă, căci și&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;el este un fiu&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;al lui Avraam.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,10,'&lt;J&gt;Pentru că Fiul Omului&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;a venit să caute și să mântuiască ce era pierdut.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,11,'&lt;pb/&gt;După ce au ascultat ei aceste &lt;i&gt;lucruri&lt;/i&gt;, &lt;i&gt;Isus&lt;/i&gt; a mai spus o pildă, pentru că era aproape de Ierusalim, iar ei&lt;f&gt;ⓔ&lt;/f&gt; credeau că Împărăția lui Dumnezeu avea să se arate îndată.');
INSERT INTO &quot;verses&quot; VALUES (490,19,12,'El&lt;f&gt;ⓕ&lt;/f&gt; a zis: &lt;J&gt;„Un om de neam ales a plecat într-o țară îndepărtată ca să primească domnia și apoi să se întoarcă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,13,'&lt;J&gt;A chemat zece dintre robii săi, le-a dat zece mine&lt;/J&gt;&lt;f&gt;[65]&lt;/f&gt; &lt;J&gt;și le-a spus: «Faceți negustorie până mă întorc!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,14,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;cetățenii lui îl urau și au trimis după el o solie, zicând: «Nu vrem ca acesta să domnească peste noi!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,15,'&lt;J&gt;Când s-a întors, după ce luase domnia, a poruncit să fie chemați robii aceia cărora le dăduse banii, ca să vadă cât au câștigat cu negustoria.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,16,'&lt;J&gt;Cel dintâi s-a înfățișat și i-a zis: «Stăpâne, mina ta a făcut zece mine.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,17,'&lt;J&gt;El i-a zis: «Bine, rob bun! Fiindcă ai fost credincios&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;în puțin, primește stăpânirea a zece cetăți!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,18,'&lt;J&gt;Al doilea a venit și a zis: «Stăpâne, mina ta a făcut cinci mine.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,19,'&lt;J&gt;El i-a zis și acestuia: «Tu vei fi peste cinci cetăți!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,20,'&lt;J&gt;Altul a venit și a zis: «Stăpâne, iată-ți mina! Am păstrat-o într-un ștergar,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,21,'&lt;J&gt;căci m-am temut de tine&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt;&lt;J&gt;, fiindcă ești om aspru; iei ce n-ai pus și seceri ce n-ai semănat.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,22,'&lt;i&gt;&lt;J&gt;Stăpânul&lt;/i&gt; i-a zis: «Din&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;vorbele tale te voi judeca, rob rău! Ai știut&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;că sunt om aspru, care ia ce n-a pus și seceră ce n-a semănat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,23,'&lt;J&gt;Atunci, de ce nu mi-ai dat banii la un zaraf? La întoarcere, i-aș fi luat cu dobândă.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,24,'&lt;J&gt;Apoi le-a zis celor ce erau de față: «Luați-i mina și dați-i-o celui ce are zece mine!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,25,'&lt;J&gt;«Stăpâne», i-au zis ei, «el are &lt;i&gt;deja&lt;/i&gt; zece mine.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,26,'&lt;J&gt;«Vă spun&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt;&lt;J&gt;: oricui are i se va da, dar celui ce nu are i se va lua și ce are.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,27,'&lt;J&gt;Cât despre vrăjmașii mei, care n-au vrut să domnesc peste ei, aduceți-i aici și ucideți-i în fața mea!»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,28,'&lt;pb/&gt;După ce a spus aceste &lt;i&gt;cuvinte&lt;/i&gt;, &lt;i&gt;Isus&lt;/i&gt; a mers&lt;f&gt;ⓜ&lt;/f&gt; înainte, urcând spre Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (490,19,29,'Când&lt;f&gt;ⓝ&lt;/f&gt; S-a apropiat de Betfaghe și de Betania, lângă muntele numit al Măslinilor, a trimis doi ucenici,');
INSERT INTO &quot;verses&quot; VALUES (490,19,30,'zicând: &lt;J&gt;„Mergeți în satul dinaintea &lt;i&gt;voastră&lt;/i&gt; și, când veți intra în el, veți găsi legat un măgăruș pe care niciun om n-a încălecat vreodată; dezlegați-l și aduceți-l la Mine!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,31,'&lt;J&gt;Dacă vă va întreba cineva de ce-l dezlegați, să-i spuneți: «Domnul are nevoie de el.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,32,'Cei trimiși s-au dus și au găsit așa cum li se spusese.');
INSERT INTO &quot;verses&quot; VALUES (490,19,33,'Pe când dezlegau măgărușul, stăpânii lui le-au zis: „De ce dezlegați măgărușul?”');
INSERT INTO &quot;verses&quot; VALUES (490,19,34,'Ei au răspuns: „Domnul are nevoie de el.”');
INSERT INTO &quot;verses&quot; VALUES (490,19,35,'Au adus măgărușul la Isus, și-au aruncat&lt;f&gt;ⓞ&lt;/f&gt; hainele pe el și L-au așezat pe Isus deasupra.');
INSERT INTO &quot;verses&quot; VALUES (490,19,36,'Când&lt;f&gt;ⓟ&lt;/f&gt; trecea El, &lt;i&gt;oamenii&lt;/i&gt; își așterneau hainele pe drum.');
INSERT INTO &quot;verses&quot; VALUES (490,19,37,'&lt;pb/&gt;Apropiindu-se de povârnișul Muntelui Măslinilor, toată mulțimea ucenicilor, cuprinsă de bucurie, a început să-L laude pe Dumnezeu cu glas puternic pentru toate minunile pe care le văzuseră.');
INSERT INTO &quot;verses&quot; VALUES (490,19,38,'Ei ziceau: &lt;pb/&gt;&lt;t&gt;„&lt;e&gt;Binecuvântat&lt;/e&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;este&lt;/e&gt; Regele &lt;e&gt;ce vine&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;în Numele Domnului!&lt;/e&gt; &lt;f&gt;[66]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;Pace&lt;f&gt;ⓡ&lt;/f&gt; în cer&lt;/t&gt; &lt;t&gt;și slavă în înălțimi!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,39,'&lt;pb/&gt;Unii farisei din mulțime I-au zis: „Învățătorule, ceartă-ți ucenicii!”');
INSERT INTO &quot;verses&quot; VALUES (490,19,40,'Și El le-a răspuns: &lt;J&gt;„Vă spun că, dacă vor tăcea ei, pietrele&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;vor striga!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,41,'&lt;pb/&gt;Când S-a apropiat și a văzut cetatea, &lt;i&gt;Isus&lt;/i&gt; a&lt;f&gt;ⓣ&lt;/f&gt; plâns pentru ea,');
INSERT INTO &quot;verses&quot; VALUES (490,19,42,'zicând: &lt;J&gt;„Dacă ai fi cunoscut, măcar în această zi, lucrurile ce-ți &lt;i&gt;pot aduce&lt;/i&gt; pacea! Dar acum ele sunt ascunse de ochii tăi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,43,'&lt;J&gt;Vor veni peste tine zile când dușmanii te vor înconjura&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;cu întărituri, te vor împresura și te vor strânge din toate părțile.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,44,'&lt;J&gt;Te&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;vor face una cu pământul, pe tine și pe copiii tăi din mijlocul tău, și nu&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;vor lăsa în tine piatră peste piatră, fiindcă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;n-ai cunoscut vremea cercetării tale!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,45,'&lt;pb/&gt;Când&lt;f&gt;ⓨ&lt;/f&gt; a intrat în Templu, &lt;i&gt;Isus&lt;/i&gt; a început să-i scoată afară pe cei ce vindeau [și cumpărau în el],');
INSERT INTO &quot;verses&quot; VALUES (490,19,46,'zicându-le: &lt;J&gt;„Este scris:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;«Casa Mea va fi o casă de rugăciune»&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[67]&lt;/f&gt; &lt;J&gt;,&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;dar voi ați făcut din ea &lt;e&gt;o peșteră de tâlhari&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[68]&lt;/f&gt; &lt;J&gt;.”&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,19,47,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; dădea învățătură în fiecare zi în Templu. Preoții de seamă&lt;f&gt;ⓩ&lt;/f&gt;, cărturarii și mai-marii poporului căutau să-L omoare,');
INSERT INTO &quot;verses&quot; VALUES (490,19,48,'dar nu știau cum să facă, pentru că poporul se ținea după El ca să-L asculte.');
INSERT INTO &quot;verses&quot; VALUES (490,20,1,'&lt;pb/&gt;Într-una din [acele]&lt;f&gt;ⓐ&lt;/f&gt; zile, pe când dădea &lt;i&gt;Isus&lt;/i&gt; învățătură poporului și vestea Evanghelia în Templu, au venit preoții de seamă și cărturarii împreună cu bătrânii');
INSERT INTO &quot;verses&quot; VALUES (490,20,2,'și I-au zis: „Spune-ne, cu&lt;f&gt;ⓑ&lt;/f&gt; ce autoritate faci tu aceste lucruri sau cine ți-a dat această autoritate?”');
INSERT INTO &quot;verses&quot; VALUES (490,20,3,'Drept răspuns, El le-a zis: &lt;J&gt;„Vă voi întreba și eu un lucru. Spuneți-Mi:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,4,'&lt;J&gt;botezul lui Ioan era din cer sau de la oameni?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,5,'Ei însă vorbeau între ei, zicând: „Dacă spunem: «Din cer», va zice: «Atunci, de ce n-ați crezut în el?»');
INSERT INTO &quot;verses&quot; VALUES (490,20,6,'Și dacă vom spune: «De la oameni», tot poporul ne va ucide cu pietre, căci&lt;f&gt;ⓒ&lt;/f&gt; &lt;i&gt;oamenii&lt;/i&gt; sunt convinși că Ioan a fost profet.”');
INSERT INTO &quot;verses&quot; VALUES (490,20,7,'Atunci au răspuns că nu știu de unde.');
INSERT INTO &quot;verses&quot; VALUES (490,20,8,'Isus le-a zis: &lt;J&gt;„Nici Eu nu vă spun cu ce autoritate fac aceste lucruri.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,9,'&lt;pb/&gt;Apoi a început să spună poporului pilda aceasta: &lt;J&gt;„Un&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;om &lt;e&gt;a sădit o vie&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[69]&lt;/f&gt;&lt;J&gt;, a arendat-o unor lucrători și a plecat pentru un timp îndelungat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,10,'&lt;J&gt;La vremea potrivită, a trimis la lucrători un rob, ca să-i dea din rodul viei. Dar lucrătorii, după ce l-au bătut, l-au trimis &lt;i&gt;înapoi&lt;/i&gt; cu mâinile goale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,11,'&lt;J&gt;A trimis un alt rob; ei l-au bătut și pe acela, l-au batjocorit și l-au trimis &lt;i&gt;înapoi&lt;/i&gt; cu mâinile goale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,12,'&lt;J&gt;A mai trimis un al treilea rob; ei l-au rănit și pe acela și l-au scos afară.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,13,'&lt;J&gt;Stăpânul viei a zis: «Ce să fac? Îl voi trimite pe fiul meu preaiubit; poate că de el se vor rușina.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,14,'&lt;J&gt;Dar lucrătorii, văzându-l, au vorbit între ei, zicând: «Iată-l pe moștenitor. Să-l omorâm, ca moștenirea să fie a noastră!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,15,'&lt;J&gt;Și, după ce l-au scos afară din vie, l-au omorât. Acum, ce le va face stăpânul viei?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,16,'&lt;J&gt;Va veni și-i va omorî pe lucrătorii aceia, iar via o va da altora.”&lt;/J&gt; Când au auzit ei, au spus: „Nicidecum!”');
INSERT INTO &quot;verses&quot; VALUES (490,20,17,'Dar Isus i-a privit țintă și a zis: &lt;J&gt;„Ce înseamnă &lt;i&gt;cuvintele&lt;/i&gt; care au fost scrise:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;&lt;J&gt;Piatra&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓔ&lt;/f&gt; &lt;e&gt;&lt;J&gt;pe care au lepădat-o zidarii&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;&lt;J&gt;a ajuns în capul unghiului&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[70]&lt;/f&gt; &lt;J&gt;?&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,18,'&lt;pb/&gt;&lt;J&gt;Oricine va cădea peste piatra aceasta va fi zdrobit de ea&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;, iar acela peste care va cădea ea va fi spulberat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,19,'Cărturarii și preoții de seamă căutau să pună mâna pe El chiar în ceasul acela, dar se temeau de popor. Pricepuseră că pentru ei spusese pilda aceasta.');
INSERT INTO &quot;verses&quot; VALUES (490,20,20,'&lt;pb/&gt;Ei Îl pândeau&lt;f&gt;ⓖ&lt;/f&gt; &lt;i&gt;pe Isus&lt;/i&gt; și au trimis niște iscoditori, care se prefăceau că sunt drepți, ca să-L prindă cu vreun cuvânt și să-L dea pe mâna stăpânirii și a dregătorului.');
INSERT INTO &quot;verses&quot; VALUES (490,20,21,'&lt;i&gt;Aceștia&lt;/i&gt; L-au întrebat: „Învățătorule&lt;f&gt;ⓗ&lt;/f&gt;, știm că vorbești și înveți cu dreptate și nu cauți la fața oamenilor, ci cu adevărat îi înveți calea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,20,22,'Este îngăduit să dăm bir cezarului sau nu?”');
INSERT INTO &quot;verses&quot; VALUES (490,20,23,'&lt;i&gt;Isus&lt;/i&gt; le-a cunoscut viclenia și le-a spus: &lt;J&gt;[„Pentru ce Mă ispitiți?]&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,24,'&lt;J&gt;Arătați-Mi un dinar! Chipul și inscripția ale cui sunt?”&lt;/J&gt; „Ale cezarului”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (490,20,25,'El le-a zis: &lt;J&gt;„Atunci dați-i cezarului ce este al cezarului și lui Dumnezeu ce este al lui Dumnezeu!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,26,'Nu L-au putut prinde cu &lt;i&gt;niciun&lt;/i&gt; cuvânt în fața poporului; ci, mirați de răspunsul Lui, au tăcut.');
INSERT INTO &quot;verses&quot; VALUES (490,20,27,'&lt;pb/&gt;Au venit niște&lt;f&gt;ⓘ&lt;/f&gt; saduchei, care&lt;f&gt;ⓙ&lt;/f&gt; zic că nu este înviere, și L-au întrebat:');
INSERT INTO &quot;verses&quot; VALUES (490,20,28,'„Învățătorule, Moise ne-a scris: dacă moare cineva fiind căsătorit, dar fără să aibă copii, fratele lui s-o ia pe soția lui și să-i ridice urmaș fratelui său.&lt;f&gt;[71]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,29,'Erau șapte frați. Primul și-a luat soție și a murit fără copii.');
INSERT INTO &quot;verses&quot; VALUES (490,20,30,'La fel și al doilea: [a luat-o de soție, dar a murit fără copii].');
INSERT INTO &quot;verses&quot; VALUES (490,20,31,'A luat-o și al treilea; toți șapte au murit fără să lase copii.');
INSERT INTO &quot;verses&quot; VALUES (490,20,32,'În cele din urmă, a murit și femeia.');
INSERT INTO &quot;verses&quot; VALUES (490,20,33,'La înviere, a căruia dintre ei va fi femeia? Căci toți șapte au avut-o de soție.”');
INSERT INTO &quot;verses&quot; VALUES (490,20,34,'Isus le-a răspuns: &lt;J&gt;„Fiii acestui veac se însoară și se mărită,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,35,'&lt;J&gt;dar cei ce vor fi găsiți vrednici să aibă parte de veacul viitor și de învierea din morți nu se vor însura, nici nu se vor mărita.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,36,'&lt;J&gt;Nici nu vor putea muri, căci vor fi&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;ca îngerii; fii ai lui Dumnezeu și fii&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;ai învierii vor fi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,37,'&lt;J&gt;Dar că morții învie, și Moise a arătat, &lt;i&gt;în locul&lt;/i&gt; despre rug, când Îl numește pe Domnul &lt;e&gt;Dumnezeul lui Avraam, Dumnezeul lui Isaac și Dumnezeul lui Iacov&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[72]&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,38,'&lt;J&gt;Dumnezeu nu este al celor morți, ci al celor vii, fiindcă pentru El toți&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;sunt vii.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,39,'Unii dintre cărturari au răspuns: „Învățătorule, bine ai vorbit!”');
INSERT INTO &quot;verses&quot; VALUES (490,20,40,'Și nu mai îndrăzneau să-L întrebe nimic.');
INSERT INTO &quot;verses&quot; VALUES (490,20,41,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„Cum&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;se zice că Hristosul este fiul lui David?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,42,'&lt;J&gt;Căci însuși David spune în cartea Psalmilor:&lt;/J&gt; &lt;pb/&gt;&lt;t&gt;&lt;J&gt;&lt;e&gt;Domnul a zis&lt;/e&gt;&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;&lt;J&gt;Domnului meu:&lt;/J&gt;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;«Șezi la dreapta Mea&lt;/e&gt;&lt;/J&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,43,'&lt;t&gt;&lt;J&gt;&lt;e&gt;până voi pune pe vrăjmașii Tăi&lt;/e&gt;&lt;/J&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;reazem picioarelor Tale!»&lt;/e&gt;&lt;/J&gt;&lt;f&gt;[73]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,44,'&lt;pb/&gt;Așadar, David Îl numește Domn; cum este El fiul lui?”');
INSERT INTO &quot;verses&quot; VALUES (490,20,45,'&lt;pb/&gt;Pe când tot poporul asculta, Isus le-a zis&lt;f&gt;ⓟ&lt;/f&gt; ucenicilor Săi:');
INSERT INTO &quot;verses&quot; VALUES (490,20,46,'&lt;J&gt;„Feriți-vă&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;de cărturarii cărora le place să umble în haine lungi și să fie salutați prin piețe&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;, &lt;i&gt;să aibă&lt;/i&gt; scaunele dintâi în sinagogi și locurile de cinste la ospețe;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,20,47,'&lt;J&gt;casele văduvelor&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;le mănâncă și fac rugăciuni lungi de ochii lumii. Aceștia vor primi o mai mare osândă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,1,'&lt;pb/&gt;&lt;i&gt;Isus&lt;/i&gt; Și-a ridicat ochii și i-a&lt;f&gt;ⓣ&lt;/f&gt; văzut pe niște bogați aruncându-și darurile în vistierie.');
INSERT INTO &quot;verses&quot; VALUES (490,21,2,'A văzut și o văduvă săracă aruncând acolo doi bănuți');
INSERT INTO &quot;verses&quot; VALUES (490,21,3,'Și a zis: &lt;J&gt;„Adevărat vă spun că&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;această văduvă săracă a dat mai mult decât toți;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,4,'&lt;J&gt;căci toți aceștia au aruncat în &lt;i&gt;lada cu&lt;/i&gt; daruri din prisosul lor, dar ea, din sărăcia ei, a aruncat tot ce avea pentru a-și duce traiul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,5,'&lt;pb/&gt;Pe când vorbeau unii&lt;f&gt;ⓥ&lt;/f&gt; despre Templu, că era împodobit cu pietre frumoase și daruri, &lt;i&gt;Isus&lt;/i&gt; a zis:');
INSERT INTO &quot;verses&quot; VALUES (490,21,6,'&lt;J&gt;„Cât despre cele pe care le priviți, vor veni zile când nu va&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;rămâne piatră peste piatră care să nu fie dărâmată.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,7,'„Învățătorule”, L-au întrebat ei, „când se vor întâmpla aceste lucruri și care este semnul că vor avea loc?”');
INSERT INTO &quot;verses&quot; VALUES (490,21,8,'&lt;i&gt;Isus&lt;/i&gt; a răspuns: &lt;J&gt;„Luați seama&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;să nu fiți înșelați! Vor veni mulți în Numele Meu, zicând: «Eu sunt» și «Vremea s-a apropiat». Să nu mergeți după ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,9,'&lt;J&gt;Când veți auzi de războaie și răscoale, să nu vă înspăimântați. Trebuie să se întâmple aceste lucruri mai întâi, dar sfârșitul nu &lt;i&gt;va fi&lt;/i&gt; îndată.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,10,'&lt;pb/&gt;El le-a mai zis: &lt;J&gt;„Un popor&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;se va ridica împotriva altui popor și o împărăție — împotriva altei împărății.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,11,'&lt;J&gt;Vor fi mari cutremure de pământ și, pe alocuri, foamete și ciume; vor fi lucruri înfricoșătoare și semne mari pe cer.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,12,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;înainte de toate acestea, vor pune mâna pe voi și vă vor prigoni, &lt;i&gt;vă&lt;/i&gt; vor da pe mâna sinagogilor și&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;&lt;J&gt;vă vor târî&lt;/i&gt; în temnițe, vă vor duce&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;înaintea regilor și a dregătorilor din&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;pricina Numelui Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,13,'&lt;i&gt;&lt;J&gt;Acestea&lt;/i&gt; vi&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;se vor întâmpla ca &lt;i&gt;să fiți&lt;/i&gt; mărturie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,14,'&lt;J&gt;Luați aminte&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt;&lt;J&gt;: să nu vă pregătiți dinainte cuvântul de apărare,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,15,'&lt;J&gt;căci Eu vă voi da un cuvânt și o înțelepciune cărora niciunul dintre vrăjmașii voștri nu le va putea sta împotrivă sau răspunde&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,16,'&lt;J&gt;Veți&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;fi dați în mâinile lor până și de părinții, frații, rudele și prietenii voștri; pe unii dintre voi îi vor omorî&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,17,'&lt;J&gt;Veți&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;fi urâți de toți din pricina Numelui Meu,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,18,'&lt;J&gt;dar&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;niciun fir de păr din cap nu vi se va pierde.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,19,'&lt;J&gt;Prin răbdarea voastră vă veți câștiga sufletele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,20,'&lt;pb/&gt;&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;veți vedea Ierusalimul înconjurat de oști, să știți că pustiirea lui este aproape.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,21,'&lt;J&gt;Atunci, cei din Iudeea să fugă în munți, cei din mijlocul &lt;i&gt;Ierusalimului&lt;/i&gt; să plece, iar cei din împrejurimi să nu intre în el,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,22,'&lt;J&gt;căci acelea vor fi zile de răzbunare, ca să&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;se împlinească tot ce e scris.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,23,'&lt;J&gt;Vai&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;de femeile însărcinate și de cele care alăptează în zilele acelea! Căci va fi un necaz mare pe pământ și urgie împotriva acestui popor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,24,'&lt;J&gt;Vor cădea sub ascuțișul sabiei, vor fi duși în robie la toate neamurile, iar Ierusalimul va fi călcat în picioare de păgâni până&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;se vor împlini vremurile neamurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,25,'&lt;pb/&gt;&lt;J&gt;Vor&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;fi semne în soare, în lună și în stele. Și pe pământ va fi necaz printre neamuri, care vor fi năucite de vuietul mării și al valurilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,26,'&lt;J&gt;Oamenii își vor da sufletul de groază în așteptarea lucrurilor care vor veni asupra lumii, căci&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;puterile cerurilor&lt;/J&gt;&lt;f&gt;[74]&lt;/f&gt; &lt;J&gt;vor fi clătinate.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,27,'&lt;J&gt;Atunci, Îl vor vedea &lt;e&gt;pe Fiul Omului venind pe nor&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[75]&lt;/f&gt; &lt;J&gt;cu putere și slavă mare&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,28,'&lt;J&gt;Când vor începe să se întâmple aceste lucruri, priviți în sus și ridicați-vă capetele, fiindcă răscumpărarea voastră se&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;apropie!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,29,'&lt;pb/&gt;Și&lt;f&gt;ⓢ&lt;/f&gt; le-a spus o pildă: &lt;J&gt;„Uitați-vă la smochin și la toți copacii!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,30,'&lt;J&gt;Când îi vedeți înfrunzind, știți că vara este aproape.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,31,'&lt;J&gt;Tot așa și voi, când veți vedea aceste lucruri împlinindu-se, să știți că Împărăția lui Dumnezeu este aproape.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,32,'&lt;J&gt;Adevărat vă spun că nicidecum nu va trece generația aceasta că se vor și împlini aceste lucruri.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,33,'&lt;J&gt;Cerul&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;și pământul vor trece, dar cuvintele Mele nu vor trece.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,34,'&lt;pb/&gt;&lt;J&gt;Luați seama&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;la voi înșivă: să nu vi se îngreuneze inimile cu îmbuibare și beție sau cu îngrijorările vieții, și astfel ziua aceea să vină pe neașteptate asupra voastră,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,35,'&lt;J&gt;ca un laț&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt;&lt;J&gt;; căci &lt;i&gt;ea&lt;/i&gt; va veni peste toți cei ce locuiesc pe fața întregului pământ.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,36,'&lt;J&gt;Vegheați în toată vremea&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt;&lt;J&gt;, rugându-vă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;să aveți putere să scăpați de toate câte vor veni și să stați&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;în picioare înaintea Fiului Omului!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,21,37,'&lt;pb/&gt;În timpul zilei, &lt;i&gt;Isus&lt;/i&gt; dădea învățătură&lt;f&gt;ⓩ&lt;/f&gt; în Templu, iar&lt;f&gt;ⓐ&lt;/f&gt; nopțile mergea să le petreacă pe muntele numit al Măslinilor.');
INSERT INTO &quot;verses&quot; VALUES (490,21,38,'Tot poporul venea dis-de-dimineață la El, în Templu, ca să-L asculte.');
INSERT INTO &quot;verses&quot; VALUES (490,22,1,'&lt;pb/&gt;Se&lt;f&gt;ⓑ&lt;/f&gt; apropia sărbătoarea Azimelor, numită Paște.');
INSERT INTO &quot;verses&quot; VALUES (490,22,2,'Preoții de seamă și cărturarii &lt;i&gt;tot&lt;/i&gt; căutau&lt;f&gt;ⓒ&lt;/f&gt; cum să-L omoare &lt;i&gt;pe Isus&lt;/i&gt;, căci se temeau de popor.');
INSERT INTO &quot;verses&quot; VALUES (490,22,3,'Satana&lt;f&gt;ⓓ&lt;/f&gt; a intrat în Iuda, zis și Iscariotul, care era din numărul celor doisprezece.');
INSERT INTO &quot;verses&quot; VALUES (490,22,4,'&lt;i&gt;Acesta&lt;/i&gt; s-a dus să se înțeleagă cu preoții de seamă și cu căpeteniile &lt;i&gt;străjerilor Templului&lt;/i&gt; cum să-L dea în mâinile lor.');
INSERT INTO &quot;verses&quot; VALUES (490,22,5,'Ei s-au bucurat și s-au&lt;f&gt;ⓔ&lt;/f&gt; învoit să-i dea bani.');
INSERT INTO &quot;verses&quot; VALUES (490,22,6,'&lt;i&gt;Iuda&lt;/i&gt; a încuviințat și căuta un prilej potrivit să-L dea &lt;i&gt;pe Isus&lt;/i&gt; în mâinile lor fără &lt;i&gt;știrea&lt;/i&gt; mulțimii.');
INSERT INTO &quot;verses&quot; VALUES (490,22,7,'&lt;pb/&gt;A venit ziua&lt;f&gt;ⓕ&lt;/f&gt; Sărbătorii Azimelor, când trebuia jertfit &lt;i&gt;mielul de&lt;/i&gt; Paște.');
INSERT INTO &quot;verses&quot; VALUES (490,22,8,'&lt;i&gt;Isus&lt;/i&gt; i-a trimis pe Petru și pe Ioan, zicând: &lt;J&gt;„Mergeți și pregătiți-ne Paștele, ca să mâncăm!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,9,'„Unde vrei să-l pregătim?”, L-au întrebat ei.');
INSERT INTO &quot;verses&quot; VALUES (490,22,10,'El le-a răspuns: &lt;J&gt;„Iată, când veți intra în cetate, vă va ieși în cale un om care duce un urcior cu apă; mergeți după el în casa în care va intra&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,11,'&lt;J&gt;și spuneți-i stăpânului casei: «Învățătorul îți zice: ‘Unde este odaia în care să mănânc Paștele cu ucenicii Mei?’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,12,'&lt;J&gt;Și vă va arăta o cameră mare, sus, gata așternută; acolo să pregătiți.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,13,'Ei au plecat și au găsit după cum le spusese El. Și au pregătit Paștele.');
INSERT INTO &quot;verses&quot; VALUES (490,22,14,'&lt;pb/&gt;Când&lt;f&gt;ⓖ&lt;/f&gt; a sosit ceasul, &lt;i&gt;Isus&lt;/i&gt; a stat la masă și, împreună cu El, [cei doisprezece] apostoli.');
INSERT INTO &quot;verses&quot; VALUES (490,22,15,'El le-a zis: &lt;J&gt;„Am dorit mult să mănânc Paștele acesta cu voi înainte de patima Mea;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,16,'&lt;J&gt;căci vă spun că nu-l voi mai mânca până&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;când nu se va împlini în Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,17,'Și luând un pahar, a adus mulțumire și a zis: &lt;J&gt;„Luați-l și împărțiți-l între voi;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,18,'&lt;J&gt;căci vă spun&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;că nu voi mai bea de acum din rodul viței până când nu va veni Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,19,'Apoi&lt;f&gt;ⓙ&lt;/f&gt; a luat o pâine, a adus mulțumire, a frânt-o și le-a dat-o, zicând: &lt;J&gt;„Acesta este trupul Meu, care se dă pentru voi. Să faceți aceasta&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;în amintirea Mea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,20,'La fel &lt;i&gt;a făcut&lt;/i&gt; cu paharul, după ce au cinat, zicând: &lt;J&gt;„Acest&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;pahar &lt;i&gt;este&lt;/i&gt; legământul cel nou în sângele Meu, care se varsă pentru voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,21,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt;&lt;J&gt;, iată, mâna celui ce mă vinde &lt;i&gt;este&lt;/i&gt; cu Mine la masă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,22,'&lt;J&gt;Fiul Omului negreșit&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;se duce, după cum&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;este rânduit. Dar vai de omul acela prin care este vândut!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,23,'Și&lt;f&gt;ⓟ&lt;/f&gt; au început să se întrebe unii pe alții cine dintre ei avea să facă lucrul acesta.');
INSERT INTO &quot;verses&quot; VALUES (490,22,24,'&lt;pb/&gt;Între &lt;i&gt;ucenici&lt;/i&gt; s-a&lt;f&gt;ⓠ&lt;/f&gt; iscat și o neînțelegere: care dintre ei să fie socotit mai mare?');
INSERT INTO &quot;verses&quot; VALUES (490,22,25,'&lt;i&gt;Isus&lt;/i&gt; le-a&lt;f&gt;ⓡ&lt;/f&gt; zis: &lt;J&gt;„Regii popoarelor domnesc peste ele și cei ce le stăpânesc sunt numiți binefăcători.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,26,'&lt;J&gt;Voi&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;să nu fiți așa! Ci&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;acela care este mai mare între voi să fie ca cel mai mic, iar cel ce cârmuiește, ca cel ce slujește.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,27,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;cine este mai mare: cel ce stă la masă sau cel ce slujește? Oare nu cine stă la masă? Și totuși, Eu&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;sunt în mijlocul vostru ca cel ce slujește.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,28,'&lt;pb/&gt;&lt;J&gt;Voi sunteți cei care ați rămas totdeauna cu Mine în încercările&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;Mele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,29,'&lt;J&gt;De aceea vă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;pregătesc Împărăția, așa cum Tatăl Meu Mi-a pregătit-o Mie,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,30,'&lt;J&gt;ca&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;să mâncați și să beți la masa Mea, în Împărăția Mea, și să&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;stați pe tronuri pentru a judeca cele douăsprezece seminții ale lui Israel.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,31,'&lt;pb/&gt;[Domnul a zis]: &lt;J&gt;„Simone, Simone, iată, Satana&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;v-a cerut ca să vă cearnă&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;ca pe grâu,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,32,'&lt;J&gt;însă Eu&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;M-am rugat pentru tine ca să nu ți se piardă credința. Iar tu, după ce te&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;vei întoarce, să-i întărești pe frații tăi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,33,'Dar el i-a răspuns: „Doamne, cu Tine sunt gata să merg și în temniță, și la moarte.”');
INSERT INTO &quot;verses&quot; VALUES (490,22,34,'&lt;i&gt;Isus&lt;/i&gt;&lt;f&gt;ⓔ&lt;/f&gt; i-a zis: &lt;J&gt;„Petre, îți spun că nu va cânta astăzi cocoșul, că vei și tăgădui de trei ori că Mă cunoști.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,35,'&lt;pb/&gt;Apoi&lt;f&gt;ⓕ&lt;/f&gt; le-a mai zis: &lt;J&gt;„Când v-am trimis fără pungă, fără traistă și fără sandale, ați dus lipsă de ceva?”&lt;/J&gt; „De nimic”, I-au răspuns ei.');
INSERT INTO &quot;verses&quot; VALUES (490,22,36,'Și El le-a zis: &lt;J&gt;„Acum, dimpotrivă, cine are o pungă s-o ia; la fel și traista; iar cine nu are sabie să-și vândă haina și să-și cumpere &lt;i&gt;una&lt;/i&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,37,'&lt;J&gt;Căci vă spun că trebuie să se împlinească în Mine ceea ce este scris: &lt;e&gt;El&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;&lt;J&gt;a fost socotit în rândul celor nelegiuiți.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[76]&lt;/f&gt; &lt;J&gt;Și cele privitoare la Mine se împlinesc.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,38,'„Doamne”, I-au spus ei, „iată aici două săbii.” El le-a răspuns: &lt;J&gt;„Ajunge!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,39,'&lt;pb/&gt;După ce&lt;f&gt;ⓗ&lt;/f&gt; a ieșit, a mers&lt;f&gt;ⓘ&lt;/f&gt;, ca de obicei, pe Muntele Măslinilor, iar ucenicii L-au urmat.');
INSERT INTO &quot;verses&quot; VALUES (490,22,40,'Când&lt;f&gt;ⓙ&lt;/f&gt; a ajuns acolo, le-a zis: &lt;J&gt;„Rugați-vă să nu ajungeți în ispită&lt;/J&gt;&lt;f&gt;[77]&lt;/f&gt;&lt;J&gt;!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,41,'Apoi&lt;f&gt;ⓚ&lt;/f&gt; S-a depărtat de ei ca la o aruncătură de piatră, a îngenuncheat și a început să Se roage,');
INSERT INTO &quot;verses&quot; VALUES (490,22,42,'zicând: &lt;J&gt;„Tată, dacă voiești, îndepărtează paharul acesta de la Mine! Dar nu&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;voia Mea să se facă, ci a Ta.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,43,'Atunci I s-a arătat un înger&lt;f&gt;ⓜ&lt;/f&gt; din cer, care L-a întărit.');
INSERT INTO &quot;verses&quot; VALUES (490,22,44,'Iar El, &lt;i&gt;cuprins de&lt;/i&gt; agonie&lt;f&gt;ⓝ&lt;/f&gt;, a început să Se roage și mai fierbinte; iar sudoarea I s-a făcut ca niște picături de sânge, care cădeau pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (490,22,45,'După ce S-a ridicat de la rugăciune, a venit la ucenici și i-a găsit adormiți de întristare.');
INSERT INTO &quot;verses&quot; VALUES (490,22,46,'Atunci le-a zis: &lt;J&gt;„De ce dormiți? Ridicați-vă și rugați-vă&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;să nu ajungeți în ispită!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,47,'&lt;pb/&gt;Pe când vorbea El încă, iată&lt;f&gt;ⓟ&lt;/f&gt; că &lt;i&gt;a venit&lt;/i&gt; o mulțime, și cel ce se numea Iuda, unul dintre cei doisprezece, mergea în fruntea ei. El s-a apropiat de Isus ca să-L sărute.');
INSERT INTO &quot;verses&quot; VALUES (490,22,48,'Și Isus i-a zis: &lt;J&gt;„Iuda, cu o sărutare Îl vinzi tu pe Fiul Omului?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,49,'Văzând ce avea să se întâmple, cei ce erau cu &lt;i&gt;Isus&lt;/i&gt; au zis: „Doamne, să lovim cu sabia?”');
INSERT INTO &quot;verses&quot; VALUES (490,22,50,'Și unul&lt;f&gt;ⓠ&lt;/f&gt; dintre ei l-a lovit pe slujitorul marelui-preot și i-a tăiat urechea dreaptă.');
INSERT INTO &quot;verses&quot; VALUES (490,22,51,'Dar Isus a zis: &lt;J&gt;„Încetați! Până aici!”&lt;/J&gt; Și atingându-i &lt;i&gt;aceluia&lt;/i&gt; urechea, l-a vindecat.');
INSERT INTO &quot;verses&quot; VALUES (490,22,52,'Isus&lt;f&gt;ⓡ&lt;/f&gt; le-a zis apoi celor ce veniseră împotriva Lui, preoților de seamă, căpeteniilor &lt;i&gt;străjerilor&lt;/i&gt; Templului și bătrânilor: &lt;J&gt;„Ați ieșit ca după un tâlhar, cu săbii și cu ciomege?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,53,'&lt;J&gt;În toate zilele eram cu voi în Templu, și n-ați pus mâna pe Mine. Dar&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;acesta este ceasul vostru și puterea întunericului.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,54,'&lt;pb/&gt;După ce&lt;f&gt;ⓣ&lt;/f&gt; L-au prins, L-au luat și L-au dus în casa marelui-preot. Petru Îl urma&lt;f&gt;ⓤ&lt;/f&gt; de departe.');
INSERT INTO &quot;verses&quot; VALUES (490,22,55,'&lt;i&gt;Slujitorii&lt;/i&gt; au&lt;f&gt;ⓥ&lt;/f&gt; aprins un foc în mijlocul curții și stăteau jos, iar Petru s-a așezat printre ei.');
INSERT INTO &quot;verses&quot; VALUES (490,22,56,'O slujnică l-a văzut stând lângă foc, l-a privit țintă și a zis: „Și acesta era cu el.”');
INSERT INTO &quot;verses&quot; VALUES (490,22,57,'Dar &lt;i&gt;Petru&lt;/i&gt; a tăgăduit, zicând: „Femeie, nu-L cunosc!”');
INSERT INTO &quot;verses&quot; VALUES (490,22,58,'Puțin&lt;f&gt;ⓦ&lt;/f&gt; mai târziu, l-a văzut altul și a zis: „Și tu ești dintre ei.” Dar Petru a răspuns: „Omule, nu sunt!”');
INSERT INTO &quot;verses&quot; VALUES (490,22,59,'Un ceas&lt;f&gt;ⓧ&lt;/f&gt; mai târziu, altul a întărit &lt;i&gt;acest lucru&lt;/i&gt;, zicând: „Cu adevărat și acesta era cu el, căci este galileean.”');
INSERT INTO &quot;verses&quot; VALUES (490,22,60,'Petru a răspuns: „Omule, nu știu ce spui!” Îndată, pe când el încă vorbea, a cântat cocoșul.');
INSERT INTO &quot;verses&quot; VALUES (490,22,61,'Domnul S-a întors și l-a privit țintă pe Petru. Atunci&lt;f&gt;ⓨ&lt;/f&gt;, Petru și-a amintit de cuvântul pe care i-l spusese Domnul: &lt;J&gt;„Înainte&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;să cânte cocoșul astăzi, te vei lepăda de Mine de trei ori.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,62,'&lt;i&gt;Așa că&lt;/i&gt; a ieșit afară și a plâns cu amar.');
INSERT INTO &quot;verses&quot; VALUES (490,22,63,'&lt;pb/&gt;Oamenii&lt;f&gt;ⓐ&lt;/f&gt; care-L păzeau [pe Isus] Îl batjocoreau și-L băteau.');
INSERT INTO &quot;verses&quot; VALUES (490,22,64,'L-au legat la ochi, [Îl loveau peste față] și-L întrebau: „Profețește, cine te-a lovit?”');
INSERT INTO &quot;verses&quot; VALUES (490,22,65,'Și rosteau împotriva Lui multe alte blasfemii.');
INSERT INTO &quot;verses&quot; VALUES (490,22,66,'&lt;pb/&gt;Când&lt;f&gt;ⓑ&lt;/f&gt; s-a făcut ziuă, bătrânii poporului, preoții de seamă și cărturarii s-au&lt;f&gt;ⓒ&lt;/f&gt; adunat și L-au adus &lt;i&gt;pe Isus&lt;/i&gt; la Sinedriul lor.');
INSERT INTO &quot;verses&quot; VALUES (490,22,67,'Ei I-au zis: „Dacă&lt;f&gt;ⓓ&lt;/f&gt; ești tu Hristosul, spune-ne!” Isus le-a răspuns: &lt;J&gt;„Dacă vă voi spune, nu veți crede.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,68,'&lt;J&gt;Dacă vă voi întreba, nu-Mi veți răspunde, [nici nu-Mi veți da drumul].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,69,'&lt;J&gt;De acum&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt;&lt;J&gt;, Fiul Omului va sta la dreapta puterii lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,70,'Atunci toți au zis: „Așadar, ești tu Fiul lui Dumnezeu?” Și El le-a răspuns: &lt;J&gt;„Voi înșivă spuneți&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;că sunt.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,22,71,'Atunci&lt;f&gt;ⓖ&lt;/f&gt; ei au zis: „Ce nevoie mai avem de mărturie? Noi înșine am auzit-o din gura lui.”');
INSERT INTO &quot;verses&quot; VALUES (490,23,1,'&lt;pb/&gt;S-au ridicat&lt;f&gt;ⓗ&lt;/f&gt; apoi cu toții și L-au dus înaintea lui Pilat.');
INSERT INTO &quot;verses&quot; VALUES (490,23,2,'Au început să-L învinuiască, zicând: „Pe &lt;i&gt;omul&lt;/i&gt; acesta L-am găsit ducând&lt;f&gt;ⓘ&lt;/f&gt; neamul nostru în rătăcire, interzicând&lt;f&gt;ⓙ&lt;/f&gt; plata birului către cezar și zicând că&lt;f&gt;ⓚ&lt;/f&gt; El este Hristosul, Regele.”');
INSERT INTO &quot;verses&quot; VALUES (490,23,3,'Pilat&lt;f&gt;ⓛ&lt;/f&gt; L-a întrebat: „Ești tu regele iudeilor?” Isus i-a răspuns: &lt;J&gt;„Tu însuți o spui.”&lt;/J&gt;&lt;f&gt;[78]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,4,'Pilat le-a zis preoților de seamă și mulțimilor: „Eu nu găsesc nicio&lt;f&gt;ⓜ&lt;/f&gt; vină în omul acesta.”');
INSERT INTO &quot;verses&quot; VALUES (490,23,5,'Dar ei stăruiau și ziceau: „Întărâtă poporul, dând învățătură prin toată Iudeea, începând din Galileea și până aici.”');
INSERT INTO &quot;verses&quot; VALUES (490,23,6,'&lt;pb/&gt;Când a auzit Pilat [de Galileea], a întrebat dacă omul este galileean.');
INSERT INTO &quot;verses&quot; VALUES (490,23,7,'Și aflând că era sub stăpânirea&lt;f&gt;ⓝ&lt;/f&gt; lui Irod, L-a trimis la Irod, care era și el în Ierusalim în acele zile.');
INSERT INTO &quot;verses&quot; VALUES (490,23,8,'Văzându-L pe Isus, Irod s-a bucurat nespus; căci de multă vreme voia&lt;f&gt;ⓞ&lt;/f&gt; să-L vadă, fiindcă&lt;f&gt;ⓟ&lt;/f&gt; auzise despre El și spera să vadă vreun semn săvârșit de El.');
INSERT INTO &quot;verses&quot; VALUES (490,23,9,'I-a pus multe întrebări, dar &lt;i&gt;Isus&lt;/i&gt; nu i-a răspuns nimic.');
INSERT INTO &quot;verses&quot; VALUES (490,23,10,'Preoții de seamă și cărturarii stăteau &lt;i&gt;de față&lt;/i&gt; și-L învinuiau cu înverșunare.');
INSERT INTO &quot;verses&quot; VALUES (490,23,11,'Irod&lt;f&gt;ⓠ&lt;/f&gt; și soldații lui L-au tratat cu dispreț și, după ce și-au bătut joc de El, L-au îmbrăcat cu o haină strălucitoare și &lt;i&gt;Irod&lt;/i&gt; L-a trimis înapoi la Pilat.');
INSERT INTO &quot;verses&quot; VALUES (490,23,12,'În ziua aceea Irod și Pilat&lt;f&gt;ⓡ&lt;/f&gt; s-au împrietenit, căci până atunci fuseseră dușmani.');
INSERT INTO &quot;verses&quot; VALUES (490,23,13,'&lt;pb/&gt;Pilat&lt;f&gt;ⓢ&lt;/f&gt; i-a adunat pe preoții de seamă, pe conducători și pe cei din popor');
INSERT INTO &quot;verses&quot; VALUES (490,23,14,'și le-a zis: „Mi l-ați&lt;f&gt;ⓣ&lt;/f&gt; adus pe omul acesta ca și cum ar stârni poporul &lt;i&gt;la răscoală&lt;/i&gt;. Dar iată că, după ce l-am judecat&lt;f&gt;ⓤ&lt;/f&gt; înaintea voastră, n-am găsit în el nicio vină din cele pentru care îl pârâți.');
INSERT INTO &quot;verses&quot; VALUES (490,23,15,'Și nici Irod, căci ni l-a trimis înapoi; iată că n-a făcut nimic vrednic de moarte.');
INSERT INTO &quot;verses&quot; VALUES (490,23,16,'Așa că&lt;f&gt;ⓥ&lt;/f&gt;, după ce îl voi pedepsi, îi voi da drumul.” [');
INSERT INTO &quot;verses&quot; VALUES (490,23,17,'La fiecare sărbătoare, Pilat trebuia&lt;f&gt;ⓦ&lt;/f&gt; să le elibereze pe cineva.]');
INSERT INTO &quot;verses&quot; VALUES (490,23,18,'Ei au&lt;f&gt;ⓧ&lt;/f&gt; strigat cu toții, zicând: „La moarte cu &lt;i&gt;omul&lt;/i&gt; acesta; eliberează-ni-l pe Barabba!”');
INSERT INTO &quot;verses&quot; VALUES (490,23,19,'Acesta fusese aruncat în temniță pentru o răscoală petrecută în cetate și pentru un omor.');
INSERT INTO &quot;verses&quot; VALUES (490,23,20,'Pilat le-a vorbit din nou, fiindcă avea de gând să-I dea drumul lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (490,23,21,'Dar ei strigau &lt;i&gt;mai departe&lt;/i&gt;: „Răstignește-l! Răstignește-l!”');
INSERT INTO &quot;verses&quot; VALUES (490,23,22,'&lt;i&gt;Pilat&lt;/i&gt; le-a zis pentru a treia oară: „Dar ce rău a făcut? Eu n-am găsit nicio vină de moarte în el. După ce îl voi pedepsi, îi voi da drumul.”');
INSERT INTO &quot;verses&quot; VALUES (490,23,23,'Dar ei stăruiau cu strigăte puternice și cereau să fie răstignit. Iar strigătele lor [și ale preoților de seamă] au biruit.');
INSERT INTO &quot;verses&quot; VALUES (490,23,24,'Pilat&lt;f&gt;ⓨ&lt;/f&gt; a hotărât să li se împlinească cererea.');
INSERT INTO &quot;verses&quot; VALUES (490,23,25,'Le-a eliberat pe cel ce fusese aruncat în temniță pentru răscoală și omor, pe care-l cereau ei, iar pe Isus L-a dat &lt;i&gt;să I se facă&lt;/i&gt; după voia lor.');
INSERT INTO &quot;verses&quot; VALUES (490,23,26,'&lt;pb/&gt;Pe&lt;f&gt;ⓩ&lt;/f&gt; când Îl duceau, au pus mâna pe un anume Simon din Cirene, care venea de la câmp, și i-au pus crucea &lt;i&gt;în spate&lt;/i&gt;, ca s-o ducă în urma lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (490,23,27,'După Isus mergea o mare mulțime de oameni și femei care se tânguiau și Îl jeleau.');
INSERT INTO &quot;verses&quot; VALUES (490,23,28,'Isus S-a întors către ele și le-a zis: &lt;J&gt;„Fiice ale Ierusalimului, nu Mă plângeți pe Mine, ci plângeți-vă pe voi și pe copiii voștri,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,29,'&lt;J&gt;căci&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt;&lt;J&gt;, iată, vor veni zile când se va zice: «Ferice de cele sterpe, ferice de pântecele care n-au născut și de sânii care n-au alăptat!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,30,'&lt;J&gt;Atunci&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;vor începe &lt;e&gt;să zică munților: «Cădeți peste noi!» și dealurilor: «Acoperiți-ne!»&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[79]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,31,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;dacă așa i se face pomului verde, ce i se va face celui uscat?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,32,'Împreună&lt;f&gt;ⓓ&lt;/f&gt; cu El duceau și doi răufăcători, ca să fie omorâți.');
INSERT INTO &quot;verses&quot; VALUES (490,23,33,'Când&lt;f&gt;ⓔ&lt;/f&gt; au ajuns la locul numit „Căpățâna”, i-au răstignit acolo pe El și pe răufăcători: unul la dreapta și altul la stânga.');
INSERT INTO &quot;verses&quot; VALUES (490,23,34,'[[Dar Isus zicea: &lt;J&gt;„Tată, iartă-i&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;, căci nu&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;știu ce fac!”&lt;/J&gt;]] &lt;e&gt;Ei și-au împărțit&lt;/e&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;hainele Lui, trăgând la sorți.&lt;/e&gt;&lt;f&gt;[80]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,35,'&lt;pb/&gt;Poporul&lt;f&gt;ⓘ&lt;/f&gt; stătea &lt;i&gt;acolo&lt;/i&gt; și &lt;e&gt;privea&lt;/e&gt;. Conducătorii&lt;f&gt;ⓙ&lt;/f&gt; &lt;e&gt;Își băteau joc de El&lt;/e&gt;&lt;f&gt;[81]&lt;/f&gt;, zicând: „Pe alții i-a izbăvit; să se izbăvească pe sine, dacă este Hristosul lui Dumnezeu, Alesul!”');
INSERT INTO &quot;verses&quot; VALUES (490,23,36,'Soldații de asemenea își băteau joc de El; se apropiau, Îi &lt;e&gt;dădeau oțet&lt;/e&gt;&lt;f&gt;[82]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,37,'și-I ziceau: „Dacă tu ești regele iudeilor, izbăvește-te pe tine însuți!”');
INSERT INTO &quot;verses&quot; VALUES (490,23,38,'Deasupra&lt;f&gt;ⓚ&lt;/f&gt; Lui era o inscripție [cu litere grecești, latinești și evreiești]: „Acesta este regele iudeilor.”');
INSERT INTO &quot;verses&quot; VALUES (490,23,39,'&lt;pb/&gt;Unul&lt;f&gt;ⓛ&lt;/f&gt; dintre răufăcătorii răstigniți a început să-L batjocorească, zicând: „Nu ești tu Hristosul? Izbăvește-te pe tine și &lt;i&gt;izbăvește-ne și&lt;/i&gt; pe noi!”');
INSERT INTO &quot;verses&quot; VALUES (490,23,40,'Dar celălalt l-a mustrat și i-a zis: „Oare nu te temi de Dumnezeu, tu, care ești sub aceeași osândă?');
INSERT INTO &quot;verses&quot; VALUES (490,23,41,'Noi primim pe drept ceea ce merităm pentru faptele noastre, însă Acesta n-a făcut niciun rău.”');
INSERT INTO &quot;verses&quot; VALUES (490,23,42,'Apoi a zis: „Isuse, adu-Ți aminte de mine când vei veni în Împărăția Ta!”&lt;f&gt;[83]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,43,'[Isus] i-a răspuns: &lt;J&gt;„Adevărat îți spun: astăzi vei fi cu Mine în rai.”&lt;/J&gt;&lt;f&gt;[84]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,23,44,'&lt;pb/&gt;Era&lt;f&gt;ⓜ&lt;/f&gt; pe la ceasul al șaselea&lt;f&gt;[85]&lt;/f&gt; și s-a făcut întuneric în toată țara până la ceasul al nouălea.');
INSERT INTO &quot;verses&quot; VALUES (490,23,45,'Soarele s-a întunecat, iar catapeteasma&lt;f&gt;ⓝ&lt;/f&gt; Templului s-a rupt în două.');
INSERT INTO &quot;verses&quot; VALUES (490,23,46,'Isus a strigat cu glas puternic, zicând: &lt;J&gt;„Tată&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt;&lt;J&gt;, &lt;e&gt;în mâinile Tale Îmi încredințez duhul!”&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[86]&lt;/f&gt; Și&lt;f&gt;ⓟ&lt;/f&gt; după ce a vorbit astfel, Și-a dat duhul.');
INSERT INTO &quot;verses&quot; VALUES (490,23,47,'Când a văzut ce s-a întâmplat, centurionul&lt;f&gt;ⓠ&lt;/f&gt; L-a slăvit pe Dumnezeu, zicând: „Cu adevărat, omul acesta era drept!”');
INSERT INTO &quot;verses&quot; VALUES (490,23,48,'Și toată mulțimea care se adunase în fața acelei priveliști, când a văzut cele petrecute, s-a întors &lt;i&gt;acasă&lt;/i&gt; bătându-se în piept.');
INSERT INTO &quot;verses&quot; VALUES (490,23,49,'Toți&lt;f&gt;ⓡ&lt;/f&gt; cunoscuții lui &lt;i&gt;Isus&lt;/i&gt; și femeile care-L urmaseră din Galileea stăteau la o oarecare depărtare și priveau la &lt;i&gt;toate&lt;/i&gt; acestea.');
INSERT INTO &quot;verses&quot; VALUES (490,23,50,'&lt;pb/&gt;Era&lt;f&gt;ⓢ&lt;/f&gt; un membru al Sinedriului, pe nume Iosif, om bun și drept,');
INSERT INTO &quot;verses&quot; VALUES (490,23,51,'care nu se învoise cu sfatul și fapta celorlalți. El era din Arimateea, o cetate a iudeilor, și&lt;f&gt;ⓣ&lt;/f&gt; aștepta Împărăția lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (490,23,52,'A mers la Pilat și i-a cerut trupul lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (490,23,53,'L-a&lt;f&gt;ⓤ&lt;/f&gt; coborât &lt;i&gt;de pe cruce&lt;/i&gt;, L-a înfășurat într-un giulgiu și L-a pus într-un mormânt săpat în stâncă, în care nu mai fusese pus nimeni.');
INSERT INTO &quot;verses&quot; VALUES (490,23,54,'Era ziua&lt;f&gt;ⓥ&lt;/f&gt; pregătirii și începea ziua sabatului.');
INSERT INTO &quot;verses&quot; VALUES (490,23,55,'Femeile care-L&lt;f&gt;ⓦ&lt;/f&gt; însoțiseră &lt;i&gt;pe Isus&lt;/i&gt; din Galileea l-au urmat &lt;i&gt;pe Iosif&lt;/i&gt;, au văzut&lt;f&gt;ⓧ&lt;/f&gt; mormântul și cum a fost pus trupul lui &lt;i&gt;Isus&lt;/i&gt; și');
INSERT INTO &quot;verses&quot; VALUES (490,23,56,'s-au întors să&lt;f&gt;ⓨ&lt;/f&gt; pregătească miresme și miruri. Apoi, în ziua de sabat, s-au odihnit, după&lt;f&gt;ⓩ&lt;/f&gt; poruncă.');
INSERT INTO &quot;verses&quot; VALUES (490,24,1,'&lt;pb/&gt;În ziua&lt;f&gt;ⓐ&lt;/f&gt; întâi a săptămânii, &lt;i&gt;femeile acestea&lt;/i&gt; [și altele împreună cu ele] au venit la mormânt dis-de-dimineață și au adus&lt;f&gt;ⓑ&lt;/f&gt; miresmele pe care le pregătiseră.');
INSERT INTO &quot;verses&quot; VALUES (490,24,2,'Au&lt;f&gt;ⓒ&lt;/f&gt; găsit piatra de la mormânt rostogolită,');
INSERT INTO &quot;verses&quot; VALUES (490,24,3,'au intrat&lt;f&gt;ⓓ&lt;/f&gt;, dar n-au găsit trupul Domnului Isus.');
INSERT INTO &quot;verses&quot; VALUES (490,24,4,'Și pe când erau nedumerite de lucrul acesta, iată&lt;f&gt;ⓔ&lt;/f&gt; că s-au ivit înaintea lor doi bărbați în haine strălucitoare.');
INSERT INTO &quot;verses&quot; VALUES (490,24,5,'Înspăimântate, ele s-au plecat cu fața la pământ, dar ei le-au zis: „De ce-L căutați pe Cel Viu între cei morți?');
INSERT INTO &quot;verses&quot; VALUES (490,24,6,'Nu este aici, a înviat. Amintiți-vă&lt;f&gt;ⓕ&lt;/f&gt; ce v-a spus pe când era încă în Galileea,');
INSERT INTO &quot;verses&quot; VALUES (490,24,7,'când zicea că Fiul Omului trebuie să fie dat în mâinile oamenilor păcătoși, să fie răstignit și a treia zi să învie!”');
INSERT INTO &quot;verses&quot; VALUES (490,24,8,'Atunci, &lt;i&gt;ele&lt;/i&gt; și-au&lt;f&gt;ⓖ&lt;/f&gt; amintit de cuvintele lui &lt;i&gt;Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,24,9,'&lt;pb/&gt;Când&lt;f&gt;ⓗ&lt;/f&gt; s-au întors de la mormânt, le-au povestit toate aceste lucruri celor unsprezece și tuturor celorlalți.');
INSERT INTO &quot;verses&quot; VALUES (490,24,10,'&lt;i&gt;Femeile acestea&lt;/i&gt; erau: Maria Magdalena, Ioana&lt;f&gt;ⓘ&lt;/f&gt;, Maria lui Iacov și celelalte &lt;i&gt;care erau&lt;/i&gt; împreună cu ele. Le spuneau apostolilor &lt;i&gt;toate&lt;/i&gt; acestea,');
INSERT INTO &quot;verses&quot; VALUES (490,24,11,'însă &lt;i&gt;lor&lt;/i&gt; cuvintele &lt;i&gt;femeilor&lt;/i&gt; li&lt;f&gt;ⓙ&lt;/f&gt; se păreau aiureli și nu le credeau.');
INSERT INTO &quot;verses&quot; VALUES (490,24,12,'Dar&lt;f&gt;ⓚ&lt;/f&gt; Petru s-a ridicat și a alergat la mormânt, s-a aplecat și a văzut doar fâșiile de pânză pe jos; apoi a plecat acasă uimit de cele petrecute.');
INSERT INTO &quot;verses&quot; VALUES (490,24,13,'&lt;pb/&gt;Și iată că, în aceeași zi, doi&lt;f&gt;ⓛ&lt;/f&gt; dintre &lt;i&gt;ucenici&lt;/i&gt; mergeau într-un sat numit Emaus, aflat la o depărtare de șaizeci de stadii&lt;f&gt;[87]&lt;/f&gt; de Ierusalim,');
INSERT INTO &quot;verses&quot; VALUES (490,24,14,'și vorbeau despre tot ce se petrecuse.');
INSERT INTO &quot;verses&quot; VALUES (490,24,15,'Pe când vorbeau ei și discutau, Isus Însuși&lt;f&gt;ⓜ&lt;/f&gt; S-a apropiat și mergea împreună cu ei,');
INSERT INTO &quot;verses&quot; VALUES (490,24,16,'dar ochii&lt;f&gt;ⓝ&lt;/f&gt; lor erau împiedicați să-L recunoască.');
INSERT INTO &quot;verses&quot; VALUES (490,24,17,'El le-a zis: &lt;J&gt;„Ce vorbe sunt acestea pe care le schimbați între voi pe drum?”&lt;/J&gt; Și ei s-au oprit triști.');
INSERT INTO &quot;verses&quot; VALUES (490,24,18,'Drept răspuns, unul dintre ei, numit&lt;f&gt;ⓞ&lt;/f&gt; Cleopa, I-a zis: „Tu ești singurul străin în Ierusalim, de nu știi ce s-a întâmplat în el zilele acestea?”');
INSERT INTO &quot;verses&quot; VALUES (490,24,19,'&lt;J&gt;„Ce?”&lt;/J&gt;, i-a întrebat El. Și ei I-au răspuns: „Ce s-a întâmplat cu Isus din Nazaret, care&lt;f&gt;ⓟ&lt;/f&gt; a fost un profet puternic&lt;f&gt;ⓠ&lt;/f&gt; în faptă și cuvânt, înaintea lui Dumnezeu și a întregului popor.');
INSERT INTO &quot;verses&quot; VALUES (490,24,20,'Cum&lt;f&gt;ⓡ&lt;/f&gt; preoții de seamă și mai-marii noștri L-au dat să fie osândit la moarte și L-au răstignit.');
INSERT INTO &quot;verses&quot; VALUES (490,24,21,'Noi nădăjduiam că El este Cel&lt;f&gt;ⓢ&lt;/f&gt; care îl va izbăvi pe Israel; totuși, este a treia zi de când s-au petrecut &lt;i&gt;toate&lt;/i&gt; acestea.');
INSERT INTO &quot;verses&quot; VALUES (490,24,22,'Ba încă niște&lt;f&gt;ⓣ&lt;/f&gt; femei de-ale noastre ne-au și tulburat: au mers dis-de-dimineață la mormânt,');
INSERT INTO &quot;verses&quot; VALUES (490,24,23,'nu I-au găsit trupul și au venit spunând că ar fi avut o vedenie cu îngeri, care ziceau că El este viu.');
INSERT INTO &quot;verses&quot; VALUES (490,24,24,'Unii dintre cei ce erau cu noi au&lt;f&gt;ⓤ&lt;/f&gt; mers la mormânt și au găsit așa cum spuseseră femeile, dar pe El nu L-au văzut.”');
INSERT INTO &quot;verses&quot; VALUES (490,24,25,'Atunci &lt;i&gt;Isus&lt;/i&gt; le-a zis: &lt;J&gt;„O, nepricepuți și greoi la inimă &lt;i&gt;când e vorba&lt;/i&gt; să credeți tot ce au spus profeții!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,26,'&lt;J&gt;Oare nu&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;trebuia să îndure Hristosul aceste lucruri și să intre în slava Sa?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,27,'Și&lt;f&gt;ⓦ&lt;/f&gt; a început de la Moise&lt;f&gt;ⓧ&lt;/f&gt; și de la toți&lt;f&gt;ⓨ&lt;/f&gt; profeții și le-a tâlcuit din toate Scripturile cele privitoare la El.');
INSERT INTO &quot;verses&quot; VALUES (490,24,28,'&lt;pb/&gt;Când s-au apropiat de satul unde mergeau, El&lt;f&gt;ⓩ&lt;/f&gt; S-a făcut că vrea să meargă mai departe.');
INSERT INTO &quot;verses&quot; VALUES (490,24,29,'Dar ei&lt;f&gt;ⓐ&lt;/f&gt; au stăruit, zicând: „Rămâi cu noi, căci se înserează și ziua este pe sfârșite!” Și a intrat să rămână cu ei.');
INSERT INTO &quot;verses&quot; VALUES (490,24,30,'Pe când stătea la masă cu ei, a&lt;f&gt;ⓑ&lt;/f&gt; luat pâinea, a rostit binecuvântarea și, după ce a frânt-o, le-a dat-o.');
INSERT INTO &quot;verses&quot; VALUES (490,24,31,'Atunci li s-au deschis ochii și L-au recunoscut, dar El S-a făcut nevăzut dinaintea lor.');
INSERT INTO &quot;verses&quot; VALUES (490,24,32,'Ei au zis unul către altul: „Nu ne ardea inima în noi când ne vorbea pe drum și ne deslușea Scripturile?”');
INSERT INTO &quot;verses&quot; VALUES (490,24,33,'S-au ridicat chiar în ceasul acela și s-au întors la Ierusalim. I-au găsit pe cei unsprezece și pe cei ce erau cu ei adunați');
INSERT INTO &quot;verses&quot; VALUES (490,24,34,'și zicând: „A înviat Domnul cu adevărat și i S-a arătat lui Simon&lt;f&gt;ⓒ&lt;/f&gt;!”');
INSERT INTO &quot;verses&quot; VALUES (490,24,35,'Au povestit și ei cele &lt;i&gt;petrecute&lt;/i&gt; pe drum și cum li S-a făcut cunoscut la frângerea pâinii.');
INSERT INTO &quot;verses&quot; VALUES (490,24,36,'&lt;pb/&gt;Pe&lt;f&gt;ⓓ&lt;/f&gt; când vorbeau ei astfel, [Isus Însuși] a stat în mijlocul lor și le-a zis: &lt;J&gt;„Pace vouă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,37,'Speriați și cuprinși de frică, ei credeau că văd un duh&lt;f&gt;ⓔ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,24,38,'Dar El le-a zis: &lt;J&gt;„De ce sunteți tulburați și de ce vi se ridică îndoieli în inimă?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,39,'&lt;J&gt;Priviți-Mi mâinile și picioarele: Eu sunt; atingeți-Mă&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;și vedeți: un duh nu are carne și oase, așa cum vedeți că am Eu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,40,'Și după ce a zis aceste &lt;i&gt;vorbe&lt;/i&gt;, le-a arătat mâinile și picioarele &lt;i&gt;Sale&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (490,24,41,'Fiindcă ei, de&lt;f&gt;ⓖ&lt;/f&gt; bucurie, încă nu credeau și se mirau, &lt;i&gt;El&lt;/i&gt; le-a zis: &lt;J&gt;„Aveți ceva de mâncare aici?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,42,'I-au dat o bucată de pește fript [și un fagure de miere].');
INSERT INTO &quot;verses&quot; VALUES (490,24,43,'El&lt;f&gt;ⓗ&lt;/f&gt; &lt;i&gt;le-a&lt;/i&gt; luat și a mâncat înaintea lor.');
INSERT INTO &quot;verses&quot; VALUES (490,24,44,'&lt;pb/&gt;Apoi le-a zis: &lt;J&gt;„Acestea&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;sunt cuvintele pe care vi le-am spus pe când încă eram cu voi: trebuie să se împlinească tot ce este scris despre Mine în Legea lui Moise, în Profeți și în Psalmi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,45,'Atunci le-a&lt;f&gt;ⓙ&lt;/f&gt; deschis mintea, ca să înțeleagă Scripturile.');
INSERT INTO &quot;verses&quot; VALUES (490,24,46,'Și le-a zis: &lt;J&gt;„Așa&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;este scris [și așa trebuia &lt;i&gt;să fie&lt;/i&gt;]: Hristos să pătimească și să învie din morți a treia zi&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,47,'&lt;J&gt;și să fie propovăduite&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt;&lt;J&gt;, în Numele Lui, pocăința și iertarea păcatelor printre toate neamurile&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt;&lt;J&gt;, începând din Ierusalim.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,48,'&lt;J&gt;Voi&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;sunteți martori ai acestor &lt;i&gt;lucruri&lt;/i&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,49,'&lt;J&gt;Și, iată&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt;&lt;J&gt;, Eu voi trimite peste voi făgăduința Tatălui Meu, dar voi rămâneți în cetate până când veți fi îmbrăcați cu putere de sus!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (490,24,50,'&lt;pb/&gt;El i-a dus până&lt;f&gt;ⓟ&lt;/f&gt; aproape de Betania și, ridicându-Și mâinile, i-a binecuvântat.');
INSERT INTO &quot;verses&quot; VALUES (490,24,51,'Pe când îi binecuvânta, S-a despărțit de ei și a fost înălțat la cer.');
INSERT INTO &quot;verses&quot; VALUES (490,24,52,'După ce&lt;f&gt;ⓠ&lt;/f&gt; I s-au închinat, s-au întors&lt;f&gt;ⓡ&lt;/f&gt; în Ierusalim cu mare bucurie');
INSERT INTO &quot;verses&quot; VALUES (490,24,53,'și tot timpul erau în Templu, [lăudându-L și] binecuvântându-L&lt;f&gt;ⓢ&lt;/f&gt; pe Dumnezeu. [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (500,1,1,'&lt;pb/&gt;La început era&lt;f&gt;ⓐ&lt;/f&gt; Cuvântul, și Cuvântul era cu&lt;f&gt;ⓑ&lt;/f&gt; Dumnezeu, și&lt;f&gt;ⓒ&lt;/f&gt; Cuvântul era Dumnezeu&lt;f&gt;ⓓ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,1,2,'El era la început cu Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (500,1,3,'Toate prin El&lt;f&gt;ⓔ&lt;/f&gt; s-au făcut, și nimic din ce s-a făcut nu s-a făcut fără El.');
INSERT INTO &quot;verses&quot; VALUES (500,1,4,'În El&lt;f&gt;ⓕ&lt;/f&gt; era viața, și&lt;f&gt;ⓖ&lt;/f&gt; viața era lumina oamenilor.');
INSERT INTO &quot;verses&quot; VALUES (500,1,5,'Lumina&lt;f&gt;ⓗ&lt;/f&gt; strălucește în întuneric, și întunericul n-a biruit-o.');
INSERT INTO &quot;verses&quot; VALUES (500,1,6,'&lt;pb/&gt;A&lt;f&gt;ⓘ&lt;/f&gt; venit un om trimis de Dumnezeu: numele lui era Ioan.');
INSERT INTO &quot;verses&quot; VALUES (500,1,7,'El a venit spre mărturie, ca să mărturisească despre Lumină, pentru ca toți să creadă prin el.');
INSERT INTO &quot;verses&quot; VALUES (500,1,8,'Nu era el Lumina, ci a venit ca să mărturisească despre Lumină.');
INSERT INTO &quot;verses&quot; VALUES (500,1,9,'Lumina&lt;f&gt;ⓙ&lt;/f&gt; aceasta era adevărata Lumină, care, venind în lume, luminează pe orice om.');
INSERT INTO &quot;verses&quot; VALUES (500,1,10,'El era în lume, și lumea prin El s-a făcut, dar lumea nu L-a cunoscut.');
INSERT INTO &quot;verses&quot; VALUES (500,1,11,'A venit la ai&lt;f&gt;ⓚ&lt;/f&gt; Săi, și ai Săi nu L-au primit.');
INSERT INTO &quot;verses&quot; VALUES (500,1,12,'Dar tuturor&lt;f&gt;ⓛ&lt;/f&gt; celor ce L-au primit le-a dat dreptul să devină copii ai lui Dumnezeu, anume celor ce cred în Numele Său;');
INSERT INTO &quot;verses&quot; VALUES (500,1,13,'aceștia nu din sânge, nici din voia firii, nici din voia vreunui om, ci din Dumnezeu s-au născut&lt;f&gt;ⓜ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,1,14,'&lt;pb/&gt;Și&lt;f&gt;ⓝ&lt;/f&gt; Cuvântul S-a făcut trup&lt;f&gt;ⓞ&lt;/f&gt; și a&lt;f&gt;ⓟ&lt;/f&gt; locuit printre noi, și noi am privit slava Lui: o slavă ca a singurului născut din Tatăl, plin de har&lt;f&gt;ⓠ&lt;/f&gt; și de adevăr.');
INSERT INTO &quot;verses&quot; VALUES (500,1,15,'Ioan&lt;f&gt;ⓡ&lt;/f&gt; a mărturisit despre El și a strigat: „El este Acela despre care ziceam: «Cel ce vine după mine este înaintea mea, pentru că&lt;f&gt;ⓢ&lt;/f&gt; era înainte de mine.»”');
INSERT INTO &quot;verses&quot; VALUES (500,1,16,'Și noi toți am primit din plinătatea Lui și har după har;');
INSERT INTO &quot;verses&quot; VALUES (500,1,17,'căci Legea&lt;f&gt;ⓣ&lt;/f&gt; prin Moise a fost dată, însă harul&lt;f&gt;ⓤ&lt;/f&gt; și adevărul&lt;f&gt;ⓥ&lt;/f&gt; prin Isus Hristos au venit.');
INSERT INTO &quot;verses&quot; VALUES (500,1,18,'Nimeni&lt;f&gt;ⓦ&lt;/f&gt; n-a văzut vreodată pe Dumnezeu; singurul Dumnezeu&lt;f&gt;[1]&lt;/f&gt;, care este în sânul Tatălui, Acela L-a făcut cunoscut.');
INSERT INTO &quot;verses&quot; VALUES (500,1,19,'&lt;pb/&gt;Aceasta este mărturisirea lui Ioan, când iudeii&lt;f&gt;[2]&lt;/f&gt; din Ierusalim au trimis la el pe niște preoți și leviți să-l întrebe: „Tu cine ești?”');
INSERT INTO &quot;verses&quot; VALUES (500,1,20,'El a mărturisit răspicat și a adeverit: „Nu sunt eu Hristosul!”&lt;f&gt;ⓧ&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,1,21,'Și ei l-au întrebat: „Dar cine ești? Ești Ilie&lt;f&gt;ⓨ&lt;/f&gt;?” Și el a zis: „Nu sunt!” „Ești Profetul&lt;f&gt;ⓩ&lt;/f&gt;?” Dar el a răspuns: „Nu!”');
INSERT INTO &quot;verses&quot; VALUES (500,1,22,'Atunci i-au zis: „Cine ești? Ca să dăm un răspuns celor ce ne-au trimis. Ce zici tu despre tine însuți?”');
INSERT INTO &quot;verses&quot; VALUES (500,1,23,'El a zis&lt;f&gt;ⓐ&lt;/f&gt;: &lt;pb/&gt;&lt;t&gt;„Eu sunt &lt;e&gt;glasul celui ce strigă în pustiu:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;«Neteziți calea Domnului!»&lt;/e&gt;”&lt;f&gt;[3]&lt;/f&gt;,&lt;/t&gt; &lt;pb/&gt;cum a zis profetul Isaia.');
INSERT INTO &quot;verses&quot; VALUES (500,1,24,'Trimișii erau dintre farisei.');
INSERT INTO &quot;verses&quot; VALUES (500,1,25,'Ei l-au întrebat și i-au zis: „Atunci de ce botezi, dacă nu ești nici Hristosul, nici Ilie, nici Profetul?”');
INSERT INTO &quot;verses&quot; VALUES (500,1,26,'Drept răspuns, Ioan le-a zis: „Eu&lt;f&gt;ⓑ&lt;/f&gt; botez în apă; dar în mijlocul vostru stă Unul pe care voi nu-L cunoașteți,');
INSERT INTO &quot;verses&quot; VALUES (500,1,27,'care&lt;f&gt;ⓒ&lt;/f&gt; vine după mine [și care este înaintea mea], căruia eu nu sunt vrednic să-I dezleg cureaua încălțămintei.”');
INSERT INTO &quot;verses&quot; VALUES (500,1,28,'Aceste lucruri s-au petrecut în Betania&lt;f&gt;ⓓ&lt;/f&gt;&lt;f&gt;[4]&lt;/f&gt;, dincolo de Iordan, unde boteza Ioan.');
INSERT INTO &quot;verses&quot; VALUES (500,1,29,'&lt;pb/&gt;A doua zi, Ioan L-a văzut pe Isus venind la el și a zis: „Iată Mielul&lt;f&gt;ⓔ&lt;/f&gt; lui Dumnezeu, care&lt;f&gt;ⓕ&lt;/f&gt; ridică păcatul lumii!');
INSERT INTO &quot;verses&quot; VALUES (500,1,30,'El&lt;f&gt;ⓖ&lt;/f&gt; este Acela despre care ziceam eu: «După mine vine un om care este înaintea mea, căci era înainte de mine.»');
INSERT INTO &quot;verses&quot; VALUES (500,1,31,'Eu nu-L cunoșteam, dar tocmai pentru&lt;f&gt;ⓗ&lt;/f&gt; aceasta am venit să botez în apă: ca El să fie făcut cunoscut lui Israel.”');
INSERT INTO &quot;verses&quot; VALUES (500,1,32,'Ioan&lt;f&gt;ⓘ&lt;/f&gt; a mărturisit și a zis: „Am văzut Duhul coborându-Se din cer ca un porumbel și rămânând peste El.');
INSERT INTO &quot;verses&quot; VALUES (500,1,33,'Eu nu-L cunoșteam, dar Cel ce m-a trimis să botez în apă mi-a zis: «Acela peste care vei vedea Duhul coborându-Se și rămânând este Cel ce&lt;f&gt;ⓙ&lt;/f&gt; botează în Duhul Sfânt.»');
INSERT INTO &quot;verses&quot; VALUES (500,1,34,'Iar eu am văzut și am mărturisit că El este Fiul lui Dumnezeu.”');
INSERT INTO &quot;verses&quot; VALUES (500,1,35,'&lt;pb/&gt;A doua zi, Ioan stătea iarăși cu doi dintre ucenicii săi.');
INSERT INTO &quot;verses&quot; VALUES (500,1,36,'Și, când L-a văzut pe Isus trecând pe acolo, a zis: „Iată&lt;f&gt;ⓚ&lt;/f&gt; Mielul lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (500,1,37,'Cei doi ucenici l-au auzit rostind aceste vorbe și au mers după Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,1,38,'Isus S-a întors și, când i-a văzut că merg după El, le-a zis: &lt;J&gt;„Ce căutați?”&lt;/J&gt; Ei I-au răspuns: „&lt;e&gt;Rabbí&lt;/e&gt;” — care, tradus, înseamnă: „Învățătorule” — „unde locuiești?”');
INSERT INTO &quot;verses&quot; VALUES (500,1,39,'&lt;J&gt;„Veniți și veți vedea!”&lt;/J&gt;, le-a zis El. S-au dus și au văzut unde locuiește, și în ziua aceea au rămas la El; era cam pe la ceasul al zecelea.');
INSERT INTO &quot;verses&quot; VALUES (500,1,40,'Unul dintre cei doi care îl auziseră pe Ioan și care merseseră după Isus era Andrei&lt;f&gt;ⓛ&lt;/f&gt;, fratele lui Simon Petru.');
INSERT INTO &quot;verses&quot; VALUES (500,1,41,'El l-a găsit întâi pe fratele său, Simon, și i-a zis: „Noi L-am găsit pe Mesia” (care, tradus, înseamnă: „Hristos”).');
INSERT INTO &quot;verses&quot; VALUES (500,1,42,'Și l-a adus la Isus. În timp ce-l privea, Isus i-a zis: &lt;J&gt;„Tu ești Simon, fiul lui Ioan&lt;/J&gt;&lt;f&gt;[5]&lt;/f&gt;&lt;J&gt;; tu&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;te vei chema Chifa&lt;/J&gt;&lt;f&gt;[6]&lt;/f&gt;&lt;J&gt;”&lt;/J&gt; (care, tradus, înseamnă: „Petru”).');
INSERT INTO &quot;verses&quot; VALUES (500,1,43,'&lt;pb/&gt;A doua zi, Isus a vrut să Se ducă în Galileea și l-a găsit pe Filip. Isus i-a zis: &lt;J&gt;„Vino după Mine!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,1,44,'Filip era din Betsaida, cetatea lui Andrei și a lui Petru.');
INSERT INTO &quot;verses&quot; VALUES (500,1,45,'Filip l-a găsit pe Natanael&lt;f&gt;ⓝ&lt;/f&gt; și i-a zis: „Noi L-am găsit pe Acela despre care au scris Moise&lt;f&gt;ⓞ&lt;/f&gt;, în Lege, și Profeții&lt;f&gt;ⓟ&lt;/f&gt;: pe Isus din Nazaret&lt;f&gt;ⓠ&lt;/f&gt;, fiul lui Iosif!”');
INSERT INTO &quot;verses&quot; VALUES (500,1,46,'Natanael i-a zis: „Poate ieși ceva bun din Nazaret&lt;f&gt;ⓡ&lt;/f&gt;?” „Vino și vezi!”, i-a răspuns Filip.');
INSERT INTO &quot;verses&quot; VALUES (500,1,47,'Isus l-a văzut pe Natanael venind spre El și a zis despre el: &lt;J&gt;„Iată cu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;adevărat un israelit în care nu este vicleșug.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,1,48,'„De unde mă cunoști?”, I-a zis Natanael. Drept răspuns, Isus i-a zis: &lt;J&gt;„Te-am văzut înainte să te cheme Filip, când erai sub smochin.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,1,49,'Natanael I-a răspuns: „&lt;e&gt;Rabbí&lt;/e&gt;, Tu ești&lt;f&gt;ⓣ&lt;/f&gt; Fiul lui Dumnezeu, Tu ești Regele lui Israel!”');
INSERT INTO &quot;verses&quot; VALUES (500,1,50,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Pentru că ți-am spus că te-am văzut sub smochin crezi? Lucruri mai mari decât acestea vei vedea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,1,51,'Apoi i-a zis: &lt;J&gt;„Adevărat, adevărat vă spun: [de acum&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;încolo] veți vedea &lt;e&gt;cerul deschis și pe îngerii lui Dumnezeu suindu-se și coborându-se&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[7]&lt;/f&gt; &lt;J&gt;peste Fiul Omului.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,2,1,'&lt;pb/&gt;A treia zi s-a făcut o nuntă în Cana&lt;f&gt;ⓥ&lt;/f&gt; Galileei, și mama lui Isus era acolo.');
INSERT INTO &quot;verses&quot; VALUES (500,2,2,'La nuntă a fost chemat și Isus cu ucenicii Săi.');
INSERT INTO &quot;verses&quot; VALUES (500,2,3,'Și, fiindcă se terminase vinul, mama lui Isus I-a zis: „Nu mai au vin.”');
INSERT INTO &quot;verses&quot; VALUES (500,2,4,'Isus i-a răspuns: &lt;J&gt;„Ce&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;am Eu a face cu tine, femeie&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt;&lt;J&gt;? Nu Mi-a venit încă ceasul&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,2,5,'Mama Lui le-a zis slujitorilor: „Să faceți orice vă va spune!”');
INSERT INTO &quot;verses&quot; VALUES (500,2,6,'Erau acolo șase vase pentru apă, din piatră, puse după&lt;f&gt;ⓩ&lt;/f&gt; obiceiul de curățire al iudeilor; fiecare cuprindea câte două sau trei vedre&lt;f&gt;[8]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,2,7,'Isus le-a zis: &lt;J&gt;„Umpleți vasele cu apă!”&lt;/J&gt; Și le-au umplut până sus.');
INSERT INTO &quot;verses&quot; VALUES (500,2,8,'&lt;J&gt;„Scoateți acum”&lt;/J&gt;, le-a zis El, &lt;J&gt;„și duceți nunului&lt;/J&gt;&lt;f&gt;[9]&lt;/f&gt;&lt;J&gt;!”&lt;/J&gt; Și i-au dus.');
INSERT INTO &quot;verses&quot; VALUES (500,2,9,'Când a gustat nunul apa&lt;f&gt;ⓐ&lt;/f&gt; făcută vin (căci nu știa de unde este, dar slujitorii care scoseseră apa știau), l-a chemat pe mire');
INSERT INTO &quot;verses&quot; VALUES (500,2,10,'și i-a zis: „Orice om pune la masă întâi vinul cel bun; și, când nuntașii au băut bine, îl pune pe cel mai puțin bun; dar tu ai păstrat vinul cel bun până acum.”');
INSERT INTO &quot;verses&quot; VALUES (500,2,11,'Acest început al semnelor l-a făcut Isus în Cana Galileei. El Și-a&lt;f&gt;ⓑ&lt;/f&gt; arătat slava, și ucenicii Săi au crezut în El.');
INSERT INTO &quot;verses&quot; VALUES (500,2,12,'După aceea, S-a coborât la Capernaum, împreună cu mama, frații&lt;f&gt;ⓒ&lt;/f&gt; și ucenicii Săi; dar n-au rămas multe zile acolo.');
INSERT INTO &quot;verses&quot; VALUES (500,2,13,'&lt;pb/&gt;Paștele&lt;f&gt;ⓓ&lt;/f&gt; iudeilor era aproape, și Isus S-a suit la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (500,2,14,'În Templu i-a găsit&lt;f&gt;ⓔ&lt;/f&gt; pe cei ce vindeau vite, oi și porumbei și pe schimbătorii de bani stând jos.');
INSERT INTO &quot;verses&quot; VALUES (500,2,15,'A făcut un bici de funii și i-a alungat pe toți din Templu, precum și oile și vitele; iar schimbătorilor le-a risipit pe jos banii și le-a răsturnat mesele.');
INSERT INTO &quot;verses&quot; VALUES (500,2,16,'Și celor ce vindeau porumbei le-a zis: &lt;J&gt;„Luați acestea de aici și nu faceți din Casa&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;Tatălui Meu o casă de negustorie!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,2,17,'Ucenicii Săi și-au adus aminte că este scris: &lt;e&gt;Râvna pentru casa Ta Mă mistuie.&lt;/e&gt;&lt;f&gt;[10]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,2,18,'Iudeii au luat cuvântul și I-au zis: „Prin ce&lt;f&gt;ⓖ&lt;/f&gt; semn ne arăți că ai dreptul să faci aceste lucruri?”');
INSERT INTO &quot;verses&quot; VALUES (500,2,19,'Drept răspuns, Isus le-a zis: &lt;J&gt;„Dărâmați&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;Templul acesta, și în trei zile îl voi ridica!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,2,20,'Iudeii au zis: „De patruzeci și șase de ani se &lt;i&gt;tot&lt;/i&gt; zidește Templul acesta, și tu îl vei ridica în trei zile?”');
INSERT INTO &quot;verses&quot; VALUES (500,2,21,'Dar El vorbea despre&lt;f&gt;ⓘ&lt;/f&gt; Templul trupului Său.');
INSERT INTO &quot;verses&quot; VALUES (500,2,22,'De aceea, când a înviat din morți, ucenicii Săi și-au&lt;f&gt;ⓙ&lt;/f&gt; adus aminte că spusese vorba aceasta; și au crezut Scriptura și cuvântul pe care-l spusese Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,2,23,'&lt;pb/&gt;Pe când era Isus în Ierusalim, la sărbătoarea Paștelui, mulți au crezut în Numele Lui, căci vedeau semnele pe care le făcea.');
INSERT INTO &quot;verses&quot; VALUES (500,2,24,'Dar Isus nu Se încredea în ei, pentru că îi cunoștea pe toți.');
INSERT INTO &quot;verses&quot; VALUES (500,2,25,'Și n-avea nevoie să-I facă cineva mărturisiri despre vreun om, fiindcă El&lt;f&gt;ⓚ&lt;/f&gt; Însuși știa ce este în om.');
INSERT INTO &quot;verses&quot; VALUES (500,3,1,'&lt;pb/&gt;Între farisei era un om cu numele Nicodim, un conducător al iudeilor.');
INSERT INTO &quot;verses&quot; VALUES (500,3,2,'Acesta&lt;f&gt;ⓛ&lt;/f&gt; a venit la Isus noaptea și I-a zis: „&lt;e&gt;Rabbí&lt;/e&gt;&lt;f&gt;[11]&lt;/f&gt;, știm că de la Dumnezeu ai venit ca învățător, căci nimeni&lt;f&gt;ⓜ&lt;/f&gt; nu poate face aceste semne pe care le faci Tu dacă nu este&lt;f&gt;ⓝ&lt;/f&gt; Dumnezeu cu el.”');
INSERT INTO &quot;verses&quot; VALUES (500,3,3,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Adevărat, adevărat îți spun: dacă&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;un om nu se naște din nou&lt;/J&gt;&lt;f&gt;[12]&lt;/f&gt;&lt;J&gt;, nu poate să vadă Împărăția lui Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,4,'&lt;pb/&gt;Nicodim I-a zis: „Cum se poate naște un om, bătrân fiind? Poate el să intre a doua oară în pântecele mamei sale și să se nască?”');
INSERT INTO &quot;verses&quot; VALUES (500,3,5,'Isus i-a răspuns: &lt;J&gt;„Adevărat, adevărat îți spun: dacă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;nu se naște cineva din apă și Duh, nu poate să intre în Împărăția lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,6,'&lt;J&gt;Ce este născut din trup este trup, și ce este născut din Duh este duh.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,7,'&lt;J&gt;Să nu te miri că ți-am zis: «Trebuie să vă nașteți din nou.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,8,'&lt;J&gt;Vântul&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;suflă încotro vrea și-i auzi vuietul, dar nu știi de unde vine, nici încotro merge. Tot așa este cu oricine e născut din Duhul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,9,'&lt;pb/&gt;Nicodim I-a zis: „Cum este cu putință așa ceva?”');
INSERT INTO &quot;verses&quot; VALUES (500,3,10,'Isus i-a răspuns: &lt;J&gt;„Tu ești învățătorul lui Israel, și nu cunoști aceste lucruri?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,11,'&lt;J&gt;Adevărat, adevărat îți spun: noi vorbim ce cunoaștem și mărturisim ce am văzut; și voi nu primiți mărturia noastră.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,12,'&lt;J&gt;Dacă despre lucruri pământești v-am vorbit și nu credeți, cum veți crede când vă voi vorbi despre lucrurile cerești?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,13,'&lt;J&gt;Nimeni&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;nu s-a suit în cer, în afară de Cel ce S-a coborât din cer, adică Fiul Omului.&lt;/J&gt;&lt;f&gt;[13]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,14,'&lt;pb/&gt;&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;, după cum a înălțat Moise șarpele în pustiu, tot așa trebuie să fie înălțat&lt;/J&gt;&lt;f&gt;[14]&lt;/f&gt; &lt;J&gt;și Fiul Omului,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,15,'&lt;J&gt;pentru ca oricine crede [să nu piară], ci să aibă în El viață&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;veșnică.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,16,'&lt;J&gt;Fiindcă&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;atât de mult a iubit Dumnezeu lumea, încât L-a dat pe singurul Său Fiu, pentru ca oricine crede în El să nu piară, ci să aibă viață veșnică.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,17,'&lt;J&gt;Căci Dumnezeu nu L-a&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;trimis pe Fiul [Său] în lume ca să judece lumea, ci ca lumea să fie mântuită prin El.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,18,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;crede în El nu este judecat, dar cine nu crede a și fost judecat, pentru că n-a crezut în Numele singurului Fiu al lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,19,'&lt;J&gt;Și judecata este aceasta&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt;&lt;J&gt;: Lumina a venit în lume, dar oamenii au iubit mai mult întunericul decât Lumina, pentru că faptele lor erau rele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,20,'&lt;J&gt;Căci oricine&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;săvârșește fapte rele urăște Lumina și nu vine la Lumină, pentru ca faptele lui să nu fie date la iveală.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,21,'&lt;J&gt;Dar cine înfăptuiește adevărul vine la Lumină, pentru ca să i se arate faptele, fiindcă sunt făcute în Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,3,22,'&lt;pb/&gt;După aceea, Isus și ucenicii Săi au venit în ținutul Iudeei; și stătea acolo cu ei și&lt;f&gt;ⓩ&lt;/f&gt; boteza.');
INSERT INTO &quot;verses&quot; VALUES (500,3,23,'Ioan boteza și el în Enon, în apropiere de Salim&lt;f&gt;ⓐ&lt;/f&gt;, pentru că acolo erau multe ape; și&lt;f&gt;ⓑ&lt;/f&gt; oamenii veneau și se botezau.');
INSERT INTO &quot;verses&quot; VALUES (500,3,24,'Căci&lt;f&gt;ⓒ&lt;/f&gt; Ioan încă nu fusese aruncat în temniță.');
INSERT INTO &quot;verses&quot; VALUES (500,3,25,'Între ucenicii lui Ioan și un iudeu s-a iscat o neînțelegere cu privire la curățire.');
INSERT INTO &quot;verses&quot; VALUES (500,3,26,'Au venit deci la Ioan și i-au zis: „&lt;e&gt;Rabbí&lt;/e&gt;, Cel ce era cu tine dincolo de Iordan și despre&lt;f&gt;ⓓ&lt;/f&gt; care ai mărturisit tu iată că botează, și toți se duc la El.”');
INSERT INTO &quot;verses&quot; VALUES (500,3,27,'Drept răspuns, Ioan le-a zis: „Omul nu poate primi&lt;f&gt;ⓔ&lt;/f&gt; nimic dacă nu-i este dat din cer.');
INSERT INTO &quot;verses&quot; VALUES (500,3,28,'Voi înșivă îmi sunteți martori că am zis: Nu&lt;f&gt;ⓕ&lt;/f&gt; sunt eu Hristosul, ci sunt&lt;f&gt;ⓖ&lt;/f&gt; trimis înaintea Lui.');
INSERT INTO &quot;verses&quot; VALUES (500,3,29,'Cine&lt;f&gt;ⓗ&lt;/f&gt; are mireasă este mire, dar prietenul&lt;f&gt;ⓘ&lt;/f&gt; mirelui, care stă și-l ascultă, se bucură nespus de glasul mirelui; iar această bucurie, care este a mea, este deplină.');
INSERT INTO &quot;verses&quot; VALUES (500,3,30,'El trebuie să crească, iar eu, să mă micșorez.”');
INSERT INTO &quot;verses&quot; VALUES (500,3,31,'&lt;pb/&gt;Cel care&lt;f&gt;ⓙ&lt;/f&gt; vine de sus este&lt;f&gt;ⓚ&lt;/f&gt; deasupra tuturor; cel care&lt;f&gt;ⓛ&lt;/f&gt; este de pe pământ este pământesc și vorbește ca de pe pământ. Cel care&lt;f&gt;ⓜ&lt;/f&gt; vine din cer este deasupra tuturor.');
INSERT INTO &quot;verses&quot; VALUES (500,3,32,'El mărturisește ce&lt;f&gt;ⓝ&lt;/f&gt; a văzut și a auzit, dar nimeni nu primește mărturia Lui.');
INSERT INTO &quot;verses&quot; VALUES (500,3,33,'Cine primește mărturia Lui&lt;f&gt;ⓞ&lt;/f&gt; întărește faptul că Dumnezeu este adevărat.');
INSERT INTO &quot;verses&quot; VALUES (500,3,34,'Căci&lt;f&gt;ⓟ&lt;/f&gt; Acela pe care L-a trimis Dumnezeu vorbește cuvintele lui Dumnezeu, pentru că Dumnezeu nu-I dă Duhul cu&lt;f&gt;ⓠ&lt;/f&gt; măsură.');
INSERT INTO &quot;verses&quot; VALUES (500,3,35,'Tatăl&lt;f&gt;ⓡ&lt;/f&gt; Îl iubește pe Fiul și toate I le-a dat în mână.');
INSERT INTO &quot;verses&quot; VALUES (500,3,36,'Cine&lt;f&gt;ⓢ&lt;/f&gt; crede în Fiul are viața veșnică, dar cine se împotrivește Fiului nu va vedea viața, ci mânia lui Dumnezeu rămâne peste el.');
INSERT INTO &quot;verses&quot; VALUES (500,4,1,'&lt;pb/&gt;Când a aflat Isus că fariseii au auzit că El face și botează&lt;f&gt;ⓣ&lt;/f&gt; mai mulți ucenici decât Ioan');
INSERT INTO &quot;verses&quot; VALUES (500,4,2,'(deși Isus nu boteza El Însuși, ci ucenicii Săi),');
INSERT INTO &quot;verses&quot; VALUES (500,4,3,'a părăsit Iudeea și S-a întors în Galileea.');
INSERT INTO &quot;verses&quot; VALUES (500,4,4,'Trebuia să treacă prin Samaria');
INSERT INTO &quot;verses&quot; VALUES (500,4,5,'și a ajuns lângă o cetate a Samariei numită Sihar, aproape de ogorul pe&lt;f&gt;ⓤ&lt;/f&gt; care-l dăduse Iacov fiului său Iosif.');
INSERT INTO &quot;verses&quot; VALUES (500,4,6,'Acolo se afla fântâna lui Iacov. Isus, istovit de călătorie, stătea lângă fântână. Era cam pe la ceasul al șaselea.');
INSERT INTO &quot;verses&quot; VALUES (500,4,7,'&lt;pb/&gt;O femeie din Samaria a venit să scoată apă. &lt;J&gt;„Dă-Mi să beau”&lt;/J&gt;, i-a zis Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,4,8,'(Căci ucenicii Săi se duseseră în cetate să cumpere de mâncare.)');
INSERT INTO &quot;verses&quot; VALUES (500,4,9,'Femeia samariteană I-a zis: „Cum, Tu, iudeu fiind, ceri să bei de la mine, o femeie samariteană?” (Iudeii&lt;f&gt;ⓥ&lt;/f&gt;, într-adevăr, n-au legături cu samaritenii.)');
INSERT INTO &quot;verses&quot; VALUES (500,4,10,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Dacă ai fi cunoscut tu darul lui Dumnezeu și cine este Cel ce-ți zice: «Dă-Mi să beau!», tu însăți I-ai fi cerut să bei, și El ți-ar fi dat apă&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;vie.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,11,'„Doamne”, I-a zis femeia, „nici vas nu ai, iar fântâna este adâncă; atunci de unde ai această apă vie?');
INSERT INTO &quot;verses&quot; VALUES (500,4,12,'Doar nu ești Tu mai mare decât părintele nostru Iacov, care ne-a dat fântâna și a băut din ea el însuși cu fiii lui și vitele lui!”');
INSERT INTO &quot;verses&quot; VALUES (500,4,13,'Isus i-a răspuns: &lt;J&gt;„Oricine bea din apa aceasta va înseta iarăși.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,14,'&lt;J&gt;Dar cine&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;va bea din apa pe care i-o voi da Eu nicidecum nu va înseta, în veac; ba încă apa pe care i-o voi da Eu se&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;va preface în el într-un izvor de apă care țâșnește în viața veșnică.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,15,'„Doamne”, I-a zis&lt;f&gt;ⓩ&lt;/f&gt; femeia, „dă-mi această apă, ca să nu mai însetez și să nu mai vin până aici să scot!”');
INSERT INTO &quot;verses&quot; VALUES (500,4,16,'&lt;pb/&gt;El i-a spus: &lt;J&gt;„Du-te, cheamă-l pe bărbatul tău și vino aici!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,17,'Femeia I-a răspuns: „N-am bărbat.” Isus i-a zis: &lt;J&gt;„Bine ai spus: «N-am bărbat»!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,18,'&lt;J&gt;Pentru că cinci bărbați ai avut; și acela pe care-l ai acum nu-ți este bărbat. Aici ai spus adevărul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,19,'„Doamne”, I-a zis femeia, „văd&lt;f&gt;ⓐ&lt;/f&gt; că ești profet!');
INSERT INTO &quot;verses&quot; VALUES (500,4,20,'Părinții noștri s-au închinat pe muntele acesta&lt;f&gt;ⓑ&lt;/f&gt;; iar voi ziceți că în Ierusalim&lt;f&gt;ⓒ&lt;/f&gt; este locul unde trebuie să ne închinăm.”');
INSERT INTO &quot;verses&quot; VALUES (500,4,21,'Isus i-a spus: &lt;J&gt;„Crede-Mă, femeie: vine ceasul când&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;nu vă veți închina Tatălui nici pe muntele acesta, nici în Ierusalim.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,22,'&lt;J&gt;Voi vă închinați la ce&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;nu cunoașteți; noi ne închinăm la ce cunoaștem, căci&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;mântuirea este de la iudei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,23,'&lt;J&gt;Dar vine ceasul, și acum a și venit, când adevărații închinători se vor închina Tatălui în duh&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;și adevăr&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt;&lt;J&gt;, fiindcă Tatăl astfel de închinători caută.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,24,'&lt;J&gt;Dumnezeu&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;este Duh, și cine I se închină Lui trebuie să I se închine în duh și adevăr.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,25,'„Știu”, I-a zis femeia, „că va veni Mesia (care Se numește Hristos); când va veni El, ne&lt;f&gt;ⓙ&lt;/f&gt; va spune toate lucrurile.”');
INSERT INTO &quot;verses&quot; VALUES (500,4,26,'Isus i-a zis: &lt;J&gt;„Eu Sunt&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt;&lt;J&gt;: Cel care-ți vorbește!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,27,'&lt;pb/&gt;Atunci au venit ucenicii Săi, și se mirau că vorbește cu o femeie. Totuși, niciunul nu I-a zis: „Ce cauți?” sau „Despre ce vorbești cu ea?”');
INSERT INTO &quot;verses&quot; VALUES (500,4,28,'Atunci, femeia și-a lăsat urciorul, s-a dus în cetate și le-a zis oamenilor:');
INSERT INTO &quot;verses&quot; VALUES (500,4,29,'„Veniți să vedeți un om care mi-a spus&lt;f&gt;ⓛ&lt;/f&gt; tot ce am făcut! Nu cumva Acesta este Hristosul?”');
INSERT INTO &quot;verses&quot; VALUES (500,4,30,'Ei au ieșit din cetate și veneau spre El.');
INSERT INTO &quot;verses&quot; VALUES (500,4,31,'&lt;pb/&gt;În timpul acesta, ucenicii Îl rugau să mănânce și-I ziceau: „&lt;e&gt;Rabbí&lt;/e&gt;, mănâncă!”');
INSERT INTO &quot;verses&quot; VALUES (500,4,32,'Dar El le-a zis: &lt;J&gt;„Eu am de mâncat o mâncare pe care voi n-o cunoașteți.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,33,'Ucenicii și-au zis atunci unii altora: „Nu cumva I-a adus cineva să mănânce?”');
INSERT INTO &quot;verses&quot; VALUES (500,4,34,'Isus le-a zis: &lt;J&gt;„Mâncarea&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;Mea este să fac voia Celui ce M-a trimis și să împlinesc lucrarea Lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,35,'&lt;J&gt;Nu ziceți voi că mai sunt patru luni până la seceriș? Iată, Eu vă spun: ridicați-vă ochii și priviți holdele, care&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;sunt albe, gata pentru seceriș!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,36,'&lt;J&gt;Acum, cine&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;seceră primește o plată și strânge rod pentru viața veșnică, pentru ca și cel ce seamănă, și cel ce seceră să se bucure împreună.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,37,'&lt;J&gt;Căci, în această privință, este adevărată zicerea: «Unul seamănă, iar altul seceră.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,38,'&lt;J&gt;Eu v-am trimis să secerați acolo unde nu voi v-ați ostenit; alții s-au ostenit, și voi ați intrat în osteneala lor.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,39,'&lt;pb/&gt;Mulți samariteni din cetatea aceea au crezut în Isus din pricina&lt;f&gt;ⓟ&lt;/f&gt; cuvântului femeii, care mărturisea: „Mi-a spus tot ce am făcut!”');
INSERT INTO &quot;verses&quot; VALUES (500,4,40,'Când au venit samaritenii la El, L-au rugat să rămână la ei. Și El a rămas acolo două zile.');
INSERT INTO &quot;verses&quot; VALUES (500,4,41,'Mult mai mulți au crezut în El din pricina cuvântului Său.');
INSERT INTO &quot;verses&quot; VALUES (500,4,42,'Și ziceau femeii: „Acum nu mai credem din pricina spuselor tale, ci din pricină că L-am auzit noi înșine&lt;f&gt;ⓠ&lt;/f&gt; și știm că Acesta este cu adevărat [Hristosul], Mântuitorul lumii.”');
INSERT INTO &quot;verses&quot; VALUES (500,4,43,'&lt;pb/&gt;După cele două zile, Isus a plecat de acolo [ca să Se ducă] în Galileea.');
INSERT INTO &quot;verses&quot; VALUES (500,4,44,'Căci&lt;f&gt;ⓡ&lt;/f&gt; El Însuși spusese că un profet nu este prețuit în propria sa patrie&lt;f&gt;ⓢ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,4,45,'Când a ajuns în Galileea, a fost primit bine de galileeni, care văzuseră tot ce făcuse la Ierusalim în timpul sărbătorii; căci&lt;f&gt;ⓣ&lt;/f&gt; fuseseră și ei la sărbătoare.');
INSERT INTO &quot;verses&quot; VALUES (500,4,46,'&lt;pb/&gt;Isus S-a întors deci în Cana Galileei, unde&lt;f&gt;ⓤ&lt;/f&gt; preschimbase apa în vin. În Capernaum era un slujbaș domnesc al cărui fiu era bolnav.');
INSERT INTO &quot;verses&quot; VALUES (500,4,47,'Acela a aflat că Isus venise din Iudeea în Galileea, s-a dus la El și L-a rugat să vină și să-l vindece pe fiul său, care era pe moarte.');
INSERT INTO &quot;verses&quot; VALUES (500,4,48,'Isus i-a zis: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;nu vedeți semne și minuni, cu niciun chip nu credeți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,4,49,'Slujbașul domnesc I-a zis: „Doamne, vino până nu moare micuțul meu!”');
INSERT INTO &quot;verses&quot; VALUES (500,4,50,'&lt;J&gt;„Du-te”&lt;/J&gt;, i-a zis Isus, &lt;J&gt;„fiul tău trăiește!”&lt;/J&gt; Omul acela a crezut cuvintele pe care i le spusese Isus și a plecat.');
INSERT INTO &quot;verses&quot; VALUES (500,4,51,'Pe când cobora el, l-au întâmpinat robii lui și i-au adus vestea că fiul lui trăiește.');
INSERT INTO &quot;verses&quot; VALUES (500,4,52,'El i-a întrebat de ceasul în care a început să-i fie mai bine. Și ei i-au zis: „Ieri, în ceasul al șaptelea, l-a lăsat febra.”');
INSERT INTO &quot;verses&quot; VALUES (500,4,53,'Tatăl a cunoscut că tocmai în ceasul acela îi spusese Isus: &lt;J&gt;„Fiul tău trăiește.”&lt;/J&gt; Și au crezut el și toată casa lui.');
INSERT INTO &quot;verses&quot; VALUES (500,4,54,'Acesta este al doilea semn făcut de Isus după ce S-a întors din Iudeea în Galileea.');
INSERT INTO &quot;verses&quot; VALUES (500,5,1,'&lt;pb/&gt;După aceea&lt;f&gt;ⓦ&lt;/f&gt;, era o sărbătoare a iudeilor; și Isus S-a suit la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (500,5,2,'În Ierusalim, lângă Poarta Oilor, este o scăldătoare&lt;f&gt;ⓧ&lt;/f&gt;, numită în evreiește Betesda&lt;f&gt;[15]&lt;/f&gt;, care are cinci pridvoare.');
INSERT INTO &quot;verses&quot; VALUES (500,5,3,'În pridvoarele acestea, zăceau o mulțime de bolnavi, orbi, șchiopi, uscați, [care așteptau mișcarea apei.');
INSERT INTO &quot;verses&quot; VALUES (500,5,4,'Căci un înger al Domnului se cobora, din când în când, în scăldătoare și tulbura apa. Și cel dintâi care se cobora după tulburarea apei se făcea sănătos, orice boală ar fi avut.]');
INSERT INTO &quot;verses&quot; VALUES (500,5,5,'Era acolo un om bolnav de treizeci și opt de ani.');
INSERT INTO &quot;verses&quot; VALUES (500,5,6,'Isus, când l-a văzut zăcând și fiindcă știa că este bolnav de multă vreme, i-a zis: &lt;J&gt;„Vrei să te faci sănătos?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,7,'Bolnavul i-a răspuns: „Doamne, n-am pe nimeni să mă arunce în scăldătoare când se tulbură apa și, până să mă duc eu, se coboară altul înaintea mea.”');
INSERT INTO &quot;verses&quot; VALUES (500,5,8,'&lt;J&gt;„Scoală-te&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;, i-a zis Isus, &lt;J&gt;„ridică-ți patul&lt;/J&gt;&lt;f&gt;[16]&lt;/f&gt; &lt;J&gt;și umblă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,9,'Îndată omul s-a făcut sănătos, și-a ridicat patul și umbla. &lt;pb/&gt;Dar ziua aceea era&lt;f&gt;ⓩ&lt;/f&gt; o zi de sabat.');
INSERT INTO &quot;verses&quot; VALUES (500,5,10,'Iudeii ziceau deci celui ce fusese vindecat: „Este sabat; nu-ți&lt;f&gt;ⓐ&lt;/f&gt; este îngăduit să-ți ridici patul.”');
INSERT INTO &quot;verses&quot; VALUES (500,5,11,'El le-a răspuns: „Cel ce m-a făcut sănătos mi-a zis: &lt;J&gt;«Ridică-ți patul și umblă!»&lt;/J&gt;”');
INSERT INTO &quot;verses&quot; VALUES (500,5,12,'Ei l-au întrebat: „Cine este omul acela care ți-a zis: &lt;J&gt;«Ridică[-ți patul] și umblă»&lt;/J&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (500,5,13,'Dar cel vindecat nu știa cine este, căci Isus ieșise din mulțimea care era în locul acela.');
INSERT INTO &quot;verses&quot; VALUES (500,5,14,'După aceea, Isus l-a găsit în Templu și i-a zis: &lt;J&gt;„Iată că te-ai făcut sănătos; de acum să nu&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;mai păcătuiești, ca să nu ți se întâmple ceva mai rău.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,15,'Omul acela s-a dus și le-a spus iudeilor că Isus era cel care-l făcuse sănătos.');
INSERT INTO &quot;verses&quot; VALUES (500,5,16,'De aceea Îl urmăreau iudeii pe Isus [și căutau să-L omoare]: fiindcă făcea acestea în sabat.');
INSERT INTO &quot;verses&quot; VALUES (500,5,17,'Dar Isus le-a răspuns: &lt;J&gt;„Tatăl&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;Meu lucrează până acum, și Eu de asemenea lucrez.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,18,'Tocmai de aceea căutau&lt;f&gt;ⓓ&lt;/f&gt; și mai mult iudeii să-L omoare, nu numai fiindcă dezlega sabatul, ci și pentru că spunea că Dumnezeu este Tatăl Său și Se făcea&lt;f&gt;ⓔ&lt;/f&gt; pe Sine egal cu Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (500,5,19,'&lt;pb/&gt;Isus a luat cuvântul și le-a zis: &lt;J&gt;„Adevărat, adevărat vă spun: Fiul&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;nu poate face nimic de la Sine, în afară de ceea ce Îl vede pe Tatăl făcând; tot ce face Tatăl, face și Fiul întocmai.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,20,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;Tatăl Îl iubește pe Fiul și-I arată tot ce face; și-I va arăta lucrări mai mari decât acestea, ca voi să vă minunați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,21,'&lt;J&gt;Căci, după cum Tatăl învie morții și le dă viață, tot așa&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;și Fiul dă viață cui vrea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,22,'&lt;J&gt;Tatăl nici nu judecă pe nimeni, ci toată judecata a&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;dat-o Fiului,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,23,'&lt;J&gt;pentru ca toți să-L cinstească pe Fiul cum Îl cinstesc pe Tatăl. Cine&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;nu-L cinstește pe Fiul nu-L cinstește pe Tatăl, care L-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,24,'&lt;pb/&gt;&lt;J&gt;Adevărat, adevărat vă spun: cine&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;ascultă cuvântul Meu și crede în Cel ce M-a trimis are viața veșnică și nu ajunge sub osândă, ci&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;a trecut din moarte la viață.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,25,'&lt;J&gt;Adevărat, adevărat vă spun: vine ceasul, și acum a și venit, când cei morți&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;vor auzi glasul Fiului lui Dumnezeu, și cei ce-l vor asculta vor învia.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,26,'&lt;J&gt;Căci, după cum Tatăl are viața în Sine Însuși, tot așa I-a dat și Fiului să aibă viața în Sine Însuși.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,27,'&lt;J&gt;Și I-a&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;dat autoritate să judece, întrucât&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;este Fiul Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,28,'&lt;J&gt;Nu vă mirați de lucrul acesta, pentru că vine ceasul când toți cei din morminte vor auzi glasul Lui&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,29,'&lt;J&gt;și&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;vor ieși afară din ele: cei ce au făcut binele vor învia pentru viață, iar cei ce&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;au făcut răul vor învia pentru judecată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,30,'&lt;pb/&gt;&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;nu pot face nimic de la Mine Însumi: judec după cum aud; și judecata Mea este dreaptă, pentru că nu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;caut voia Mea, ci voia [Tatălui], care M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,31,'&lt;pb/&gt;&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;Eu mărturisesc despre Mine Însumi, mărturia Mea nu este adevărată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,32,'&lt;J&gt;Este un Altul&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;care mărturisește despre Mine; și știu că mărturia pe care o dă El despre Mine este adevărată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,33,'&lt;J&gt;Voi ați trimis la Ioan, și&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;el a mărturisit pentru adevăr.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,34,'&lt;J&gt;Nu că Eu am nevoie de mărturia unui om&lt;/J&gt;&lt;f&gt;[17]&lt;/f&gt;&lt;J&gt;, ci spun acestea ca să fiți mântuiți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,35,'&lt;J&gt;Ioan era făclia ce arde și&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;luminează, iar voi&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;ați vrut să vă veseliți o vreme la lumina lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,36,'&lt;pb/&gt;&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;însă am o mărturie mai mare decât a lui Ioan, căci lucrările pe care Mi le-a dat Tatăl să le împlinesc, tocmai lucrările&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;acestea pe care le fac mărturisesc despre Mine că Tatăl M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,37,'&lt;J&gt;Și Tatăl, care M-a trimis, a&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;mărturisit El Însuși despre Mine. Voi nu&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;I-ați auzit niciodată glasul și nici nu I-ați văzut chipul,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,38,'&lt;J&gt;și Cuvântul Lui nu rămâne în voi, pentru că nu credeți în Cel pe care L-a trimis El.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,39,'&lt;J&gt;Voi cercetați&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;Scripturile pentru că socotiți că în ele&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;aveți viața veșnică, și tocmai ele mărturisesc despre Mine!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,40,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;nu vreți să veniți la Mine, ca să aveți viață.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,41,'&lt;pb/&gt;&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;nu primesc o slavă care vine de la oameni.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,42,'&lt;J&gt;Dar știu că n-aveți dragoste de Dumnezeu în voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,43,'&lt;J&gt;Eu am venit în Numele Tatălui Meu, și nu Mă primiți; dacă ar veni un altul, în numele lui însuși, pe acela l-ați primi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,44,'&lt;J&gt;Cum&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;ați putea crede voi, care primiți slava pe care v-o dați unii altora și nu căutați slava&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;care vine de la singurul Dumnezeu?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,45,'&lt;pb/&gt;&lt;J&gt;Să nu credeți că Eu vă voi învinui înaintea Tatălui; cel&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;care vă învinuiește este Moise, în care v-ați pus nădejdea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,46,'&lt;J&gt;Căci, dacă l-ați crede pe Moise, ați crede și în Mine, pentru că&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;el a scris despre Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,5,47,'&lt;J&gt;Dar, dacă nu credeți cele scrise de el, cum veți crede cuvintele Mele?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,1,'&lt;pb/&gt;După aceea&lt;f&gt;ⓚ&lt;/f&gt;, Isus S-a dus dincolo de Marea Galileei, adică a Tiberiadei.');
INSERT INTO &quot;verses&quot; VALUES (500,6,2,'O mare mulțime Îl urma, pentru că vedea semnele pe care le făcea cu cei bolnavi.');
INSERT INTO &quot;verses&quot; VALUES (500,6,3,'Isus S-a suit pe munte și stătea acolo cu ucenicii Săi.');
INSERT INTO &quot;verses&quot; VALUES (500,6,4,'Paștele, sărbătoarea iudeilor, era&lt;f&gt;ⓛ&lt;/f&gt; aproape.');
INSERT INTO &quot;verses&quot; VALUES (500,6,5,'Isus&lt;f&gt;ⓜ&lt;/f&gt; Și-a ridicat ochii și, când a văzut că o mare mulțime vine spre El, i-a zis lui Filip: &lt;J&gt;„De unde să cumpărăm pâini ca să mănânce oamenii aceștia?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,6,'Spunea lucrul acesta ca să-l încerce, pentru că știa ce avea să facă.');
INSERT INTO &quot;verses&quot; VALUES (500,6,7,'Filip i-a răspuns: „Dacă am cumpăra pâini de&lt;f&gt;ⓝ&lt;/f&gt; două sute de dinari, tot n-ar ajunge nici cât să capete fiecare puțintel din ele.”');
INSERT INTO &quot;verses&quot; VALUES (500,6,8,'Unul din ucenicii Săi, Andrei, fratele lui Simon Petru, I-a zis:');
INSERT INTO &quot;verses&quot; VALUES (500,6,9,'„Este aici un băiețel care are cinci pâini de orz și doi pești, dar&lt;f&gt;ⓞ&lt;/f&gt; ce sunt acestea la atâția?”');
INSERT INTO &quot;verses&quot; VALUES (500,6,10,'Isus a zis: &lt;J&gt;„Puneți-i pe oameni să se așeze!”&lt;/J&gt; În locul acela era multă iarbă. Așadar, s-au așezat, numărul bărbaților fiind de aproape cinci mii.');
INSERT INTO &quot;verses&quot; VALUES (500,6,11,'Isus a luat pâinile și, după ce a adus mulțumire, le-a împărțit [ucenicilor, iar ucenicii le-au împărțit] celor ce se așezaseră; de asemenea, și din pești cât au vrut.');
INSERT INTO &quot;verses&quot; VALUES (500,6,12,'Când s-au săturat, Isus le-a zis ucenicilor Săi: &lt;J&gt;„Strângeți bucățile rămase, ca să nu se piardă nimic!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,13,'Le-au adunat deci și au umplut douăsprezece coșuri cu bucăți rămase de la cele cinci pâini de orz, care au prisosit celor ce mâncaseră.');
INSERT INTO &quot;verses&quot; VALUES (500,6,14,'Oamenii aceia, când au văzut semnul pe care-l făcuse Isus, ziceau: „Cu adevărat, acesta este Profetul&lt;f&gt;ⓟ&lt;/f&gt; care urma să vină în lume.”');
INSERT INTO &quot;verses&quot; VALUES (500,6,15,'Isus, fiindcă știa că vor veni să-L ia cu de-a sila ca să-L facă rege, S-a retras iarăși pe munte, numai El singur.');
INSERT INTO &quot;verses&quot; VALUES (500,6,16,'&lt;pb/&gt;Când&lt;f&gt;ⓠ&lt;/f&gt; s-a înserat, ucenicii Lui s-au coborât la mare.');
INSERT INTO &quot;verses&quot; VALUES (500,6,17,'S-au suit într-o barcă și s-au pornit să treacă marea, spre Capernaum. (Se întunecase, și Isus încă nu venise la ei.)');
INSERT INTO &quot;verses&quot; VALUES (500,6,18,'Sufla un vânt puternic, și marea era agitată.');
INSERT INTO &quot;verses&quot; VALUES (500,6,19,'După ce au vâslit cam douăzeci și cinci sau treizeci de stadii&lt;f&gt;[18]&lt;/f&gt;, L-au văzut pe Isus umblând pe mare și apropiindu-Se de barcă. Și s-au înfricoșat.');
INSERT INTO &quot;verses&quot; VALUES (500,6,20,'Dar El le-a zis: &lt;J&gt;„Eu sunt; nu vă temeți!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,21,'Voiau să-L ia în barcă, iar barca a sosit îndată la locul spre care mergeau.');
INSERT INTO &quot;verses&quot; VALUES (500,6,22,'&lt;pb/&gt;A doua zi, mulțimea care rămăsese de cealaltă parte a mării a băgat de seamă că acolo nu mai era decât o barcă și că Isus nu Se suise cu ucenicii Săi în barcă, ci ucenicii plecaseră singuri.');
INSERT INTO &quot;verses&quot; VALUES (500,6,23,'Au sosit și alte bărci din Tiberiada, aproape de locul unde mâncaseră ei pâinea, după ce Domnul adusese mulțumire.');
INSERT INTO &quot;verses&quot; VALUES (500,6,24,'Când au văzut cei din mulțime că nici Isus, nici ucenicii Săi nu erau acolo, s-au suit și ei în bărci și s-au dus la Capernaum să-L caute pe Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,6,25,'Când L-au găsit, dincolo de mare, I-au zis: „&lt;e&gt;Rabbí&lt;/e&gt;, când ai ajuns aici?”');
INSERT INTO &quot;verses&quot; VALUES (500,6,26,'Drept răspuns, Isus le-a zis: &lt;J&gt;„Adevărat, adevărat vă spun: Mă căutați nu pentru că ați văzut semne, ci pentru că ați mâncat din pâini și v-ați săturat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,27,'&lt;J&gt;Lucrați nu pentru mâncarea care piere, ci pentru&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;mâncarea care dăinuie în viața veșnică și pe care v-o va da Fiul Omului, căci pe&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;El L-a pecetluit Tatăl, Dumnezeu!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,28,'Ei I-au zis: „Ce să facem ca să înfăptuim lucrările lui Dumnezeu?”');
INSERT INTO &quot;verses&quot; VALUES (500,6,29,'Isus le-a răspuns: &lt;J&gt;„Aceasta&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;este lucrarea lui Dumnezeu: să credeți în Acela pe care L-a trimis El.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,30,'&lt;pb/&gt;Ei I-au zis: „Ce&lt;f&gt;ⓤ&lt;/f&gt; semn faci tu ca să-l vedem și să credem în tine? Ce lucrezi tu?');
INSERT INTO &quot;verses&quot; VALUES (500,6,31,'Părinții&lt;f&gt;ⓥ&lt;/f&gt; noștri au mâncat mană în pustiu, după cum este scris: &lt;e&gt;Pâine din cer le-a dat să mănânce.&lt;/e&gt;&lt;f&gt;[19]&lt;/f&gt;”');
INSERT INTO &quot;verses&quot; VALUES (500,6,32,'Isus le-a zis: &lt;J&gt;„Adevărat, adevărat vă spun: nu Moise v-a dat pâinea din cer, ci Tatăl Meu vă dă adevărata Pâine din cer,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,33,'&lt;J&gt;căci Pâinea lui Dumnezeu este cea care se coboară din cer și dă viață lumii.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,34,'„Doamne&lt;f&gt;ⓦ&lt;/f&gt;”, I-au zis ei, „dă-ne totdeauna această pâine!”');
INSERT INTO &quot;verses&quot; VALUES (500,6,35,'Isus le-a zis: &lt;J&gt;„Eu&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;sunt Pâinea vieții. Cine&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;vine la Mine nicidecum nu va flămânzi, și cine crede în Mine nu va înseta niciodată.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,36,'&lt;pb/&gt;&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;v-am spus că M-ați și văzut, și nu credeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,37,'&lt;J&gt;Tot&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;ce-Mi dă Tatăl va ajunge la Mine, și pe&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;cel ce vine la Mine nicidecum nu-l voi izgoni afară,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,38,'&lt;J&gt;căci M-am coborât din cer ca să fac nu&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;voia Mea, ci&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;voia Celui ce M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,39,'&lt;J&gt;Și aceasta este voia Celui ce M-a trimis: să&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;nu pierd pe niciunul din cei pe care Mi i-a dat El, ci să-i înviez în ziua de apoi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,40,'&lt;J&gt;Aceasta este voia Tatălui Meu: ca oricine&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;vede pe Fiul și crede în El să aibă viață veșnică; și Eu îl voi învia în ziua de apoi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,41,'&lt;pb/&gt;Iudeii murmurau împotriva Lui, pentru că zisese: &lt;J&gt;„Eu sunt Pâinea care s-a coborât din cer.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,42,'Și ziceau: „Oare&lt;f&gt;ⓖ&lt;/f&gt; nu este acesta Isus, fiul lui Iosif, pe ai cărui tată și mamă îi cunoaștem? Atunci cum zice el: &lt;J&gt;«Eu M-am coborât din cer»&lt;/J&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (500,6,43,'Isus le-a răspuns: &lt;J&gt;„Nu murmurați între voi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,44,'&lt;J&gt;Nimeni&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;nu poate veni la Mine dacă nu-l atrage Tatăl, care M-a trimis; și Eu îl voi învia în ziua de apoi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,45,'&lt;J&gt;În&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;profeți este scris: &lt;e&gt;Toți vor fi învățați de Dumnezeu.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[20]&lt;/f&gt; &lt;J&gt;Așa că oricine a auzit de la Tatăl și a învățat de la El vine la Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,46,'&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;că cineva a văzut pe Tatăl, în afară de&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;Cel care este de la Dumnezeu; Acesta L-a văzut pe Tatăl.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,47,'&lt;J&gt;Adevărat, adevărat vă spun: cine&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;crede în Mine are viață veșnică.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,48,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;sunt Pâinea vieții.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,49,'&lt;J&gt;Părinții&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;voștri au mâncat mană în pustiu și au murit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,50,'&lt;J&gt;Aceasta este Pâinea&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;care se coboară din cer ca omul să mănânce din ea și să nu moară.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,51,'&lt;J&gt;Eu sunt Pâinea&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;vie, care&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;s-a coborât din cer. Dacă mănâncă cineva din Pâinea aceasta, va trăi în veac; și Pâinea pe care o voi da Eu este trupul Meu, [pe care îl voi da] pentru viața lumii.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,52,'&lt;pb/&gt;Iudeii se certau&lt;f&gt;ⓡ&lt;/f&gt; între ei și ziceau: „Cum&lt;f&gt;ⓢ&lt;/f&gt; poate omul acesta să ne dea trupul lui să-l mâncăm?”');
INSERT INTO &quot;verses&quot; VALUES (500,6,53,'Isus le-a spus: &lt;J&gt;„Adevărat, adevărat vă spun: dacă nu mâncați&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;trupul Fiului Omului și nu beți sângele Lui, n-aveți viață în voi înșivă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,54,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;mănâncă trupul Meu și bea sângele Meu are viață veșnică; și Eu îl voi învia în ziua de apoi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,55,'&lt;J&gt;Căci trupul Meu este cu adevărat o hrană, și sângele Meu este cu adevărat o băutură.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,56,'&lt;J&gt;Cine mănâncă trupul Meu și bea sângele Meu rămâne&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;în Mine, și Eu rămân în el.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,57,'&lt;J&gt;După cum M-a trimis pe Mine Tatăl, care este viu, și Eu trăiesc prin Tatăl, tot așa cel care Mă mănâncă pe Mine va trăi și el prin Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,58,'&lt;J&gt;Aceasta&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;este Pâinea care s-a coborât din cer: nu precum au mâncat părinții voștri [mană] și au murit. Cine mănâncă Pâinea aceasta va trăi în veac.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,59,'Isus a spus acestea în sinagogă, când îi învăța pe oameni în Capernaum.');
INSERT INTO &quot;verses&quot; VALUES (500,6,60,'&lt;pb/&gt;Mulți&lt;f&gt;ⓧ&lt;/f&gt; din ucenicii Lui, când au auzit aceste cuvinte, au zis: „Greu este cuvântul acesta! Cine poate să-l primească?”');
INSERT INTO &quot;verses&quot; VALUES (500,6,61,'Isus, care știa în Sine că ucenicii Săi murmurau în această privință, le-a zis: &lt;J&gt;„Cuvântul acesta vă face să vă poticniți?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,62,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;dacă ați vedea pe Fiul Omului suindu-Se unde era mai înainte?…&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,63,'&lt;J&gt;Duhul&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;este cel care dă viață; carnea nu folosește la nimic; cuvintele pe care vi le-am spus Eu sunt duh și viață.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,64,'&lt;J&gt;Dar între voi sunt&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;unii care nu cred.”&lt;/J&gt; Căci Isus&lt;f&gt;ⓑ&lt;/f&gt; știa de la început cine erau cei ce nu cred și cine era cel ce Îl va vinde.');
INSERT INTO &quot;verses&quot; VALUES (500,6,65,'Și a adăugat: &lt;J&gt;„Tocmai de aceea v-am&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;spus că nimeni nu poate să vină la Mine dacă nu i-a fost dat de Tatăl [Meu].”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,66,'&lt;pb/&gt;De&lt;f&gt;ⓓ&lt;/f&gt; atunci, mulți din ucenicii Lui s-au întors și nu mai umblau cu El.');
INSERT INTO &quot;verses&quot; VALUES (500,6,67,'Atunci, Isus le-a spus celor doisprezece: &lt;J&gt;„Voi nu vreți să vă duceți?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,68,'„Doamne”, I-a răspuns Simon Petru, „la cine să ne ducem? Tu ai cuvintele&lt;f&gt;ⓔ&lt;/f&gt; vieții veșnice.');
INSERT INTO &quot;verses&quot; VALUES (500,6,69,'Și&lt;f&gt;ⓕ&lt;/f&gt; noi am crezut și am ajuns la cunoștința că Tu ești [Hristosul], Sfântul lui Dumnezeu.”');
INSERT INTO &quot;verses&quot; VALUES (500,6,70,'Isus le-a răspuns: &lt;J&gt;„Nu&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;v-am ales Eu pe voi, cei doisprezece? Și totuși, unul&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;din voi este diavol.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,6,71,'Vorbea despre Iuda, fiul lui Simon Iscariotul, căci el avea să-L vândă: el, unul din cei doisprezece.');
INSERT INTO &quot;verses&quot; VALUES (500,7,1,'&lt;pb/&gt;După aceea, Isus străbătea Galileea; nu voia să treacă prin Iudeea, fiindcă&lt;f&gt;ⓘ&lt;/f&gt; iudeii căutau să-L omoare.');
INSERT INTO &quot;verses&quot; VALUES (500,7,2,'Și&lt;f&gt;ⓙ&lt;/f&gt; sărbătoarea iudeilor, Așezarea Corturilor, era aproape.');
INSERT INTO &quot;verses&quot; VALUES (500,7,3,'Frații&lt;f&gt;ⓚ&lt;/f&gt; Săi I-au zis: „Pleacă de aici și du-te în Iudeea, ca să vadă și ucenicii tăi lucrările pe care le faci!');
INSERT INTO &quot;verses&quot; VALUES (500,7,4,'Nimeni nu lucrează în ascuns când caută să ajungă cunoscut. Dacă faci acestea, arată-te lumii!”');
INSERT INTO &quot;verses&quot; VALUES (500,7,5,'Căci nici&lt;f&gt;ⓛ&lt;/f&gt; chiar frații Săi nu credeau în El.');
INSERT INTO &quot;verses&quot; VALUES (500,7,6,'Isus le-a zis: &lt;J&gt;„Vremea&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;Mea n-a sosit încă, dar vouă vremea totdeauna vă este prielnică.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,7,'&lt;J&gt;Pe voi lumea nu&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;vă poate urî, dar pe Mine Mă urăște, căci&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;Eu mărturisesc despre ea că faptele ei sunt rele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,8,'&lt;J&gt;Voi suiți-vă la sărbătoare; Eu nu Mă sui la sărbătoarea aceasta, căci&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;vremea Mea nu s-a împlinit încă.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,9,'După ce le-a spus aceste lucruri, a rămas în Galileea.');
INSERT INTO &quot;verses&quot; VALUES (500,7,10,'&lt;pb/&gt;După ce s-au suit frații Săi la sărbătoare, S-a suit și El, dar nu pe față, ci pe ascuns.');
INSERT INTO &quot;verses&quot; VALUES (500,7,11,'Iudeii&lt;f&gt;ⓠ&lt;/f&gt; Îl căutau la sărbătoare și ziceau: „Unde este?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,12,'Era mult freamăt&lt;f&gt;ⓡ&lt;/f&gt; cu privire la El în mulțime. Unii&lt;f&gt;ⓢ&lt;/f&gt; spuneau: „Este un om bun”, dar alții spuneau: „Ba nu, ci duce mulțimea în rătăcire.”');
INSERT INTO &quot;verses&quot; VALUES (500,7,13,'Totuși, nimeni nu vorbea pe față despre El, de&lt;f&gt;ⓣ&lt;/f&gt; frica iudeilor.');
INSERT INTO &quot;verses&quot; VALUES (500,7,14,'&lt;pb/&gt;Pe la jumătatea sărbătorii, Isus S-a suit la Templu și a început să dea învățătură.');
INSERT INTO &quot;verses&quot; VALUES (500,7,15,'Iar iudeii&lt;f&gt;ⓤ&lt;/f&gt; se mirau, zicând: „Cum de știe acesta carte fără să fi învățat?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,16,'Isus le-a răspuns: &lt;J&gt;„Învățătura Mea&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;nu este a Mea, ci a Celui ce M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,17,'&lt;J&gt;Dacă vrea cineva să facă voia Lui, va cunoaște dacă învățătura este de la Dumnezeu sau dacă Eu vorbesc de la Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,18,'&lt;J&gt;Cel&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;ce vorbește de la sine își caută propria slavă; dar cel ce caută slava celui ce l-a trimis, acela este adevărat și în el nu este nedreptate.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,19,'&lt;pb/&gt;&lt;J&gt;Oare&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;nu v-a dat Moise Legea? Totuși, nimeni dintre voi nu împlinește Legea. De&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;ce căutați să Mă omorâți?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,20,'Mulțimea I-a răspuns: „Ai demon&lt;f&gt;ⓩ&lt;/f&gt;. Cine caută să te omoare?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,21,'Drept răspuns, Isus le-a zis: &lt;J&gt;„O lucrare am făcut, și toți vă mirați.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,22,'&lt;J&gt;Moise&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;v-a dat circumcizia (nu că ea vine de la Moise, ci&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;de la patriarhi), și voi îl circumcideți pe om în ziua sabatului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,23,'&lt;J&gt;Dacă un om primește circumcizia în ziua sabatului, ca să nu se calce Legea lui Moise, vă mâniați pe Mine că am&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;însănătoșit un om în ziua sabatului?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,24,'&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;judecați după înfățișare, ci judecați după judecata cea dreaptă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,25,'&lt;pb/&gt;Unii dintre locuitorii Ierusalimului spuneau: „Nu este el acela pe care caută să-l omoare?');
INSERT INTO &quot;verses&quot; VALUES (500,7,26,'Și iată că vorbește pe față și ei nu-i spun nimic! Nu cumva căpeteniile au cunoscut&lt;f&gt;ⓔ&lt;/f&gt; cu adevărat că acesta este Hristosul?');
INSERT INTO &quot;verses&quot; VALUES (500,7,27,'Dar&lt;f&gt;ⓕ&lt;/f&gt; pe acesta îl știm de unde este; când însă va veni Hristosul, nimeni nu va ști de unde este.”');
INSERT INTO &quot;verses&quot; VALUES (500,7,28,'Atunci Isus, care dădea învățătură în Templu, a strigat, zicând: &lt;J&gt;„Mă&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;cunoașteți și Mă știți de unde sunt! Eu&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;n-am venit de la Mine Însumi, însă Cel ce M-a trimis este&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;adevărat, numai că voi&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;nu-L cunoașteți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,29,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;Îl cunosc, căci de la El sunt și El M-a trimis.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,30,'Căutau&lt;f&gt;ⓛ&lt;/f&gt; deci să-L prindă, dar nimeni&lt;f&gt;ⓜ&lt;/f&gt; n-a pus mâna pe El, căci încă nu-I sosise ceasul.');
INSERT INTO &quot;verses&quot; VALUES (500,7,31,'&lt;pb/&gt;Din mulțime însă mulți&lt;f&gt;ⓝ&lt;/f&gt; au crezut în El și spuneau: „Când va veni Hristosul, va face oare mai multe semne decât a făcut acesta?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,32,'Fariseii au auzit mulțimea murmurând acestea despre El, și preoții de seamă și fariseii au trimis niște gărzi să-L prindă.');
INSERT INTO &quot;verses&quot; VALUES (500,7,33,'Isus a zis: &lt;J&gt;„Încă puțină&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vreme sunt cu voi și apoi Mă duc la Cel ce M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,34,'&lt;J&gt;Mă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;veți căuta, și nu Mă veți găsi; și unde sunt Eu, voi nu puteți veni.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,35,'Iudeii și-au spus între ei: „Unde are de gând să se ducă acesta, încât nu-l vom găsi? Doar n-are de gând să se ducă la cei&lt;f&gt;ⓠ&lt;/f&gt; răspândiți printre greci și să-i învețe pe greci!?');
INSERT INTO &quot;verses&quot; VALUES (500,7,36,'Ce înseamnă cuvântul acesta pe care l-a spus: &lt;J&gt;«Mă veți căuta, și nu Mă veți găsi; și unde sunt Eu, voi nu puteți veni»&lt;/J&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,37,'&lt;pb/&gt;În&lt;f&gt;ⓡ&lt;/f&gt; ziua cea din urmă — ziua cea mare a sărbătorii — Isus a stat în picioare și a strigat: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;însetează cineva, să vină la Mine și să bea!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,38,'&lt;J&gt;Cel ce crede în Mine, precum zice Scriptura, râuri de apă vie vor curge din&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;inima lui.”&lt;/J&gt;&lt;f&gt;[21]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,7,39,'(Spunea&lt;f&gt;ⓤ&lt;/f&gt; aceasta despre Duhul, pe care aveau să-L primească cei ce vor fi crezut în El. Căci Duhul [Sfânt] încă nu fusese dat, fiindcă Isus încă nu fusese proslăvit&lt;f&gt;ⓥ&lt;/f&gt;.)');
INSERT INTO &quot;verses&quot; VALUES (500,7,40,'&lt;pb/&gt;Unii din mulțime, când au auzit aceste cuvinte, spuneau: „Acesta este cu adevărat Profetul&lt;f&gt;ⓦ&lt;/f&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (500,7,41,'Alții spuneau: „Acesta&lt;f&gt;ⓧ&lt;/f&gt; este Hristosul.” Dar alții spuneau: „Doar nu din Galileea vine Hristosul&lt;f&gt;ⓨ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (500,7,42,'Nu zice&lt;f&gt;ⓩ&lt;/f&gt; Scriptura că Hristosul vine din sămânța lui David și din Betleem, satul de unde&lt;f&gt;ⓐ&lt;/f&gt; era David?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,43,'S-a&lt;f&gt;ⓑ&lt;/f&gt; făcut deci dezbinare în mulțime din pricina Lui.');
INSERT INTO &quot;verses&quot; VALUES (500,7,44,'Unii&lt;f&gt;ⓒ&lt;/f&gt; din ei voiau să-L prindă, dar nimeni n-a pus mâna pe El.');
INSERT INTO &quot;verses&quot; VALUES (500,7,45,'Atunci gărzile s-au întors la preoții de seamă și la farisei. Și aceștia le-au spus: „De ce nu l-ați adus?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,46,'Gărzile au răspuns: „Niciodată&lt;f&gt;ⓓ&lt;/f&gt; n-a vorbit vreun om ca omul acesta.”');
INSERT INTO &quot;verses&quot; VALUES (500,7,47,'Fariseii le-au răspuns: „Doar n-ați fost și voi duși în rătăcire!');
INSERT INTO &quot;verses&quot; VALUES (500,7,48,'A&lt;f&gt;ⓔ&lt;/f&gt; crezut în el vreunul dintre conducători sau dintre farisei?');
INSERT INTO &quot;verses&quot; VALUES (500,7,49,'Dar mulțimea aceasta, care nu cunoaște Legea, este blestemată!”');
INSERT INTO &quot;verses&quot; VALUES (500,7,50,'Nicodim, cel care&lt;f&gt;ⓕ&lt;/f&gt; venise la Isus mai înainte și care era unul dintre ei, le-a zis:');
INSERT INTO &quot;verses&quot; VALUES (500,7,51,'„Oare Legea&lt;f&gt;ⓖ&lt;/f&gt; noastră osândește pe vreun om fără să-l asculte mai întâi și să știe ce face?”');
INSERT INTO &quot;verses&quot; VALUES (500,7,52,'Drept răspuns, ei i-au zis: „Doar nu ești și tu din&lt;f&gt;ⓗ&lt;/f&gt; Galileea! Cercetează și vezi că din Galileea nu s-a ridicat niciun profet!”');
INSERT INTO &quot;verses&quot; VALUES (500,7,53,'&lt;pb/&gt;&lt;f&gt;[22]&lt;/f&gt; [[Și s-a întors fiecare acasă.');
INSERT INTO &quot;verses&quot; VALUES (500,8,1,'&lt;pb/&gt;Isus S-a dus la Muntele Măslinilor.');
INSERT INTO &quot;verses&quot; VALUES (500,8,2,'Dar, dis-de-dimineață, a venit din nou la Templu; și tot poporul venea la El. După ce S-a așezat, a început să-i învețe.');
INSERT INTO &quot;verses&quot; VALUES (500,8,3,'Iar cărturarii și fariseii au adus o femeie prinsă în adulter și, punând-o în mijloc,');
INSERT INTO &quot;verses&quot; VALUES (500,8,4,'I-au spus: „Învățătorule, femeia aceasta a fost prinsă chiar când săvârșea adulterul.');
INSERT INTO &quot;verses&quot; VALUES (500,8,5,'Moise&lt;f&gt;ⓘ&lt;/f&gt;, în Lege, ne-a poruncit să ucidem cu pietre pe astfel de femei: tu deci ce zici?”');
INSERT INTO &quot;verses&quot; VALUES (500,8,6,'Vorbeau astfel ca să-L ispitească și să aibă cu ce să-L învinuiască. Dar Isus S-a aplecat și scria cu degetul pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (500,8,7,'Cum ei stăruiau cu întrebările, S-a ridicat și le-a zis: &lt;J&gt;„Cine&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;este fără păcat între voi să arunce primul cu piatra în ea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,8,'Apoi S-a aplecat iarăși și scria cu degetul pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (500,8,9,'Cei ce au auzit [au fost mustrați&lt;f&gt;ⓚ&lt;/f&gt; de cugetul lor și] au ieșit afară, unul câte unul, începând cu bătrânii, [până la cei din urmă]. A rămas El singur cu femeia, care stătea în mijloc.');
INSERT INTO &quot;verses&quot; VALUES (500,8,10,'Isus S-a ridicat și [când n-a mai văzut pe nimeni decât pe femeie] i-a zis: &lt;J&gt;„Femeie, unde sunt [pârâșii tăi]? Nimeni nu te-a osândit?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,11,'„Nimeni, Doamne”, I-a răspuns ea. Și Isus i-a zis: &lt;J&gt;„Nici&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;Eu nu te osândesc. Du-te și de acum să&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;nu mai păcătuiești!”&lt;/J&gt;]]');
INSERT INTO &quot;verses&quot; VALUES (500,8,12,'&lt;pb/&gt;Isus le-a vorbit din nou și le-a zis: &lt;J&gt;„Eu&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;sunt Lumina lumii; cine Mă urmează pe Mine nicidecum nu va umbla în întuneric, ci va avea lumina vieții.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,13,'Fariseii I-au zis atunci: „Tu&lt;f&gt;ⓞ&lt;/f&gt; mărturisești despre tine însuți; mărturia ta nu este adevărată.”');
INSERT INTO &quot;verses&quot; VALUES (500,8,14,'Drept răspuns, Isus le-a zis: &lt;J&gt;„Chiar dacă Eu mărturisesc despre Mine Însumi, mărturia Mea este adevărată; căci Eu știu de unde am venit și încotro Mă duc, dar voi&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;nu știți nici de unde vin, nici încotro Mă duc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,15,'&lt;J&gt;Voi&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;judecați după înfățișare; Eu&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;nu judec pe nimeni.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,16,'&lt;J&gt;Și chiar dacă judec Eu, judecata Mea este adevărată, pentru că nu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;sunt singur, ci suntem Eu și Tatăl, care M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,17,'&lt;J&gt;În&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;Legea voastră este scris că mărturia a doi oameni este adevărată:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,18,'&lt;J&gt;Eu sunt cel care mărturisește despre Mine; tot despre Mine mărturisește&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;și Tatăl, care M-a trimis.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,19,'Ei I-au zis deci: „Unde este tatăl tău?” Isus a răspuns: &lt;J&gt;„Nici pe Mine&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;nu Mă cunoașteți, nici pe Tatăl Meu. Dacă&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;M-ați cunoaște pe Mine, L-ați cunoaște și pe Tatăl Meu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,20,'Aceste cuvinte le-a rostit în vistierie&lt;f&gt;ⓧ&lt;/f&gt;, pe când dădea învățătură în Templu; și nimeni&lt;f&gt;ⓨ&lt;/f&gt; n-a pus mâna pe El, pentru că nu-I sosise încă&lt;f&gt;ⓩ&lt;/f&gt; ceasul.');
INSERT INTO &quot;verses&quot; VALUES (500,8,21,'&lt;pb/&gt;Isus le-a mai spus: &lt;J&gt;„Eu Mă duc, și Mă&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;veți căuta și veți&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;muri în păcatul vostru; acolo unde Mă duc Eu, voi nu puteți veni.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,22,'Iudeii ziceau: „Doar nu se va omorî singur, de zice: &lt;J&gt;«Acolo unde Mă duc Eu, voi nu puteți veni»&lt;/J&gt;!”');
INSERT INTO &quot;verses&quot; VALUES (500,8,23,'&lt;J&gt;„Voi&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;sunteți de jos”&lt;/J&gt;, le-a zis El, &lt;J&gt;„Eu sunt de sus. Voi&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;sunteți din lumea aceasta, Eu nu sunt din lumea aceasta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,24,'&lt;J&gt;De aceea&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;v-am spus că veți muri în păcatele voastre, căci&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;, dacă nu credeți că Eu Sunt, veți muri în păcatele voastre.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,25,'„Tu cine ești?”, I-au zis ei. Isus le-a răspuns: &lt;J&gt;„Ceea ce vă spun de la început.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,26,'&lt;J&gt;Multe am de spus și de osândit cu privire la voi, dar Cel ce&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;M-a trimis este adevărat; și Eu&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt;&lt;J&gt;, ce am auzit de la El, aceea spun lumii.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,27,'Ei n-au înțeles că le vorbea despre Tatăl.');
INSERT INTO &quot;verses&quot; VALUES (500,8,28,'Isus le-a mai zis: &lt;J&gt;„Când veți înălța&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;pe Fiul Omului, atunci&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;veți cunoaște că&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;Eu Sunt și că nu fac nimic de la Mine Însumi, ci vorbesc după cum&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;M-a învățat Tatăl Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,29,'&lt;J&gt;Cel&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;ce M-a trimis este cu Mine; [Tatăl]&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;nu M-a lăsat singur, pentru că&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;fac totdeauna ce-I este plăcut Lui.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,30,'&lt;pb/&gt;Pe când vorbea Isus acestea, mulți&lt;f&gt;ⓟ&lt;/f&gt; au crezut în El.');
INSERT INTO &quot;verses&quot; VALUES (500,8,31,'Și Isus le-a zis iudeilor care crezuseră în El: &lt;J&gt;„Dacă rămâneți în cuvântul Meu, sunteți cu adevărat ucenicii Mei;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,32,'&lt;J&gt;veți cunoaște Adevărul&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt;&lt;J&gt;, și Adevărul vă va face liberi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,33,'Ei I-au răspuns: „Noi&lt;f&gt;ⓡ&lt;/f&gt; suntem sămânța lui Avraam și n-am fost niciodată robi nimănui. Cum zici tu: &lt;J&gt;«Veți fi liberi»&lt;/J&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (500,8,34,'&lt;J&gt;„Adevărat, adevărat vă spun”&lt;/J&gt;, le-a răspuns Isus: &lt;J&gt;„Oricine&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;trăiește în păcat este rob al păcatului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,35,'&lt;J&gt;Iar robul&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;nu rămâne pentru totdeauna în casă; fiul însă rămâne pentru totdeauna.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,36,'&lt;J&gt;Deci, dacă&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;Fiul vă face liberi, veți fi cu adevărat liberi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,37,'&lt;pb/&gt;&lt;J&gt;Știu că sunteți sămânța lui Avraam, dar căutați&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;să Mă omorâți pentru că nu pătrunde în voi cuvântul Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,38,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;spun ce am văzut la Tatăl [Meu], iar voi faceți ce ați auzit de la tatăl [vostru].”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,39,'Ei I-au răspuns: „Tatăl nostru este Avraam&lt;f&gt;ⓧ&lt;/f&gt;.” Isus le-a spus: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;erați copii ai lui Avraam, făceați faptele lui Avraam.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,40,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;acum căutați să Mă omorâți pe Mine, un om, care v-am spus adevărul pe&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;care l-am auzit de la Dumnezeu. Așa ceva Avraam n-a făcut.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,41,'&lt;J&gt;Voi faceți faptele tatălui vostru.”&lt;/J&gt; Ei I-au spus: „Noi nu suntem născuți din desfrânare; avem un&lt;f&gt;ⓑ&lt;/f&gt; singur Tată: pe Dumnezeu.”');
INSERT INTO &quot;verses&quot; VALUES (500,8,42,'Isus le-a zis: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;era Dumnezeu Tatăl vostru, Mă iubeați și pe Mine, căci&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;Eu am ieșit și vin de la Dumnezeu; n-am&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;venit de la Mine Însumi, ci El M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,43,'&lt;J&gt;De&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;ce nu înțelegeți vorbirea Mea? Pentru că nu puteți asculta cuvântul Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,44,'&lt;J&gt;Voi&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;sunteți de la tatăl vostru, Diavolul, și vreți să împliniți poftele tatălui vostru. El de la început a fost ucigaș de oameni și nu&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;stă în adevăr, pentru că nu este adevăr în el. Ori de câte ori spune o minciună, vorbește din ale sale, căci este mincinos și tatăl minciunii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,45,'&lt;J&gt;Iar pe Mine, pentru că spun adevărul, nu Mă credeți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,46,'&lt;J&gt;Cine din voi Mă poate dovedi vinovat de păcat? Dacă spun adevărul, de ce nu Mă credeți?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,47,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;este din Dumnezeu ascultă cuvintele lui Dumnezeu; voi de aceea nu ascultați: pentru că nu sunteți din Dumnezeu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,48,'&lt;pb/&gt;Iudeii I-au răspuns: „Nu zicem noi bine că ești samaritean și că ai demon&lt;f&gt;ⓙ&lt;/f&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (500,8,49,'&lt;J&gt;„Nu am demon”&lt;/J&gt;, le-a răspuns Isus, &lt;J&gt;„ci Îl cinstesc pe Tatăl Meu, dar voi nu Mă cinstiți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,50,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;nu caut slava Mea; este cine să o caute și să judece.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,51,'&lt;J&gt;Adevărat, adevărat vă spun: dacă&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;păzește cineva cuvântul Meu, nu va vedea moartea în veac.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,52,'„Acum”, I-au zis iudeii, „vedem bine că ai demon; Avraam&lt;f&gt;ⓜ&lt;/f&gt; a murit, la fel și profeții; iar tu spui: &lt;J&gt;«Dacă păzește cineva cuvântul Meu, cu niciun chip nu va gusta moartea, în veac.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,53,'Doar nu ești tu mai mare decât părintele nostru Avraam, care a murit! Profeții, și ei au murit. Cine te crezi?”');
INSERT INTO &quot;verses&quot; VALUES (500,8,54,'Isus a răspuns: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;Mă slăvesc Eu Însumi, slava Mea nu este nimic; Tatăl&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;Meu este cel care Mă slăvește; El, despre care voi ziceți că este Dumnezeul nostru,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,55,'&lt;J&gt;și totuși nu-L&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;cunoașteți. Eu Îl cunosc; dacă aș zice că nu-L cunosc, aș fi mincinos, asemenea vouă. Dar Îl cunosc și păzesc Cuvântul Lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,56,'&lt;J&gt;Tatăl vostru Avraam a&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;tresăltat de bucurie că are să vadă ziua Mea; a&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;văzut-o și s-a bucurat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,57,'„N-ai nici cincizeci de ani”, I-au spus iudeii, „și l-ai văzut pe Avraam!?”');
INSERT INTO &quot;verses&quot; VALUES (500,8,58,'Isus le-a spus: &lt;J&gt;„Adevărat, adevărat vă spun: mai înainte să fie Avraam, Eu Sunt&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,8,59,'Atunci au&lt;f&gt;ⓣ&lt;/f&gt; luat pietre ca să arunce în El. Dar Isus S-a ascuns și a ieșit din Templu, [trecând&lt;f&gt;ⓤ&lt;/f&gt; prin mijlocul lor. Și a trecut astfel pe lângă ei].');
INSERT INTO &quot;verses&quot; VALUES (500,9,1,'&lt;pb/&gt;Pe când trecea, Isus a văzut un orb din naștere.');
INSERT INTO &quot;verses&quot; VALUES (500,9,2,'Ucenicii Săi L-au întrebat: „&lt;e&gt;Rabbí&lt;/e&gt;, cine&lt;f&gt;ⓥ&lt;/f&gt; a păcătuit: omul acesta sau părinții lui, de s-a născut orb?”');
INSERT INTO &quot;verses&quot; VALUES (500,9,3,'Isus a răspuns: &lt;J&gt;„N-au păcătuit nici omul acesta, nici părinții lui, ci&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;s-a născut așa ca să se arate lucrările lui Dumnezeu în el.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,9,4,'&lt;J&gt;Cât este ziuă, trebuie&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;să facem lucrările Celui ce M-a trimis; vine noaptea, când nimeni nu mai poate să lucreze.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,9,5,'&lt;J&gt;Cât sunt în lume, sunt&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;Lumina lumii.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,9,6,'După ce a spus acestea, a&lt;f&gt;ⓩ&lt;/f&gt; scuipat pe pământ și a făcut tină din scuipat. Apoi a uns ochii orbului cu tină');
INSERT INTO &quot;verses&quot; VALUES (500,9,7,'și i-a spus: &lt;J&gt;„Du-te de te spală în&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;scăldătoarea Siloamului”&lt;/J&gt; (care, tradus, înseamnă: „Trimis”). El&lt;f&gt;ⓑ&lt;/f&gt; s-a dus, s-a spălat și s-a întors văzând.');
INSERT INTO &quot;verses&quot; VALUES (500,9,8,'&lt;pb/&gt;Vecinii și cei ce-l văzuseră înainte ca cerșetor spuneau: „Nu este acesta cel ce stătea și cerșea?”');
INSERT INTO &quot;verses&quot; VALUES (500,9,9,'Unii spuneau: „El este.” Alții spuneau: „Nu, dar seamănă cu el.” Iar el zicea: „Eu sunt.”');
INSERT INTO &quot;verses&quot; VALUES (500,9,10,'Deci i-au zis: „Cum ți s-au deschis ochii?”');
INSERT INTO &quot;verses&quot; VALUES (500,9,11,'El a răspuns: „Omul&lt;f&gt;ⓒ&lt;/f&gt; căruia I se zice Isus a făcut tină, mi-a uns ochii și mi-a zis: &lt;J&gt;«Du-te la Siloam și spală-te!»&lt;/J&gt; M-am dus, m-am spălat și mi-am căpătat vederea.”');
INSERT INTO &quot;verses&quot; VALUES (500,9,12,'„Unde este omul acela?”, l-au întrebat ei. El a răspuns: „Nu știu.”');
INSERT INTO &quot;verses&quot; VALUES (500,9,13,'&lt;pb/&gt;L-au adus la farisei pe cel ce fusese orb mai înainte.');
INSERT INTO &quot;verses&quot; VALUES (500,9,14,'Și era sabat ziua când făcuse Isus tină și-i deschisese ochii.');
INSERT INTO &quot;verses&quot; VALUES (500,9,15,'Din nou, fariseii l-au întrebat și ei cum și-a căpătat vederea. Și el le-a zis: „Mi-a pus tină pe ochi, m-am spălat și văd.”');
INSERT INTO &quot;verses&quot; VALUES (500,9,16,'Atunci, unii din farisei au început să zică: „Omul acesta nu este de la Dumnezeu, pentru că nu ține sabatul.” Dar alții spuneau: „Cum&lt;f&gt;ⓓ&lt;/f&gt; poate un om păcătos să facă asemenea semne?” Și&lt;f&gt;ⓔ&lt;/f&gt; era dezbinare între ei.');
INSERT INTO &quot;verses&quot; VALUES (500,9,17,'Iarăși l-au întrebat pe orb: „Tu ce zici despre el, acum că ți-a deschis ochii?” „Este&lt;f&gt;ⓕ&lt;/f&gt; un profet”, le-a răspuns el.');
INSERT INTO &quot;verses&quot; VALUES (500,9,18,'&lt;pb/&gt;Iudeii n-au crezut despre el că fusese orb și că își căpătase vederea până nu i-au chemat pe părinții lui și');
INSERT INTO &quot;verses&quot; VALUES (500,9,19,'i-au întrebat: „Acesta este fiul vostru, despre care spuneți că s-a născut orb? Cum de vede acum?”');
INSERT INTO &quot;verses&quot; VALUES (500,9,20,'Drept răspuns, părinții lui au zis: „Știm că acesta este fiul nostru și că s-a născut orb.');
INSERT INTO &quot;verses&quot; VALUES (500,9,21,'Însă cum de vede acum sau cine i-a deschis ochii, noi nu știm. Întrebați-l pe el: are o vârstă, va vorbi el însuși despre sine!”');
INSERT INTO &quot;verses&quot; VALUES (500,9,22,'Părinții lui au vorbit astfel pentru că se&lt;f&gt;ⓖ&lt;/f&gt; temeau de iudei, căci iudeii se învoiseră ca, dacă va mărturisi cineva că Isus este Hristosul, să&lt;f&gt;ⓗ&lt;/f&gt; fie dat afară din sinagogă.');
INSERT INTO &quot;verses&quot; VALUES (500,9,23,'De aceea au spus părinții lui: „Are o vârstă, întrebați-l pe el!”');
INSERT INTO &quot;verses&quot; VALUES (500,9,24,'&lt;pb/&gt;Așadar l-au chemat a doua oară pe omul care fusese orb și i-au zis: „Dă&lt;f&gt;ⓘ&lt;/f&gt; slavă lui Dumnezeu: noi&lt;f&gt;ⓙ&lt;/f&gt; știm că omul acesta este un păcătos.”');
INSERT INTO &quot;verses&quot; VALUES (500,9,25,'El a răspuns: „Dacă este un păcătos, nu știu; eu una știu: eram orb, și acum văd!”');
INSERT INTO &quot;verses&quot; VALUES (500,9,26,'L-au întrebat deci: „Ce ți-a făcut? Cum ți-a deschis ochii?”');
INSERT INTO &quot;verses&quot; VALUES (500,9,27,'„Deja v-am spus”, le-a răspuns el, „și n-ați ascultat. Pentru ce vreți să mai auziți încă o dată? Doar nu vreți și voi să vă faceți ucenicii Lui!”');
INSERT INTO &quot;verses&quot; VALUES (500,9,28,'Ei l-au ocărât și i-au zis: „Ucenicul lui ești tu! Noi suntem ucenicii lui Moise!');
INSERT INTO &quot;verses&quot; VALUES (500,9,29,'Știm că lui Moise i-a vorbit Dumnezeu; dar acesta nu&lt;f&gt;ⓚ&lt;/f&gt; știm de unde este.”');
INSERT INTO &quot;verses&quot; VALUES (500,9,30,'„Aici&lt;f&gt;ⓛ&lt;/f&gt; e lucrul de mirare”, le-a răspuns omul acela, „că voi nu știți de unde este, dar El mi-a deschis ochii!');
INSERT INTO &quot;verses&quot; VALUES (500,9,31,'Știm că Dumnezeu&lt;f&gt;ⓜ&lt;/f&gt; nu-i ascultă pe păcătoși; ci, dacă este cineva temător de Dumnezeu și face voia Lui, pe acela îl ascultă.');
INSERT INTO &quot;verses&quot; VALUES (500,9,32,'De când lumea nu s-a auzit să fi deschis cineva ochii unui orb din naștere.');
INSERT INTO &quot;verses&quot; VALUES (500,9,33,'Dacă&lt;f&gt;ⓝ&lt;/f&gt; omul acesta n-ar fi de la Dumnezeu, n-ar putea face nimic.”');
INSERT INTO &quot;verses&quot; VALUES (500,9,34,'„Tu ești născut cu totul în&lt;f&gt;ⓞ&lt;/f&gt; păcat”, i-au răspuns ei, „și vrei să ne înveți pe noi?” Și l-au dat afară.');
INSERT INTO &quot;verses&quot; VALUES (500,9,35,'&lt;pb/&gt;Isus a auzit că l-au dat afară și, când l-a găsit, i-a zis: &lt;J&gt;„Crezi tu în Fiul&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;Omului&lt;/J&gt;&lt;f&gt;[23]&lt;/f&gt;&lt;J&gt;?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,9,36,'El a răspuns: „Și cine este, Doamne, ca să cred în El?”');
INSERT INTO &quot;verses&quot; VALUES (500,9,37,'&lt;J&gt;„L-ai și văzut”&lt;/J&gt;, i-a zis Isus, &lt;J&gt;„și Cel&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;ce vorbește cu tine, Acela este!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,9,38,'„Cred, Doamne”, I-a zis el; și I s-a închinat.');
INSERT INTO &quot;verses&quot; VALUES (500,9,39,'&lt;pb/&gt;Apoi, Isus a zis: &lt;J&gt;„Eu pentru&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;judecată am venit în lumea aceasta: pentru ca&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;cei ce nu văd să vadă, și cei ce văd să ajungă orbi.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,9,40,'Unii din fariseii care erau lângă El au auzit acestea și I-au&lt;f&gt;ⓣ&lt;/f&gt; zis: „Doar n-om fi și noi orbi!”');
INSERT INTO &quot;verses&quot; VALUES (500,9,41,'&lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;ați fi orbi”&lt;/J&gt;, le-a răspuns Isus, &lt;J&gt;„n-ați avea păcat. Acum, că ziceți: «Vedem», păcatul vostru rămâne.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,1,'&lt;pb/&gt;&lt;J&gt;Adevărat, adevărat vă spun: cine nu intră pe ușă în staulul oilor, ci sare prin altă parte, este un hoț și un tâlhar.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,2,'&lt;J&gt;Dar cine intră pe ușă este păstorul oilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,3,'&lt;J&gt;Portarul îi deschide, și oile aud glasul lui; el își cheamă oile sale pe nume și le duce afară.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,4,'&lt;J&gt;După ce le scoate pe toate care sunt ale sale, merge înaintea lor; și oile îl urmează, pentru că îi cunosc glasul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,5,'&lt;J&gt;Pe un străin nu-l vor urma nicidecum, ci vor fugi de el, pentru că nu cunosc glasul străinilor.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,6,'Isus le-a spus această pildă, dar ei n-au înțeles despre ce le vorbea.');
INSERT INTO &quot;verses&quot; VALUES (500,10,7,'&lt;pb/&gt;Isus le-a mai spus: &lt;J&gt;„Adevărat, adevărat vă spun: Eu sunt Ușa oilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,8,'&lt;J&gt;Toți câți au venit înainte de Mine sunt hoți și tâlhari, dar oile n-au ascultat de ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,9,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;sunt Ușa. Dacă intră cineva prin Mine, va fi mântuit: va intra și va ieși și va găsi pășune.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,10,'&lt;J&gt;Hoțul nu vine decât să fure, să înjunghie și să nimicească. Eu am venit ca oile să aibă viață, și s-o aibă din belșug.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,11,'&lt;pb/&gt;&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;sunt Păstorul cel bun. Păstorul cel bun își dă viața pentru oi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,12,'&lt;J&gt;Cel plătit, care nu este păstor și ale cărui oi nu sunt ale sale, când vede lupul venind, lasă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;oile și fuge, iar lupul le răpește și le risipește.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,13,'&lt;i&gt;&lt;J&gt;Cel plătit fuge&lt;/i&gt; pentru că este plătit și nu-i pasă de oi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,14,'&lt;pb/&gt;&lt;J&gt;Eu sunt Păstorul cel bun. Eu Îmi cunosc oile&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;, și ele Mă cunosc pe Mine,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,15,'&lt;J&gt;așa cum&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;Mă cunoaște pe Mine Tatăl și cum Îl cunosc Eu pe Tatăl; și&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;Îmi dau viața pentru oi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,16,'&lt;J&gt;Mai am și alte&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;oi, care nu sunt din staulul acesta; și pe acelea trebuie să le aduc. Ele vor asculta de glasul Meu și&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;vor fi o turmă, cu un Păstor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,17,'&lt;J&gt;De aceea Mă iubește Tatăl, pentru că&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;Eu Îmi dau viața ca iarăși să o iau.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,18,'&lt;J&gt;Nimeni nu Mi-o ia, ci o dau Eu de la Mine. Am&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;autoritate&lt;/J&gt;&lt;f&gt;[24]&lt;/f&gt; &lt;J&gt;s-o dau și am autoritate s-o iau iarăși; porunca aceasta&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;am primit-o de la Tatăl Meu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,19,'&lt;pb/&gt;Din pricina acestor cuvinte, iarăși&lt;f&gt;ⓖ&lt;/f&gt; s-a făcut dezbinare între iudei.');
INSERT INTO &quot;verses&quot; VALUES (500,10,20,'Mulți dintre ei spuneau: „Are demon&lt;f&gt;ⓗ&lt;/f&gt; și a înnebunit. De ce-l ascultați?”');
INSERT INTO &quot;verses&quot; VALUES (500,10,21,'Alții spuneau: „Cuvintele acestea nu sunt cuvintele unui demonizat. Poate&lt;f&gt;ⓘ&lt;/f&gt; un demon să&lt;f&gt;ⓙ&lt;/f&gt; deschidă ochii orbilor?”');
INSERT INTO &quot;verses&quot; VALUES (500,10,22,'&lt;pb/&gt;În Ierusalim era atunci Sărbătoarea Înnoirii Templului&lt;f&gt;[25]&lt;/f&gt;. Era iarnă,');
INSERT INTO &quot;verses&quot; VALUES (500,10,23,'și Isus Se plimba prin Templu, prin&lt;f&gt;ⓚ&lt;/f&gt; Pridvorul lui Solomon.');
INSERT INTO &quot;verses&quot; VALUES (500,10,24,'Iudeii L-au înconjurat și I-au spus: „Până când ne vei ține sufletele în încordare? Dacă tu ești Hristosul, spune-ne-o deslușit!”');
INSERT INTO &quot;verses&quot; VALUES (500,10,25,'&lt;J&gt;„V-am spus”&lt;/J&gt;, le-a răspuns Isus, &lt;J&gt;„și nu credeți. Lucrările&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;pe care le fac Eu în Numele Tatălui Meu, ele mărturisesc despre Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,26,'&lt;J&gt;Dar&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;voi nu credeți, pentru că, după cum v-am spus, nu sunteți din oile Mele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,27,'&lt;J&gt;Oile&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;Mele ascultă glasul Meu; Eu le cunosc, și ele Mă urmează.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,28,'&lt;J&gt;Eu le dau viață veșnică, în veac nu&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vor pieri și nimeni nu le va răpi din mâna Mea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,29,'&lt;J&gt;Tatăl&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;Meu, care&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Mi le-a dat, este mai mare decât toți și nimeni nu le poate răpi din mâna Tatălui [Meu].&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,30,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;și Tatăl una suntem.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,31,'&lt;pb/&gt;Iudeii&lt;f&gt;ⓢ&lt;/f&gt; au luat iarăși pietre ca să-L ucidă.');
INSERT INTO &quot;verses&quot; VALUES (500,10,32,'Isus le-a răspuns: &lt;J&gt;„V-am arătat multe lucrări bune, care vin de la Tatăl [Meu]: pentru care dintre ele vreți să aruncați cu pietre în Mine?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,33,'Iudeii I-au răspuns: „Nu pentru o lucrare bună aruncăm noi cu pietre în tine, ci pentru blasfemie și pentru că tu, om fiind, te&lt;f&gt;ⓣ&lt;/f&gt; dai drept Dumnezeu.”');
INSERT INTO &quot;verses&quot; VALUES (500,10,34,'Isus le-a răspuns: &lt;J&gt;„Nu este scris în Legea voastră: &lt;e&gt;Eu am zis: Sunteți dumnezei&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[26]&lt;/f&gt;&lt;J&gt;?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,35,'&lt;J&gt;Dacă &lt;i&gt;Legea&lt;/i&gt; i-a numit dumnezei pe&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;aceia cărora li se adresa Cuvântul lui Dumnezeu, și Scriptura nu poate fi desființată,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,36,'&lt;J&gt;cum ziceți voi: «Hulești!» Celui pe care&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;Tatăl L-a sfințit și L-a trimis&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;în lume? Și aceasta, pentru că&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;am zis: «Sunt Fiul&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;lui Dumnezeu!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,37,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;nu fac lucrările Tatălui Meu, să nu Mă credeți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,38,'&lt;J&gt;Dar, dacă le fac, și nu Mă credeți pe Mine, credeți&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;măcar lucrările acestea, ca să cunoașteți și să știți că&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;Tatăl este în Mine și Eu sunt în Tatăl.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,10,39,'Atunci&lt;f&gt;ⓒ&lt;/f&gt; au căutat iarăși să-L prindă, dar El a scăpat din mâinile lor.');
INSERT INTO &quot;verses&quot; VALUES (500,10,40,'&lt;pb/&gt;Isus S-a dus iarăși dincolo de Iordan, în locul unde&lt;f&gt;ⓓ&lt;/f&gt; botezase Ioan la început, și a rămas acolo.');
INSERT INTO &quot;verses&quot; VALUES (500,10,41,'Mulți veneau la El și spuneau: „Ioan n-a făcut niciun semn, dar&lt;f&gt;ⓔ&lt;/f&gt; tot ce a spus Ioan despre Omul acesta era adevărat.”');
INSERT INTO &quot;verses&quot; VALUES (500,10,42,'Și&lt;f&gt;ⓕ&lt;/f&gt; mulți au crezut în El în locul acela.');
INSERT INTO &quot;verses&quot; VALUES (500,11,1,'&lt;pb/&gt;Un oarecare Lazăr din Betania, satul Mariei&lt;f&gt;ⓖ&lt;/f&gt; și al Martei, sora ei, era bolnav.');
INSERT INTO &quot;verses&quot; VALUES (500,11,2,'(Maria era&lt;f&gt;ⓗ&lt;/f&gt; aceea care L-a uns pe Domnul cu mir și I-a șters picioarele cu părul ei; Lazăr, fratele ei, era bolnav.)');
INSERT INTO &quot;verses&quot; VALUES (500,11,3,'Surorile au trimis la Isus să-I spună: „Doamne, iată că cel pe care-l iubești este bolnav.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,4,'Dar Isus, când a auzit, a spus: &lt;J&gt;„Boala aceasta nu este spre moarte, ci&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;spre slava lui Dumnezeu, pentru ca Fiul lui Dumnezeu să fie proslăvit prin ea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,5,'Și Isus îi iubea pe Marta, pe sora ei și pe Lazăr.');
INSERT INTO &quot;verses&quot; VALUES (500,11,6,'Totuși, când a auzit că Lazăr este bolnav, a&lt;f&gt;ⓙ&lt;/f&gt; mai zăbovit două zile în locul în care era;');
INSERT INTO &quot;verses&quot; VALUES (500,11,7,'apoi, le-a zis ucenicilor: &lt;J&gt;„Să ne întoarcem în Iudeea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,8,'„&lt;e&gt;Rabbí&lt;/e&gt;”, I-au zis ucenicii, „nu demult&lt;f&gt;ⓚ&lt;/f&gt; căutau iudeii să Te ucidă cu pietre, și Tu Te duci iarăși acolo?”');
INSERT INTO &quot;verses&quot; VALUES (500,11,9,'Isus a răspuns: &lt;J&gt;„Nu sunt douăsprezece ceasuri în zi? Dacă&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;umblă cineva ziua, nu se poticnește, pentru că vede lumina lumii acesteia.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,10,'&lt;J&gt;Dar, dacă&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;umblă noaptea, se poticnește, pentru că lumina nu este în el.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,11,'După aceste vorbe, le-a zis: &lt;J&gt;„Lazăr, prietenul nostru, doarme&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;, dar Mă duc să-l trezesc.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,12,'Ucenicii I-au zis: „Doamne, dacă doarme, se va face bine!”');
INSERT INTO &quot;verses&quot; VALUES (500,11,13,'(Isus vorbise despre moartea lui, dar ei credeau că vorbește despre somnul obișnuit.)');
INSERT INTO &quot;verses&quot; VALUES (500,11,14,'Atunci, Isus le-a spus lămurit: &lt;J&gt;„Lazăr a murit!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,15,'&lt;J&gt;Și Mă bucur că n-am fost acolo, pentru voi, ca să credeți. Dar acum, haideți să mergem la el!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,16,'Atunci, Toma, zis Geamănul, le-a spus celorlalți ucenici: „Să mergem și noi să murim cu El!”');
INSERT INTO &quot;verses&quot; VALUES (500,11,17,'&lt;pb/&gt;Când a venit, Isus a aflat că Lazăr era deja de patru zile în mormânt.');
INSERT INTO &quot;verses&quot; VALUES (500,11,18,'Și, fiindcă Betania era aproape de Ierusalim, cam la cincisprezece stadii&lt;f&gt;[27]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (500,11,19,'mulți dintre iudei veniseră la Marta și la Maria ca să le mângâie pentru moartea fratelui lor.');
INSERT INTO &quot;verses&quot; VALUES (500,11,20,'Când a auzit Marta că vine Isus, I-a ieșit înainte, iar Maria stătea în casă.');
INSERT INTO &quot;verses&quot; VALUES (500,11,21,'Marta I-a zis lui Isus: „Doamne, dacă ai fi fost aici, fratele meu n-ar fi murit!');
INSERT INTO &quot;verses&quot; VALUES (500,11,22,'Dar și acum, știu că orice&lt;f&gt;ⓞ&lt;/f&gt; vei cere de la Dumnezeu, Dumnezeu Îți va da.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,23,'Isus i-a zis: &lt;J&gt;„Fratele tău va învia.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,24,'„Știu&lt;f&gt;ⓟ&lt;/f&gt;”, I-a răspuns Marta, „că va învia la înviere, în ziua de apoi.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,25,'Isus i-a zis: &lt;J&gt;„Eu sunt Învierea&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;și Viața&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;! Cine&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;crede în Mine, chiar dacă ar fi să moară, va trăi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,26,'&lt;J&gt;Și oricine trăiește și crede în Mine nu va muri în veac. Crezi aceasta?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,27,'„Da, Doamne”, I-a zis ea, „cred&lt;f&gt;ⓣ&lt;/f&gt; că Tu ești Hristosul, Fiul lui Dumnezeu, care trebuia să vină în lume.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,28,'&lt;pb/&gt;După ce a spus aceste cuvinte, s-a dus și a chemat-o în taină pe sora sa, Maria, și i-a zis: „A venit Învățătorul și te cheamă.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,29,'Maria, cum a auzit, s-a sculat degrabă și s-a dus la El.');
INSERT INTO &quot;verses&quot; VALUES (500,11,30,'Căci Isus încă nu intrase în sat, ci era tot în locul unde Îl întâmpinase Marta.');
INSERT INTO &quot;verses&quot; VALUES (500,11,31,'Iudeii&lt;f&gt;ⓤ&lt;/f&gt; care erau cu Maria în casă și o mângâiau, când au văzut-o sculându-se repede și ieșind, au mers după ea, căci ziceau: „Se duce la mormânt, ca să plângă acolo.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,32,'Maria, când a ajuns unde era Isus și L-a văzut, a căzut la picioarele Lui și I-a zis: „Doamne&lt;f&gt;ⓥ&lt;/f&gt;, dacă ai fi fost aici, fratele meu n-ar fi murit!”');
INSERT INTO &quot;verses&quot; VALUES (500,11,33,'Isus, când i-a văzut plângând pe ea și pe iudeii care veniseră cu ea, S-a înfiorat în duhul Său și S-a tulburat.');
INSERT INTO &quot;verses&quot; VALUES (500,11,34,'Și a zis: &lt;J&gt;„Unde l-ați pus?”&lt;/J&gt; „Doamne”, I-au răspuns ei, „vino și vezi!”');
INSERT INTO &quot;verses&quot; VALUES (500,11,35,'Isus plângea&lt;f&gt;ⓦ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,11,36,'Atunci, iudeii au zis: „Iată cât îl iubea de mult!”');
INSERT INTO &quot;verses&quot; VALUES (500,11,37,'Și unii din ei au zis: „El, care&lt;f&gt;ⓧ&lt;/f&gt; a deschis ochii orbului, nu putea face ca nici omul acesta să nu moară?”');
INSERT INTO &quot;verses&quot; VALUES (500,11,38,'&lt;pb/&gt;Isus S-a înfiorat din nou în Sine și S-a dus la mormânt; era o peșteră, la intrarea căreia fusese pusă o piatră.');
INSERT INTO &quot;verses&quot; VALUES (500,11,39,'&lt;J&gt;„Dați piatra la o parte!”&lt;/J&gt;, a zis Isus. Marta, sora celui care murise, I-a spus: „Doamne, miroase greu, căci este mort de patru zile.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,40,'Isus i-a zis: &lt;J&gt;„Nu ți-am spus că, dacă vei crede, vei vedea&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;slava lui Dumnezeu?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,41,'Au dat deci piatra la o parte [din locul unde zăcea mortul]. Și Isus a ridicat ochii în sus și a zis: &lt;J&gt;„Tată, Îți mulțumesc că M-ai ascultat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,42,'&lt;J&gt;Știam că totdeauna Mă asculți, dar pentru&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;mulțimea care stă împrejur vorbesc astfel, ca să creadă că Tu M-ai trimis.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,43,'După ce a rostit aceste cuvinte, a strigat cu glas tare: &lt;J&gt;„Lazăre, vino afară!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,44,'Și mortul a ieșit cu mâinile și picioarele legate cu fâșii de pânză și cu fața&lt;f&gt;ⓐ&lt;/f&gt; înfășurată într-un ștergar. Isus le-a zis: &lt;J&gt;„Dezlegați-l și lăsați-l să meargă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,11,45,'&lt;pb/&gt;Mulți dintre iudeii care veniseră la Maria și care&lt;f&gt;ⓑ&lt;/f&gt; văzuseră ce a făcut Isus au crezut în El.');
INSERT INTO &quot;verses&quot; VALUES (500,11,46,'Dar unii dintre ei s-au dus la farisei și le-au spus ce făcuse Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,11,47,'Atunci&lt;f&gt;ⓒ&lt;/f&gt;, preoții de seamă și fariseii au adunat Sinedriul și au zis: „Ce&lt;f&gt;ⓓ&lt;/f&gt; vom face? Omul acesta face multe minuni!');
INSERT INTO &quot;verses&quot; VALUES (500,11,48,'Dacă-l lăsăm așa, toți vor crede în el și vor veni romanii și vor nimici și locul nostru, și neamul.”');
INSERT INTO &quot;verses&quot; VALUES (500,11,49,'Unul din ei, Caiafa&lt;f&gt;ⓔ&lt;/f&gt;, care era mare-preot în anul acela, le-a zis: „Voi nu știți nimic;');
INSERT INTO &quot;verses&quot; VALUES (500,11,50,'și nici nu&lt;f&gt;ⓕ&lt;/f&gt; vă gândiți că este în folosul vostru să moară un om pentru popor și să nu piară tot neamul!”');
INSERT INTO &quot;verses&quot; VALUES (500,11,51,'Dar aceasta nu de la el a spus-o, ci, fiind mare-preot în anul acela, a profețit că Isus avea să moară pentru neam.');
INSERT INTO &quot;verses&quot; VALUES (500,11,52,'Și&lt;f&gt;ⓖ&lt;/f&gt; nu numai pentru neam, ci&lt;f&gt;ⓗ&lt;/f&gt; și ca să-i adune laolaltă pe copiii lui Dumnezeu cei risipiți.');
INSERT INTO &quot;verses&quot; VALUES (500,11,53,'Din ziua aceea, au hotărât să-L omoare.');
INSERT INTO &quot;verses&quot; VALUES (500,11,54,'De aceea&lt;f&gt;ⓘ&lt;/f&gt;, Isus nu mai umbla pe față printre iudei, ci a plecat de acolo în ținutul de lângă pustiu, într-o cetate numită Efraim&lt;f&gt;ⓙ&lt;/f&gt;, și a rămas acolo împreună cu ucenicii.');
INSERT INTO &quot;verses&quot; VALUES (500,11,55,'&lt;pb/&gt;Paștele iudeilor era&lt;f&gt;ⓚ&lt;/f&gt; aproape, și mulți oameni din ținutul acela s-au suit la Ierusalim, înainte de Paște, ca să se curățească.');
INSERT INTO &quot;verses&quot; VALUES (500,11,56,'Îl&lt;f&gt;ⓛ&lt;/f&gt; căutau pe Isus și vorbeau între ei stând în Templu: „Ce credeți? Oare chiar nu va veni la sărbătoare?”');
INSERT INTO &quot;verses&quot; VALUES (500,11,57,'(Iar preoții de seamă și fariseii dăduseră porunci ca, dacă va ști cineva unde este, să le dea de știre ca să-L prindă.)');
INSERT INTO &quot;verses&quot; VALUES (500,12,1,'&lt;pb/&gt;Cu șase zile înainte de Paște, Isus a venit în Betania, unde&lt;f&gt;ⓜ&lt;/f&gt; era Lazăr, [care fusese mort și] pe care El îl înviase din morți.');
INSERT INTO &quot;verses&quot; VALUES (500,12,2,'Acolo, I-au&lt;f&gt;ⓝ&lt;/f&gt; pregătit o cină. Marta slujea, iar Lazăr era unul din cei ce stăteau la masă cu El.');
INSERT INTO &quot;verses&quot; VALUES (500,12,3,'Maria&lt;f&gt;ⓞ&lt;/f&gt; a luat o litră&lt;f&gt;[28]&lt;/f&gt; de mir de nard curat, de mare preț, I-a uns picioarele lui Isus și I le-a șters cu părul ei; și s-a umplut casa de mireasma mirului.');
INSERT INTO &quot;verses&quot; VALUES (500,12,4,'Unul dintre ucenicii Lui, Iuda Iscariotul, fiul lui Simon, care avea să-L vândă, a zis:');
INSERT INTO &quot;verses&quot; VALUES (500,12,5,'„De ce nu s-a vândut acest mir cu trei sute de dinari și să se fi dat săracilor?”');
INSERT INTO &quot;verses&quot; VALUES (500,12,6,'Zicea lucrul acesta nu pentru că-i păsa de săraci, ci pentru că era hoț și, ca unul care ținea&lt;f&gt;ⓟ&lt;/f&gt; punga, lua din ce se punea în ea.');
INSERT INTO &quot;verses&quot; VALUES (500,12,7,'Dar Isus a zis: &lt;J&gt;„Las-o în pace, căci ea l-a păstrat pentru ziua îngropării Mele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,8,'&lt;J&gt;Pe&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;săraci îi aveți totdeauna cu voi, dar pe Mine nu Mă aveți totdeauna.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,9,'&lt;pb/&gt;O mare mulțime de iudei a aflat că Isus este acolo și a venit nu numai pentru Isus, ci și ca să-l vadă pe Lazăr, pe&lt;f&gt;ⓡ&lt;/f&gt; care-l înviase din morți.');
INSERT INTO &quot;verses&quot; VALUES (500,12,10,'Preoții de seamă&lt;f&gt;ⓢ&lt;/f&gt; au hotărât să-l omoare și pe Lazăr,');
INSERT INTO &quot;verses&quot; VALUES (500,12,11,'căci&lt;f&gt;ⓣ&lt;/f&gt; din pricina lui mulți iudei plecau de la ei și credeau în Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,12,12,'&lt;pb/&gt;A doua zi&lt;f&gt;ⓤ&lt;/f&gt;, mulțimea cea mare care venise la sărbătoare, cum a auzit că vine Isus în Ierusalim,');
INSERT INTO &quot;verses&quot; VALUES (500,12,13,'a luat ramuri de palmier și I-a ieșit în întâmpinare, strigând: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Osana!&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Binecuvântat este Cel ce vine în Numele Domnului,&lt;/e&gt; &lt;f&gt;[29]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Regele lui Israel!&lt;/e&gt; &lt;f&gt;[30]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,14,'&lt;pb/&gt;Isus a găsit&lt;f&gt;ⓥ&lt;/f&gt; un măgăruș și a încălecat pe el, după cum este scris:');
INSERT INTO &quot;verses&quot; VALUES (500,12,15,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Nu te teme, fiica Sionului;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;iată, Regele tău vine,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;călare pe mânzul unei măgărițe.&lt;/e&gt; &lt;f&gt;[31]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,16,'&lt;pb/&gt;Ucenicii Lui n-au&lt;f&gt;ⓦ&lt;/f&gt; înțeles acestea la început, dar&lt;f&gt;ⓧ&lt;/f&gt;, când a fost proslăvit Isus, și-au&lt;f&gt;ⓨ&lt;/f&gt; adus aminte că acestea erau scrise despre El și că se împliniseră cu privire la El.');
INSERT INTO &quot;verses&quot; VALUES (500,12,17,'Mulțimea care fusese cu El când îl chemase pe Lazăr din mormânt și-l înviase din morți mărturisea despre El.');
INSERT INTO &quot;verses&quot; VALUES (500,12,18,'De aceea&lt;f&gt;ⓩ&lt;/f&gt; I-a și venit în întâmpinare mulțimea: pentru că auzise că făcuse acest semn.');
INSERT INTO &quot;verses&quot; VALUES (500,12,19,'Fariseii au zis deci între ei: „Vedeți&lt;f&gt;ⓐ&lt;/f&gt;?! Nu vă alegeți cu nimic! Iată, lumea se duce după el!”');
INSERT INTO &quot;verses&quot; VALUES (500,12,20,'&lt;pb/&gt;Erau și&lt;f&gt;ⓑ&lt;/f&gt; greci printre cei care se suiseră să se închine la sărbătoare.');
INSERT INTO &quot;verses&quot; VALUES (500,12,21,'Aceștia s-au apropiat de Filip, care&lt;f&gt;ⓒ&lt;/f&gt; era din Betsaida Galileei, și l-au rugat, zicând: „Domnule, am vrea să-L vedem pe Isus.”');
INSERT INTO &quot;verses&quot; VALUES (500,12,22,'Filip s-a dus și i-a spus lui Andrei, apoi Andrei și Filip s-au dus și I-au spus lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,12,23,'Drept răspuns, Isus le-a zis: &lt;J&gt;„A&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;sosit ceasul să fie proslăvit Fiul Omului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,24,'&lt;J&gt;Adevărat, adevărat vă spun: dacă bobul&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;de grâu care a căzut în pământ nu moare, rămâne singur; dar, dacă moare, aduce mult rod.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,25,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;își iubește viața o va pierde, și cine își urăște viața în lumea aceasta o va păstra pentru viața veșnică.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,26,'&lt;J&gt;Dacă Îmi slujește cineva, să Mă urmeze; și unde&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;sunt Eu, acolo va fi și slujitorul Meu. Dacă Îmi slujește cineva, Tatăl îl va cinsti.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,27,'&lt;pb/&gt;&lt;J&gt;Acum,&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;&lt;J&gt;sufletul Meu este tulburat.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[32]&lt;/f&gt; &lt;J&gt;Și ce voi zice: «Tată, &lt;e&gt;izbăvește-Mă&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[33]&lt;/f&gt; &lt;J&gt;din ceasul acesta»? Dar&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;tocmai de aceea am ajuns până în ceasul acesta!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,28,'&lt;J&gt;Tată, proslăvește Numele Tău!”&lt;/J&gt; Și&lt;f&gt;ⓙ&lt;/f&gt; din cer s-a auzit un glas, [care zicea]: „L-am proslăvit și-L voi mai proslăvi!”');
INSERT INTO &quot;verses&quot; VALUES (500,12,29,'Mulțimea care stătea și asculta spunea că a fost un tunet. Alții ziceau: „Un înger a vorbit cu El!”');
INSERT INTO &quot;verses&quot; VALUES (500,12,30,'Isus a răspuns: &lt;J&gt;„Nu pentru Mine s-a auzit glasul acesta&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt;&lt;J&gt;, ci pentru voi!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,31,'&lt;J&gt;Acum are loc judecata lumii acesteia; acum stăpânitorul&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;lumii acesteia va fi aruncat afară.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,32,'&lt;J&gt;Iar Eu, după ce&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;voi fi înălțat de pe pământ, îi voi atrage pe&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;toți la Mine.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,33,'(Aceasta o zicea&lt;f&gt;ⓞ&lt;/f&gt; arătând cu ce moarte avea să moară.)');
INSERT INTO &quot;verses&quot; VALUES (500,12,34,'Mulțimea I-a răspuns: „Noi&lt;f&gt;ⓟ&lt;/f&gt; am auzit din Lege că Hristosul rămâne în veac. Cum de zici Tu că Fiul Omului trebuie să fie înălțat? Cine este acest Fiu al Omului?”');
INSERT INTO &quot;verses&quot; VALUES (500,12,35,'Isus le-a zis: &lt;J&gt;„Încă&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;puțină vreme Lumina este printre voi. Umblați&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;câtă vreme aveți Lumina, ca să nu vă cuprindă întunericul! Cine&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;umblă în întuneric nu știe încotro merge.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,36,'&lt;J&gt;Câtă vreme aveți Lumina printre voi, credeți în Lumină, ca să fiți fii&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;ai Luminii!”&lt;/J&gt; Isus le-a spus aceste lucruri, apoi a plecat și S-a&lt;f&gt;ⓤ&lt;/f&gt; ascuns de ei.');
INSERT INTO &quot;verses&quot; VALUES (500,12,37,'&lt;pb/&gt;Măcar că făcuse atâtea semne înaintea lor, tot nu credeau în El,');
INSERT INTO &quot;verses&quot; VALUES (500,12,38,'ca să se împlinească cele spuse de profetul Isaia: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Doamne, cine a crezut propovăduirea noastră,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și brațul Domnului cui s-a descoperit?&lt;/e&gt; &lt;f&gt;[34]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,39,'&lt;pb/&gt;De aceea nu puteau crede, pentru că Isaia a mai zis:');
INSERT INTO &quot;verses&quot; VALUES (500,12,40,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Le-a orbit ochii&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și le-a împietrit inima,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca să nu vadă cu ochii,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;să nu înțeleagă cu inima,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;să nu se întoarcă la Dumnezeu și să-i vindec.&lt;/e&gt; &lt;f&gt;[35]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,41,'&lt;pb/&gt;Acestea&lt;f&gt;ⓥ&lt;/f&gt; le-a zis Isaia fiindcă a văzut slava Lui și a vorbit despre El.');
INSERT INTO &quot;verses&quot; VALUES (500,12,42,'Totuși, chiar dintre conducători, mulți au crezut în El; dar, de frica fariseilor&lt;f&gt;ⓦ&lt;/f&gt;, nu-L mărturiseau pe față, ca să nu fie dați afară din sinagogă.');
INSERT INTO &quot;verses&quot; VALUES (500,12,43,'Căci&lt;f&gt;ⓧ&lt;/f&gt; au iubit slava oamenilor mai mult decât slava lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (500,12,44,'&lt;pb/&gt;Isus a strigat și a zis: &lt;J&gt;„Cine&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;crede în Mine nu în Mine crede, ci în Acela care M-a trimis pe Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,45,'&lt;J&gt;Și cine&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;Mă vede pe Mine Îl vede pe Acela care M-a trimis pe Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,46,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;ca Lumină am venit în lume, pentru ca oricine crede în Mine să nu rămână în întuneric.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,47,'&lt;J&gt;Dacă aude cineva cuvintele Mele și nu le păzește, nu Eu&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;îl judec, căci n-am&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;venit să judec lumea, ci să mântuiesc lumea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,48,'&lt;J&gt;Pe cine&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;Mă nesocotește și nu primește cuvintele Mele are cine să-l judece: cuvântul&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;pe care l-am rostit, acela îl va judeca în ziua de apoi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,49,'&lt;J&gt;Pentru că Eu&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;n-am vorbit de la Mine Însumi, ci Tatăl, care M-a trimis, El Mi-a dat poruncă ce&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;să spun și ce să vorbesc.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,12,50,'&lt;J&gt;Și știu că porunca Lui este viață veșnică. De aceea, cuvintele pe care le spun, le spun așa cum Mi le-a spus Tatăl.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,1,'&lt;pb/&gt;Înainte&lt;f&gt;ⓗ&lt;/f&gt; de sărbătoarea Paștelui, Isus, fiindcă știa că I-a&lt;f&gt;ⓘ&lt;/f&gt; sosit ceasul să plece din lumea aceasta la Tatăl și fiindcă îi iubea pe ai Săi, care erau în lume, i-a iubit până la capăt.');
INSERT INTO &quot;verses&quot; VALUES (500,13,2,'În timpul cinei, după ce Diavolul&lt;f&gt;ⓙ&lt;/f&gt; pusese în inima lui Iuda Iscariotul, fiul lui Simon, gândul să-L vândă,');
INSERT INTO &quot;verses&quot; VALUES (500,13,3,'[Isus], fiindcă știa că Tatăl&lt;f&gt;ⓚ&lt;/f&gt; Îi dăduse toate lucrurile în mâini, că&lt;f&gt;ⓛ&lt;/f&gt; de la Dumnezeu a venit și la Dumnezeu Se duce,');
INSERT INTO &quot;verses&quot; VALUES (500,13,4,'S-a&lt;f&gt;ⓜ&lt;/f&gt; sculat de la masă, Și-a pus deoparte veșmintele, a luat un ștergar și S-a încins cu el.');
INSERT INTO &quot;verses&quot; VALUES (500,13,5,'Apoi a turnat apă într-un lighean și a început să spele picioarele ucenicilor și să le șteargă cu ștergarul cu care era încins.');
INSERT INTO &quot;verses&quot; VALUES (500,13,6,'A venit deci la Simon Petru, care I-a zis: „Doamne, Tu&lt;f&gt;ⓝ&lt;/f&gt; să-mi speli mie picioarele?”');
INSERT INTO &quot;verses&quot; VALUES (500,13,7,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Ce fac Eu, tu nu pricepi acum, dar&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vei pricepe după aceea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,8,'Petru I-a zis: „Nicidecum nu-mi vei spăla picioarele, în veac!” Isus i-a răspuns: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;nu te spăl Eu, nu vei avea parte cu Mine.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,9,'„Doamne”, I-a zis Simon Petru, „atunci nu numai picioarele, ci și mâinile și capul!”');
INSERT INTO &quot;verses&quot; VALUES (500,13,10,'Isus i-a zis: &lt;J&gt;„Cine s-a îmbăiat n-are trebuință să-și spele decât picioarele, ca să fie curat de tot. Voi&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;sunteți curați, dar nu toți.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,11,'(Căci&lt;f&gt;ⓡ&lt;/f&gt; îl știa pe cel ce avea să-L vândă; de aceea a zis: &lt;J&gt;„Nu sunteți toți curați.”&lt;/J&gt;)');
INSERT INTO &quot;verses&quot; VALUES (500,13,12,'&lt;pb/&gt;După ce le-a spălat picioarele, Și-a luat veșmintele, S-a așezat iarăși la masă și le-a zis: &lt;J&gt;„Înțelegeți voi ce v-am făcut Eu?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,13,'&lt;J&gt;Voi&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;Mă numiți Învățătorul și Domnul, și bine ziceți, căci sunt.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,14,'&lt;J&gt;Deci&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt;&lt;J&gt;, dacă Eu, Domnul și Învățătorul, v-am spălat picioarele, și voi sunteți&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;datori să vă spălați picioarele unii altora.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,15,'&lt;J&gt;Pentru că Eu v-am&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;dat o pildă, ca și voi să faceți cum v-am făcut Eu vouă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,16,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt;&lt;J&gt;, adevărat vă spun: robul nu este mai mare decât stăpânul său, nici apostolul, mai mare decât cel ce l-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,17,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;știți aceste lucruri, ferice de voi dacă le faceți!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,18,'&lt;pb/&gt;&lt;J&gt;Nu vorbesc despre voi toți; îi cunosc pe cei pe care i-am ales. Dar trebuie să se împlinească Scriptura: &lt;e&gt;Cel ce mănâncă pâinea Mea și-a ridicat călcâiul împotriva Mea.&lt;/J&gt;&lt;/e&gt; &lt;f&gt;[36]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,19,'&lt;J&gt;De acum&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt;&lt;J&gt;, vă spun înainte de a se împlini, ca să credeți, când se va împlini, că Eu Sunt.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,20,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt;&lt;J&gt;, adevărat vă spun: cine primește pe acela pe care-l trimit Eu pe Mine Mă primește; și cine Mă primește pe Mine Îl primește pe Cel care M-a trimis pe Mine.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,21,'&lt;pb/&gt;După ce a spus aceste&lt;f&gt;ⓐ&lt;/f&gt; cuvinte, Isus S-a&lt;f&gt;ⓑ&lt;/f&gt; tulburat în duhul Său, a mărturisit și a zis: &lt;J&gt;„Adevărat, adevărat vă spun: unul&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;din voi Mă va vinde.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,22,'Ucenicii se uitau unii la alții și nu înțelegeau despre cine vorbește.');
INSERT INTO &quot;verses&quot; VALUES (500,13,23,'Unul dintre ucenici, cel pe care-l iubea Isus, stătea la masă culcat&lt;f&gt;ⓓ&lt;/f&gt; pe sânul lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,13,24,'Simon Petru i-a făcut semn să întrebe despre cine era vorba.');
INSERT INTO &quot;verses&quot; VALUES (500,13,25,'Ucenicul acela s-a rezemat pe pieptul lui Isus și I-a zis: „Doamne, cine este?”');
INSERT INTO &quot;verses&quot; VALUES (500,13,26,'Isus i-a răspuns: &lt;J&gt;„Este acela pentru care voi întinge bucățica și căruia i-o voi da.”&lt;/J&gt; Întingând deci bucățica, a luat-o și i-a dat-o lui Iuda, fiul lui Simon Iscariotul.');
INSERT INTO &quot;verses&quot; VALUES (500,13,27,'De cum&lt;f&gt;ⓔ&lt;/f&gt; a luat Iuda bucățica, a intrat Satana în el. Isus i-a zis: &lt;J&gt;„Ce ai de făcut, fă repede!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,28,'Dar nimeni dintre cei care stăteau la masă n-a înțeles de ce îi vorbise astfel.');
INSERT INTO &quot;verses&quot; VALUES (500,13,29,'Unii credeau că, de vreme ce Iuda&lt;f&gt;ⓕ&lt;/f&gt; avea punga, Isus voia să-i spună: „Cumpără ce ne trebuie pentru sărbătoare!” sau îi cerea să dea ceva săracilor.');
INSERT INTO &quot;verses&quot; VALUES (500,13,30,'Iuda, după ce a luat bucățica, a ieșit de îndată. Era noapte.');
INSERT INTO &quot;verses&quot; VALUES (500,13,31,'&lt;pb/&gt;După ce a ieșit Iuda, Isus a zis: &lt;J&gt;„Acum&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt;&lt;J&gt;, Fiul Omului a fost proslăvit, și Dumnezeu&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;a fost proslăvit în El.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,32,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;Dumnezeu a fost proslăvit în El, și Dumnezeu Îl va proslăvi în Sine Însuși, și-L va proslăvi îndată&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,33,'&lt;J&gt;Copilașilor, încă puțin mai sunt cu voi. Mă veți căuta și&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt;&lt;J&gt;, precum le-am spus iudeilor — unde Mă duc Eu, voi nu puteți veni — tot așa vă spun și vouă acum.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,34,'&lt;J&gt;Vă dau o poruncă&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;nouă: să vă iubiți unii pe alții; cum v-am iubit Eu, așa să vă iubiți și voi unii pe alții.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,35,'&lt;J&gt;Prin aceasta&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;vor cunoaște toți că sunteți ucenicii Mei: dacă veți avea dragoste unii pentru alții.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,36,'&lt;pb/&gt;„Doamne”, I-a zis Simon Petru, „unde Te duci?” Isus i-a răspuns: &lt;J&gt;„Unde Mă duc [Eu], tu nu poți să Mă urmezi acum, dar Mă vei urma după aceea&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,13,37,'„Doamne”, I-a zis Petru, „de ce nu pot să Te urmez acum? Eu îmi voi da viața&lt;f&gt;ⓞ&lt;/f&gt; pentru Tine.”');
INSERT INTO &quot;verses&quot; VALUES (500,13,38,'Isus i-a răspuns: &lt;J&gt;„Îți vei da viața pentru Mine? Adevărat, adevărat îți spun: nu va cânta cocoșul, și te vei lepăda de Mine de trei ori.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,1,'&lt;pb/&gt;&lt;J&gt;Să nu&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;vi se tulbure inima. Aveți credință în Dumnezeu și aveți credință în Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,2,'&lt;J&gt;În casa Tatălui Meu sunt multe locașuri. Dacă nu &lt;i&gt;ar fi așa&lt;/i&gt;, v-aș fi spus&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Eu oare: «Mă duc să vă pregătesc un loc»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,3,'&lt;J&gt;Și dacă Mă voi duce și vă voi pregăti un loc, Mă voi întoarce&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;și vă voi lua cu Mine, pentru ca&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt;&lt;J&gt;, acolo unde sunt Eu, să fiți și voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,4,'&lt;J&gt;Și unde Mă duc, știți calea într-acolo.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,5,'&lt;pb/&gt;„Doamne”, I-a zis Toma, „nu știm unde Te duci. Cum putem să știm calea într-acolo?”');
INSERT INTO &quot;verses&quot; VALUES (500,14,6,'Isus i-a zis: &lt;J&gt;„Eu sunt Calea&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt;&lt;J&gt;, Adevărul&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;și Viața&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt;&lt;J&gt;. Nimeni&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;nu vine la Tatăl decât prin Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,7,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Mă cunoașteți pe Mine, Îl veți cunoaște și pe Tatăl Meu. Și de acum încolo Îl cunoașteți; și L-ați și văzut.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,8,'&lt;pb/&gt;„Doamne”, i-a zis Filip, „arată-ne pe Tatăl și ne este de ajuns!”');
INSERT INTO &quot;verses&quot; VALUES (500,14,9,'Isus i-a zis: &lt;J&gt;„De atâta vreme sunt cu voi și nu M-ai cunoscut, Filipe? Cine&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;M-a văzut pe Mine L-a văzut pe Tatăl. Cum de zici tu: «Arată-ne pe Tatăl»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,10,'&lt;J&gt;Nu crezi că Eu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;sunt în Tatăl, și Tatăl este în Mine? Cuvintele pe care vi le spun nu&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;de la Mine le spun, ci Tatăl, care rămâne în Mine, El face lucrările Sale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,11,'&lt;J&gt;Credeți-Mă că Eu sunt în Tatăl, și Tatăl este în Mine; dacă nu pentru altceva, măcar pentru lucrările acestea să credeți&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt;&lt;J&gt;.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,12,'&lt;pb/&gt;&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;, adevărat vă spun: cine crede în Mine va face și el lucrările pe care le fac Eu; ba încă și mai mari decât acestea va face, pentru că Eu Mă duc la Tatăl.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,13,'&lt;J&gt;Și&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;orice veți cere în Numele Meu, voi face, pentru ca Tatăl să fie proslăvit în Fiul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,14,'&lt;J&gt;Dacă veți cere ceva de la Mine în Numele Meu, voi face.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,15,'&lt;pb/&gt;&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;Mă iubiți, veți păzi poruncile Mele.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,16,'&lt;J&gt;Și Eu Îl voi ruga pe Tatăl, și El vă va da un alt&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;Mângâietor&lt;/J&gt;&lt;f&gt;[37]&lt;/f&gt;&lt;J&gt;, ca să fie cu voi în veac;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,17,'&lt;J&gt;și anume Duhul&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;adevărului, pe&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;care lumea nu-L poate primi, pentru că nu-L vede și nici nu-L cunoaște. Voi însă Îl cunoașteți, căci rămâne cu voi și&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;va fi în voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,18,'&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;vă voi lăsa orfani; Mă voi&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;întoarce la voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,19,'&lt;J&gt;Încă puțin, și lumea nu Mă va mai vedea, dar voi&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;Mă veți vedea; pentru că&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;Eu trăiesc, și voi veți trăi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,20,'&lt;J&gt;În ziua aceea, veți cunoaște că Eu&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;sunt în Tatăl Meu, că voi sunteți în Mine și că Eu sunt în voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,21,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;are poruncile Mele și le păzește, acela Mă iubește; și cine Mă iubește va fi iubit de Tatăl Meu. Eu îl voi iubi și Mă voi arăta lui.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,22,'Iuda, nu Iscariotul, I-a zis&lt;f&gt;ⓟ&lt;/f&gt;: „Doamne, cum se face că Te vei arăta nouă, și nu lumii?”');
INSERT INTO &quot;verses&quot; VALUES (500,14,23,'Drept răspuns, Isus i-a zis: &lt;J&gt;„Dacă&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Mă iubește cineva, va păzi cuvântul Meu, și Tatăl Meu îl va iubi și Noi vom veni la el și&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;Ne vom face locaș la el.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,24,'&lt;J&gt;Cine nu Mă iubește nu păzește cuvintele Mele. Și cuvântul&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;pe care-l auziți nu este al Meu, ci al Tatălui, care M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,25,'&lt;pb/&gt;&lt;J&gt;V-am spus aceste lucruri cât mai sunt cu voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,26,'&lt;J&gt;Dar Mângâietorul&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt;&lt;J&gt;, Duhul Sfânt, pe care-L va trimite Tatăl în Numele Meu, vă&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;va învăța toate lucrurile și vă va aduce aminte de tot ce v-am spus Eu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,27,'&lt;J&gt;Vă las pacea&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt;&lt;J&gt;, vă dau pacea Mea. Nu v-o dau cum o dă lumea. Să&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;nu vi se tulbure inima, nici să nu vi se înspăimânte!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,28,'&lt;J&gt;Ați auzit că v-am&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;spus: «Mă duc&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;și Mă voi întoarce la voi.» Dacă M-ați iubi, v-ați bucura că v-am zis: «Mă duc la Tatăl», căci Tatăl&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;este mai mare decât Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,29,'&lt;J&gt;Și v-am spus acestea acum&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt;&lt;J&gt;, înainte să se întâmple, ca să credeți atunci când se vor întâmpla.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,30,'&lt;J&gt;Nu voi mai vorbi mult cu voi, căci&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;vine stăpânul lumii acesteia. El nu are nimic în Mine,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,14,31,'&lt;J&gt;dar vine pentru ca lumea să știe că Eu Îl iubesc pe Tatăl și că fac așa cum&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;Mi-a poruncit Tatăl. Ridicați-vă, să plecăm de aici!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,1,'&lt;pb/&gt;&lt;J&gt;Eu sunt adevărata Viță, și Tatăl Meu este Lucrătorul viei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,2,'&lt;J&gt;Pe orice&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;mlădiță care este în Mine și nu aduce rod, El o taie, și pe orice mlădiță care aduce rod, o curățește, ca să aducă și mai mult rod.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,3,'&lt;J&gt;Acum, voi&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;sunteți curați din pricina cuvântului pe care vi l-am spus.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,4,'&lt;J&gt;Rămâneți&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;în Mine, și Eu voi rămâne în voi! După cum mlădița nu poate aduce rod de la sine dacă nu rămâne în viță, tot așa nici voi nu puteți aduce rod dacă nu rămâneți în Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,5,'&lt;J&gt;Eu sunt Vița, voi sunteți mlădițele. Cine rămâne în Mine și în cine rămân Eu aduce mult rod&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt;&lt;J&gt;, căci fără Mine nu puteți face nimic.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,6,'&lt;J&gt;Dacă nu rămâne cineva în Mine, este&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;aruncat afară, ca mlădiță neroditoare, și se usucă, apoi mlădițele uscate sunt strânse, sunt aruncate în foc și ard.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,7,'&lt;J&gt;Dacă rămâneți în Mine și dacă rămân în voi cuvintele Mele, cereți orice&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;veți vrea, și vi se va face!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,8,'&lt;J&gt;Dacă aduceți mult rod, prin aceasta&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;Tatăl Meu va fi proslăvit, și voi&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;veți fi astfel ucenicii Mei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,9,'&lt;pb/&gt;&lt;J&gt;Cum M-a iubit pe Mine Tatăl, așa v-am iubit și Eu pe voi. Rămâneți în dragostea Mea!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,10,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;păziți poruncile Mele, veți rămâne în dragostea Mea, după cum și Eu am păzit poruncile Tatălui Meu și rămân în dragostea Lui.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,11,'&lt;J&gt;V-am spus aceste lucruri pentru ca bucuria Mea să fie în voi, și bucuria&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;voastră să fie deplină.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,12,'&lt;J&gt;Aceasta&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;este porunca Mea: să vă iubiți unii pe alții cum v-am iubit Eu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,13,'&lt;J&gt;Nu este mai mare&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;dragoste decât să-și dea cineva viața pentru prietenii săi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,14,'&lt;J&gt;Voi sunteți prietenii Mei dacă faceți&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;ce vă poruncesc Eu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,15,'&lt;J&gt;Nu vă mai numesc robi, pentru că robul nu știe ce face stăpânul său, ci v-am numit prieteni, pentru că&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;v-am făcut cunoscut tot ce am auzit de la Tatăl Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,16,'&lt;J&gt;Nu&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;voi M-ați ales pe Mine, ci Eu v-am ales pe voi și v-am&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;rânduit să mergeți și să aduceți rod, și rodul vostru să rămână, pentru ca orice&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;veți cere de la Tatăl, în Numele Meu, să vă dea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,17,'&lt;J&gt;Iată ce vă poruncesc&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt;&lt;J&gt;: să vă iubiți unii pe alții!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,18,'&lt;pb/&gt;&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;vă urăște lumea, știți că pe Mine M-a urât mai înainte de voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,19,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;ați fi din lume, lumea ar iubi ce este al ei; dar, pentru că&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;nu sunteți din lume, ci Eu v-am ales din lume, de aceea lumea vă urăște.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,20,'&lt;J&gt;Aduceți-vă aminte de cuvântul pe care vi l-am spus: «Robul&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;nu este mai mare decât stăpânul său»! Dacă pe Mine M-au prigonit, și pe voi vă vor prigoni; dacă au păzit cuvântul Meu, și pe al vostru îl vor păzi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,21,'&lt;J&gt;Dar toate acestea&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;vi le vor face din pricina Numelui Meu, pentru că nu-L cunosc pe Cel ce M-a trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,22,'&lt;J&gt;Dacă&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;n-aș fi venit și nu le-aș fi vorbit, n-ar avea păcat; dar&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;acum, nu au nicio scuză pentru păcatul lor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,23,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;Mă urăște pe Mine Îl urăște și pe Tatăl Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,24,'&lt;J&gt;Dacă n-aș fi făcut între ei lucrări&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;pe care nimeni altul nu le-a făcut, n-ar avea păcat; dar acum, le-au văzut și Ne-au urât și pe Mine, și pe Tatăl Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,25,'&lt;J&gt;Dar aceasta s-a întâmplat ca să se împlinească vorba din Legea lor: &lt;e&gt;M-au urât fără temei.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[38]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,26,'&lt;pb/&gt;&lt;J&gt;Când&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;va veni Mângâietorul, pe care-L voi trimite de la Tatăl, adică Duhul adevărului, care de la Tatăl purcede, va mărturisi&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;despre Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,15,27,'&lt;J&gt;Și voi&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;de asemenea veți mărturisi, căci de la început ați&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;fost cu Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,1,'&lt;pb/&gt;&lt;J&gt;V-am spus acestea ca să&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;nu vă poticniți.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,2,'&lt;J&gt;Vă&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;vor da afară din sinagogi, ba încă va veni ceasul când oricine&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;vă va ucide va crede că aduce o slujbă lui Dumnezeu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,3,'&lt;J&gt;Și se vor purta astfel&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;cu voi pentru că nu Ne-au cunoscut nici pe Tatăl, nici pe Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,4,'&lt;J&gt;V-am spus acestea&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;pentru ca, atunci când va veni ceasul lor, să vă aduceți aminte că vi le-am spus. Nu&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;vi le-am spus de la început, pentru că eram cu voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,5,'&lt;pb/&gt;&lt;J&gt;Acum Mă duc&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;la Cel ce M-a trimis, și nimeni din voi nu Mă întreabă: «Unde Te duci?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,6,'&lt;J&gt;Dar, pentru că v-am spus acestea, întristarea&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;v-a umplut inima.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,7,'&lt;J&gt;Totuși, vă spun adevărul: vă este de folos să Mă duc, căci, dacă nu Mă duc [Eu], Mângâietorul&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;nu va veni la voi. Dar, dacă Mă duc, vi-L voi trimite.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,8,'&lt;J&gt;Și când va veni El, va dovedi lumea vinovată în ce privește păcatul, dreptatea și judecata.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,9,'&lt;J&gt;În ce privește păcatul, fiindcă ei nu cred în Mine;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,10,'&lt;J&gt;în ce privește dreptatea, fiindcă Mă duc la Tatăl și nu Mă veți mai vedea;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,11,'&lt;J&gt;în ce privește judecata, fiindcă stăpânitorul&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;lumii acesteia este judecat.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,12,'&lt;pb/&gt;&lt;J&gt;Mai am încă multe să vă spun, dar&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;acum nu le puteți purta.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,13,'&lt;J&gt;Când va veni Mângâietorul, Duhul&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;adevărului, vă&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;va călăuzi în tot adevărul, căci nu va vorbi de la El, ci va vorbi tot ce va fi auzit și vă va vesti cele viitoare.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,14,'&lt;J&gt;El Mă va proslăvi, pentru că va lua din ce este al Meu și vi le va vesti.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,15,'&lt;J&gt;Toate&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;câte le are Tatăl sunt ale Mele, de aceea am zis că va lua din ce este al Meu și vi le va vesti.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,16,'&lt;pb/&gt;&lt;J&gt;Peste puțină&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;vreme, nu Mă veți mai vedea, apoi iarăși, peste puțină vreme, Mă veți vedea, [fiindcă&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Mă duc la Tatăl].”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,17,'Unii din ucenicii Săi ziceau între ei: „Ce înseamnă cuvintele acestea: &lt;J&gt;«Peste puțină vreme, nu Mă veți vedea, apoi iarăși, peste puțină vreme, Mă veți vedea»&lt;/J&gt; și &lt;J&gt;«fiindcă Mă duc la Tatăl»&lt;/J&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (500,16,18,'Ziceau deci: „Ce înseamnă aceasta: &lt;J&gt;«Peste puțină vreme»&lt;/J&gt;? Nu știm ce vrea să spună.”');
INSERT INTO &quot;verses&quot; VALUES (500,16,19,'&lt;pb/&gt;Isus a cunoscut că voiau să-L întrebe și le-a zis: &lt;J&gt;„Vă întrebați între voi ce înseamnă cuvintele: «Peste puțină vreme, nu Mă veți vedea, apoi iarăși, peste puțină vreme, Mă veți vedea»?&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,20,'&lt;J&gt;Adevărat, adevărat vă spun: voi veți plânge și vă veți tângui, iar lumea se va bucura; vă veți întrista, dar întristarea voastră se va preface în bucurie.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,21,'&lt;J&gt;Femeia&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt;&lt;J&gt;, când dă naștere, are parte de întristare, pentru că i-a sosit ceasul, dar, după ce a născut pruncul, nu-și mai amintește suferința, de bucurie că s-a născut un om pe lume.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,22,'&lt;J&gt;Și voi aveți acum parte de întristare, dar Eu iarăși vă voi vedea, inima vi se&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;va bucura, și bucuria voastră nimeni n-o va răpi de la voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,23,'&lt;pb/&gt;&lt;J&gt;În ziua aceea, nu Mă veți mai întreba nimic. Adevărat&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;, adevărat vă spun: orice veți cere de la Tatăl, în Numele Meu, vă va da.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,24,'&lt;J&gt;Până acum n-ați cerut nimic în Numele Meu: cereți și veți primi, pentru ca&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;bucuria voastră să fie deplină.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,25,'&lt;pb/&gt;&lt;J&gt;V-am spus acestea în pilde. Vine ceasul când nu vă voi mai vorbi în pilde, ci vă voi vorbi deslușit despre Tatăl.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,26,'&lt;J&gt;În ziua aceea&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt;&lt;J&gt;, veți cere în Numele Meu și nu vă zic că voi ruga pe Tatăl pentru voi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,27,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;Tatăl Însuși vă iubește, pentru că voi M-ați iubit și ați&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;crezut că de la Dumnezeu am ieșit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,28,'&lt;J&gt;Am ieșit&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;de la Tatăl și am venit în lume; acum, las lumea și Mă duc la Tatăl.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,29,'&lt;pb/&gt;Ucenicii Săi I-au zis: „Iată că acum vorbești deslușit și nu spui nicio pildă.');
INSERT INTO &quot;verses&quot; VALUES (500,16,30,'Acum cunoaștem că&lt;f&gt;ⓖ&lt;/f&gt; toate le știi și nu ai nevoie să Te întrebe cineva; de aceea credem că de la Dumnezeu ai ieșit.”');
INSERT INTO &quot;verses&quot; VALUES (500,16,31,'&lt;J&gt;„Acum credeți?”&lt;/J&gt;, le-a răspuns Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,16,32,'&lt;J&gt;„Iată&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt;&lt;J&gt;, vine ceasul, și a și venit, când veți fi risipiți fiecare&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;la ale sale, iar pe Mine Mă veți lăsa singur; dar&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;nu sunt singur, căci Tatăl este cu Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,16,33,'&lt;J&gt;Toate acestea vi le-am spus ca să aveți pace în&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;Mine. În&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;lume veți avea necazuri, dar&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;îndrăzniți, Eu am biruit&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;lumea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,1,'&lt;pb/&gt;După ce a vorbit astfel, Isus Și-a ridicat ochii spre cer și a zis: &lt;J&gt;„Tată, a&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;sosit ceasul! Proslăvește-L pe Fiul Tău, ca și Fiul Tău să Te proslăvească pe Tine,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,2,'&lt;J&gt;după cum&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;I-ai dat autoritate peste orice făptură, ca să dea viață veșnică tuturor acelora pe care I i-ai dat Tu!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,3,'&lt;J&gt;Și viața veșnică este aceasta: să Te cunoască pe Tine, singurul&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Dumnezeu adevărat, și pe Isus Hristos, pe care&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;L-ai trimis Tu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,4,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;Te-am proslăvit pe pământ, căci am&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;împlinit lucrarea pe care&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;Mi-ai dat-o s-o fac.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,5,'&lt;J&gt;Și acum, Tată, proslăvește-Mă la Tine Însuți cu slava pe&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;care o aveam la Tine înainte de a fi lumea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,6,'&lt;pb/&gt;&lt;J&gt;Am făcut cunoscut Numele Tău oamenilor pe care&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;Mi i-ai dat din lume. Ai Tăi erau, și Mi i-ai dat Mie; și ei au păzit Cuvântul Tău.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,7,'&lt;J&gt;Acum au cunoscut că tot ce Mi-ai dat vine de la Tine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,8,'&lt;J&gt;Căci cuvintele pe care&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Mi le-ai dat Tu li le-am dat lor, și ei le-au primit și au cunoscut cu adevărat că de la Tine am ieșit și au crezut&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;că Tu M-ai trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,9,'&lt;pb/&gt;&lt;J&gt;Pentru ei Mă rog. Nu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;pentru lume, ci pentru cei pe care Mi i-ai dat Tu, căci sunt ai Tăi;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,10,'&lt;J&gt;tot ce este al Meu este al Tău, iar ce este al Tău&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;este al Meu; și Eu sunt proslăvit în ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,11,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;nu mai sunt în lume, dar ei sunt în lume, și Eu vin la Tine. Sfinte Tată, păzește-i în Numele Tău, pe care Mi L-ai dat, ca&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;ei să fie una, asemenea&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;Nouă.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,12,'&lt;J&gt;Când eram cu ei [în lume], îi păzeam Eu în Numele Tău, pe&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;care Mi L-ai dat, și&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;i-am ocrotit, astfel încât niciunul din ei n-a pierit, în afară&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;de fiul pierzării, ca&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;să se împlinească Scriptura.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,13,'&lt;J&gt;Dar, acum, vin la Tine și spun acestea cât sunt încă în lume, pentru ca ei să aibă bucuria Mea deplină în ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,14,'&lt;J&gt;Le-am&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;dat Cuvântul Tău; și&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;lumea i-a urât, fiindcă ei nu sunt din lume, după cum&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;nici Eu nu sunt din lume.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,15,'&lt;J&gt;Nu Te rog să-i iei din lume, ci să-i&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;păzești de Cel Rău.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,16,'&lt;J&gt;Ei&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;nu sunt din lume, după cum nici Eu nu sunt din lume.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,17,'&lt;J&gt;Sfințește-i&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;prin adevărul [Tău]: Cuvântul&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;Tău este adevăr.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,18,'&lt;J&gt;Precum&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;M-ai trimis Tu pe Mine în lume, așa i-am trimis și Eu pe ei în lume.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,19,'&lt;J&gt;Pentru ei Mă sfințesc pe Mine Însumi, ca și ei să fie sfințiți prin adevăr.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,20,'&lt;pb/&gt;&lt;J&gt;Nu numai pentru aceștia Mă rog, ci și pentru cei care vor crede în Mine prin cuvântul lor,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,21,'&lt;J&gt;ca&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;toți să fie una, precum Tu&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;, Tată, ești în Mine, și Eu, în Tine, ca și ei să fie [una] în Noi, pentru ca lumea să creadă că Tu M-ai trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,22,'&lt;J&gt;Și slava pe care Mi-ai dat-o Mie le-am dat-o lor, ca&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;să fie una, precum și Noi [suntem] una —&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,23,'&lt;J&gt;Eu în ei, și Tu în Mine — pentru ca&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;ei să fie în chip desăvârșit una, ca să cunoască lumea că Tu M-ai trimis și că i-ai iubit cum M-ai iubit pe Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,24,'&lt;pb/&gt;&lt;J&gt;Tată&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;, vreau ca, acolo unde sunt Eu, să fie împreună cu Mine și aceia pe care Mi i-ai dat Tu, ca să vadă slava Mea, slavă pe care Mi-ai dat-o fiindcă&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;M-ai iubit înainte de întemeierea lumii.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,25,'&lt;J&gt;Bunule Tată, lumea&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;nu Te-a cunoscut, dar Eu&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;Te-am cunoscut, și aceștia&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;au cunoscut că Tu M-ai trimis.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,17,26,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;le-am făcut cunoscut Numele Tău și Îl voi mai face cunoscut, pentru ca dragostea cu&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;care M-ai iubit Tu să fie în ei, și Eu să fiu în ei.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,1,'&lt;pb/&gt;După ce a rostit aceste vorbe, Isus a plecat cu ucenicii Săi dincolo de pârâul Chedron&lt;f&gt;ⓑ&lt;/f&gt;, unde era o grădină, în care au intrat El și ucenicii Săi.');
INSERT INTO &quot;verses&quot; VALUES (500,18,2,'Iuda, vânzătorul, știa și el locul, pentru că&lt;f&gt;ⓒ&lt;/f&gt; Isus Se adunase de multe ori acolo cu ucenicii Săi.');
INSERT INTO &quot;verses&quot; VALUES (500,18,3,'Așadar, Iuda a luat o cohortă de soldați și gărzi de la preoții de seamă și de la farisei și a venit acolo cu felinare, cu făclii și cu arme.');
INSERT INTO &quot;verses&quot; VALUES (500,18,4,'Isus, care știa tot ce avea să I se întâmple, a ieșit și le-a zis: &lt;J&gt;„Pe cine căutați?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,5,'Ei I-au răspuns: „Pe Isus Nazarineanul!” Isus le-a zis: &lt;J&gt;„Eu Sunt!”&lt;/J&gt; Iuda, vânzătorul, era și el cu ei.');
INSERT INTO &quot;verses&quot; VALUES (500,18,6,'Când le-a zis Isus: &lt;J&gt;„Eu Sunt”&lt;/J&gt;, ei s-au dat înapoi și au căzut la pământ.');
INSERT INTO &quot;verses&quot; VALUES (500,18,7,'El i-a întrebat din nou: &lt;J&gt;„Pe cine căutați?”&lt;/J&gt; „Pe Isus Nazarineanul”, I-au zis ei.');
INSERT INTO &quot;verses&quot; VALUES (500,18,8,'Isus a răspuns: &lt;J&gt;„V-am spus că Eu Sunt. Dacă pe Mine Mă căutați, lăsați-i pe aceștia să se ducă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,9,'(A zis aceasta ca să se împlinească vorba pe care o spusese: &lt;J&gt;„N-am pierdut pe&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;niciunul din aceia pe care Mi i-ai dat.”&lt;/J&gt;)');
INSERT INTO &quot;verses&quot; VALUES (500,18,10,'Simon Petru, care avea o sabie, a scos-o, l-a lovit pe slujitorul marelui-preot și i-a tăiat urechea dreaptă. Slujitorul se numea Malhus.');
INSERT INTO &quot;verses&quot; VALUES (500,18,11,'Isus i-a zis lui Petru: &lt;J&gt;„Bagă-ți sabia în teacă! Paharul&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;pe care Mi l-a dat Tatăl să-l beau oare nu-l voi bea?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,12,'&lt;pb/&gt;Cohorta, căpitanul și gărzile din partea iudeilor L-au prins pe Isus și L-au legat.');
INSERT INTO &quot;verses&quot; VALUES (500,18,13,'L-au&lt;f&gt;ⓕ&lt;/f&gt; dus întâi la Anna&lt;f&gt;ⓖ&lt;/f&gt;, căci el era socrul lui Caiafa, care era mare-preot în anul acela.');
INSERT INTO &quot;verses&quot; VALUES (500,18,14,'Caiafa&lt;f&gt;ⓗ&lt;/f&gt; era cel ce dăduse iudeilor sfatul acesta: „Este de folos să moară un singur om pentru popor.”');
INSERT INTO &quot;verses&quot; VALUES (500,18,15,'&lt;pb/&gt;Simon Petru&lt;f&gt;ⓘ&lt;/f&gt; Îl urma pe Isus împreună cu un alt ucenic. Ucenicul acela era cunoscut marelui-preot și a intrat după Isus în curtea marelui-preot.');
INSERT INTO &quot;verses&quot; VALUES (500,18,16,'Petru&lt;f&gt;ⓙ&lt;/f&gt; însă a rămas afară la poartă. Celălalt ucenic, care era cunoscut marelui-preot, a ieșit, a vorbit cu portăreasa și l-a băgat pe Petru înăuntru.');
INSERT INTO &quot;verses&quot; VALUES (500,18,17,'Slujnica portăreasă i-a zis lui Petru: „Nu cumva și tu ești unul din ucenicii omului acestuia?” „Nu sunt!”, a răspuns el.');
INSERT INTO &quot;verses&quot; VALUES (500,18,18,'Slujitorii și gărzile care erau acolo făcuseră un foc de cărbuni, căci era frig, și se încălzeau. Petru stătea și el cu ei și se încălzea.');
INSERT INTO &quot;verses&quot; VALUES (500,18,19,'&lt;pb/&gt;Marele-preot L-a întrebat pe Isus despre ucenicii Lui și despre învățătura Lui.');
INSERT INTO &quot;verses&quot; VALUES (500,18,20,'Isus i-a răspuns: &lt;J&gt;„Eu&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;am vorbit lumii pe față; întotdeauna am dat învățătură în sinagogă și în Templu, unde se adună toți iudeii, și n-am spus nimic în ascuns.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,21,'&lt;J&gt;Pentru ce Mă întrebi pe Mine? Întreabă-i pe cei ce au auzit ce le-am vorbit; iată, aceștia știu ce am spus.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,22,'Când a spus El acestea, una dintre gărzile aflate acolo I-a&lt;f&gt;ⓛ&lt;/f&gt; dat o palmă lui Isus, zicând: „Așa răspunzi marelui-preot?”');
INSERT INTO &quot;verses&quot; VALUES (500,18,23,'Isus i-a răspuns: &lt;J&gt;„Dacă am vorbit rău, arată ce am spus rău; dar, dacă am vorbit bine, de ce Mă bați?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,24,'Anna&lt;f&gt;ⓜ&lt;/f&gt; L-a trimis legat la marele-preot Caiafa.');
INSERT INTO &quot;verses&quot; VALUES (500,18,25,'&lt;pb/&gt;Simon Petru stătea și se încălzea. Ei&lt;f&gt;ⓝ&lt;/f&gt; i-au zis: „Nu cumva și tu ești dintre ucenicii lui?” El a tăgăduit și a zis: „Nu sunt!”');
INSERT INTO &quot;verses&quot; VALUES (500,18,26,'Unul din slujitorii marelui-preot, rudă cu cel căruia îi tăiase Petru urechea, a zis: „Nu te-am văzut eu cu el în grădină?”');
INSERT INTO &quot;verses&quot; VALUES (500,18,27,'Petru iarăși a tăgăduit și îndată&lt;f&gt;ⓞ&lt;/f&gt; a cântat cocoșul.');
INSERT INTO &quot;verses&quot; VALUES (500,18,28,'&lt;pb/&gt;L-au adus&lt;f&gt;ⓟ&lt;/f&gt; pe Isus de la Caiafa la pretoriu. Era dimineață. Ei n-au intrat în pretoriu, ca să nu se întineze, ci să poată mânca Paștele.');
INSERT INTO &quot;verses&quot; VALUES (500,18,29,'Atunci a ieșit Pilat afară la ei și le-a zis: „Ce învinuire aduceți împotriva omului acestuia?”');
INSERT INTO &quot;verses&quot; VALUES (500,18,30,'Drept răspuns, ei i-au zis: „Dacă acesta n-ar fi fost un răufăcător, nu l-am fi dat noi în mâinile tale!”');
INSERT INTO &quot;verses&quot; VALUES (500,18,31,'Pilat le-a zis: „Luați-l voi și judecați-l după Legea voastră!” „Nouă nu ne este îngăduit de Lege să omorâm pe nimeni”, i-au zis iudeii.');
INSERT INTO &quot;verses&quot; VALUES (500,18,32,'Aceasta s-a întâmplat ca&lt;f&gt;ⓠ&lt;/f&gt; să se împlinească vorbele lui Isus prin care arătase cu ce moarte avea să moară.');
INSERT INTO &quot;verses&quot; VALUES (500,18,33,'&lt;pb/&gt;Pilat&lt;f&gt;ⓡ&lt;/f&gt; a intrat iarăși în pretoriu, L-a chemat pe Isus și I-a zis: „Ești tu regele iudeilor?”');
INSERT INTO &quot;verses&quot; VALUES (500,18,34,'Isus i-a răspuns: &lt;J&gt;„De la tine zici lucrul acesta sau ți l-au spus alții despre Mine?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,35,'Pilat a răspuns: „Oare sunt eu iudeu? Neamul tău și preoții de seamă te-au dat în mâinile mele. Ce ai făcut?”');
INSERT INTO &quot;verses&quot; VALUES (500,18,36,'&lt;J&gt;„Împărăția&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;Mea nu este din lumea aceasta”&lt;/J&gt;, a răspuns&lt;f&gt;ⓣ&lt;/f&gt; Isus. &lt;J&gt;„Dacă Împărăția Mea era din lumea aceasta, slujitorii Mei s-ar fi luptat ca să nu fiu dat în mâinile iudeilor. Dar, acum, Împărăția Mea nu este&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;de aici.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,37,'„Atunci, un rege tot ești!”, I-a zis Pilat. &lt;J&gt;„Tu însuți spui că sunt rege”&lt;/J&gt;&lt;f&gt;[39]&lt;/f&gt;, a răspuns Isus. &lt;J&gt;„Eu pentru aceasta M-am născut și am venit în lume: ca să mărturisesc despre adevăr. Oricine este din adevăr ascultă glasul Meu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,18,38,'Pilat I-a zis: „Ce este adevărul?” &lt;pb/&gt;După ce a spus aceste vorbe, Pilat a ieșit iarăși afară la iudei și le-a zis: „Eu&lt;f&gt;ⓥ&lt;/f&gt; nu găsesc nicio vină în el.');
INSERT INTO &quot;verses&quot; VALUES (500,18,39,'Dar&lt;f&gt;ⓦ&lt;/f&gt;, fiindcă la voi este obiceiul să vă eliberez pe cineva de Paște, vreți să vi-l eliberez pe regele iudeilor?”');
INSERT INTO &quot;verses&quot; VALUES (500,18,40,'Atunci&lt;f&gt;ⓧ&lt;/f&gt;, toți au strigat din nou: „Nu pe acesta, ci pe Barabba!” Iar Barabba&lt;f&gt;ⓨ&lt;/f&gt; era un tâlhar.');
INSERT INTO &quot;verses&quot; VALUES (500,19,1,'&lt;pb/&gt;Atunci&lt;f&gt;ⓩ&lt;/f&gt;, Pilat L-a luat pe Isus și a pus să-L biciuiască.');
INSERT INTO &quot;verses&quot; VALUES (500,19,2,'Soldații au împletit o cunună de spini, I-au pus-o pe cap și L-au îmbrăcat cu o haină purpurie.');
INSERT INTO &quot;verses&quot; VALUES (500,19,3,'Apoi, s-au apropiat de El și ziceau: „Plecăciune, regele iudeilor!” Și-I dădeau palme.');
INSERT INTO &quot;verses&quot; VALUES (500,19,4,'Pilat a ieșit iarăși afară și le-a zis iudeilor: „Iată, vi-l aduc afară, ca&lt;f&gt;ⓐ&lt;/f&gt; să știți că nu găsesc nicio vină în el.”');
INSERT INTO &quot;verses&quot; VALUES (500,19,5,'Isus a ieșit deci afară purtând cununa de spini și haina purpurie. „Iată omul!”, le-a zis Pilat.');
INSERT INTO &quot;verses&quot; VALUES (500,19,6,'Când L-au văzut, preoții de seamă și gărzile au început să strige: „Răstignește-l! Răstignește-l!” „Luați-l voi și răstigniți-l”, le-a zis&lt;f&gt;ⓑ&lt;/f&gt; Pilat, „căci eu nu găsesc nicio vină în el!”');
INSERT INTO &quot;verses&quot; VALUES (500,19,7,'Iudeii i-au răspuns: „Noi avem o Lege&lt;f&gt;ⓒ&lt;/f&gt;, și după Legea aceasta, el trebuie să moară, pentru că s-a&lt;f&gt;ⓓ&lt;/f&gt; făcut pe sine Fiul lui Dumnezeu.”');
INSERT INTO &quot;verses&quot; VALUES (500,19,8,'&lt;pb/&gt;Când a auzit Pilat aceste cuvinte, s-a înspăimântat și mai mult.');
INSERT INTO &quot;verses&quot; VALUES (500,19,9,'A intrat iarăși în pretoriu și I-a zis lui Isus: „De unde ești tu?” Dar&lt;f&gt;ⓔ&lt;/f&gt; Isus nu i-a dat niciun răspuns.');
INSERT INTO &quot;verses&quot; VALUES (500,19,10,'Pilat I-a zis: „Mie nu-mi vorbești? Nu știi că am autoritate să-ți dau drumul și am autoritate să te răstignesc?”');
INSERT INTO &quot;verses&quot; VALUES (500,19,11,'&lt;J&gt;„N-ai&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;avea nicio autoritate asupra Mea dacă nu ți-ar fi dată de sus”&lt;/J&gt;, i-a răspuns Isus. &lt;J&gt;„De aceea, cine Mă dă în mâinile tale are un mai mare păcat.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,19,12,'De atunci, Pilat căuta să-I dea drumul. Dar iudeii strigau: „Dacă&lt;f&gt;ⓖ&lt;/f&gt; îi dai drumul omului acestuia, nu ești prieten cu cezarul. Oricine&lt;f&gt;ⓗ&lt;/f&gt; se face pe sine rege este împotriva cezarului.”');
INSERT INTO &quot;verses&quot; VALUES (500,19,13,'&lt;pb/&gt;Când a auzit Pilat aceste cuvinte, L-a scos pe Isus afară și a șezut pe scaunul de judecător, în locul numit „Pardosit cu pietre”&lt;f&gt;[40]&lt;/f&gt;, iar pe evreiește &lt;e&gt;Gabbathá&lt;/e&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,19,14,'Era&lt;f&gt;ⓘ&lt;/f&gt; ziua pregătirii Paștelui, cam pe la ceasul al șaselea. Pilat le-a zis iudeilor: „Iată regele vostru!”');
INSERT INTO &quot;verses&quot; VALUES (500,19,15,'Dar ei au strigat: „Ia-l, ia-l, răstignește-l!” „Să-l răstignesc pe regele vostru?”, le-a zis Pilat. Preoții de seamă au răspuns: „Noi n-avem&lt;f&gt;ⓙ&lt;/f&gt; alt rege decât pe cezar!”');
INSERT INTO &quot;verses&quot; VALUES (500,19,16,'Atunci&lt;f&gt;ⓚ&lt;/f&gt;, L-a dat în mâinile lor, ca să fie răstignit. Au luat deci pe Isus [și L-au dus să-L răstignească].');
INSERT INTO &quot;verses&quot; VALUES (500,19,17,'&lt;pb/&gt;Isus, ducându-Și&lt;f&gt;ⓛ&lt;/f&gt; crucea, a&lt;f&gt;ⓜ&lt;/f&gt; ajuns la așa-numitul „Loc al Căpățânii”, care în evreiește se cheamă Golgota.');
INSERT INTO &quot;verses&quot; VALUES (500,19,18,'Acolo a fost răstignit și, împreună cu El, au fost răstigniți alți doi, de o parte și de alta, iar El la mijloc.');
INSERT INTO &quot;verses&quot; VALUES (500,19,19,'Pilat&lt;f&gt;ⓝ&lt;/f&gt; a făcut și o inscripție, pe care a pus-o deasupra crucii. Pe ea era scris: &lt;pb/&gt;„Isus Nazarineanul, regele iudeilor”.');
INSERT INTO &quot;verses&quot; VALUES (500,19,20,'&lt;pb/&gt;Mulți din iudei au citit această inscripție, pentru că locul unde fusese răstignit Isus era aproape de cetate; era scrisă în evreiește, latinește și grecește.');
INSERT INTO &quot;verses&quot; VALUES (500,19,21,'Preoții de seamă ai iudeilor i-au zis lui Pilat: „Nu scrie: «Regele iudeilor», ci: «El a zis: &lt;J&gt;‘Eu sunt Regele iudeilor’&lt;/J&gt;»!”');
INSERT INTO &quot;verses&quot; VALUES (500,19,22,'„Ce am scris, am scris!”, a răspuns Pilat.');
INSERT INTO &quot;verses&quot; VALUES (500,19,23,'&lt;pb/&gt;Soldații&lt;f&gt;ⓞ&lt;/f&gt;, după ce L-au răstignit pe Isus, I-au luat hainele, pe care le-au împărțit în patru, câte o parte pentru fiecare soldat, și cămașa. Cămașa însă era fără cusătură, țesută întreagă de sus până jos.');
INSERT INTO &quot;verses&quot; VALUES (500,19,24,'Au zis deci între ei: „Să n-o sfâșiem, ci să tragem la sorți a cui să fie!” Aceasta s-a întâmplat ca să se împlinească Scriptura, care zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Și-au împărțit hainele Mele între ei,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și pentru veșmântul Meu au tras la sorți.&lt;/e&gt; &lt;f&gt;[41]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;Astfel au făcut soldații.');
INSERT INTO &quot;verses&quot; VALUES (500,19,25,'&lt;pb/&gt;Lângă&lt;f&gt;ⓟ&lt;/f&gt; crucea lui Isus, stăteau mama Lui și sora mamei Lui, Maria, soția lui Cleopa&lt;f&gt;ⓠ&lt;/f&gt;, și Maria Magdalena.');
INSERT INTO &quot;verses&quot; VALUES (500,19,26,'Când i-a văzut Isus pe mama Sa și, lângă ea, pe&lt;f&gt;ⓡ&lt;/f&gt; ucenicul pe care-l iubea, a zis mamei Sale: &lt;J&gt;„Femeie&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt;&lt;J&gt;, iată fiul tău!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,19,27,'Apoi, a zis ucenicului: &lt;J&gt;„Iată mama ta!”&lt;/J&gt; Și, din ceasul acela, ucenicul a luat-o la el acasă&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,19,28,'&lt;pb/&gt;După aceea, Isus, care știa că acum totul s-a sfârșit&lt;f&gt;[42]&lt;/f&gt;, ca&lt;f&gt;ⓤ&lt;/f&gt; să se împlinească Scriptura, a zis: &lt;J&gt;„Mi-e sete!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,19,29,'Acolo era un vas plin cu oțet. Soldații au pus&lt;f&gt;ⓥ&lt;/f&gt; într-o ramură de isop un burete plin cu oțet și I l-au dus la gură.');
INSERT INTO &quot;verses&quot; VALUES (500,19,30,'Când a luat Isus oțetul, a zis: &lt;J&gt;„S-a&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;sfârșit!”&lt;/J&gt; Apoi Și-a plecat capul și Și-a dat duhul.');
INSERT INTO &quot;verses&quot; VALUES (500,19,31,'&lt;pb/&gt;Fiindcă&lt;f&gt;ⓧ&lt;/f&gt; era ziua pregătirii și ca să&lt;f&gt;ⓨ&lt;/f&gt; nu rămână trupurile pe cruce în sabat — căci era mare ziua aceea de sabat — iudeii l-au rugat pe Pilat să zdrobească fluierele picioarelor celor răstigniți și să fie luați de pe cruce.');
INSERT INTO &quot;verses&quot; VALUES (500,19,32,'Soldații au venit deci și au zdrobit fluierele picioarelor celui dintâi, apoi pe ale celuilalt care fusese răstignit împreună cu El.');
INSERT INTO &quot;verses&quot; VALUES (500,19,33,'Când au venit la Isus și au văzut că deja murise, nu I-au zdrobit fluierele picioarelor,');
INSERT INTO &quot;verses&quot; VALUES (500,19,34,'dar unul din soldați I-a străpuns coasta cu sulița; și îndată a&lt;f&gt;ⓩ&lt;/f&gt; ieșit din ea sânge și apă.');
INSERT INTO &quot;verses&quot; VALUES (500,19,35,'Cel ce a văzut a dat mărturie, și mărturia lui este adevărată; el știe că spune adevărul, pentru ca și voi să credeți.');
INSERT INTO &quot;verses&quot; VALUES (500,19,36,'Acestea s-au întâmplat ca să se împlinească Scriptura: &lt;e&gt;Niciun os nu I se va zdrobi.&lt;/e&gt;&lt;f&gt;[43]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,19,37,'Și iarăși, altă Scriptură zice: &lt;e&gt;Vor vedea pe cine au străpuns.&lt;/e&gt;&lt;f&gt;[44]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,19,38,'&lt;pb/&gt;După&lt;f&gt;ⓐ&lt;/f&gt; aceea, Iosif din Arimateea, care era ucenic al lui Isus, dar pe ascuns, de&lt;f&gt;ⓑ&lt;/f&gt; frica iudeilor, l-a rugat pe Pilat să-i îngăduie să ia trupul lui Isus de pe cruce. Și Pilat i-a dat voie. El a venit deci și a luat trupul lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,19,39,'A venit și Nicodim&lt;f&gt;ⓒ&lt;/f&gt;, care se dusese la Isus noaptea, la început, și a adus un amestec de smirnă și aloe, ca la o sută de litre&lt;f&gt;[45]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (500,19,40,'Au luat deci trupul lui Isus și l-au&lt;f&gt;ⓓ&lt;/f&gt; înfășurat în pânze de in, cu miresme, precum au iudeii obicei să înmormânteze.');
INSERT INTO &quot;verses&quot; VALUES (500,19,41,'În locul unde fusese răstignit Isus era o grădină, și în grădină era un mormânt nou, în care nu mai fusese pus nimeni.');
INSERT INTO &quot;verses&quot; VALUES (500,19,42,'Fiindcă&lt;f&gt;ⓔ&lt;/f&gt; la iudei era ziua pregătirii și pentru că&lt;f&gt;ⓕ&lt;/f&gt; mormântul era aproape, L-au pus acolo pe Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,20,1,'&lt;pb/&gt;În ziua&lt;f&gt;ⓖ&lt;/f&gt; dintâi a săptămânii, Maria Magdalena s-a dus dis-de-dimineață la mormânt, pe când era încă întuneric, și a văzut că piatra fusese luată de pe mormânt.');
INSERT INTO &quot;verses&quot; VALUES (500,20,2,'A alergat la Simon Petru și la celălalt&lt;f&gt;ⓗ&lt;/f&gt; ucenic, pe care-l iubea Isus, și le-a zis: „L-au luat pe Domnul din mormânt și nu știm unde L-au pus.”');
INSERT INTO &quot;verses&quot; VALUES (500,20,3,'Petru&lt;f&gt;ⓘ&lt;/f&gt; și celălalt ucenic au ieșit și au plecat spre mormânt.');
INSERT INTO &quot;verses&quot; VALUES (500,20,4,'Au început să alerge amândoi împreună, dar celălalt ucenic alerga mai repede decât Petru și a ajuns cel dintâi la mormânt.');
INSERT INTO &quot;verses&quot; VALUES (500,20,5,'S-a aplecat și a văzut fâșiile&lt;f&gt;ⓙ&lt;/f&gt; de pânză jos, dar n-a intrat.');
INSERT INTO &quot;verses&quot; VALUES (500,20,6,'Simon Petru, care venea după el, a ajuns și el, a intrat în mormânt și a văzut fâșiile de pânză jos.');
INSERT INTO &quot;verses&quot; VALUES (500,20,7,'Iar ștergarul&lt;f&gt;ⓚ&lt;/f&gt; care fusese pus pe capul lui Isus nu era cu fâșiile de pânză, ci deoparte, înfășurat într-un loc.');
INSERT INTO &quot;verses&quot; VALUES (500,20,8,'Atunci, celălalt ucenic, care ajunsese cel dintâi la mormânt, a intrat și el, a văzut și a crezut.');
INSERT INTO &quot;verses&quot; VALUES (500,20,9,'Căci tot nu pricepeau Scriptura&lt;f&gt;ⓛ&lt;/f&gt; — că Isus trebuia să învie din morți.');
INSERT INTO &quot;verses&quot; VALUES (500,20,10,'Apoi, ucenicii s-au întors acasă.');
INSERT INTO &quot;verses&quot; VALUES (500,20,11,'&lt;pb/&gt;Maria însă&lt;f&gt;ⓜ&lt;/f&gt; stătea afară lângă mormânt și plângea. Pe când plângea, s-a aplecat să se uite în mormânt.');
INSERT INTO &quot;verses&quot; VALUES (500,20,12,'Și a văzut doi îngeri în alb șezând unde fusese culcat trupul lui Isus: unul la cap și altul la picioare.');
INSERT INTO &quot;verses&quot; VALUES (500,20,13,'„Femeie”, i-au zis ei, „pentru ce plângi?” Ea le-a răspuns: „Pentru că L-au luat pe Domnul meu și nu știu unde L-au pus.”');
INSERT INTO &quot;verses&quot; VALUES (500,20,14,'După ce&lt;f&gt;ⓝ&lt;/f&gt; a zis aceste vorbe, s-a întors și L-a văzut pe Isus stând acolo în picioare, dar nu știa că este Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,20,15,'&lt;J&gt;„Femeie”&lt;/J&gt;, i-a zis Isus, &lt;J&gt;„pentru ce plângi? Pe cine cauți?”&lt;/J&gt; Ea a crezut că este grădinarul și I-a zis: „Domnule, dacă L-ai luat tu, spune-mi unde L-ai pus, și mă voi duce să-L iau!”');
INSERT INTO &quot;verses&quot; VALUES (500,20,16,'Isus i-a zis: &lt;J&gt;„Marie!”&lt;/J&gt; Ea s-a întors și I-a zis în evreiește: „&lt;e&gt;Rabbuní!&lt;/e&gt;” (adică „Învățătorule!”).');
INSERT INTO &quot;verses&quot; VALUES (500,20,17,'&lt;J&gt;„Nu Mă ține”&lt;/J&gt;, i-a zis Isus, &lt;J&gt;„căci încă nu M-am suit la Tatăl [Meu]. Ci du-te la frații&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;Mei și spune-le: «Mă&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;sui la Tatăl Meu și Tatăl vostru, la Dumnezeul&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Meu și Dumnezeul vostru»!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,18,'Maria Magdalena s-a&lt;f&gt;ⓡ&lt;/f&gt; dus și le-a vestit ucenicilor că L-a văzut pe Domnul și că El i-a spus aceste lucruri.');
INSERT INTO &quot;verses&quot; VALUES (500,20,19,'&lt;pb/&gt;În&lt;f&gt;ⓢ&lt;/f&gt; seara aceleiași zile, cea dintâi a săptămânii, pe când ușile locului unde erau adunați ucenicii erau încuiate de frica iudeilor, a venit Isus, a stat în mijlocul lor și le-a zis: &lt;J&gt;„Pace vouă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,20,'Și după ce a zis aceste vorbe, le-a arătat mâinile și coasta. Ucenicii&lt;f&gt;ⓣ&lt;/f&gt; s-au bucurat când L-au văzut pe Domnul.');
INSERT INTO &quot;verses&quot; VALUES (500,20,21,'Isus&lt;f&gt;ⓤ&lt;/f&gt; le-a zis din nou: &lt;J&gt;„Pace vouă! Cum&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;M-a trimis pe Mine Tatăl, așa vă trimit și Eu pe voi!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,22,'După aceste cuvinte, a suflat asupra lor și le-a zis: &lt;J&gt;„Luați Duh Sfânt!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,23,'&lt;J&gt;Celor cărora&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;le veți ierta păcatele le vor fi iertate, și celor cărora le veți ține păcatele le vor fi ținute.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,24,'&lt;pb/&gt;Toma, zis&lt;f&gt;ⓧ&lt;/f&gt; Geamănul, unul din cei doisprezece, nu era cu ei când a venit Isus.');
INSERT INTO &quot;verses&quot; VALUES (500,20,25,'Ceilalți ucenici i-au zis deci: „L-am văzut pe Domnul!” Dar el le-a răspuns: „Dacă nu voi vedea în mâinile Lui semnul cuielor și dacă nu voi pune degetul meu în semnul cuielor și dacă nu voi pune mâna mea în coasta Lui, nicidecum nu voi crede!”');
INSERT INTO &quot;verses&quot; VALUES (500,20,26,'După opt zile, ucenicii lui Isus erau iarăși înăuntru, și Toma era și el cu ei. Deși ușile erau încuiate, a venit Isus, a stat în mijloc și le-a zis: &lt;J&gt;„Pace vouă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,27,'Apoi i-a zis lui Toma: &lt;J&gt;„Adu-ți&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;degetul încoace și uită-te la mâinile Mele și adu-ți mâna și pune-o în coasta Mea; și nu fi necredincios, ci credincios!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,28,'Drept răspuns, Toma I-a zis: „Domnul meu și Dumnezeul meu!”');
INSERT INTO &quot;verses&quot; VALUES (500,20,29,'Isus i-a zis: &lt;J&gt;„Pentru că M-ai văzut, ai crezut? Ferice&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;de cei ce n-au văzut, și au crezut!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,20,30,'&lt;pb/&gt;Isus a mai făcut înaintea ucenicilor Săi multe&lt;f&gt;ⓐ&lt;/f&gt; alte semne, care nu sunt scrise în cartea aceasta.');
INSERT INTO &quot;verses&quot; VALUES (500,20,31,'Dar&lt;f&gt;ⓑ&lt;/f&gt; acestea au fost scrise pentru ca voi să credeți că Isus este Hristosul, Fiul lui Dumnezeu, și&lt;f&gt;ⓒ&lt;/f&gt;, crezând, să aveți viață în Numele Lui.');
INSERT INTO &quot;verses&quot; VALUES (500,21,1,'&lt;pb/&gt;După aceea, Isus li S-a mai arătat ucenicilor Săi la Marea Tiberiadei. Iată cum li S-a arătat:');
INSERT INTO &quot;verses&quot; VALUES (500,21,2,'Simon Petru, Toma, zis Geamănul, Natanael,&lt;f&gt;ⓓ&lt;/f&gt; cel din Cana Galileei, fiii&lt;f&gt;ⓔ&lt;/f&gt; lui Zebedei și alți doi din ucenicii lui Isus erau împreună.');
INSERT INTO &quot;verses&quot; VALUES (500,21,3,'Simon Petru le-a zis: „Mă duc să pescuiesc.” „Mergem și noi cu tine”, i-au zis ei. Au ieșit și s-au suit în barcă, dar în noaptea aceea n-au prins nimic.');
INSERT INTO &quot;verses&quot; VALUES (500,21,4,'Dimineața, Isus stătea pe țărm, dar ucenicii nu&lt;f&gt;ⓕ&lt;/f&gt; știau că este El.');
INSERT INTO &quot;verses&quot; VALUES (500,21,5,'&lt;J&gt;„Prieteni”&lt;/J&gt;&lt;f&gt;[46]&lt;/f&gt;, le-a zis Isus&lt;f&gt;ⓖ&lt;/f&gt;, &lt;J&gt;„nu aveți ceva de mâncare?”&lt;/J&gt; Ei i-au răspuns: „Nu.”');
INSERT INTO &quot;verses&quot; VALUES (500,21,6,'El le-a zis&lt;f&gt;ⓗ&lt;/f&gt;: &lt;J&gt;„Aruncați mreaja în partea dreaptă a bărcii și veți găsi!”&lt;/J&gt; Au aruncat-o deci și n-o mai puteau trage de mulțimea peștilor.');
INSERT INTO &quot;verses&quot; VALUES (500,21,7,'Atunci, ucenicul&lt;f&gt;ⓘ&lt;/f&gt; pe care-l iubea Isus i-a zis lui Petru: „Este Domnul!” Când a auzit Simon Petru că este Domnul, și-a pus haina pe el și s-a încins, căci era fără haină, și s-a aruncat în mare.');
INSERT INTO &quot;verses&quot; VALUES (500,21,8,'Ceilalți ucenici au venit cu o barcă mică, pentru că nu erau departe de țărm, ci cam la două sute de coți, ca să tragă mreaja cu pești.');
INSERT INTO &quot;verses&quot; VALUES (500,21,9,'Când s-au coborât pe țărm, au văzut acolo un foc de cărbuni, pește pus deasupra și pâine.');
INSERT INTO &quot;verses&quot; VALUES (500,21,10,'Isus le-a zis: &lt;J&gt;„Aduceți din peștii pe care i-ați prins acum!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,11,'Simon Petru s-a suit și a tras la țărm mreaja, plină cu o sută cincizeci și trei de pești mari; și, măcar că erau atâția, nu s-a rupt mreaja.');
INSERT INTO &quot;verses&quot; VALUES (500,21,12,'&lt;J&gt;„Veniți&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;să mâncați!”&lt;/J&gt;, le-a zis Isus. Și niciunul din ucenici nu cuteza să-L întrebe: „Cine ești?”, căci știau că este Domnul.');
INSERT INTO &quot;verses&quot; VALUES (500,21,13,'Isus S-a apropiat, a luat pâinea și le-a dat-o; la fel și cu peștele.');
INSERT INTO &quot;verses&quot; VALUES (500,21,14,'Aceasta era a&lt;f&gt;ⓚ&lt;/f&gt; treia oară când Se arăta Isus ucenicilor Săi după ce a înviat din morți.');
INSERT INTO &quot;verses&quot; VALUES (500,21,15,'&lt;pb/&gt;După ce au prânzit, Isus i-a zis lui Simon Petru: &lt;J&gt;„Simone, fiul lui Ioan&lt;/J&gt;&lt;f&gt;[47]&lt;/f&gt;&lt;J&gt;, Mă iubești tu mai mult decât aceștia?”&lt;/J&gt; „Da, Doamne”, I-a răspuns Petru, „Tu știi că Te iubesc.” Isus i-a zis: &lt;J&gt;„Paște mielușeii Mei!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,16,'I-a zis a doua oară: &lt;J&gt;„Simone, fiul lui Ioan, Mă iubești?”&lt;/J&gt; „Da, Doamne”, I-a răspuns Petru, „Tu știi că Te iubesc.” Isus i-a zis&lt;f&gt;ⓛ&lt;/f&gt;: &lt;J&gt;„Păstorește oile Mele!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,17,'Isus i-a zis a treia oară: &lt;J&gt;„Simone, fiul lui Ioan, Mă iubești?”&lt;/J&gt; Petru s-a întristat că-i zisese a treia oară: &lt;J&gt;„Mă iubești?”&lt;/J&gt; Și I-a răspuns: „Doamne, Tu toate&lt;f&gt;ⓜ&lt;/f&gt; le știi; știi că Te iubesc.” Isus i-a zis: &lt;J&gt;„Paște oile Mele!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,18,'&lt;J&gt;Adevărat&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;, adevărat îți spun: atunci când erai mai tânăr, te încingeai singur și te duceai unde voiai; dar, când vei îmbătrâni, îți vei întinde mâinile și altul te va încinge și te va duce unde nu vei vrea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,19,'A zis lucrul acesta arătând cu&lt;f&gt;ⓞ&lt;/f&gt; ce fel de moarte va proslăvi Petru pe Dumnezeu. Și, după ce a vorbit astfel, i-a zis: &lt;J&gt;„Urmează-Mă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,20,'&lt;pb/&gt;Petru s-a întors și l-a văzut venind după ei pe ucenicul pe&lt;f&gt;ⓟ&lt;/f&gt; care-l iubea Isus (cel care se rezemase la cină pe pieptul lui Isus și zisese: „Doamne, cine este cel ce Te va vinde?”).');
INSERT INTO &quot;verses&quot; VALUES (500,21,21,'Când l-a văzut, Petru I-a zis lui Isus: „Doamne, dar cu acesta ce va fi?”');
INSERT INTO &quot;verses&quot; VALUES (500,21,22,'Isus i-a răspuns: &lt;J&gt;„Dacă vreau ca el să rămână până&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;voi veni, ce-ți pasă ție? Tu urmează-Mă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,23,'Astfel a ieșit cuvântul acesta printre frați: că ucenicul acela nu va muri. Însă Isus nu-i spusese lui Petru că ucenicul nu va muri, ci: &lt;J&gt;„Dacă vreau ca el să rămână până voi veni, ce-ți pasă ție?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (500,21,24,'&lt;pb/&gt;Acesta este ucenicul care dă mărturie despre aceste lucruri și care le-a scris. Și știm&lt;f&gt;ⓡ&lt;/f&gt; că mărturia lui este adevărată.');
INSERT INTO &quot;verses&quot; VALUES (500,21,25,'&lt;pb/&gt;Mai&lt;f&gt;ⓢ&lt;/f&gt; sunt multe altele pe care le-a făcut Isus, dar, dacă s-ar scrie cu de-amănuntul, cred&lt;f&gt;ⓣ&lt;/f&gt; că lumea toată n-ar putea cuprinde cărțile scrise. Amin!');
INSERT INTO &quot;verses&quot; VALUES (510,1,1,'&lt;pb/&gt;Teofile&lt;f&gt;ⓐ&lt;/f&gt;, în prima carte am istorisit tot ce a început Isus să facă și să-i învețe &lt;i&gt;pe oameni&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (510,1,2,'până&lt;f&gt;ⓑ&lt;/f&gt; în ziua în care a fost luat la cer, după ce le-a dat&lt;f&gt;ⓒ&lt;/f&gt; porunci apostolilor pe care-i alesese prin Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (510,1,3,'După patima Lui, li S-a înfățișat viu&lt;f&gt;ⓓ&lt;/f&gt; prin multe dovezi, li S-a arătat timp de patruzeci de zile și a vorbit cu ei cele privitoare la Împărăția lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,1,4,'Pe&lt;f&gt;ⓔ&lt;/f&gt; când mâncau ei împreună, le-a poruncit să nu se îndepărteze de Ierusalim, ci să aștepte acolo făgăduința Tatălui, &lt;J&gt;„pe care&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;”&lt;/J&gt;&lt;i&gt;, le-a zis El,&lt;/i&gt; &lt;J&gt;„ați auzit-o de la Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,1,5,'&lt;J&gt;Căci&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;Ioan a botezat în apă, dar voi, nu după multe zile, veți&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;fi botezați în Duhul Sfânt.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,1,6,'&lt;pb/&gt;Cei ce erau adunați L-au întrebat, zicând: „Doamne&lt;f&gt;ⓘ&lt;/f&gt;, în vremea aceasta ai de gând să restaurezi Împărăția lui Israel?”');
INSERT INTO &quot;verses&quot; VALUES (510,1,7,'El le-a răspuns: &lt;J&gt;„Nu&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;vă este &lt;i&gt;dat&lt;/i&gt; vouă să știți vremurile sau soroacele pe care Tatăl le-a hotărât prin autoritatea Sa.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,1,8,'&lt;J&gt;Ci voi veți&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;primi putere când Se va coborî&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;Duhul Sfânt peste voi și-Mi veți&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;fi martori în Ierusalim, în toată Iudeea, în Samaria și până la capătul pământului.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,1,9,'După&lt;f&gt;ⓝ&lt;/f&gt; ce a vorbit astfel, pe când se uitau la El, &lt;i&gt;Isus&lt;/i&gt; S-a înălțat &lt;i&gt;la cer&lt;/i&gt; și un nor L-a ascuns de privirile lor.');
INSERT INTO &quot;verses&quot; VALUES (510,1,10,'Și pe când stăteau ei cu privirile ațintite spre cer, iar El Se înălța, iată că li s-au înfățișat doi bărbați &lt;i&gt;îmbrăcați&lt;/i&gt; în&lt;f&gt;ⓞ&lt;/f&gt; haine albe.');
INSERT INTO &quot;verses&quot; VALUES (510,1,11,'Ei au zis: „Bărbați galileeni, de ce stați și vă uitați la cer? Acest Isus care S-a înălțat la cer dintre voi va&lt;f&gt;ⓟ&lt;/f&gt; veni în același fel cum L-ați văzut mergând la cer.”');
INSERT INTO &quot;verses&quot; VALUES (510,1,12,'&lt;pb/&gt;Atunci&lt;f&gt;ⓠ&lt;/f&gt;, s-au întors în Ierusalim de pe muntele numit al Măslinilor, care este lângă Ierusalim, departe cât un drum în zi de sabat&lt;f&gt;[1]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,1,13,'Au intrat &lt;i&gt;în casă&lt;/i&gt; și au urcat în&lt;f&gt;ⓡ&lt;/f&gt; odaia de sus, unde stăteau &lt;i&gt;de obicei&lt;/i&gt;. Erau &lt;i&gt;acolo&lt;/i&gt;: Petru, Iacov, Ioan, Andrei, Filip, Toma, Bartolomeu, Matei, Iacov, fiul lui Alfeu, Simon Zelotul și Iuda, fiul lui Iacov.');
INSERT INTO &quot;verses&quot; VALUES (510,1,14,'Toți aceștia&lt;f&gt;ⓢ&lt;/f&gt; stăruiau într-un cuget în rugăciune [și în cereri], împreună cu femeile&lt;f&gt;ⓣ&lt;/f&gt;, cu Maria, mama lui Isus, și cu frații&lt;f&gt;ⓤ&lt;/f&gt; Lui.');
INSERT INTO &quot;verses&quot; VALUES (510,1,15,'&lt;pb/&gt;În zilele acelea, Petru s-a ridicat în mijlocul fraților — numărul celor adunați fiind cam de o sută douăzeci — și a zis:');
INSERT INTO &quot;verses&quot; VALUES (510,1,16,'„Fraților, trebuia să se împlinească Scriptura&lt;f&gt;ⓥ&lt;/f&gt; spusă mai înainte de Duhul Sfânt, prin gura lui David, despre Iuda, care&lt;f&gt;ⓦ&lt;/f&gt; a fost călăuza celor ce L-au prins pe Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,1,17,'El, care era din numărul nostru, părtaș aceleiași slujbe,');
INSERT INTO &quot;verses&quot; VALUES (510,1,18,'a cumpărat&lt;f&gt;ⓧ&lt;/f&gt; un ogor cu plata&lt;f&gt;ⓨ&lt;/f&gt; nelegiuirii, a căzut cu capul înainte, a plesnit la mijloc și i s-au vărsat toate măruntaiele.');
INSERT INTO &quot;verses&quot; VALUES (510,1,19,'&lt;i&gt;Lucrul acesta&lt;/i&gt; a ajuns atât de cunoscut de toți locuitorii Ierusalimului, încât ogorul acela a fost numit în limba lor: &lt;e&gt;Hacheldamáh&lt;/e&gt;, adică: «Ogorul sângelui».');
INSERT INTO &quot;verses&quot; VALUES (510,1,20,'Căci este scris în cartea Psalmilor: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Pustie să-i rămână locuința&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și nimeni să nu locuiască în ea!&lt;/e&gt; &lt;f&gt;[2]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;și &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Slujba lui s-o ia altul!&lt;/e&gt; &lt;f&gt;[3]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,1,21,'&lt;pb/&gt;Trebuie deci ca unul dintre bărbații care ne-au însoțit în tot timpul cât a trăit Domnul Isus printre noi,');
INSERT INTO &quot;verses&quot; VALUES (510,1,22,'începând&lt;f&gt;ⓩ&lt;/f&gt; de la botezul lui Ioan până în ziua când S-a înălțat de la noi, să fie martor&lt;f&gt;ⓐ&lt;/f&gt; al învierii Lui împreună cu noi.”');
INSERT INTO &quot;verses&quot; VALUES (510,1,23,'Ei au pus înainte doi &lt;i&gt;bărbați&lt;/i&gt;: pe Iosif, numit Barsaba&lt;f&gt;ⓑ&lt;/f&gt;, zis și Iust, și pe Matia.');
INSERT INTO &quot;verses&quot; VALUES (510,1,24,'Apoi s-au rugat, zicând: „Doamne, Tu, care&lt;f&gt;ⓒ&lt;/f&gt; cunoști inimile tuturor, arată-ne pe care dintre aceștia doi l-ai ales');
INSERT INTO &quot;verses&quot; VALUES (510,1,25,'să ia parte la această slujire și apostolie din care Iuda a căzut, mergând unde îi &lt;i&gt;era&lt;/i&gt; locul.”');
INSERT INTO &quot;verses&quot; VALUES (510,1,26,'Au tras la sorți și sorțul a căzut pe Matia, care a fost adăugat &lt;i&gt;la numărul&lt;/i&gt; celor unsprezece apostoli.');
INSERT INTO &quot;verses&quot; VALUES (510,2,1,'&lt;pb/&gt;Când a sosit Ziua&lt;f&gt;ⓓ&lt;/f&gt; Cincizecimii, erau&lt;f&gt;ⓔ&lt;/f&gt; toți împreună în același loc.');
INSERT INTO &quot;verses&quot; VALUES (510,2,2,'Deodată, a venit din cer un sunet ca vuietul unui vânt puternic și a&lt;f&gt;ⓕ&lt;/f&gt; umplut toată casa unde erau ei.');
INSERT INTO &quot;verses&quot; VALUES (510,2,3,'Li s-au arătat niște limbi ca de foc, ce s-au împărțit și s-au așezat câte una pe fiecare dintre ei.');
INSERT INTO &quot;verses&quot; VALUES (510,2,4,'Și toți s-au&lt;f&gt;ⓖ&lt;/f&gt; umplut de Duhul Sfânt și au început să vorbească&lt;f&gt;ⓗ&lt;/f&gt; în alte limbi, după cum le dădea Duhul să vorbească.');
INSERT INTO &quot;verses&quot; VALUES (510,2,5,'&lt;pb/&gt;În Ierusalim se aflau &lt;i&gt;atunci&lt;/i&gt; iudei, oameni evlavioși din toate neamurile de sub cer.');
INSERT INTO &quot;verses&quot; VALUES (510,2,6,'Când s-a auzit sunetul acela, mulțimea s-a adunat și era nedumerită, pentru că fiecare îi auzea vorbind în limba lui.');
INSERT INTO &quot;verses&quot; VALUES (510,2,7,'Erau uimiți și se minunau, zicând: „Toți aceștia care vorbesc nu sunt galileeni&lt;f&gt;ⓘ&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (510,2,8,'Cum de îi auzim fiecare în limba în care ne-am născut?');
INSERT INTO &quot;verses&quot; VALUES (510,2,9,'Parți, mezi, elamiți, locuitori din Mesopotamia, Iudeea, Capadocia, Pont, Asia&lt;f&gt;[4]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (510,2,10,'Frigia, Pamfilia, Egipt, părțile Libiei dinspre Cirene, pelerini din Roma,');
INSERT INTO &quot;verses&quot; VALUES (510,2,11,'iudei sau prozeliți, cretani și arabi — îi auzim vorbind în limbile noastre despre lucrările mărețe ale lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (510,2,12,'Toți erau uimiți și, neștiind ce să creadă, își ziceau unii altora: „Ce înseamnă aceasta?”');
INSERT INTO &quot;verses&quot; VALUES (510,2,13,'Alții își băteau joc și spuneau: „Sunt plini de must!”');
INSERT INTO &quot;verses&quot; VALUES (510,2,14,'&lt;pb/&gt;Atunci, Petru s-a ridicat împreună cu cei unsprezece, și-a înălțat glasul și le-a zis: „Bărbați iudei și voi toți care locuiți în Ierusalim, să știți lucrul acesta și să luați aminte la cuvintele mele!');
INSERT INTO &quot;verses&quot; VALUES (510,2,15,'&lt;i&gt;Oamenii&lt;/i&gt; aceștia nu sunt beți, cum credeți voi, căci&lt;f&gt;ⓙ&lt;/f&gt; este abia ceasul al treilea din zi.');
INSERT INTO &quot;verses&quot; VALUES (510,2,16,'Ci aceasta este ce a fost spus prin profetul Ioel:');
INSERT INTO &quot;verses&quot; VALUES (510,2,17,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Așa va fi&lt;/e&gt; în&lt;f&gt;ⓚ&lt;/f&gt; zilele de pe urmă, zice Dumnezeu:&lt;/t&gt; &lt;t&gt;&lt;e&gt;voi&lt;/e&gt; &lt;f&gt;ⓛ&lt;/f&gt; &lt;e&gt;turna din Duhul Meu peste orice făptură;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;fiii voștri și fiicele&lt;/e&gt; &lt;f&gt;ⓜ&lt;/f&gt; &lt;e&gt;voastre vor profeți,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;tinerii voștri vor avea vedenii&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și bătrânii voștri vor visa vise!&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,18,'&lt;t&gt;&lt;e&gt;Da, chiar și peste robii Mei și peste roabele Mele&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;voi turna din Duhul Meu în zilele acelea&lt;/e&gt; și&lt;f&gt;ⓝ&lt;/f&gt; vor profeți.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,19,'&lt;t&gt;&lt;e&gt;Voi face minuni&lt;/e&gt; sus, &lt;e&gt;în cer,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și&lt;/e&gt; semne jos, &lt;e&gt;pe pământ:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;sânge, foc și nor de fum;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,20,'&lt;t&gt;&lt;e&gt;soarele&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;se va preschimba în întuneric&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și luna, în sânge,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;înainte să vină ziua Domnului cea mare și strălucită.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,21,'&lt;t&gt;&lt;e&gt;Atunci, oricine&lt;/e&gt; &lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;va chema Numele Domnului va fi mântuit.&lt;/e&gt;&lt;f&gt;[5]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,22,'&lt;pb/&gt;Bărbați israeliți, ascultați cuvintele acestea! Pe Isus din Nazaret, om adeverit de Dumnezeu înaintea voastră prin&lt;f&gt;ⓠ&lt;/f&gt; faptele puternice, minunile și semnele pe care le-a făcut Dumnezeu prin El în mijlocul vostru, după cum bine știți,');
INSERT INTO &quot;verses&quot; VALUES (510,2,23,'pe Acesta, predat&lt;f&gt;ⓡ&lt;/f&gt; potrivit&lt;f&gt;ⓢ&lt;/f&gt; planului și preștiinței lui Dumnezeu, voi L-ați răstignit și L-ați omorât prin mâna celor nelegiuiți;');
INSERT INTO &quot;verses&quot; VALUES (510,2,24,'pe El&lt;f&gt;ⓣ&lt;/f&gt; Dumnezeu L-a înviat, dezlegându-L de chinurile morții, pentru că nu era cu putință să fie ținut de ea.');
INSERT INTO &quot;verses&quot; VALUES (510,2,25,'Căci David zice despre El: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Eu Îl aveam totdeauna pe Domnul înaintea ochilor mei,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;căci El este la dreapta mea, ca să nu mă clatin.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,26,'&lt;t&gt;&lt;e&gt;De aceea, mi s-a bucurat inima și mi s-a veselit limba;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;chiar și trupul meu va sălășlui în nădejde,&lt;/e&gt; &lt;f&gt;[6]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,27,'&lt;t&gt;&lt;e&gt;căci nu vei lăsa sufletul meu în Locuința Morților&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și nu vei îngădui ca sfântul Tău să vadă putrezirea.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,28,'&lt;t&gt;&lt;e&gt;Mi-ai făcut cunoscute căile vieții&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și mă vei umple de bucurie înaintea feței Tale.&lt;/e&gt; &lt;f&gt;[7]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,29,'&lt;pb/&gt;Fraților, despre&lt;f&gt;ⓤ&lt;/f&gt; patriarhul David să-mi fie îngăduit a spune cu îndrăzneală că a murit și a fost îngropat, iar mormântul lui este în mijlocul nostru până în ziua de azi.');
INSERT INTO &quot;verses&quot; VALUES (510,2,30,'Profet fiind, el&lt;f&gt;ⓥ&lt;/f&gt; a știut că Dumnezeu &lt;e&gt;S-a jurat să așeze pe tronul lui pe unul care să fie rodul coapselor lui&lt;/e&gt;&lt;f&gt;[8]&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (510,2,31,'despre învierea lui Hristos a profețit și a vorbit el, când a zis că&lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;n-a fost lăsat în Locuința Morților, iar trupul Lui n-a văzut putrezirea&lt;/e&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,2,32,'Dumnezeu L-a înviat pe acest Isus, iar noi&lt;f&gt;ⓧ&lt;/f&gt; toți suntem martori ai Lui.');
INSERT INTO &quot;verses&quot; VALUES (510,2,33,'După ce S-a înălțat&lt;f&gt;ⓨ&lt;/f&gt; la dreapta lui Dumnezeu și a primit&lt;f&gt;ⓩ&lt;/f&gt; de la Tatăl făgăduința Duhului Sfânt&lt;f&gt;ⓐ&lt;/f&gt;, a turnat ce vedeți și auziți.');
INSERT INTO &quot;verses&quot; VALUES (510,2,34,'Căci David nu s-a suit în ceruri, ci el zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Domnul a zis&lt;/e&gt; &lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;Domnului meu:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;«Șezi la dreapta Mea&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,35,'&lt;t&gt;&lt;e&gt;până-i voi pune pe vrăjmașii Tăi reazem picioarelor Tale!»&lt;/e&gt; &lt;f&gt;[9]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,2,36,'&lt;pb/&gt;De aceea, să știe negreșit toată casa lui Israel că Dumnezeu L-a&lt;f&gt;ⓒ&lt;/f&gt; făcut Domn și Hristos pe acest Isus pe care voi L-ați răstignit.”');
INSERT INTO &quot;verses&quot; VALUES (510,2,37,'&lt;pb/&gt;Auzind ei aceste &lt;i&gt;cuvinte&lt;/i&gt;, au&lt;f&gt;ⓓ&lt;/f&gt; fost străpunși în inimă și le-au zis lui Petru și celorlalți apostoli: „Fraților, ce să facem?”');
INSERT INTO &quot;verses&quot; VALUES (510,2,38,'Petru le-a răspuns: „Pocăiți-vă&lt;f&gt;ⓔ&lt;/f&gt; și fiecare dintre voi să fie botezat în Numele lui Isus Hristos, spre iertarea păcatelor voastre, și veți primi darul Sfântului Duh!');
INSERT INTO &quot;verses&quot; VALUES (510,2,39,'Căci făgăduința este pentru voi, pentru&lt;f&gt;ⓕ&lt;/f&gt; copiii voștri și pentru&lt;f&gt;ⓖ&lt;/f&gt; toți cei de departe, oricâți va chema Domnul, Dumnezeul nostru.”');
INSERT INTO &quot;verses&quot; VALUES (510,2,40,'Și cu multe alte cuvinte îi sfătuia și-i îndemna, zicând: „Mântuiți-vă din &lt;i&gt;mijlocul&lt;/i&gt; acestei generații strâmbe!”');
INSERT INTO &quot;verses&quot; VALUES (510,2,41,'Cei ce au primit cuvântul lui au fost botezați, și în ziua aceea s-au adăugat cam trei mii de suflete.');
INSERT INTO &quot;verses&quot; VALUES (510,2,42,'&lt;pb/&gt;Ei&lt;f&gt;ⓗ&lt;/f&gt; stăruiau în învățătura apostolilor, în părtășie, în frângerea pâinii și în rugăciuni.');
INSERT INTO &quot;verses&quot; VALUES (510,2,43,'Fiecare suflet era plin de teamă, iar prin apostoli se făceau multe&lt;f&gt;ⓘ&lt;/f&gt; minuni și semne.');
INSERT INTO &quot;verses&quot; VALUES (510,2,44,'Toți cei ce credeau erau împreună la un loc și aveau&lt;f&gt;ⓙ&lt;/f&gt; totul în comun.');
INSERT INTO &quot;verses&quot; VALUES (510,2,45,'Își vindeau proprietățile și averile și le împărțeau tuturor, după nevoia fiecăruia.');
INSERT INTO &quot;verses&quot; VALUES (510,2,46,'În&lt;f&gt;ⓚ&lt;/f&gt; fiecare zi mergeau&lt;f&gt;ⓛ&lt;/f&gt; toți într-un cuget la Templu, frângeau&lt;f&gt;ⓜ&lt;/f&gt; pâinea în case și luau hrana cu bucurie și curăție de inimă.');
INSERT INTO &quot;verses&quot; VALUES (510,2,47,'Îl lăudau pe Dumnezeu și erau plăcuți&lt;f&gt;ⓝ&lt;/f&gt; înaintea întregului popor. Și Domnul&lt;f&gt;ⓞ&lt;/f&gt; adăuga în fiecare zi la numărul lor pe cei ce erau mântuiți.');
INSERT INTO &quot;verses&quot; VALUES (510,3,1,'&lt;pb/&gt;Petru și Ioan urcau spre&lt;f&gt;ⓟ&lt;/f&gt; Templu la ceasul al nouălea, care era &lt;i&gt;ceasul&lt;/i&gt; rugăciunii&lt;f&gt;ⓠ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,3,2,'&lt;i&gt;Acolo&lt;/i&gt; era&lt;f&gt;ⓡ&lt;/f&gt; un om olog din naștere, pe care-l purtau și îl puneau în fiecare zi la poarta Templului numită Frumoasă, ca să&lt;f&gt;ⓢ&lt;/f&gt; le ceară milostenie celor ce intrau în Templu.');
INSERT INTO &quot;verses&quot; VALUES (510,3,3,'Văzându-i pe Petru și pe Ioan că urmau să intre în Templu, le-a cerut milostenie.');
INSERT INTO &quot;verses&quot; VALUES (510,3,4,'Petru, ca și Ioan, l-a privit țintă și i-a zis: „Uită-te la noi!”');
INSERT INTO &quot;verses&quot; VALUES (510,3,5,'Și el îi privea cu atenție, așteptând să primească ceva de la ei.');
INSERT INTO &quot;verses&quot; VALUES (510,3,6,'Petru i-a zis: „Nu am nici argint, nici aur, dar ce am, aceea îți dau: în&lt;f&gt;ⓣ&lt;/f&gt; Numele lui Isus Hristos din Nazaret, ridică-te și umblă!”');
INSERT INTO &quot;verses&quot; VALUES (510,3,7,'Și l-a prins de mâna dreaptă și l-a ridicat. Îndată i s-au întărit tălpile și gleznele,');
INSERT INTO &quot;verses&quot; VALUES (510,3,8,'a sărit&lt;f&gt;ⓤ&lt;/f&gt; în picioare și a început să umble, apoi a intrat cu ei în Templu, umblând, sărind și lăudându-L pe Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,3,9,'Tot poporul l-a&lt;f&gt;ⓥ&lt;/f&gt; văzut umblând și lăudându-L pe Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,3,10,'Îl cunoșteau că era cel ce stătea&lt;f&gt;ⓦ&lt;/f&gt; la Poarta Frumoasă a Templului ca &lt;i&gt;să ceară&lt;/i&gt; milostenie, și au fost cuprinși de înfiorare și de uimire pentru ce i se întâmplase.');
INSERT INTO &quot;verses&quot; VALUES (510,3,11,'&lt;pb/&gt;Fiindcă el se ținea de Petru și de Ioan, tot poporul, uimit, a alergat către ei în porticul numit&lt;f&gt;ⓧ&lt;/f&gt; al lui Solomon.');
INSERT INTO &quot;verses&quot; VALUES (510,3,12,'Când a văzut &lt;i&gt;aceasta&lt;/i&gt;, Petru a vorbit poporului &lt;i&gt;astfel&lt;/i&gt;: „Bărbați israeliți, de ce vă mirați de lucrul acesta și de ce ne priviți stăruitor, ca și cum prin puterea sau evlavia noastră l-am făcut să umble?');
INSERT INTO &quot;verses&quot; VALUES (510,3,13,'&lt;e&gt;Dumnezeul&lt;/e&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;lui Avraam, Dumnezeul lui Isaac și Dumnezeul lui Iacov, Dumnezeul părinților noștri&lt;/e&gt;&lt;f&gt;[10]&lt;/f&gt; L-a proslăvit&lt;f&gt;ⓩ&lt;/f&gt; pe Robul&lt;f&gt;ⓐ&lt;/f&gt; Său Isus, pe care voi L-ați predat&lt;f&gt;ⓑ&lt;/f&gt; și L-ați tăgăduit înaintea lui Pilat, măcar că acela avea de gând să-I dea drumul.');
INSERT INTO &quot;verses&quot; VALUES (510,3,14,'Voi L-ați&lt;f&gt;ⓒ&lt;/f&gt; tăgăduit pe Cel&lt;f&gt;ⓓ&lt;/f&gt; Sfânt și Drept&lt;f&gt;ⓔ&lt;/f&gt; și ați cerut să vi se dăruiască un ucigaș.');
INSERT INTO &quot;verses&quot; VALUES (510,3,15,'L-ați omorât pe Întemeietorul&lt;f&gt;[11]&lt;/f&gt; vieții, pe&lt;f&gt;ⓕ&lt;/f&gt; care Dumnezeu L-a înviat din morți; noi&lt;f&gt;ⓖ&lt;/f&gt; suntem martori ai Lui.');
INSERT INTO &quot;verses&quot; VALUES (510,3,16,'Prin credința în&lt;f&gt;ⓗ&lt;/f&gt; Numele lui &lt;i&gt;Isus&lt;/i&gt;, Numele Său l-a întărit pe acesta pe care-l vedeți și-l cunoașteți; credința care &lt;i&gt;vine&lt;/i&gt; prin El i-a dat &lt;i&gt;omului&lt;/i&gt; acestuia o sănătate deplină în fața voastră, a tuturor.');
INSERT INTO &quot;verses&quot; VALUES (510,3,17,'&lt;pb/&gt;Acum, fraților, știu că voi, la fel ca mai-marii voștri, din&lt;f&gt;ⓘ&lt;/f&gt; neștiință ați făcut așa.');
INSERT INTO &quot;verses&quot; VALUES (510,3,18,'Dar Dumnezeu a împlinit astfel ce vestise&lt;f&gt;ⓙ&lt;/f&gt; mai dinainte prin&lt;f&gt;ⓚ&lt;/f&gt; gura tuturor profeților — că Hristosul Său va pătimi.');
INSERT INTO &quot;verses&quot; VALUES (510,3,19,'Pocăiți-vă, așadar,&lt;f&gt;ⓛ&lt;/f&gt; și întoarceți-vă, ca să vi se șteargă păcatele,');
INSERT INTO &quot;verses&quot; VALUES (510,3,20,'așa încât să vină de la Domnul vremuri de înviorare și să-L trimită pe Cel dinainte rânduit pentru voi: pe Isus Hristos,');
INSERT INTO &quot;verses&quot; VALUES (510,3,21,'pe&lt;f&gt;ⓜ&lt;/f&gt; care cerul trebuie să-L primească până la vremea restaurării&lt;f&gt;ⓝ&lt;/f&gt; tuturor &lt;i&gt;lucrurilor&lt;/i&gt;, despre care a&lt;f&gt;ⓞ&lt;/f&gt; vorbit Dumnezeu prin gura sfinților Săi profeți din vremuri străvechi!');
INSERT INTO &quot;verses&quot; VALUES (510,3,22,'Moise le-a zis [părinților noștri]: &lt;e&gt;Domnul, Dumnezeul vostru, vă va ridica dintre frații voștri un profet&lt;/e&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;ca mine; pe el să-l ascultați în tot ce vă va spune!&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,3,23,'&lt;e&gt;Și oricine nu-l va asculta pe acel profet să fie nimicit din mijlocul poporului.&lt;/e&gt;&lt;f&gt;[12]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,3,24,'De asemenea, toți profeții, de la Samuel la cei de după el, au vorbit și au vestit aceste zile.');
INSERT INTO &quot;verses&quot; VALUES (510,3,25,'Voi&lt;f&gt;ⓠ&lt;/f&gt; sunteți fiii profeților și ai legământului pe care l-a făcut Dumnezeu cu părinții noștri, când i-a zis lui Avraam: &lt;e&gt;Prin seminția&lt;/e&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;e&gt;ta vor fi binecuvântate toate neamurile pământului.&lt;/e&gt;&lt;f&gt;[13]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,3,26,'După ce L-a ridicat&lt;f&gt;[14]&lt;/f&gt; pe Robul Său [Isus], Dumnezeu L-a trimis&lt;f&gt;ⓢ&lt;/f&gt; mai întâi la voi&lt;f&gt;ⓣ&lt;/f&gt;, ca să vă binecuvânteze, dacă vă veți întoarce&lt;f&gt;ⓤ&lt;/f&gt; fiecare de la răutățile voastre.”');
INSERT INTO &quot;verses&quot; VALUES (510,4,1,'&lt;pb/&gt;Pe când vorbeau ei poporului, au venit peste ei preoții, căpitanul &lt;i&gt;gărzii&lt;/i&gt; Templului și saducheii,');
INSERT INTO &quot;verses&quot; VALUES (510,4,2,'furioși&lt;f&gt;ⓥ&lt;/f&gt; că învățau poporul și vesteau învierea din morți în Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,4,3,'Au pus mâna pe ei și i-au aruncat în temniță până a doua zi, căci se înserase.');
INSERT INTO &quot;verses&quot; VALUES (510,4,4,'Însă mulți dintre cei ce auziseră cuvântul au crezut, iar numărul bărbaților &lt;i&gt;credincioși&lt;/i&gt; s-a ridicat la aproape cinci mii.');
INSERT INTO &quot;verses&quot; VALUES (510,4,5,'&lt;pb/&gt;A doua zi, mai-marii &lt;i&gt;poporului&lt;/i&gt;, bătrânii și cărturarii s-au adunat la Ierusalim');
INSERT INTO &quot;verses&quot; VALUES (510,4,6,'împreună cu marele-preot Ana&lt;f&gt;ⓦ&lt;/f&gt;, cu Caiafa, Ioan, Alexandru și toți cei din neamul marilor-preoți.');
INSERT INTO &quot;verses&quot; VALUES (510,4,7,'I-au pus &lt;i&gt;pe Petru și Ioan&lt;/i&gt; în mijlocul lor și i-au întrebat: „Cu&lt;f&gt;ⓧ&lt;/f&gt; ce putere sau în numele cui ați făcut voi &lt;i&gt;lucrul&lt;/i&gt; acesta?”');
INSERT INTO &quot;verses&quot; VALUES (510,4,8,'Atunci&lt;f&gt;ⓨ&lt;/f&gt; Petru, plin de Duhul Sfânt, le-a zis: „Mai-mari ai poporului și bătrâni [ai lui Israel]!');
INSERT INTO &quot;verses&quot; VALUES (510,4,9,'Fiindcă suntem cercetați astăzi pentru o binefacere făcută unui om bolnav (prin cine a fost el vindecat),');
INSERT INTO &quot;verses&quot; VALUES (510,4,10,'să știți voi toți și întreg poporul lui Israel: &lt;i&gt;omul&lt;/i&gt; acesta se înfățișează înaintea voastră sănătos în&lt;f&gt;ⓩ&lt;/f&gt; Numele lui Isus Hristos din Nazaret, pe&lt;f&gt;ⓐ&lt;/f&gt; care voi L-ați răstignit, dar pe care Dumnezeu L-a înviat din morți.');
INSERT INTO &quot;verses&quot; VALUES (510,4,11,'El&lt;f&gt;ⓑ&lt;/f&gt; este piatra lepădată de voi, zidarii, care a ajuns în capul unghiului.');
INSERT INTO &quot;verses&quot; VALUES (510,4,12,'În nimeni altul nu este mântuire, căci nu&lt;f&gt;ⓒ&lt;/f&gt; este sub cer niciun alt nume dat oamenilor în care trebuie să fim mântuiți.”');
INSERT INTO &quot;verses&quot; VALUES (510,4,13,'&lt;pb/&gt;Văzând ei îndrăzneala lui Petru și a lui Ioan și dându-și seama&lt;f&gt;ⓓ&lt;/f&gt; că erau oameni neînvățați și de rând, s-au mirat și au înțeles că fuseseră cu Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,4,14,'Dar, fiindcă-l vedeau stând în picioare lângă&lt;f&gt;ⓔ&lt;/f&gt; ei pe omul care fusese vindecat, n-au putut zice nimic împotrivă.');
INSERT INTO &quot;verses&quot; VALUES (510,4,15,'Le-au poruncit să iasă din Sinedriu și s-au sfătuit între ei,');
INSERT INTO &quot;verses&quot; VALUES (510,4,16,'zicând: „Ce&lt;f&gt;ⓕ&lt;/f&gt; să le facem acestor oameni? Căci toți locuitorii Ierusalimului știu&lt;f&gt;ⓖ&lt;/f&gt; că prin ei s-a făcut o minune vădită și n-o putem tăgădui.');
INSERT INTO &quot;verses&quot; VALUES (510,4,17,'Dar, ca să nu se răspândească și mai mult vestea în popor, să-i amenințăm să nu mai vorbească nimănui în numele acesta!”');
INSERT INTO &quot;verses&quot; VALUES (510,4,18,'După ce i-au chemat&lt;f&gt;ⓗ&lt;/f&gt;, le-au poruncit să nu mai vorbească și să nu mai învețe cu niciun chip în Numele lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,4,19,'Drept răspuns, Petru și Ioan le-au zis: „Judecați voi dacă&lt;f&gt;ⓘ&lt;/f&gt; este drept înaintea lui Dumnezeu să ascultăm mai mult de voi decât de Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (510,4,20,'căci&lt;f&gt;ⓙ&lt;/f&gt; nu putem să nu vorbim despre ce am văzut și am auzit!”');
INSERT INTO &quot;verses&quot; VALUES (510,4,21,'I-au amenințat din nou și i-au lăsat să plece, neștiind cum să-i pedepsească din cauza&lt;f&gt;ⓚ&lt;/f&gt; poporului, fiindcă&lt;f&gt;ⓛ&lt;/f&gt; toți Îl slăveau pe Dumnezeu pentru cele petrecute.');
INSERT INTO &quot;verses&quot; VALUES (510,4,22,'Căci omul cu care se făcuse această minune de vindecare avea peste patruzeci de ani.');
INSERT INTO &quot;verses&quot; VALUES (510,4,23,'&lt;pb/&gt;După&lt;f&gt;ⓜ&lt;/f&gt; ce li s-a dat drumul, ei au venit la ai lor și le-au istorisit ce le spuseseră preoții de seamă și bătrânii.');
INSERT INTO &quot;verses&quot; VALUES (510,4,24,'Când au auzit ei &lt;i&gt;aceste lucruri,&lt;/i&gt; și-au ridicat într-un cuget glasul către Dumnezeu și au zis: „Stăpâne [Doamne], Tu, care&lt;f&gt;ⓝ&lt;/f&gt; ai făcut cerul, pământul, marea și tot ce este în ele,');
INSERT INTO &quot;verses&quot; VALUES (510,4,25,'Tu ai zis prin Duhul Sfânt, prin gura părintelui nostru David, robul Tău: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Pentru ce s-au întărâtat neamurile&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și popoarele au făcut planuri deșarte?&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,4,26,'&lt;t&gt;&lt;e&gt;Împărații pământului s-au ridicat,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;domnitorii s-au adunat laolaltă&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;împotriva Domnului și împotriva Unsului Său.&lt;/e&gt; &lt;f&gt;[15]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,4,27,'&lt;pb/&gt;Într-adevăr&lt;f&gt;ⓞ&lt;/f&gt;, împotriva Robului Tău celui sfânt&lt;f&gt;ⓟ&lt;/f&gt;, Isus, pe&lt;f&gt;ⓠ&lt;/f&gt; care L-ai uns, s-au adunat în cetatea aceasta Irod și Pilat din Pont, împreună cu neamurile și mulțimile lui Israel,');
INSERT INTO &quot;verses&quot; VALUES (510,4,28,'ca&lt;f&gt;ⓡ&lt;/f&gt; să facă ceea ce mâna Ta și sfatul Tău hotărâseră mai înainte.');
INSERT INTO &quot;verses&quot; VALUES (510,4,29,'Și acum, Doamne, uită-Te la amenințările lor, dă-le &lt;i&gt;putere&lt;/i&gt; robilor Tăi să&lt;f&gt;ⓢ&lt;/f&gt; poată vesti Cuvântul Tău cu toată îndrăzneala');
INSERT INTO &quot;verses&quot; VALUES (510,4,30,'și întinde-Ți mâna, ca&lt;f&gt;ⓣ&lt;/f&gt; să se facă vindecări, semne și minuni prin&lt;f&gt;ⓤ&lt;/f&gt; Numele Robului&lt;f&gt;ⓥ&lt;/f&gt; Tău celui sfânt, Isus!”');
INSERT INTO &quot;verses&quot; VALUES (510,4,31,'După ce s-au rugat ei, s-a&lt;f&gt;ⓦ&lt;/f&gt; cutremurat locul unde erau adunați; toți s-au umplut de Duhul Sfânt și&lt;f&gt;ⓧ&lt;/f&gt; vesteau Cuvântul lui Dumnezeu cu îndrăzneală.');
INSERT INTO &quot;verses&quot; VALUES (510,4,32,'&lt;pb/&gt;Mulțimea credincioșilor era&lt;f&gt;ⓨ&lt;/f&gt; o inimă și un suflet și niciunul&lt;f&gt;ⓩ&lt;/f&gt; nu zicea că averea proprie este a lui, ci pe toate le aveau în comun.');
INSERT INTO &quot;verses&quot; VALUES (510,4,33,'Apostolii mărturiseau cu multă&lt;f&gt;ⓐ&lt;/f&gt; putere despre&lt;f&gt;ⓑ&lt;/f&gt; învierea Domnului Isus și un mare&lt;f&gt;ⓒ&lt;/f&gt; har era peste toți.');
INSERT INTO &quot;verses&quot; VALUES (510,4,34,'Nu era niciun nevoiaș printre ei, fiindcă cei&lt;f&gt;ⓓ&lt;/f&gt; care aveau ogoare sau case le vindeau, aduceau prețul celor vândute');
INSERT INTO &quot;verses&quot; VALUES (510,4,35,'și-l&lt;f&gt;ⓔ&lt;/f&gt; puneau la picioarele apostolilor, apoi&lt;f&gt;ⓕ&lt;/f&gt; se împărțea fiecăruia după cum avea nevoie.');
INSERT INTO &quot;verses&quot; VALUES (510,4,36,'&lt;pb/&gt;Iosif, numit de apostoli și Barnaba, care înseamnă „fiul mângâierii”, un levit de neam din Cipru,');
INSERT INTO &quot;verses&quot; VALUES (510,4,37,'a vândut un ogor pe care-l avea&lt;f&gt;ⓖ&lt;/f&gt;, a adus banii și i-a pus la picioarele apostolilor.');
INSERT INTO &quot;verses&quot; VALUES (510,5,1,'&lt;pb/&gt;Dar un om numit Anania, împreună cu Safira, soția lui, a vândut un ogor');
INSERT INTO &quot;verses&quot; VALUES (510,5,2,'și&lt;f&gt;ⓗ&lt;/f&gt; a păstrat &lt;i&gt;o parte&lt;/i&gt; din preț cu știrea soției lui. Apoi a adus &lt;i&gt;cealaltă&lt;/i&gt; parte și a pus-o la picioarele apostolilor.');
INSERT INTO &quot;verses&quot; VALUES (510,5,3,'Petru&lt;f&gt;ⓘ&lt;/f&gt; i-a zis: „Anania, de ce ți-a&lt;f&gt;ⓙ&lt;/f&gt; umplut Satana inima ca să-L minți pe Duhul Sfânt și să păstrezi o parte din prețul ogorului?');
INSERT INTO &quot;verses&quot; VALUES (510,5,4,'Dacă n-o vindeai, nu rămânea a ta? Și după ce ai vândut-o, nu rămâneau &lt;i&gt;banii&lt;/i&gt; în stăpânirea ta? Cum de ți-ai pus în gând să faci acest lucru? Nu pe oameni i-ai mințit, ci pe Dumnezeu.”');
INSERT INTO &quot;verses&quot; VALUES (510,5,5,'Când a auzit aceste cuvinte, Anania a căzut&lt;f&gt;ⓚ&lt;/f&gt; și și-a dat sufletul. O mare frică i-a cuprins pe toți cei ce ascultau &lt;i&gt;aceste lucruri&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,5,6,'Tinerii s-au ridicat, l-au&lt;f&gt;ⓛ&lt;/f&gt; acoperit, l-au dus afară și l-au îngropat.');
INSERT INTO &quot;verses&quot; VALUES (510,5,7,'&lt;pb/&gt;Cam după trei ceasuri a intrat și soția lui, fără să știe ce se întâmplase.');
INSERT INTO &quot;verses&quot; VALUES (510,5,8,'Petru i-a zis: „Spune-mi, cu atât ați vândut ogorul?” Ea a răspuns: „Da, cu atât.”');
INSERT INTO &quot;verses&quot; VALUES (510,5,9,'Atunci, Petru i-a zis: „Cum de v-ați învoit să-L&lt;f&gt;ⓜ&lt;/f&gt; ispitiți pe Duhul Domnului? Iată, picioarele celor ce l-au îngropat pe bărbatul tău sunt la ușă și te vor duce și pe tine!”');
INSERT INTO &quot;verses&quot; VALUES (510,5,10,'Ea&lt;f&gt;ⓝ&lt;/f&gt; a căzut dintr-odată la picioarele lui și și-a dat sufletul. Când au intrat tinerii, au găsit-o moartă; au scos-o afară și au îngropat-o lângă bărbatul ei.');
INSERT INTO &quot;verses&quot; VALUES (510,5,11,'O&lt;f&gt;ⓞ&lt;/f&gt; mare frică a cuprins toată biserica și pe toți cei ce au auzit aceste &lt;i&gt;lucruri&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,5,12,'&lt;pb/&gt;Prin&lt;f&gt;ⓟ&lt;/f&gt; mâinile apostolilor se făceau multe semne și minuni în popor. Fiind toți&lt;f&gt;ⓠ&lt;/f&gt; într-un cuget, &lt;i&gt;se adunau&lt;/i&gt; în porticul lui Solomon');
INSERT INTO &quot;verses&quot; VALUES (510,5,13,'și&lt;f&gt;ⓡ&lt;/f&gt; niciunul dintre ceilalți nu cuteza să li se alăture; dar&lt;f&gt;ⓢ&lt;/f&gt; poporul îi lăuda mult.');
INSERT INTO &quot;verses&quot; VALUES (510,5,14,'Mulțimea celor ce credeau în Domnul, bărbați și femei, se mărea tot mai mult,');
INSERT INTO &quot;verses&quot; VALUES (510,5,15,'încât îi scoteau pe bolnavi pe ulițe și îi puneau pe paturi și pe rogojini, pentru ca, atunci când&lt;f&gt;ⓣ&lt;/f&gt; avea să treacă Petru, măcar umbra lui să se oprească peste vreunul dintre ei.');
INSERT INTO &quot;verses&quot; VALUES (510,5,16,'Se adunau la Ierusalim și mulțimi din cetățile vecine, aducând bolnavi și &lt;i&gt;oameni&lt;/i&gt; chinuiți&lt;f&gt;ⓤ&lt;/f&gt; de duhuri necurate, și toți se vindecau.');
INSERT INTO &quot;verses&quot; VALUES (510,5,17,'&lt;pb/&gt;Atunci&lt;f&gt;ⓥ&lt;/f&gt;, marele-preot și toți cei ce erau împreună cu el, adică gruparea saducheilor, s-au ridicat plini de invidie,');
INSERT INTO &quot;verses&quot; VALUES (510,5,18,'au&lt;f&gt;ⓦ&lt;/f&gt; pus mâna pe apostoli și i-au aruncat în temniță.');
INSERT INTO &quot;verses&quot; VALUES (510,5,19,'Dar în timpul nopții, un înger&lt;f&gt;ⓧ&lt;/f&gt; al Domnului a deschis ușile temniței, i-a scos afară și le-a zis:');
INSERT INTO &quot;verses&quot; VALUES (510,5,20,'„Mergeți, stați în Templu și vestiți poporului toate&lt;f&gt;ⓨ&lt;/f&gt; cuvintele acestei Vieți!”');
INSERT INTO &quot;verses&quot; VALUES (510,5,21,'Când au auzit &lt;i&gt;ei&lt;/i&gt;, au intrat dis-de-dimineață în Templu și au început să dea învățătură. Marele-preot&lt;f&gt;ⓩ&lt;/f&gt; și cei ce erau cu el au venit, au adunat Sinedriul, adică sfatul bătrânilor fiilor lui Israel, și au trimis la temniță să-i aducă &lt;i&gt;pe apostoli&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,5,22,'Când au sosit gărzile, nu i-au găsit în temniță. S-au întors și au dat de veste,');
INSERT INTO &quot;verses&quot; VALUES (510,5,23,'zicând: „Temnița am găsit-o bine încuiată și pe paznici stând în picioare la uși, dar când am deschis, n-am găsit pe nimeni înăuntru!”');
INSERT INTO &quot;verses&quot; VALUES (510,5,24,'Auzind aceste cuvinte, căpitanul&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;gărzii&lt;/i&gt; Templului și preoții de seamă au rămas uimiți și se întrebau cum s-a putut întâmpla așa ceva.');
INSERT INTO &quot;verses&quot; VALUES (510,5,25,'Cineva a venit și le-a dat de veste: „Iată, oamenii pe care i-ați aruncat în temniță stau în Templu și dau învățătură poporului!”');
INSERT INTO &quot;verses&quot; VALUES (510,5,26,'Atunci, căpitanul &lt;i&gt;gărzii&lt;/i&gt; Templului a mers cu străjerii ca să-i aducă, dar nu cu forța, căci&lt;f&gt;ⓑ&lt;/f&gt; se temeau să nu fie uciși cu pietre de popor.');
INSERT INTO &quot;verses&quot; VALUES (510,5,27,'&lt;pb/&gt;După ce i-au adus, i-au pus înaintea Sinedriului, iar marele-preot i-a întrebat,');
INSERT INTO &quot;verses&quot; VALUES (510,5,28,'zicând: „Nu&lt;f&gt;ⓒ&lt;/f&gt; v-am poruncit noi cu tărie să nu dați învățătură în numele acesta? Și iată că voi ați umplut Ierusalimul cu învățătura voastră și vreți&lt;f&gt;ⓓ&lt;/f&gt; să aruncați asupra noastră sângele&lt;f&gt;ⓔ&lt;/f&gt; acestui om!”');
INSERT INTO &quot;verses&quot; VALUES (510,5,29,'Drept răspuns, Petru și apostolii au zis: „Trebuie&lt;f&gt;ⓕ&lt;/f&gt; să ascultăm mai degrabă de Dumnezeu decât de oameni!');
INSERT INTO &quot;verses&quot; VALUES (510,5,30,'Dumnezeul&lt;f&gt;ⓖ&lt;/f&gt; părinților noștri L-a înviat pe Isus, pe care voi L-ați omorât atârnându-L&lt;f&gt;ⓗ&lt;/f&gt; pe lemn.');
INSERT INTO &quot;verses&quot; VALUES (510,5,31,'Pe El&lt;f&gt;ⓘ&lt;/f&gt; Dumnezeu L-a înălțat la dreapta Lui ca Întemeietor și&lt;f&gt;ⓙ&lt;/f&gt; Mântuitor&lt;f&gt;ⓚ&lt;/f&gt;, ca să-i&lt;f&gt;ⓛ&lt;/f&gt; dea lui Israel pocăința și iertarea păcatelor.');
INSERT INTO &quot;verses&quot; VALUES (510,5,32,'Noi&lt;f&gt;ⓜ&lt;/f&gt; suntem martori ai acestor lucruri, ca și Duhul Sfânt, pe&lt;f&gt;ⓝ&lt;/f&gt; care Dumnezeu L-a dat celor ce I se supun.”');
INSERT INTO &quot;verses&quot; VALUES (510,5,33,'&lt;pb/&gt;Când&lt;f&gt;ⓞ&lt;/f&gt; au auzit &lt;i&gt;aceasta&lt;/i&gt;, ei s-au mâniat cumplit și voiau să-i omoare.');
INSERT INTO &quot;verses&quot; VALUES (510,5,34,'Dar un fariseu numit Gamaliel&lt;f&gt;ⓟ&lt;/f&gt;, un învățător al Legii prețuit de tot poporul, s-a ridicat în Sinedriu și a poruncit să-i scoată pe oameni afară pentru câtva timp.');
INSERT INTO &quot;verses&quot; VALUES (510,5,35,'Apoi a zis: „Bărbați israeliți, luați seama la ce aveți de gând să faceți cu oamenii aceștia!');
INSERT INTO &quot;verses&quot; VALUES (510,5,36,'Căci nu demult s-a arătat Teuda, care zicea că este cineva și căruia i s-au alăturat cam patru sute de bărbați. El a fost omorât și toți cei ce se încredeau în el au fost risipiți și s-a ales praful de ei.');
INSERT INTO &quot;verses&quot; VALUES (510,5,37,'După el s-a ridicat Iuda Galileeanul, pe vremea recensământului, și a atras &lt;i&gt;mult&lt;/i&gt; popor de partea lui. A pierit și el, iar toți cei ce se încredeau în el au fost risipiți.');
INSERT INTO &quot;verses&quot; VALUES (510,5,38,'De aceea, vă zic: depărtați-vă de oamenii aceștia și lăsați-i! Dacă&lt;f&gt;ⓠ&lt;/f&gt; planul sau lucrul aceasta este de la oameni, va fi nimicit;');
INSERT INTO &quot;verses&quot; VALUES (510,5,39,'dar&lt;f&gt;ⓡ&lt;/f&gt; dacă este de la Dumnezeu, nu-l veți putea nimici. Nu cumva să vă treziți luptând&lt;f&gt;ⓢ&lt;/f&gt; împotriva lui Dumnezeu!” Ei l-au ascultat.');
INSERT INTO &quot;verses&quot; VALUES (510,5,40,'Și după ce i-au chemat&lt;f&gt;ⓣ&lt;/f&gt; pe apostoli, i-au bătut&lt;f&gt;ⓤ&lt;/f&gt;, le-au poruncit să nu mai vorbească în Numele lui Isus și le-au dat drumul.');
INSERT INTO &quot;verses&quot; VALUES (510,5,41,'&lt;i&gt;Apostolii&lt;/i&gt; au plecat din fața Sinedriului, bucurându-se&lt;f&gt;ⓥ&lt;/f&gt; că au fost învredniciți să fie înjosiți pentru Numele &lt;i&gt;Lui&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,5,42,'Și în fiecare zi, în&lt;f&gt;ⓦ&lt;/f&gt; Templu și în case, nu încetau&lt;f&gt;ⓧ&lt;/f&gt; să-i învețe &lt;i&gt;pe oameni&lt;/i&gt; și să-L vestească pe Isus ca Hristos.');
INSERT INTO &quot;verses&quot; VALUES (510,6,1,'&lt;pb/&gt;În zilele acelea, când s-a&lt;f&gt;ⓨ&lt;/f&gt; înmulțit numărul ucenicilor&lt;f&gt;[16]&lt;/f&gt;, &lt;i&gt;evreii&lt;/i&gt; vorbitori de greacă au început&lt;f&gt;ⓩ&lt;/f&gt; să cârtească împotriva evreilor &lt;i&gt;din Ierusalim&lt;/i&gt; pentru că văduvele lor erau trecute cu vederea la împărțirea&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;hranei&lt;/i&gt; zilnice.');
INSERT INTO &quot;verses&quot; VALUES (510,6,2,'Cei doisprezece au adunat mulțimea ucenicilor și au zis: „Nu&lt;f&gt;ⓑ&lt;/f&gt; este potrivit ca noi să lăsăm deoparte Cuvântul lui Dumnezeu ca să slujim la mese.');
INSERT INTO &quot;verses&quot; VALUES (510,6,3,'Căutați&lt;f&gt;ⓒ&lt;/f&gt; printre voi, fraților, șapte bărbați cu nume bun, plini de Duhul Sfânt și de înțelepciune, pe care să-i punem în lucrarea aceasta,');
INSERT INTO &quot;verses&quot; VALUES (510,6,4,'iar noi vom&lt;f&gt;ⓓ&lt;/f&gt; stărui în rugăciune și în slujirea Cuvântului!”');
INSERT INTO &quot;verses&quot; VALUES (510,6,5,'Vorbirea aceasta a fost pe placul întregii mulțimi. I-au ales pe Ștefan, bărbat&lt;f&gt;ⓔ&lt;/f&gt; plin de credință și de Duhul Sfânt, pe Filip&lt;f&gt;ⓕ&lt;/f&gt;, pe Prohor, pe Nicanor, pe Timon, pe Parmena și pe Nicolae&lt;f&gt;ⓖ&lt;/f&gt;, un prozelit din Antiohia.');
INSERT INTO &quot;verses&quot; VALUES (510,6,6,'I-au înfățișat înaintea apostolilor, care&lt;f&gt;ⓗ&lt;/f&gt;, după ce s-au rugat, și-au&lt;f&gt;ⓘ&lt;/f&gt; pus mâinile peste ei.');
INSERT INTO &quot;verses&quot; VALUES (510,6,7,'Cuvântul&lt;f&gt;ⓙ&lt;/f&gt; lui Dumnezeu se răspândea, numărul ucenicilor din Ierusalim creștea foarte mult și o mare mulțime de&lt;f&gt;ⓚ&lt;/f&gt; preoți se supuneau credinței.');
INSERT INTO &quot;verses&quot; VALUES (510,6,8,'&lt;pb/&gt;Ștefan, plin de har și de putere, făcea minuni și semne mari în popor.');
INSERT INTO &quot;verses&quot; VALUES (510,6,9,'S-au ridicat unii din sinagoga numită a liberților, a cirenienilor și a alexandrinilor, împreună cu cei din Cilicia și din Asia, și au început o dispută cu Ștefan,');
INSERT INTO &quot;verses&quot; VALUES (510,6,10,'dar&lt;f&gt;ⓛ&lt;/f&gt; nu puteau ține piept înțelepciunii și Duhului cu care vorbea &lt;i&gt;el&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,6,11,'Atunci&lt;f&gt;ⓜ&lt;/f&gt; au tocmit în ascuns niște oameni care să zică: „Noi l-am auzit rostind blasfemii împotriva lui Moise și împotriva lui Dumnezeu.”');
INSERT INTO &quot;verses&quot; VALUES (510,6,12,'Au întărâtat poporul, pe bătrâni și pe cărturari, s-au năpustit asupra lui, au pus mâna pe el și l-au dus în Sinedriu.');
INSERT INTO &quot;verses&quot; VALUES (510,6,13,'Au pus martori mincinoși, care ziceau: „Omul acesta nu încetează să vorbească împotriva acestui loc sfânt și împotriva Legii.');
INSERT INTO &quot;verses&quot; VALUES (510,6,14,'L-am auzit zicând&lt;f&gt;ⓝ&lt;/f&gt; că acest Isus din Nazaret va dărâma&lt;f&gt;ⓞ&lt;/f&gt; locașul acesta și va schimba obiceiurile pe care ni le-a dat Moise.”');
INSERT INTO &quot;verses&quot; VALUES (510,6,15,'Toți cei ce erau în Sinedriu și-au ațintit privirile asupra lui Ștefan și i-au văzut fața ca o față de înger.');
INSERT INTO &quot;verses&quot; VALUES (510,7,1,'&lt;pb/&gt;Marele-preot a zis: „Așa stau lucrurile?”');
INSERT INTO &quot;verses&quot; VALUES (510,7,2,'&lt;i&gt;Ștefan&lt;/i&gt; a răspuns: „Fraților&lt;f&gt;ⓟ&lt;/f&gt; și părinților, ascultați! Dumnezeul slavei i S-a arătat părintelui nostru Avraam pe când era în Mesopotamia, înainte să fi locuit în Haran,');
INSERT INTO &quot;verses&quot; VALUES (510,7,3,'și i-a zis: &lt;e&gt;«Ieși din țara ta și din familia ta și vino în țara pe care ți-o voi arăta.»&lt;/e&gt;&lt;f&gt;[17]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,4,'Atunci,&lt;f&gt;ⓠ&lt;/f&gt; el a ieșit din țara caldeenilor și s-a așezat în Haran. De acolo, după moartea tatălui său, &lt;i&gt;Dumnezeu&lt;/i&gt; l-a strămutat în țara aceasta, în care locuiți voi acum.');
INSERT INTO &quot;verses&quot; VALUES (510,7,5,'Dar nu i-a dat ca moștenire nici măcar o palmă de pământ în ea, ci i-a făgăduit&lt;f&gt;ⓡ&lt;/f&gt; că &lt;e&gt;i-o va da în stăpânire lui și seminției lui după el&lt;/e&gt;&lt;f&gt;[18]&lt;/f&gt;, deși nu avea niciun copil.');
INSERT INTO &quot;verses&quot; VALUES (510,7,6,'Dumnezeu i-a spus că &lt;e&gt;seminția lui va locui într-o țară străină, &lt;i&gt;unde&lt;/i&gt; va fi înrobită și va fi asuprită patru&lt;/e&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;sute de ani&lt;/e&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,7,7,'&lt;e&gt;«Dar Eu voi judeca neamul care îi va înrobi»&lt;/e&gt;&lt;f&gt;[19]&lt;/f&gt;, a zis Dumnezeu. «&lt;e&gt;După aceea, vor ieși&lt;/e&gt;&lt;f&gt;[20]&lt;/f&gt; și-Mi vor sluji în locul acesta.»');
INSERT INTO &quot;verses&quot; VALUES (510,7,8,'Apoi i-a dat&lt;f&gt;ⓣ&lt;/f&gt; legământul circumciziei; și astfel&lt;f&gt;ⓤ&lt;/f&gt; &lt;i&gt;Avraam&lt;/i&gt; l-a născut pe Isaac și l-a circumcis în ziua a opta, Isaac&lt;f&gt;ⓥ&lt;/f&gt; &lt;i&gt;l-a născut&lt;/i&gt; pe Iacov, și Iacov&lt;f&gt;ⓦ&lt;/f&gt;, pe cei doisprezece patriarhi.');
INSERT INTO &quot;verses&quot; VALUES (510,7,9,'&lt;pb/&gt;Iar&lt;f&gt;ⓧ&lt;/f&gt; patriarhii, fiindcă îl invidiau pe Iosif, l-au vândut în Egipt. Dar&lt;f&gt;ⓨ&lt;/f&gt; Dumnezeu era cu el');
INSERT INTO &quot;verses&quot; VALUES (510,7,10,'și l-a scos din toate necazurile lui; &lt;e&gt;i-a dat trecere&lt;/e&gt;&lt;f&gt;[21]&lt;/f&gt; și&lt;f&gt;ⓩ&lt;/f&gt; înțelepciune &lt;e&gt;înaintea lui Faraon&lt;/e&gt;, împăratul Egiptului, &lt;e&gt;care l-a pus cârmuitor peste Egipt și peste toată casa lui&lt;/e&gt;&lt;f&gt;[22]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,7,11,'Dar asupra întregului Egipt și asupra Canaanului &lt;e&gt;s-a abătut o foamete&lt;/e&gt;&lt;f&gt;[23]&lt;/f&gt;. Era mare strâmtorare, iar părinții noștri nu găseau hrană.');
INSERT INTO &quot;verses&quot; VALUES (510,7,12,'Iacov&lt;f&gt;ⓐ&lt;/f&gt; a auzit că în Egipt este grâu și i-a trimis pe părinții noștri prima oară.');
INSERT INTO &quot;verses&quot; VALUES (510,7,13,'A doua oară,&lt;f&gt;ⓑ&lt;/f&gt; Iosif li s-a făcut cunoscut fraților săi și Faraon a aflat din ce neam este Iosif.');
INSERT INTO &quot;verses&quot; VALUES (510,7,14,'Apoi&lt;f&gt;ⓒ&lt;/f&gt; Iosif a trimis să-i cheme pe Iacov, tatăl său, și întreaga&lt;f&gt;ⓓ&lt;/f&gt; familie, șaptezeci și cinci de suflete.');
INSERT INTO &quot;verses&quot; VALUES (510,7,15,'Iacov&lt;f&gt;ⓔ&lt;/f&gt; s-a coborât în Egipt; &lt;i&gt;acolo&lt;/i&gt; au murit&lt;f&gt;ⓕ&lt;/f&gt; el și părinții noștri,');
INSERT INTO &quot;verses&quot; VALUES (510,7,16,'dar&lt;f&gt;ⓖ&lt;/f&gt; au fost strămutați la Sihem și puși în mormântul&lt;f&gt;ⓗ&lt;/f&gt; pe care îl cumpărase Avraam cu preț de argint de la fiii lui Hamor, în Sihem.');
INSERT INTO &quot;verses&quot; VALUES (510,7,17,'&lt;pb/&gt;Când se apropia vremea&lt;f&gt;ⓘ&lt;/f&gt; &lt;i&gt;împlinirii&lt;/i&gt; făgăduinței pe care i-o făcuse Dumnezeu lui Avraam, poporul a&lt;f&gt;ⓙ&lt;/f&gt; crescut și s-a înmulțit în Egipt,');
INSERT INTO &quot;verses&quot; VALUES (510,7,18,'până când &lt;e&gt;peste Egipt s-a ridicat un alt împărat, care nu-l cunoștea pe Iosif&lt;/e&gt;&lt;f&gt;[24]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,7,19,'Acesta s-a purtat cu viclenie față de neamul nostru, i-a asuprit pe părinții noștri și &lt;i&gt;i-a silit&lt;/i&gt; să-și&lt;f&gt;ⓚ&lt;/f&gt; părăsească pruncii, ca să nu mai trăiască.');
INSERT INTO &quot;verses&quot; VALUES (510,7,20,'În&lt;f&gt;ⓛ&lt;/f&gt; vremea aceasta s-a născut Moise, care era&lt;f&gt;ⓜ&lt;/f&gt; plăcut înaintea lui Dumnezeu. El a fost crescut trei luni în casa tatălui &lt;i&gt;său&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,7,21,'Și când a fost&lt;f&gt;ⓝ&lt;/f&gt; părăsit, l-a luat fiica lui Faraon și l-a crescut ca pe fiul ei.');
INSERT INTO &quot;verses&quot; VALUES (510,7,22,'Moise a fost educat în toată înțelepciunea egiptenilor și era puternic&lt;f&gt;ⓞ&lt;/f&gt; în cuvinte și în fapte.');
INSERT INTO &quot;verses&quot; VALUES (510,7,23,'&lt;pb/&gt;Când&lt;f&gt;ⓟ&lt;/f&gt; a împlinit patruzeci de ani, i-a venit în gând să-i cerceteze pe frații săi, fiii lui Israel.');
INSERT INTO &quot;verses&quot; VALUES (510,7,24,'Și, văzând că unul era nedreptățit, i-a luat apărarea, l-a răzbunat pe cel asuprit și l-a omorât pe egiptean.');
INSERT INTO &quot;verses&quot; VALUES (510,7,25,'Credea că frații săi vor înțelege că Dumnezeu le va da izbăvirea prin mâna lui; dar ei n-au înțeles.');
INSERT INTO &quot;verses&quot; VALUES (510,7,26,'A&lt;f&gt;ⓠ&lt;/f&gt; doua zi, a venit la unii care se băteau și i-a îndemnat la pace, zicând: «Oameni buni, sunteți frați; de ce vă nedreptățiți unul pe altul?»');
INSERT INTO &quot;verses&quot; VALUES (510,7,27,'Dar cel care-l nedreptățea pe aproapele său l-a repezit și i-a zis: &lt;e&gt;«Cine&lt;/e&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;e&gt;te-a pus pe tine stăpân și judecător peste noi?&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,28,'&lt;e&gt;Vrei să mă omori și pe mine cum l-ai omorât ieri pe egiptean?»&lt;/e&gt;&lt;f&gt;[25]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,29,'&lt;i&gt;La auzul&lt;/i&gt; acestor vorbe&lt;f&gt;ⓢ&lt;/f&gt;, Moise a fugit și a locuit ca străin în țara Madian, unde a avut doi fii.');
INSERT INTO &quot;verses&quot; VALUES (510,7,30,'&lt;pb/&gt;Peste patruzeci de ani, &lt;e&gt;i s-a arătat un înger&lt;/e&gt; în pustiul muntelui Sinai, &lt;e&gt;în flacăra de foc a unui rug&lt;/e&gt;&lt;f&gt;[26]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,7,31,'Moise, când l-a văzut, s-a minunat de vedenia aceasta și, pe când se apropia să vadă, s-a auzit glasul Domnului:');
INSERT INTO &quot;verses&quot; VALUES (510,7,32,'&lt;e&gt;«Eu&lt;/e&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;sunt Dumnezeul părinților tăi, Dumnezeul lui Avraam, [Dumnezeul] lui Isaac și [Dumnezeul] lui Iacov.»&lt;/e&gt;&lt;f&gt;[27]&lt;/f&gt; Moise s-a cutremurat și n-a îndrăznit să privească.');
INSERT INTO &quot;verses&quot; VALUES (510,7,33,'Domnul i-a zis: &lt;e&gt;«Scoate-ți încălțămintea din picioare, căci locul pe care stai este pământ sfânt!&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,34,'&lt;e&gt;Am văzut bine apăsarea poporului Meu în Egipt, am auzit tânguirea lor și am coborât să-i izbăvesc. Vino acum, te trimit în Egipt!»&lt;/e&gt;&lt;f&gt;[28]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,35,'Pe acest Moise, pe care-l tăgăduiseră ei când au zis &lt;e&gt;«Cine te-a pus pe tine stăpân și judecător?»&lt;/e&gt;, pe acesta Dumnezeu l-a trimis conducător și izbăvitor, prin&lt;f&gt;ⓤ&lt;/f&gt; îngerul care i se arătase în rug.');
INSERT INTO &quot;verses&quot; VALUES (510,7,36,'El&lt;f&gt;ⓥ&lt;/f&gt; i-a scos &lt;i&gt;din Egipt&lt;/i&gt; și a făcut&lt;f&gt;ⓦ&lt;/f&gt; minuni și semne în Egipt, la&lt;f&gt;ⓧ&lt;/f&gt; Marea Roșie și&lt;f&gt;ⓨ&lt;/f&gt; în pustiu, &lt;i&gt;timp de&lt;/i&gt; patruzeci de ani.');
INSERT INTO &quot;verses&quot; VALUES (510,7,37,'Acest Moise le-a zis fiilor lui Israel: &lt;e&gt;«Domnul, Dumnezeul [vostru], vă va ridica dintre frații voștri un profet&lt;/e&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;e&gt;ca mine.»&lt;/e&gt;&lt;f&gt;[29]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,38,'El&lt;f&gt;ⓐ&lt;/f&gt; este acela care a fost în adunarea din pustiu, cu îngerul&lt;f&gt;ⓑ&lt;/f&gt; care i-a vorbit pe muntele Sinai și cu părinții noștri, cel care a&lt;f&gt;ⓒ&lt;/f&gt; primit cuvinte&lt;f&gt;ⓓ&lt;/f&gt; vii, ca să ni le dea nouă.');
INSERT INTO &quot;verses&quot; VALUES (510,7,39,'Dar părinții noștri n-au vrut să-i dea ascultare, ci l-au nesocotit, și în inimile lor s-au întors către Egipt.');
INSERT INTO &quot;verses&quot; VALUES (510,7,40,'I-au zis lui Aaron: &lt;e&gt;«Fă-ne niște dumnezei care să meargă înaintea noastră, căci cu acest Moise, care ne-a scos din țara Egiptului, nu știm ce s-a întâmplat!»&lt;/e&gt;&lt;f&gt;[30]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,41,'Au&lt;f&gt;ⓔ&lt;/f&gt; făcut un vițel în zilele acelea, au adus jertfă idolului și s-au bucurat de lucrul mâinilor lor.');
INSERT INTO &quot;verses&quot; VALUES (510,7,42,'Atunci, Dumnezeu S-a&lt;f&gt;ⓕ&lt;/f&gt; întors de la ei și i-a lăsat să se închine oștirii&lt;f&gt;ⓖ&lt;/f&gt; cerului, după cum este scris în cartea profeților: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Mi-ați adus voi animale înjunghiate și jertfe&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;timp de patruzeci de ani în pustiu, casă a lui Israel?&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,43,'&lt;t&gt;&lt;e&gt;Ați purtat cortul lui Moloh&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și steaua zeului vostru Refan,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;chipurile pe care le-ați făurit&lt;/e&gt; ca să vă închinați lor!&lt;/t&gt; &lt;t&gt;&lt;e&gt;Eu însă vă voi strămuta dincolo de&lt;/e&gt; Babilon.&lt;f&gt;[31]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,44,'&lt;pb/&gt;Cortul mărturiei era cu părinții noștri în pustiu, așa cum poruncise Cel ce i-a spus lui Moise să-l&lt;f&gt;ⓗ&lt;/f&gt; facă după modelul pe care-l văzuse.');
INSERT INTO &quot;verses&quot; VALUES (510,7,45,'După ce l-au primit, părinții noștri&lt;f&gt;ⓘ&lt;/f&gt;, împreună cu Iosua, l-au adus când au cucerit popoarele pe&lt;f&gt;ⓙ&lt;/f&gt; care Dumnezeu le-a izgonit dinaintea părinților noștri, și &lt;i&gt;așa a rămas&lt;/i&gt; până în zilele lui David.');
INSERT INTO &quot;verses&quot; VALUES (510,7,46,'&lt;i&gt;David&lt;/i&gt;&lt;f&gt;ⓚ&lt;/f&gt; a căpătat har înaintea lui Dumnezeu și a&lt;f&gt;ⓛ&lt;/f&gt; cerut să găsească un lăcaș pentru casa lui Iacov&lt;f&gt;[32]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (510,7,47,'dar Solomon&lt;f&gt;ⓜ&lt;/f&gt; I-a zidit o casă.');
INSERT INTO &quot;verses&quot; VALUES (510,7,48,'Cel&lt;f&gt;ⓝ&lt;/f&gt; Preaînalt însă nu locuiește în &lt;i&gt;lăcaș&lt;/i&gt; făcut de mâini &lt;i&gt;omenești&lt;/i&gt;, după cum zice profetul:');
INSERT INTO &quot;verses&quot; VALUES (510,7,49,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Cerul&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;este tronul Meu,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și pământul &lt;i&gt;este&lt;/i&gt; reazem picioarelor Mele.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Ce fel de casă Îmi veți zidi, zice Domnul,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;sau care va fi locul Meu de odihnă?&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,50,'&lt;t&gt;&lt;e&gt;Oare nu mâna Mea a făcut toate acestea?&lt;/e&gt; &lt;f&gt;[33]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,7,51,'&lt;pb/&gt;Oameni îndărătnici&lt;f&gt;ⓟ&lt;/f&gt;, care aveți inima și urechile necircumcise&lt;f&gt;ⓠ&lt;/f&gt;! Voi întotdeauna vă împotriviți Duhului Sfânt! Cum &lt;i&gt;au făcut&lt;/i&gt; părinții voștri, așa &lt;i&gt;faceți&lt;/i&gt; și voi!');
INSERT INTO &quot;verses&quot; VALUES (510,7,52,'Pe&lt;f&gt;ⓡ&lt;/f&gt; care dintre profeți nu l-au prigonit părinții voștri? I-au omorât pe cei care anunțau venirea Celui&lt;f&gt;ⓢ&lt;/f&gt; Drept, ai cărui trădători și ucigași sunteți');
INSERT INTO &quot;verses&quot; VALUES (510,7,53,'voi, care&lt;f&gt;ⓣ&lt;/f&gt; ați primit Legea prin mijlocirea îngerilor, și n-ați păzit-o!”');
INSERT INTO &quot;verses&quot; VALUES (510,7,54,'&lt;pb/&gt;Când&lt;f&gt;ⓤ&lt;/f&gt; au auzit ei aceste &lt;i&gt;cuvinte&lt;/i&gt;, clocoteau &lt;i&gt;de mânie&lt;/i&gt; în inimile lor și scrâșneau din dinți împotriva lui.');
INSERT INTO &quot;verses&quot; VALUES (510,7,55,'Dar &lt;i&gt;Ștefan&lt;/i&gt;, plin&lt;f&gt;ⓥ&lt;/f&gt; de Duhul Sfânt, și-a ațintit privirea spre cer, a văzut slava lui Dumnezeu și pe Isus stând în picioare la dreapta lui Dumnezeu');
INSERT INTO &quot;verses&quot; VALUES (510,7,56,'și a zis: „Iată, văd&lt;f&gt;ⓦ&lt;/f&gt; cerurile deschise și pe Fiul&lt;f&gt;ⓧ&lt;/f&gt; Omului stând în picioare la dreapta lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (510,7,57,'Dar ei au început să răcnească, și-au astupat urechile și s-au năpustit toți într-un gând asupra lui.');
INSERT INTO &quot;verses&quot; VALUES (510,7,58,'L-au târât&lt;f&gt;ⓨ&lt;/f&gt; afară din cetate și au&lt;f&gt;ⓩ&lt;/f&gt; început să arunce cu pietre în el. Martorii&lt;f&gt;ⓐ&lt;/f&gt; și-au pus hainele la picioarele unui tânăr numit Saul');
INSERT INTO &quot;verses&quot; VALUES (510,7,59,'și aruncau cu pietre în Ștefan, care&lt;f&gt;ⓑ&lt;/f&gt; se ruga și zicea: „Doamne Isuse, primește&lt;f&gt;ⓒ&lt;/f&gt; duhul meu!”');
INSERT INTO &quot;verses&quot; VALUES (510,7,60,'Apoi a îngenuncheat&lt;f&gt;ⓓ&lt;/f&gt; și a strigat cu glas tare: „Doamne&lt;f&gt;ⓔ&lt;/f&gt;, nu le ține în seamă păcatul acesta!” Și, după ce a vorbit astfel, a adormit.');
INSERT INTO &quot;verses&quot; VALUES (510,8,1,'&lt;pb/&gt;Saul&lt;f&gt;ⓕ&lt;/f&gt; încuviințase &lt;i&gt;și el&lt;/i&gt; uciderea lui &lt;i&gt;Ștefan&lt;/i&gt;. Și a început în acea zi o mare prigoană împotriva bisericii din Ierusalim. Toți&lt;f&gt;ⓖ&lt;/f&gt; s-au împrăștiat prin ținuturile Iudeei și Samariei, în afară de apostoli.');
INSERT INTO &quot;verses&quot; VALUES (510,8,2,'Niște bărbați evlavioși l-au îngropat pe Ștefan și l-au&lt;f&gt;ⓗ&lt;/f&gt; jelit cu mare plânset.');
INSERT INTO &quot;verses&quot; VALUES (510,8,3,'Dar Saul făcea prăpăd&lt;f&gt;ⓘ&lt;/f&gt; în Biserică; intra în case, lua cu forța bărbați și femei și-i arunca în temniță.');
INSERT INTO &quot;verses&quot; VALUES (510,8,4,'&lt;pb/&gt;Cei ce se împrăștiaseră&lt;f&gt;ⓙ&lt;/f&gt; mergeau din loc în loc și propovăduiau Cuvântul.');
INSERT INTO &quot;verses&quot; VALUES (510,8,5,'Filip&lt;f&gt;ⓚ&lt;/f&gt; a coborât în cetatea Samariei și L-a propovăduit pe Hristos.');
INSERT INTO &quot;verses&quot; VALUES (510,8,6,'Mulțimile luau aminte într-un gând la cele spuse de Filip, când îl auzeau și vedeau semnele pe care le făcea.');
INSERT INTO &quot;verses&quot; VALUES (510,8,7,'Căci din mulți demonizați&lt;f&gt;ⓛ&lt;/f&gt; ieșeau duhuri necurate, scoțând strigăte mari și mulți paralitici și șchiopi au fost vindecați.');
INSERT INTO &quot;verses&quot; VALUES (510,8,8,'Și a fost o mare bucurie în cetatea aceasta.');
INSERT INTO &quot;verses&quot; VALUES (510,8,9,'&lt;pb/&gt;Era în cetate un om pe nume Simon, care practica magia&lt;f&gt;ⓜ&lt;/f&gt; și uimea&lt;f&gt;ⓝ&lt;/f&gt; poporul Samariei, zicând &lt;i&gt;despre sine&lt;/i&gt; că este un om însemnat.');
INSERT INTO &quot;verses&quot; VALUES (510,8,10,'Toți, de la mic la mare, îl ascultau cu atenție și ziceau: „Omul acesta este puterea lui Dumnezeu, numită «cea mare».”');
INSERT INTO &quot;verses&quot; VALUES (510,8,11,'Îl ascultau deci pentru că de multă vreme îi uimea cu magia lui.');
INSERT INTO &quot;verses&quot; VALUES (510,8,12,'Dar când l-au crezut pe Filip, care le aducea vestea bună&lt;f&gt;ⓞ&lt;/f&gt; a Împărăției lui Dumnezeu și a Numelui lui Isus Hristos, au fost botezați atât bărbați, cât și femei.');
INSERT INTO &quot;verses&quot; VALUES (510,8,13,'Chiar Simon a crezut și, după ce a fost botezat, era nedespărțit de Filip și privea cu uimire minunile și semnele mari care se săvârșeau.');
INSERT INTO &quot;verses&quot; VALUES (510,8,14,'&lt;pb/&gt;Apostolii din Ierusalim, când au auzit că Samaria a primit Cuvântul lui Dumnezeu, i-au trimis la ei pe Petru și pe Ioan,');
INSERT INTO &quot;verses&quot; VALUES (510,8,15,'care s-au coborât și s-au rugat pentru ei să&lt;f&gt;ⓟ&lt;/f&gt; primească Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (510,8,16,'Căci nu Se coborâse încă&lt;f&gt;ⓠ&lt;/f&gt; peste niciunul dintre ei, ci fuseseră numai&lt;f&gt;ⓡ&lt;/f&gt; botezați în Numele&lt;f&gt;ⓢ&lt;/f&gt; Domnului Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,8,17,'Atunci, &lt;i&gt;Petru și Ioan&lt;/i&gt; și-au pus&lt;f&gt;ⓣ&lt;/f&gt; mâinile peste ei, și au primit Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (510,8,18,'Văzând Simon că Duhul [Sfânt] este dat prin punerea mâinilor apostolilor, le-a oferit bani');
INSERT INTO &quot;verses&quot; VALUES (510,8,19,'și a zis: „Dați-mi și mie autoritatea aceasta, pentru ca cei peste care îmi pun mâinile să primească Duhul Sfânt!”');
INSERT INTO &quot;verses&quot; VALUES (510,8,20,'Dar Petru i-a zis: „Banii tăi să piară împreună cu tine, fiindcă ți-ai&lt;f&gt;ⓤ&lt;/f&gt; închipuit că darul&lt;f&gt;ⓥ&lt;/f&gt; lui Dumnezeu se cumpără cu bani!');
INSERT INTO &quot;verses&quot; VALUES (510,8,21,'Tu n-ai nici parte, nici moștenire în toată lucrarea aceasta, căci inima ta nu este dreaptă înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,8,22,'Pocăiește-te de această mârșăvie a ta și roagă-te Domnului, și poate&lt;f&gt;ⓦ&lt;/f&gt; ți se va ierta gândul acesta al inimii tale,');
INSERT INTO &quot;verses&quot; VALUES (510,8,23,'căci văd că ești &lt;i&gt;plin de&lt;/i&gt; fiere amară&lt;f&gt;ⓧ&lt;/f&gt; și în lanțurile fărădelegii!”');
INSERT INTO &quot;verses&quot; VALUES (510,8,24,'Simon a răspuns: „Rugați-vă&lt;f&gt;ⓨ&lt;/f&gt; voi Domnului pentru mine, ca să nu vină asupra mea nimic din ce ați zis!”');
INSERT INTO &quot;verses&quot; VALUES (510,8,25,'După ce au adeverit și propovăduit Cuvântul Domnului, Petru și Ioan s-au întors la Ierusalim, vestind Evanghelia în multe sate ale samaritenilor.');
INSERT INTO &quot;verses&quot; VALUES (510,8,26,'&lt;pb/&gt;Un înger al Domnului i-a vorbit lui Filip, zicând: „Ridică-te și mergi spre miazăzi, pe drumul care coboară de la Ierusalim la Gaza și care este pustiu!”');
INSERT INTO &quot;verses&quot; VALUES (510,8,27,'Filip s-a ridicat și a plecat. Și iată că un&lt;f&gt;ⓩ&lt;/f&gt; etiopian, un eunuc, dregător al reginei Candace a etiopienilor și supraveghetor peste toate vistieriile ei, venise&lt;f&gt;ⓐ&lt;/f&gt; la Ierusalim ca să se închine și');
INSERT INTO &quot;verses&quot; VALUES (510,8,28,'se întorcea &lt;i&gt;de acolo&lt;/i&gt;. El stătea în carul lui și citea din profetul Isaia.');
INSERT INTO &quot;verses&quot; VALUES (510,8,29,'Duhul i-a zis lui Filip: „Mergi și apropie-te de carul acesta!”');
INSERT INTO &quot;verses&quot; VALUES (510,8,30,'Filip a alergat și l-a auzit &lt;i&gt;pe etiopian&lt;/i&gt; citind din profetul Isaia. El i-a zis: „Înțelegi tu ce citești?”');
INSERT INTO &quot;verses&quot; VALUES (510,8,31,'&lt;i&gt;Etiopianul&lt;/i&gt; a răspuns: „Cum aș putea &lt;i&gt;să înțeleg&lt;/i&gt;, dacă nu mă va călăuzi cineva?” Și l-a rugat pe Filip să urce și să stea alături de el.');
INSERT INTO &quot;verses&quot; VALUES (510,8,32,'Fragmentul din Scriptură pe care-l citea era acesta: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;El a fost dus ca o oaie la înjunghiere&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și, ca un miel fără glas înaintea celui ce-l tunde,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;așa nu și-a deschis gura;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,8,33,'&lt;t&gt;&lt;e&gt;umilit fiind, dreapta judecată i-a fost refuzată.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Și cine va vorbi despre urmașii lui?&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Căci viața i-a fost luată de pe pământ.&lt;/e&gt; &lt;f&gt;[34]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,8,34,'&lt;pb/&gt;Eunucul i-a zis lui Filip: „Te rog, despre cine vorbește profetul astfel? Despre sine sau despre altcineva?”');
INSERT INTO &quot;verses&quot; VALUES (510,8,35,'Filip a luat cuvântul, a început&lt;f&gt;ⓑ&lt;/f&gt; de la acest &lt;i&gt;loc&lt;/i&gt; din Scriptură și i L-a propovăduit pe Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,8,36,'Pe când își urmau ei drumul, au ajuns la o apă. Și eunucul a zis: „Iată apă! Ce&lt;f&gt;ⓒ&lt;/f&gt; mă împiedică să fiu botezat?”');
INSERT INTO &quot;verses&quot; VALUES (510,8,37,'[&lt;i&gt;Filip&lt;/i&gt; i-a zis: „Dacă&lt;f&gt;ⓓ&lt;/f&gt; crezi din toată inima, se poate.” &lt;i&gt;Eunucul&lt;/i&gt; a răspuns: „Cred&lt;f&gt;ⓔ&lt;/f&gt; că Isus Hristos este Fiul lui Dumnezeu.”]');
INSERT INTO &quot;verses&quot; VALUES (510,8,38,'A poruncit să stea carul, au coborât amândoi în apă și Filip l-a botezat pe eunuc.');
INSERT INTO &quot;verses&quot; VALUES (510,8,39,'Când au ieșit din apă, Duhul&lt;f&gt;ⓕ&lt;/f&gt; Domnului l-a răpit pe Filip și eunucul nu l-a mai văzut, ci s-a dus în drumul lui plin de bucurie.');
INSERT INTO &quot;verses&quot; VALUES (510,8,40,'Filip se afla la Azot. Mergând din loc în loc, propovăduia în toate cetățile, până a ajuns la Cezareea.');
INSERT INTO &quot;verses&quot; VALUES (510,9,1,'&lt;pb/&gt;Saul&lt;f&gt;ⓖ&lt;/f&gt; încă sufla amenințare și moarte asupra ucenicilor Domnului. A mers la marele-preot');
INSERT INTO &quot;verses&quot; VALUES (510,9,2,'și i-a cerut scrisori către sinagogile din Damasc ca, dacă-i va găsi pe unii că urmează Calea, bărbați și femei, să-i aducă legați la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (510,9,3,'Pe drum, când&lt;f&gt;ⓗ&lt;/f&gt; se apropia de Damasc, deodată în jurul lui a strălucit o lumină din cer.');
INSERT INTO &quot;verses&quot; VALUES (510,9,4,'El a căzut la pământ și a auzit un glas, care-i zicea: &lt;J&gt;„Saule, Saule, de ce&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;Mă prigonești?”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,9,5,'El i-a răspuns: „Cine ești, Doamne?” Și [Domnul i-a zis]: &lt;J&gt;„Eu sunt Isus, pe care-L prigonești.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,9,6,'&lt;J&gt;[E în zadar&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;să dai cu piciorul într-o țepușă!”&lt;/J&gt; Cutremurat și plin de frică, el a zis: „Doamne, ce&lt;f&gt;ⓚ&lt;/f&gt; vrei să fac?”] &lt;J&gt;„Ridică-te”&lt;/J&gt;, i-a zis Domnul, &lt;J&gt;„intră în cetate și ți se va spune ce trebuie să faci!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,9,7,'Oamenii&lt;f&gt;ⓛ&lt;/f&gt; care-l însoțeau au rămas încremeniți; auzeau glasul, dar nu vedeau pe nimeni.');
INSERT INTO &quot;verses&quot; VALUES (510,9,8,'Saul s-a ridicat de la pământ și, cu toate că ochii îi erau deschiși, nu vedea nimic. L-au luat de mână și l-au dus în Damasc.');
INSERT INTO &quot;verses&quot; VALUES (510,9,9,'Trei zile n-a văzut, n-a mâncat și n-a băut nimic.');
INSERT INTO &quot;verses&quot; VALUES (510,9,10,'&lt;pb/&gt;În Damasc era un ucenic pe nume&lt;f&gt;ⓜ&lt;/f&gt; Anania. Domnul i-a zis într-o vedenie: &lt;J&gt;„Anania!”&lt;/J&gt; „Iată-mă, Doamne”, a răspuns el.');
INSERT INTO &quot;verses&quot; VALUES (510,9,11,'Și Domnul &lt;i&gt;i-a zis&lt;/i&gt;: &lt;J&gt;„Ridică-te, mergi pe ulița numită Dreaptă și caută-l în casa lui Iuda &lt;i&gt;pe un om&lt;/i&gt; din Tars pe nume Saul&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt;&lt;J&gt;; căci, iată, el se roagă&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,9,12,'&lt;J&gt;și a văzut în vedenie un om numit Anania intrând la el și punându-și mâinile peste el ca să-și recapete vederea.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,9,13,'„Doamne”, a răspuns Anania, „am auzit de la mulți despre omul acesta, cât&lt;f&gt;ⓞ&lt;/f&gt; rău le-a făcut sfinților Tăi în Ierusalim!');
INSERT INTO &quot;verses&quot; VALUES (510,9,14,'Chiar și aici are autoritate din partea preoților de seamă ca să-i lege pe toți cei ce&lt;f&gt;ⓟ&lt;/f&gt; cheamă Numele Tău!”');
INSERT INTO &quot;verses&quot; VALUES (510,9,15,'Dar Domnul i-a zis: &lt;J&gt;„Mergi, căci el&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;Îmi este un vas ales să ducă Numele Meu înaintea neamurilor&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;, a împăraților&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;și a fiilor lui Israel;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,9,16,'&lt;J&gt;iar Eu îi&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;voi arăta tot ce trebuie să sufere pentru Numele Meu.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,9,17,'Anania&lt;f&gt;ⓤ&lt;/f&gt; a plecat și, după ce a intrat în casă, și-a pus&lt;f&gt;ⓥ&lt;/f&gt; mâinile peste &lt;i&gt;Saul&lt;/i&gt; și a zis: „Frate Saul, Domnul Isus, care ți S-a arătat pe drumul pe care veneai, m-a trimis ca să-ți recapeți vederea și să&lt;f&gt;ⓦ&lt;/f&gt; te umpli de Duhul Sfânt.”');
INSERT INTO &quot;verses&quot; VALUES (510,9,18,'Îndată au căzut de pe ochii lui un fel de solzi, și-a recăpătat vederea, apoi s-a ridicat și a fost botezat.');
INSERT INTO &quot;verses&quot; VALUES (510,9,19,'După ce a mâncat, a prins puteri. &lt;pb/&gt;[Saul]&lt;f&gt;ⓧ&lt;/f&gt; a rămas câteva zile cu ucenicii din Damasc.');
INSERT INTO &quot;verses&quot; VALUES (510,9,20,'Și îndată a început să propovăduiască în sinagogi că&lt;f&gt;ⓨ&lt;/f&gt; Isus este Fiul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,9,21,'Toți cei ce-l ascultau erau uimiți și ziceau: „Nu este el&lt;f&gt;ⓩ&lt;/f&gt; cel care făcea prăpăd în Ierusalim printre cei ce chemau Numele acesta și care a venit până aici ca să-i ducă legați înaintea preoților de seamă?”');
INSERT INTO &quot;verses&quot; VALUES (510,9,22,'Dar Saul se întărea tot mai mult și îi punea în încurcătură&lt;f&gt;ⓐ&lt;/f&gt; pe iudeii care locuiau în Damasc, dovedind că &lt;i&gt;Isus&lt;/i&gt; este Hristosul.');
INSERT INTO &quot;verses&quot; VALUES (510,9,23,'&lt;pb/&gt;După mai multe zile, iudeii s-au&lt;f&gt;ⓑ&lt;/f&gt; sfătuit să-l omoare,');
INSERT INTO &quot;verses&quot; VALUES (510,9,24,'dar uneltirea&lt;f&gt;ⓒ&lt;/f&gt; lor a ajuns la cunoștința lui Saul. Ei păzeau porțile zi și noapte, ca să-l omoare.');
INSERT INTO &quot;verses&quot; VALUES (510,9,25,'Însă ucenicii l-au luat noaptea și l-au&lt;f&gt;ⓓ&lt;/f&gt; coborât peste zid, lăsându-l jos într-un coș.');
INSERT INTO &quot;verses&quot; VALUES (510,9,26,'&lt;pb/&gt;Când a ajuns&lt;f&gt;ⓔ&lt;/f&gt; la Ierusalim, [Saul] încerca să li se alăture ucenicilor, dar toți se temeau de el, fiindcă nu credeau că este ucenic.');
INSERT INTO &quot;verses&quot; VALUES (510,9,27,'Atunci&lt;f&gt;ⓕ&lt;/f&gt;, Barnaba l-a luat, l-a dus la apostoli și le-a istorisit cum, pe drum, &lt;i&gt;Saul&lt;/i&gt; Îl văzuse pe Domnul, care i-a vorbit, și cum&lt;f&gt;ⓖ&lt;/f&gt; în Damasc propovăduise cu îndrăzneală în Numele lui Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,9,28,'El a rămas cu ei, se ducea&lt;f&gt;ⓗ&lt;/f&gt; și venea la Ierusalim și propovăduia cu îndrăzneală în Numele Domnului.');
INSERT INTO &quot;verses&quot; VALUES (510,9,29,'Vorbea și discuta aprins&lt;f&gt;ⓘ&lt;/f&gt; cu evreii vorbitori de greacă, dar&lt;f&gt;ⓙ&lt;/f&gt; ei încercau să-l omoare.');
INSERT INTO &quot;verses&quot; VALUES (510,9,30,'Când au aflat frații &lt;i&gt;despre aceasta&lt;/i&gt;, l-au coborât la Cezareea și apoi l-au trimis la Tars.');
INSERT INTO &quot;verses&quot; VALUES (510,9,31,'&lt;pb/&gt;Biserica&lt;f&gt;ⓚ&lt;/f&gt; din toată Iudeea, Galileea și Samaria avea pace, zidindu-se și umblând în frică de Domnul, și prin îmbărbătarea&lt;f&gt;ⓛ&lt;/f&gt; Duhului Sfânt se înmulțea.');
INSERT INTO &quot;verses&quot; VALUES (510,9,32,'&lt;pb/&gt;Trecând pe&lt;f&gt;ⓜ&lt;/f&gt; la toți sfinții, Petru s-a coborât și la cei ce locuiau în Lida.');
INSERT INTO &quot;verses&quot; VALUES (510,9,33,'Acolo a găsit un om numit Enea, care de opt ani zăcea în pat, fiind paralizat.');
INSERT INTO &quot;verses&quot; VALUES (510,9,34,'Petru i-a zis: „Enea, Isus Hristos te&lt;f&gt;ⓝ&lt;/f&gt; vindecă; ridică-te și strânge-ți patul!” Și &lt;i&gt;Enea&lt;/i&gt; s-a ridicat îndată.');
INSERT INTO &quot;verses&quot; VALUES (510,9,35,'Toți locuitorii din Lida și Saron&lt;f&gt;ⓞ&lt;/f&gt; l-au văzut și s-au&lt;f&gt;ⓟ&lt;/f&gt; întors la Domnul.');
INSERT INTO &quot;verses&quot; VALUES (510,9,36,'&lt;pb/&gt;În Iope era o credincioasă pe nume Tabita, care în traducere înseamnă „gazelă”&lt;f&gt;[35]&lt;/f&gt;. Ea făcea o mulțime de&lt;f&gt;ⓠ&lt;/f&gt; fapte bune și milostenii.');
INSERT INTO &quot;verses&quot; VALUES (510,9,37,'În zilele acelea, s-a îmbolnăvit și a murit. După ce au îmbăiat-o, au pus-o în odaia de sus&lt;f&gt;ⓡ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,9,38,'Fiindcă Lida era aproape de Iope, ucenicii, auzind că Petru este acolo, au trimis la el doi bărbați să-l roage: „Vino cât mai degrabă la noi!”');
INSERT INTO &quot;verses&quot; VALUES (510,9,39,'Petru s-a ridicat și a plecat împreună cu ei. Când a sosit, l-au dus în odaia de sus; toate văduvele l-au înconjurat plângând și i-au arătat cămășile și hainele pe care le făcuse Dorca pe când era cu ele.');
INSERT INTO &quot;verses&quot; VALUES (510,9,40,'Petru i-a scos&lt;f&gt;ⓢ&lt;/f&gt; pe toți afară, a îngenuncheat&lt;f&gt;ⓣ&lt;/f&gt;, s-a rugat, apoi s-a întors spre trup și a&lt;f&gt;ⓤ&lt;/f&gt; zis: „Tabita, ridică-te!” Ea a deschis ochii și, când l-a văzut pe Petru, a stat în capul oaselor.');
INSERT INTO &quot;verses&quot; VALUES (510,9,41,'El i-a dat mâna și a ridicat-o. I-a chemat pe sfinți și pe văduve și le-a înfățișat-o vie.');
INSERT INTO &quot;verses&quot; VALUES (510,9,42,'Lucrul acesta a fost cunoscut în toată cetatea Iope și&lt;f&gt;ⓥ&lt;/f&gt; mulți au crezut în Domnul.');
INSERT INTO &quot;verses&quot; VALUES (510,9,43,'Petru a rămas mai multe zile în Iope, la un tăbăcar numit Simon&lt;f&gt;ⓦ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,10,1,'&lt;pb/&gt;În Cezareea era un om numit Corneliu, centurion din cohorta Italica.');
INSERT INTO &quot;verses&quot; VALUES (510,10,2,'El era evlavios&lt;f&gt;ⓧ&lt;/f&gt; și temător&lt;f&gt;ⓨ&lt;/f&gt; de Dumnezeu împreună cu toată casa lui, făcea multe milostenii poporului și se ruga neîncetat lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,10,3,'Pe la ceasul al nouălea din zi, a văzut&lt;f&gt;ⓩ&lt;/f&gt; limpede într-o vedenie că un înger al lui Dumnezeu a intrat la el și i-a zis: „Corneliu!”');
INSERT INTO &quot;verses&quot; VALUES (510,10,4,'El s-a uitat țintă la &lt;i&gt;înger&lt;/i&gt; și, cuprins de frică, a zis: „Ce este, Doamne?” &lt;i&gt;Îngerul&lt;/i&gt; i-a zis: „Rugăciunile și milosteniile tale au urcat, spre aducere aminte, înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,10,5,'Acum, trimite niște oameni la Iope și cheamă-l pe Simon, numit și Petru!');
INSERT INTO &quot;verses&quot; VALUES (510,10,6,'El găzduiește la un tăbăcar pe nume Simon&lt;f&gt;ⓐ&lt;/f&gt;, a cărui casă este lângă mare. [Acela&lt;f&gt;ⓑ&lt;/f&gt; îți va spune ce trebuie să faci.]”');
INSERT INTO &quot;verses&quot; VALUES (510,10,7,'De îndată ce a plecat îngerul care vorbise cu el, &lt;i&gt;Corneliu&lt;/i&gt; i-a chemat pe doi dintre slujitori și un soldat evlavios dintre cei apropiați lui');
INSERT INTO &quot;verses&quot; VALUES (510,10,8,'și, după ce le-a povestit tot, i-a trimis la Iope.');
INSERT INTO &quot;verses&quot; VALUES (510,10,9,'&lt;pb/&gt;A doua zi, pe când aceștia erau pe drum și se apropiau de cetate, Petru s-a urcat&lt;f&gt;ⓒ&lt;/f&gt; pe acoperiș ca să se roage, pe la ceasul al șaselea.&lt;f&gt;[36]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,10,10,'I s-a făcut foame și a vrut să mănânce. Pe când îi pregăteau &lt;i&gt;masa&lt;/i&gt;, a căzut în extaz');
INSERT INTO &quot;verses&quot; VALUES (510,10,11,'și a&lt;f&gt;ⓓ&lt;/f&gt; văzut cerul deschizându-se și ceva ca o pânză mare ce se cobora spre pământ ținută de cele patru colțuri.');
INSERT INTO &quot;verses&quot; VALUES (510,10,12,'În ea se aflau felurite vietăți cu patru picioare, târâtoare ale pământului și păsări ale cerului.');
INSERT INTO &quot;verses&quot; VALUES (510,10,13,'Și un glas i-a zis: &lt;J&gt;„Petre, ridică-te, taie și mănâncă!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,10,14,'„Nicidecum, Doamne”, a răspuns Petru, „căci&lt;f&gt;ⓔ&lt;/f&gt; niciodată n-am mâncat ceva spurcat sau necurat!”');
INSERT INTO &quot;verses&quot; VALUES (510,10,15,'Și glasul &lt;i&gt;i-a zis&lt;/i&gt; a doua oară: &lt;J&gt;„Ce&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;a curățit Dumnezeu să nu &lt;i&gt;consideri&lt;/i&gt; spurcat!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,10,16,'Aceasta s-a petrecut de trei ori, după care îndată lucrul &lt;i&gt;acela&lt;/i&gt; a fost ridicat la cer.');
INSERT INTO &quot;verses&quot; VALUES (510,10,17,'&lt;pb/&gt;Pe când Petru, nedumerit, se întreba care este înțelesul vedeniei pe care o avusese, iată că oamenii trimiși de Corneliu stăteau la poartă, căutând să afle casa lui Simon.');
INSERT INTO &quot;verses&quot; VALUES (510,10,18,'Ei au întrebat cu glas tare dacă Simon, numit și Petru, găzduiește acolo.');
INSERT INTO &quot;verses&quot; VALUES (510,10,19,'Și pe când cugeta Petru la vedenie, Duhul&lt;f&gt;ⓖ&lt;/f&gt; i-a zis: „Iată că te caută trei oameni;');
INSERT INTO &quot;verses&quot; VALUES (510,10,20,'ridică-te&lt;f&gt;ⓗ&lt;/f&gt;, coboară-te și mergi cu ei fără șovăire, căci Eu i-am trimis!”');
INSERT INTO &quot;verses&quot; VALUES (510,10,21,'Petru s-a coborât la oamenii aceia și le-a zis: „Eu sunt cel pe care-l căutați. Ce pricină vă aduce?”');
INSERT INTO &quot;verses&quot; VALUES (510,10,22,'Ei i-au răspuns: „Centurionul Corneliu&lt;f&gt;ⓘ&lt;/f&gt;, om drept și temător de Dumnezeu, cu un nume bun&lt;f&gt;ⓙ&lt;/f&gt; în tot neamul iudeilor, a fost înștiințat de un înger sfânt să te cheme în casa lui ca să-ți asculte cuvintele.”');
INSERT INTO &quot;verses&quot; VALUES (510,10,23,'&lt;i&gt;Atunci Petru&lt;/i&gt; i-a chemat înăuntru și i-a găzduit. &lt;pb/&gt;A doua zi, s-a sculat și a plecat cu ei, iar câțiva frați din Iope l-au&lt;f&gt;ⓚ&lt;/f&gt; însoțit.');
INSERT INTO &quot;verses&quot; VALUES (510,10,24,'În ziua următoare au ajuns la Cezareea. Corneliu îi aștepta, împreună cu rudele și prietenii mai apropiați pe care-i chemase.');
INSERT INTO &quot;verses&quot; VALUES (510,10,25,'Când Petru a dat să intre, Corneliu i-a ieșit în întâmpinare, a căzut la picioarele lui și i s-a închinat.');
INSERT INTO &quot;verses&quot; VALUES (510,10,26,'Dar Petru l-a ridicat, zicându-i: „Ridică-te&lt;f&gt;ⓛ&lt;/f&gt;, și eu sunt tot om!”');
INSERT INTO &quot;verses&quot; VALUES (510,10,27,'Și vorbind cu el, a intrat în casă, unde a găsit mulți &lt;i&gt;oameni&lt;/i&gt; adunați.');
INSERT INTO &quot;verses&quot; VALUES (510,10,28,'&lt;i&gt;Petru&lt;/i&gt; le-a zis: „Știți că&lt;f&gt;ⓜ&lt;/f&gt; unui iudeu nu-i este îngăduit să se atingă de un străin sau să se apropie de el; dar mie Dumnezeu mi-a arătat să nu numesc pe niciun om spurcat sau necurat.');
INSERT INTO &quot;verses&quot; VALUES (510,10,29,'De aceea am venit fără șovăire când am fost chemat. Vă întreb deci: cu ce gând ați trimis după mine?”');
INSERT INTO &quot;verses&quot; VALUES (510,10,30,'Corneliu a răspuns: „În urmă cu patru zile, cam pe vremea aceasta, mă rugam în casa mea la ceasul al nouălea și iată că a stat înaintea mea un bărbat &lt;i&gt;îmbrăcat&lt;/i&gt; cu o haină strălucitoare');
INSERT INTO &quot;verses&quot; VALUES (510,10,31,'și a zis: «Corneliu, rugăciunea&lt;f&gt;ⓝ&lt;/f&gt; ta a fost ascultată și milosteniile tale au fost amintite înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,10,32,'Trimite la Iope și cheamă-l pe Simon, cel numit și Petru; el găzduiește în casa tăbăcarului Simon, lângă mare. [Când va veni el, îți va vorbi.]»');
INSERT INTO &quot;verses&quot; VALUES (510,10,33,'Am trimis îndată la tine și ai făcut bine că ai venit. Acum suntem toți înaintea lui Dumnezeu, ca să ascultăm tot ce ți-a poruncit Domnul.”');
INSERT INTO &quot;verses&quot; VALUES (510,10,34,'&lt;pb/&gt;Atunci, Petru a început să vorbească și a zis: „Înțeleg cu&lt;f&gt;ⓞ&lt;/f&gt; adevărat că Dumnezeu nu este părtinitor,');
INSERT INTO &quot;verses&quot; VALUES (510,10,35,'ci, în&lt;f&gt;ⓟ&lt;/f&gt; toate popoarele, oricine se teme de El și înfăptuiește dreptatea Îi este plăcut.');
INSERT INTO &quot;verses&quot; VALUES (510,10,36,'El a trimis Cuvântul Său fiilor lui Israel, vestindu-le&lt;f&gt;ⓠ&lt;/f&gt; pacea prin Isus Hristos, care este&lt;f&gt;ⓡ&lt;/f&gt; Domnul tuturor.');
INSERT INTO &quot;verses&quot; VALUES (510,10,37,'Știți și voi ce s-a întâmplat în toată Iudeea, începând din Galileea, după botezul propovăduit de Ioan:');
INSERT INTO &quot;verses&quot; VALUES (510,10,38,'cum Isus din Nazaret, care a fost uns de Dumnezeu&lt;f&gt;ⓢ&lt;/f&gt; cu Duhul Sfânt și cu putere, umbla din loc în loc, făcând bine și vindecându-i pe toți cei ce erau apăsați de Diavol, căci&lt;f&gt;ⓣ&lt;/f&gt; Dumnezeu era cu El.');
INSERT INTO &quot;verses&quot; VALUES (510,10,39,'Noi&lt;f&gt;ⓤ&lt;/f&gt; suntem martori ai tuturor &lt;i&gt;lucrărilor&lt;/i&gt; pe care le-a făcut &lt;i&gt;El&lt;/i&gt; în ținutul iudeilor și în Ierusalim. Ei L-au&lt;f&gt;ⓥ&lt;/f&gt; omorât atârnându-L pe lemn,');
INSERT INTO &quot;verses&quot; VALUES (510,10,40,'dar Dumnezeu L-a înviat a treia zi și I-a îngăduit să Se arate');
INSERT INTO &quot;verses&quot; VALUES (510,10,41,'nu&lt;f&gt;ⓦ&lt;/f&gt; întregului popor, ci nouă, martorilor mai dinainte aleși de Dumnezeu, care&lt;f&gt;ⓧ&lt;/f&gt; am mâncat și am băut împreună cu El după ce a înviat din morți.');
INSERT INTO &quot;verses&quot; VALUES (510,10,42,'&lt;i&gt;Isus&lt;/i&gt; ne-a&lt;f&gt;ⓨ&lt;/f&gt; poruncit să propovăduim poporului și să dăm mărturie că&lt;f&gt;ⓩ&lt;/f&gt; El este rânduit de Dumnezeu Judecător al celor vii&lt;f&gt;ⓐ&lt;/f&gt; și al celor morți.');
INSERT INTO &quot;verses&quot; VALUES (510,10,43,'Despre El&lt;f&gt;ⓑ&lt;/f&gt; mărturisesc toți profeții: că oricine&lt;f&gt;ⓒ&lt;/f&gt; crede în El capătă, prin Numele Lui, iertarea păcatelor.”');
INSERT INTO &quot;verses&quot; VALUES (510,10,44,'&lt;pb/&gt;Pe când rostea Petru vorbele acestea, Duhul Sfânt S-a&lt;f&gt;ⓓ&lt;/f&gt; coborât peste toți cei ce ascultau Cuvântul.');
INSERT INTO &quot;verses&quot; VALUES (510,10,45,'Toți credincioșii circumciși care veniseră cu Petru au rămas uimiți când au văzut că&lt;f&gt;ⓔ&lt;/f&gt; darul Duhului Sfânt s-a revărsat și peste neamuri;');
INSERT INTO &quot;verses&quot; VALUES (510,10,46,'căci îi auzeau vorbind în limbi și mărindu-L pe Dumnezeu. Atunci, Petru a zis:');
INSERT INTO &quot;verses&quot; VALUES (510,10,47,'„Poate oare cineva să oprească apa ca să nu fie botezați aceștia, care au primit Duhul Sfânt la fel ca&lt;f&gt;ⓕ&lt;/f&gt; noi?”');
INSERT INTO &quot;verses&quot; VALUES (510,10,48,'Și a poruncit să fie botezați în&lt;f&gt;ⓖ&lt;/f&gt; Numele [Domnului] Isus Hristos. Atunci, l-au rugat să mai rămână câteva zile [la ei].');
INSERT INTO &quot;verses&quot; VALUES (510,11,1,'&lt;pb/&gt;Apostolii și frații care erau în Iudeea au auzit că și neamurile au primit Cuvântul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,11,2,'Când a urcat Petru la Ierusalim, cei circumciși au început să-l&lt;f&gt;ⓗ&lt;/f&gt; critice,');
INSERT INTO &quot;verses&quot; VALUES (510,11,3,'zicând: „Ai&lt;f&gt;ⓘ&lt;/f&gt; intrat în &lt;i&gt;casa&lt;/i&gt; unor oameni necircumciși și&lt;f&gt;ⓙ&lt;/f&gt; ai mâncat cu ei!”');
INSERT INTO &quot;verses&quot; VALUES (510,11,4,'Petru a început să le istorisească pe&lt;f&gt;ⓚ&lt;/f&gt; rând &lt;i&gt;cele întâmplate&lt;/i&gt; și a zis:');
INSERT INTO &quot;verses&quot; VALUES (510,11,5,'„Eram&lt;f&gt;ⓛ&lt;/f&gt; în cetatea Iope și, pe când mă rugam, fiind în extaz, am avut o vedenie: ceva ca o pânză mare, ținută de cele patru colțuri, se cobora din cer și a venit până la mine.');
INSERT INTO &quot;verses&quot; VALUES (510,11,6,'M-am uitat în ea cu luare aminte și am văzut vietăți cu patru picioare de pe pământ, fiare, târâtoare și păsările cerului.');
INSERT INTO &quot;verses&quot; VALUES (510,11,7,'Și am auzit un glas, care mi-a zis: &lt;J&gt;«Petre, ridică-te, taie și mănâncă!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,11,8,'Dar eu am răspuns: «Nicidecum, Doamne, căci nimic spurcat sau necurat n-a intrat vreodată în gura mea!»');
INSERT INTO &quot;verses&quot; VALUES (510,11,9,'Și glasul mi-a zis a doua oară, din cer: &lt;J&gt;«Ce a curățit Dumnezeu să nu &lt;i&gt;consideri&lt;/i&gt; spurcat!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,11,10,'Aceasta s-a petrecut de trei ori, apoi toate au fost ridicate înapoi la cer.');
INSERT INTO &quot;verses&quot; VALUES (510,11,11,'Și iată, îndată, trei oameni trimiși din Cezareea la mine s-au oprit în fața casei în care eram.');
INSERT INTO &quot;verses&quot; VALUES (510,11,12,'Duhul&lt;f&gt;ⓜ&lt;/f&gt; mi-a spus să merg cu ei fără șovăire. Au venit cu mine și acești&lt;f&gt;ⓝ&lt;/f&gt; șase frați și am intrat în casa omului.');
INSERT INTO &quot;verses&quot; VALUES (510,11,13,'El&lt;f&gt;ⓞ&lt;/f&gt; ne-a istorisit cum a văzut în casa lui un înger ce i s-a înfățișat și i-a zis: «Trimite la Iope și cheamă-l pe Simon, numit și Petru,');
INSERT INTO &quot;verses&quot; VALUES (510,11,14,'care-ți va spune cuvinte prin care veți fi mântuiți tu și toată casa ta!»');
INSERT INTO &quot;verses&quot; VALUES (510,11,15,'Când am început să vorbesc, Duhul Sfânt S-a coborât peste ei ca&lt;f&gt;ⓟ&lt;/f&gt; și peste noi la început.');
INSERT INTO &quot;verses&quot; VALUES (510,11,16,'&lt;i&gt;Atunci&lt;/i&gt; mi-am amintit de cuvintele Domnului, când a zis: &lt;J&gt;«Ioan&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;a botezat în apă, dar voi&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;veți fi botezați în Duhul Sfânt.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,11,17,'Deci&lt;f&gt;ⓢ&lt;/f&gt;, dacă Dumnezeu le-a dat același dar ca și nouă, care am crezut în Domnul Isus Hristos, cine&lt;f&gt;ⓣ&lt;/f&gt; eram eu să mă împotrivesc lui Dumnezeu?”');
INSERT INTO &quot;verses&quot; VALUES (510,11,18,'După ce au auzit aceste &lt;i&gt;lucruri&lt;/i&gt;, s-au liniștit și L-au slăvit pe Dumnezeu, zicând: „Așadar&lt;f&gt;ⓤ&lt;/f&gt;, Dumnezeu le-a dat și neamurilor pocăința &lt;i&gt;care duce&lt;/i&gt; la viață.”');
INSERT INTO &quot;verses&quot; VALUES (510,11,19,'&lt;pb/&gt;Cei ce&lt;f&gt;ⓥ&lt;/f&gt; se împrăștiaseră din pricina prigoanei dezlănțuite pe vremea lui Ștefan au ajuns până în Fenicia, Cipru și Antiohia și le propovăduiau Cuvântul doar iudeilor.');
INSERT INTO &quot;verses&quot; VALUES (510,11,20,'Totuși, printre ei au fost câțiva bărbați din Cipru și din Cirene care au venit în Antiohia, le-au vorbit și&lt;f&gt;ⓦ&lt;/f&gt; grecilor&lt;f&gt;[37]&lt;/f&gt; și L-au vestit pe Domnul Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,11,21,'Mâna&lt;f&gt;ⓧ&lt;/f&gt; Domnului era cu ei și mare a fost numărul celor ce au crezut și s-au&lt;f&gt;ⓨ&lt;/f&gt; întors la Domnul.');
INSERT INTO &quot;verses&quot; VALUES (510,11,22,'Vestea despre ei a ajuns la urechile bisericii din Ierusalim și l-au trimis pe Barnaba&lt;f&gt;ⓩ&lt;/f&gt; până în Antiohia.');
INSERT INTO &quot;verses&quot; VALUES (510,11,23,'Când a ajuns el și a văzut harul lui Dumnezeu, s-a bucurat și i-a îndemnat&lt;f&gt;ⓐ&lt;/f&gt; pe toți să rămână cu inima alipită de Domnul,');
INSERT INTO &quot;verses&quot; VALUES (510,11,24,'fiindcă &lt;i&gt;Barnaba&lt;/i&gt; era un om bun, plin&lt;f&gt;ⓑ&lt;/f&gt; de Duhul Sfânt și de credință. Astfel&lt;f&gt;ⓒ&lt;/f&gt;, o mulțime destul &lt;i&gt;de mare&lt;/i&gt; s-a alăturat Domnului.');
INSERT INTO &quot;verses&quot; VALUES (510,11,25,'&lt;i&gt;Barnaba&lt;/i&gt; a mers apoi la Tars&lt;f&gt;ⓓ&lt;/f&gt;, ca să-l caute pe Saul');
INSERT INTO &quot;verses&quot; VALUES (510,11,26,'și, când l-a găsit, l-a adus în Antiohia. Un an întreg s-au adunat cu biserica și au dat învățătură multora. În Antiohia au fost numiți ucenicii pentru prima dată „creștini”.');
INSERT INTO &quot;verses&quot; VALUES (510,11,27,'&lt;pb/&gt;În vremea aceea, niște profeți&lt;f&gt;ⓔ&lt;/f&gt; au coborât din Ierusalim în Antiohia.');
INSERT INTO &quot;verses&quot; VALUES (510,11,28,'Unul dintre ei, pe nume Agab&lt;f&gt;ⓕ&lt;/f&gt;, s-a ridicat și a vestit prin Duhul că va fi o foamete mare în toată lumea, ceea ce s-a și întâmplat pe vremea &lt;i&gt;împăratului&lt;/i&gt; Claudiu.');
INSERT INTO &quot;verses&quot; VALUES (510,11,29,'Ucenicii au hotărât să trimită, fiecare după puterea lui, un ajutor&lt;f&gt;ⓖ&lt;/f&gt; fraților care locuiau în Iudeea.');
INSERT INTO &quot;verses&quot; VALUES (510,11,30,'Au&lt;f&gt;ⓗ&lt;/f&gt; făcut &lt;i&gt;lucrul acesta&lt;/i&gt; și au trimis &lt;i&gt;darul&lt;/i&gt; la prezbiteri&lt;f&gt;[38]&lt;/f&gt; prin Barnaba și Saul.');
INSERT INTO &quot;verses&quot; VALUES (510,12,1,'&lt;pb/&gt;Cam în aceeași vreme, regele Irod a pus mâna pe unii din biserică pentru a le face rău.');
INSERT INTO &quot;verses&quot; VALUES (510,12,2,'L-a ucis cu sabia pe Iacov, fratele&lt;f&gt;ⓘ&lt;/f&gt; lui Ioan.');
INSERT INTO &quot;verses&quot; VALUES (510,12,3,'Când a văzut că aceasta le place iudeilor, a pus mâna și pe Petru. Erau zilele&lt;f&gt;ⓙ&lt;/f&gt; &lt;i&gt;Sărbătorii&lt;/i&gt; Azimelor.');
INSERT INTO &quot;verses&quot; VALUES (510,12,4,'După ce l-a prins&lt;f&gt;ⓚ&lt;/f&gt;, l-a aruncat în temniță și l-a pus sub paza a patru cete de câte patru soldați, cu gând ca după Paște să-l aducă înaintea poporului.');
INSERT INTO &quot;verses&quot; VALUES (510,12,5,'Petru deci era păzit în temniță; dar biserica se ruga necontenit lui Dumnezeu pentru el.');
INSERT INTO &quot;verses&quot; VALUES (510,12,6,'&lt;pb/&gt;În noaptea de dinaintea &lt;i&gt;zilei&lt;/i&gt; în care Irod urma să-l înfățișeze la judecată, Petru dormea între doi soldați, legat cu două lanțuri, iar paznicii, &lt;i&gt;stând&lt;/i&gt; la ușă, păzeau temnița.');
INSERT INTO &quot;verses&quot; VALUES (510,12,7,'Și iată că a apărut un înger al Domnului și o lumină a strălucit în încăpere. Lovindu-l pe Petru în coastă, l-a&lt;f&gt;ⓛ&lt;/f&gt; trezit și i-a zis: „Scoală-te repede!” Și lanțurile i-au căzut de la mâini.');
INSERT INTO &quot;verses&quot; VALUES (510,12,8,'Apoi, îngerul i-a zis: „Încinge-te și leagă-ți sandalele!” Iar el a făcut întocmai. &lt;i&gt;Îngerul&lt;/i&gt; i-a mai zis: „Îmbracă-ți haina și urmează-mă!”');
INSERT INTO &quot;verses&quot; VALUES (510,12,9,'Petru a ieșit afară și a mers după el, fără&lt;f&gt;ⓜ&lt;/f&gt; să știe dacă ce făcea îngerul era adevărat; i se părea că are&lt;f&gt;ⓝ&lt;/f&gt; o vedenie.');
INSERT INTO &quot;verses&quot; VALUES (510,12,10,'După ce au trecut de prima pază și de a doua, au ajuns la poarta de fier, care dă în cetate, și ea&lt;f&gt;ⓞ&lt;/f&gt; li s-a deschis singură. După ce au ieșit, au mers pe o uliță și, deodată, îngerul a plecat de lângă el.');
INSERT INTO &quot;verses&quot; VALUES (510,12,11,'Când și-a venit în fire, Petru a zis: „Acum știu, într-adevăr, că Domnul l-a&lt;f&gt;ⓟ&lt;/f&gt; trimis pe îngerul Său și m-a&lt;f&gt;ⓠ&lt;/f&gt; scăpat din mâna lui Irod și de la tot ce aștepta poporul iudeilor.”');
INSERT INTO &quot;verses&quot; VALUES (510,12,12,'După ce s-a dezmeticit, s-a&lt;f&gt;ⓡ&lt;/f&gt; îndreptat spre casa Mariei, mama lui&lt;f&gt;ⓢ&lt;/f&gt; Ioan, numit și Marcu, unde erau adunați mulți și se rugau&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,12,13,'A bătut la ușa de la poartă, iar o slujnică numită Roda a venit să răspundă.');
INSERT INTO &quot;verses&quot; VALUES (510,12,14,'A recunoscut glasul lui Petru și, de bucurie, nu a deschis poarta, ci a alergat înăuntru să dea de veste că Petru stă la poartă.');
INSERT INTO &quot;verses&quot; VALUES (510,12,15,'Ei i-au zis: „Ai înnebunit!” Dar ea insista că era &lt;i&gt;Petru&lt;/i&gt;. Ei ziceau: „Este&lt;f&gt;ⓤ&lt;/f&gt; îngerul lui!”');
INSERT INTO &quot;verses&quot; VALUES (510,12,16,'Dar Petru bătea în continuare. Au deschis și, când l-au văzut, au fost cuprinși de uimire.');
INSERT INTO &quot;verses&quot; VALUES (510,12,17,'Petru le-a făcut&lt;f&gt;ⓥ&lt;/f&gt; semn cu mâna să tacă, le-a istorisit cum îl scosese Domnul din temniță și le-a zis: „Spuneți &lt;i&gt;lucrul acesta&lt;/i&gt; lui Iacov și fraților!” Apoi a ieșit și a plecat în alt loc.');
INSERT INTO &quot;verses&quot; VALUES (510,12,18,'Când s-a făcut ziuă, a fost mare tulburare printre soldați, căci &lt;i&gt;nu știau&lt;/i&gt; ce se întâmplase cu Petru.');
INSERT INTO &quot;verses&quot; VALUES (510,12,19,'Irod, după ce l-a căutat și nu l-a găsit, i-a luat la întrebări pe paznici și a poruncit să fie omorâți. &lt;i&gt;După aceea&lt;/i&gt;, a coborât din Iudeea în Cezareea și a rămas &lt;i&gt;acolo&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,12,20,'&lt;pb/&gt;[Irod] era tare mâniat pe cei din Tir și din Sidon. Dar aceștia au venit toți într-un cuget la el și, după ce l-au înduplecat pe Blastos, îngrijitorul camerei de dormit a regelui, au cerut pace, pentru că ținutul&lt;f&gt;ⓦ&lt;/f&gt; lor se hrănea din cel al regelui.');
INSERT INTO &quot;verses&quot; VALUES (510,12,21,'Într-o zi mai dinainte rânduită, Irod s-a îmbrăcat cu hainele lui regești, s-a așezat pe scaunul de judecată și le-a vorbit.');
INSERT INTO &quot;verses&quot; VALUES (510,12,22,'Poporul a strigat: „Glas de Dumnezeu, nu de om!”');
INSERT INTO &quot;verses&quot; VALUES (510,12,23,'Îndată, un înger al Domnului l-a&lt;f&gt;ⓧ&lt;/f&gt; lovit, pentru că nu&lt;f&gt;ⓨ&lt;/f&gt; I-a dat slavă lui Dumnezeu, și, mâncat de viermi, &lt;i&gt;Irod&lt;/i&gt; și-a dat sufletul.');
INSERT INTO &quot;verses&quot; VALUES (510,12,24,'&lt;pb/&gt;Însă Cuvântul&lt;f&gt;ⓩ&lt;/f&gt; lui Dumnezeu se răspândea și sporea.');
INSERT INTO &quot;verses&quot; VALUES (510,12,25,'Barnaba și Saul, după ce și-au împlinit slujba, s-au întors din&lt;f&gt;[39]&lt;/f&gt; Ierusalim, luându-l&lt;f&gt;ⓐ&lt;/f&gt; cu ei pe Ioan&lt;f&gt;ⓑ&lt;/f&gt;, numit și Marcu.');
INSERT INTO &quot;verses&quot; VALUES (510,13,1,'&lt;pb/&gt;În&lt;f&gt;ⓒ&lt;/f&gt; biserica din Antiohia erau [niște] profeți și învățători: Barnaba&lt;f&gt;ⓓ&lt;/f&gt;, Simon, numit Niger, Luciu&lt;f&gt;ⓔ&lt;/f&gt; din Cirene, Manaen, care crescuse împreună cu tetrarhul Irod, și Saul.');
INSERT INTO &quot;verses&quot; VALUES (510,13,2,'Pe când slujeau ei Domnului și posteau, Duhul Sfânt a zis: „Puneți-Mi-i deoparte&lt;f&gt;ⓕ&lt;/f&gt; pe Barnaba și pe Saul pentru lucrarea la&lt;f&gt;ⓖ&lt;/f&gt; care i-am chemat!”');
INSERT INTO &quot;verses&quot; VALUES (510,13,3,'Atunci, după ce au postit&lt;f&gt;ⓗ&lt;/f&gt; și s-au rugat, și-au pus mâinile peste ei și i-au lăsat să plece.');
INSERT INTO &quot;verses&quot; VALUES (510,13,4,'&lt;pb/&gt;&lt;i&gt;Barnaba și Saul&lt;/i&gt;, trimiși de Duhul Sfânt, au coborât la Seleucia și de acolo au călătorit pe mare până în Cipru&lt;f&gt;ⓘ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,13,5,'Ajunși în Salamina, au vestit Cuvântul lui Dumnezeu în sinagogile iudeilor. Îl aveau ca ajutor pe Ioan&lt;f&gt;ⓙ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,13,6,'După ce au străbătut toată insula până la Pafos, au întâlnit un&lt;f&gt;ⓚ&lt;/f&gt; vrăjitor, profet mincinos, un iudeu cu numele Bar-Isus,');
INSERT INTO &quot;verses&quot; VALUES (510,13,7,'care era împreună cu proconsulul Sergius Paulus, un om înțelept. Acesta &lt;i&gt;din urmă&lt;/i&gt; i-a chemat pe Barnaba și pe Saul și s-a arătat doritor să audă Cuvântul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,13,8,'Dar Elima&lt;f&gt;ⓛ&lt;/f&gt;, vrăjitorul (căci așa i se traduce numele), li se împotrivea și căuta să-l abată pe proconsul de la credință.');
INSERT INTO &quot;verses&quot; VALUES (510,13,9,'Atunci, Saul, numit și Pavel, &lt;i&gt;fiind&lt;/i&gt; plin&lt;f&gt;ⓜ&lt;/f&gt; de Duhul Sfânt, l-a țintuit cu privirea');
INSERT INTO &quot;verses&quot; VALUES (510,13,10,'și i-a zis: „&lt;i&gt;Tu&lt;/i&gt;, care &lt;i&gt;ești&lt;/i&gt; plin de toate vicleniile și de toate uneltirile, fiu al&lt;f&gt;ⓝ&lt;/f&gt; Diavolului, vrăjmaș al întregii dreptăți, nu vei înceta să strâmbi căile drepte ale Domnului?');
INSERT INTO &quot;verses&quot; VALUES (510,13,11,'Acum, iată, mâna Domnului &lt;i&gt;este&lt;/i&gt; împotriva ta: vei fi orb și nu vei vedea soarele pentru o vreme!” Îndată, peste el a căzut ceață și întuneric și bâjbâia după cineva care să-l ducă de mână.');
INSERT INTO &quot;verses&quot; VALUES (510,13,12,'Când a văzut proconsulul cele petrecute, a crezut, uimit fiind de învățătura Domnului.');
INSERT INTO &quot;verses&quot; VALUES (510,13,13,'&lt;pb/&gt;Pavel și însoțitorii lui au plecat pe mare din Pafos și s-au dus la Perga, în Pamfilia. Ioan&lt;f&gt;ⓞ&lt;/f&gt; s-a despărțit de ei și s-a întors la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (510,13,14,'Au plecat din Perga și au ajuns în Antiohia Pisidiei, iar în sabat au intrat&lt;f&gt;ⓟ&lt;/f&gt; în sinagogă și s-au așezat.');
INSERT INTO &quot;verses&quot; VALUES (510,13,15,'După citirea&lt;f&gt;ⓠ&lt;/f&gt; Legii și a Profeților, mai-marii sinagogii au trimis să le zică: „Fraților, dacă aveți un cuvânt de îndemn pentru popor, vorbiți!”');
INSERT INTO &quot;verses&quot; VALUES (510,13,16,'Pavel s-a ridicat, a făcut&lt;f&gt;ⓡ&lt;/f&gt; semn cu mâna și a zis: „Bărbați israeliți și voi care&lt;f&gt;ⓢ&lt;/f&gt; vă temeți de Dumnezeu, ascultați!');
INSERT INTO &quot;verses&quot; VALUES (510,13,17,'Dumnezeul acestui popor, Israel, i-a&lt;f&gt;ⓣ&lt;/f&gt; ales pe părinții noștri, a înălțat poporul acesta&lt;f&gt;ⓤ&lt;/f&gt; pe când locuia ca străin în țara Egiptului și&lt;f&gt;ⓥ&lt;/f&gt; l-a scos de acolo cu braț puternic.');
INSERT INTO &quot;verses&quot; VALUES (510,13,18,'Timp&lt;f&gt;ⓦ&lt;/f&gt; de patruzeci de ani i-a răbdat în pustiu');
INSERT INTO &quot;verses&quot; VALUES (510,13,19,'și, după ce a nimicit&lt;f&gt;ⓧ&lt;/f&gt; șapte popoare în țara Canaanului, le-a&lt;f&gt;ⓨ&lt;/f&gt; dat lor ca moștenire țara acestora;');
INSERT INTO &quot;verses&quot; VALUES (510,13,20,'&lt;i&gt;astfel au trecut&lt;/i&gt; cam patru sute cincizeci de ani. După &lt;i&gt;toate&lt;/i&gt; acestea le-a&lt;f&gt;ⓩ&lt;/f&gt; dat judecători, până&lt;f&gt;ⓐ&lt;/f&gt; la profetul Samuel.');
INSERT INTO &quot;verses&quot; VALUES (510,13,21,'Atunci&lt;f&gt;ⓑ&lt;/f&gt;, ei au cerut un rege și, pentru &lt;i&gt;o vreme de&lt;/i&gt; patruzeci de ani, Dumnezeu li l-a dat pe Saul, fiul lui Chis, din seminția lui Beniamin,');
INSERT INTO &quot;verses&quot; VALUES (510,13,22,'apoi l-a înlăturat&lt;f&gt;ⓒ&lt;/f&gt; și li l-a&lt;f&gt;ⓓ&lt;/f&gt; ridicat ca rege pe David, despre care a mărturisit astfel: «&lt;e&gt;L-am găsit pe David&lt;/e&gt;&lt;f&gt;[40]&lt;/f&gt;, fiul lui Ișai, &lt;e&gt;om după inima Mea&lt;/e&gt;&lt;f&gt;[41]&lt;/f&gt;, care va împlini toată voia Mea.»');
INSERT INTO &quot;verses&quot; VALUES (510,13,23,'Dintre&lt;f&gt;ⓔ&lt;/f&gt; urmașii lui, Dumnezeu, după făgăduința&lt;f&gt;ⓕ&lt;/f&gt; Sa, i-a ridicat lui Israel un Mântuitor&lt;f&gt;ⓖ&lt;/f&gt;, pe Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,13,24,'Înainte&lt;f&gt;ⓗ&lt;/f&gt; de venirea Lui, Ioan a vestit întregului popor al lui Israel botezul pocăinței.');
INSERT INTO &quot;verses&quot; VALUES (510,13,25,'Către sfârșitul alergării sale, Ioan zicea: «Cine&lt;f&gt;ⓘ&lt;/f&gt; credeți că sunt eu? Nu sunt eu &lt;i&gt;Acela&lt;/i&gt;, ci, iată, după mine vine Unul a cărui încălțăminte nu sunt vrednic să o dezleg.»');
INSERT INTO &quot;verses&quot; VALUES (510,13,26,'&lt;pb/&gt;Fraților, fii ai neamului lui Avraam și cei ce vă temeți de Dumnezeu, nouă&lt;f&gt;ⓙ&lt;/f&gt; ne-a fost trimis Cuvântul acestei mântuiri!');
INSERT INTO &quot;verses&quot; VALUES (510,13,27,'Căci cei ce locuiesc în Ierusalim și mai-marii lor nu L-au cunoscut&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;pe Isus&lt;/i&gt; și, judecându-L, au împlinit&lt;f&gt;ⓛ&lt;/f&gt; cuvintele profeților care&lt;f&gt;ⓜ&lt;/f&gt; se citesc în fiecare sabat.');
INSERT INTO &quot;verses&quot; VALUES (510,13,28,'Cu toate că&lt;f&gt;ⓝ&lt;/f&gt; nu I-au găsit nicio vină de moarte, i-au cerut lui Pilat să-L omoare.');
INSERT INTO &quot;verses&quot; VALUES (510,13,29,'Și&lt;f&gt;ⓞ&lt;/f&gt;, după ce au împlinit tot ce este scris despre El, L-au dat jos&lt;f&gt;ⓟ&lt;/f&gt; de pe lemn și L-au pus într-un mormânt.');
INSERT INTO &quot;verses&quot; VALUES (510,13,30,'Dar&lt;f&gt;ⓠ&lt;/f&gt; Dumnezeu L-a înviat din morți.');
INSERT INTO &quot;verses&quot; VALUES (510,13,31,'El&lt;f&gt;ⓡ&lt;/f&gt; S-a arătat timp de multe zile celor ce urcaseră cu El din&lt;f&gt;ⓢ&lt;/f&gt; Galileea la Ierusalim și care&lt;f&gt;ⓣ&lt;/f&gt; acum Îi sunt martori înaintea poporului.');
INSERT INTO &quot;verses&quot; VALUES (510,13,32,'Și noi vă aducem vestea bună a făgăduinței&lt;f&gt;ⓤ&lt;/f&gt; făcute părinților &lt;i&gt;noștri&lt;/i&gt;;');
INSERT INTO &quot;verses&quot; VALUES (510,13,33,'Dumnezeu a împlinit-o față de noi, copiii lor, înviindu-L pe Isus, după cum este scris în psalmul al doilea: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Tu ești Fiul Meu,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;astăzi Te-am născut.&lt;/e&gt; &lt;f&gt;[42]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,13,34,'&lt;pb/&gt;&lt;i&gt;Cât despre faptul&lt;/i&gt; că L-a înviat din morți — așa că nu Se va mai întoarce în putrezire — a zis astfel: &lt;e&gt;Vă voi da cele sfinte ale lui David, cele de încredere.&lt;/e&gt;&lt;f&gt;[43]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,13,35,'De aceea, în alt &lt;i&gt;loc&lt;/i&gt; zice: &lt;e&gt;Nu&lt;/e&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;vei îngădui ca sfântul Tău să vadă putrezirea.&lt;/e&gt;&lt;f&gt;[44]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,13,36,'David, după ce a slujit în generația lui după planul lui Dumnezeu, a murit&lt;f&gt;ⓦ&lt;/f&gt;, a fost adăugat lângă părinții săi și a văzut putrezirea.');
INSERT INTO &quot;verses&quot; VALUES (510,13,37,'Dar Cel pe care L-a înviat Dumnezeu n-a văzut putrezirea.');
INSERT INTO &quot;verses&quot; VALUES (510,13,38,'Să știți dar, fraților, că prin El&lt;f&gt;ⓧ&lt;/f&gt; vi se vestește iertarea de păcate; și de toate cele de care n-ați putut fi îndreptățiți prin Legea lui Moise');
INSERT INTO &quot;verses&quot; VALUES (510,13,39,'sunteți îndreptățiți în El toți cei ce credeți&lt;f&gt;ⓨ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,13,40,'Astfel, luați seama să nu se întâmple ce se spune în Profeți:');
INSERT INTO &quot;verses&quot; VALUES (510,13,41,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Priviți, disprețuitorilor,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;minunați-vă și pieriți,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;căci Eu fac o lucrare în zilele voastre,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;o lucrare pe care n-ați crede-o&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;dacă v-ar povesti-o cineva!&lt;/e&gt;”&lt;f&gt;[45]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,13,42,'&lt;pb/&gt;Când au ieșit afară, [cei dintre neamuri] i-au rugat să le vorbească și în sabatul următor despre aceste lucruri.');
INSERT INTO &quot;verses&quot; VALUES (510,13,43,'Și după ce s-a împrăștiat adunarea, mulți dintre iudei și dintre prozeliții evlavioși au mers după Pavel și Barnaba, care stăteau de vorbă cu ei și-i îndemnau să rămână în harul&lt;f&gt;ⓩ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,13,44,'&lt;pb/&gt;În sabatul următor, aproape toată cetatea s-a adunat să asculte Cuvântul Domnului.');
INSERT INTO &quot;verses&quot; VALUES (510,13,45,'Iudeii, văzând mulțimea, s-au umplut de invidie, au început să contrazică cele spuse&lt;f&gt;ⓐ&lt;/f&gt; de Pavel și-l batjocoreau.');
INSERT INTO &quot;verses&quot; VALUES (510,13,46,'Dar Pavel și Barnaba le-au zis cu îndrăzneală: „Vouă trebuia&lt;f&gt;ⓑ&lt;/f&gt; să vi se vestească mai întâi Cuvântul lui Dumnezeu, dar&lt;f&gt;ⓒ&lt;/f&gt;, fiindcă voi nu-l primiți și vă dovediți nevrednici de viața veșnică, iată că ne întoarcem&lt;f&gt;ⓓ&lt;/f&gt; spre neamuri.');
INSERT INTO &quot;verses&quot; VALUES (510,13,47,'Căci așa ne-a poruncit Domnul: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Te-am&lt;/e&gt; &lt;f&gt;ⓔ&lt;/f&gt; &lt;e&gt;pus ca lumină a neamurilor,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca să duci mântuirea până la marginile pământului.&lt;/e&gt; &lt;f&gt;[46]&lt;/f&gt;”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,13,48,'&lt;pb/&gt;Auzind &lt;i&gt;acestea&lt;/i&gt;, cei dintre neamuri se bucurau și preamăreau Cuvântul Domnului, iar toți cei ce erau rânduiți pentru viața veșnică au crezut.');
INSERT INTO &quot;verses&quot; VALUES (510,13,49,'Cuvântul Domnului se răspândea în tot ținutul.');
INSERT INTO &quot;verses&quot; VALUES (510,13,50,'Dar iudeii le-au întărâtat pe femeile evlavioase cu vază și pe mai-marii cetății, au stârnit&lt;f&gt;ⓕ&lt;/f&gt; o prigoană împotriva lui Pavel și a lui Barnaba și i-au izgonit din ținutul lor.');
INSERT INTO &quot;verses&quot; VALUES (510,13,51,'Ei&lt;f&gt;ⓖ&lt;/f&gt; și-au scuturat praful de pe picioare împotriva lor și au plecat în Iconium,');
INSERT INTO &quot;verses&quot; VALUES (510,13,52,'iar ucenicii erau plini&lt;f&gt;ⓗ&lt;/f&gt; de bucurie și de Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (510,14,1,'&lt;pb/&gt;În Iconium, Pavel și Barnaba au intrat împreună în sinagoga iudeilor și au vorbit, astfel că o mare mulțime de iudei și de greci au crezut.');
INSERT INTO &quot;verses&quot; VALUES (510,14,2,'Dar iudeii care n-au crezut au stârnit și au tulburat sufletele neamurilor împotriva fraților.');
INSERT INTO &quot;verses&quot; VALUES (510,14,3,'Totuși, au rămas destul de multă vreme &lt;i&gt;acolo&lt;/i&gt; și vorbeau cu îndrăzneală despre Domnul, care&lt;f&gt;ⓘ&lt;/f&gt; adeverea Cuvântul harului Său cu semne și minuni ce se făceau prin mâinile lor.');
INSERT INTO &quot;verses&quot; VALUES (510,14,4,'Mulțimea din cetate s-a dezbinat: unii erau cu iudeii, alții cu apostolii&lt;f&gt;ⓙ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,14,5,'Dar neamurile și iudeii, împreună cu mai-marii lor, au pus la cale să le facă rău&lt;f&gt;ⓚ&lt;/f&gt; și să-i ucidă cu pietre.');
INSERT INTO &quot;verses&quot; VALUES (510,14,6,'Pavel și Barnaba au prins de veste și au fugit&lt;f&gt;ⓛ&lt;/f&gt; în cetățile Licaoniei — Listra, Derbe și în împrejurimi —');
INSERT INTO &quot;verses&quot; VALUES (510,14,7,'și au propovăduit Evanghelia acolo.');
INSERT INTO &quot;verses&quot; VALUES (510,14,8,'&lt;pb/&gt;În&lt;f&gt;ⓜ&lt;/f&gt; Listra era un bărbat neputincios de picioare, olog din naștere, care nu umblase niciodată.');
INSERT INTO &quot;verses&quot; VALUES (510,14,9,'El îl asculta pe Pavel când vorbea. Acesta s-a uitat țintă la el și, văzând&lt;f&gt;ⓝ&lt;/f&gt; că are credință ca să fie vindecat,');
INSERT INTO &quot;verses&quot; VALUES (510,14,10,'a zis cu glas tare: „Ridică-te&lt;f&gt;ⓞ&lt;/f&gt; drept în picioare!” Acela s-a ridicat și a început să umble.');
INSERT INTO &quot;verses&quot; VALUES (510,14,11,'Văzând ce a făcut Pavel, mulțimile și-au ridicat glasul, zicând în limba licaoniană: „Zeii&lt;f&gt;ⓟ&lt;/f&gt; au coborât la noi în chip omenesc!”');
INSERT INTO &quot;verses&quot; VALUES (510,14,12,'Pe Barnaba îl numeau Zeus, iar pe Pavel, Hermes, fiindcă el folosea cuvântul.');
INSERT INTO &quot;verses&quot; VALUES (510,14,13,'Preotul &lt;i&gt;de la templul&lt;/i&gt; lui Zeus, care era la intrarea cetății, a adus tauri și cununi în fața porților și voia&lt;f&gt;ⓠ&lt;/f&gt;, împreună cu mulțimile, să le aducă jertfe.');
INSERT INTO &quot;verses&quot; VALUES (510,14,14,'Apostolii Barnaba și Pavel, auzind aceasta, și-au&lt;f&gt;ⓡ&lt;/f&gt; rupt hainele și s-au repezit &lt;i&gt;în mijlocul&lt;/i&gt; mulțimii, strigând:');
INSERT INTO &quot;verses&quot; VALUES (510,14,15,'„Oameni buni, de&lt;f&gt;ⓢ&lt;/f&gt; ce faceți &lt;i&gt;lucrul&lt;/i&gt; acesta? Și noi suntem&lt;f&gt;ⓣ&lt;/f&gt; oameni, la fel ca voi, dar vă propovăduim vestea cea bună ca să vă întoarceți de la cele&lt;f&gt;ⓤ&lt;/f&gt; deșarte&lt;f&gt;ⓥ&lt;/f&gt; la Dumnezeul cel viu, care&lt;f&gt;ⓦ&lt;/f&gt; a făcut cerul, pământul, marea și tot ce este în ele.');
INSERT INTO &quot;verses&quot; VALUES (510,14,16,'În generațiile trecute, El&lt;f&gt;ⓧ&lt;/f&gt; a lăsat toate neamurile să umble pe căile lor.');
INSERT INTO &quot;verses&quot; VALUES (510,14,17,'Totuși&lt;f&gt;ⓨ&lt;/f&gt;, nu a rămas fără mărturie, întrucât v-a făcut bine, v-a dat&lt;f&gt;ⓩ&lt;/f&gt; ploi din cer, timpuri roditoare, belșug de hrană și v-a umplut inimile de bucurie.”');
INSERT INTO &quot;verses&quot; VALUES (510,14,18,'Vorbind astfel, abia au potolit mulțimile să nu le aducă jertfe.');
INSERT INTO &quot;verses&quot; VALUES (510,14,19,'&lt;pb/&gt;Însă din&lt;f&gt;ⓐ&lt;/f&gt; Antiohia și Iconium au venit niște iudei, care au înduplecat mulțimile; l-au lovit cu pietre&lt;f&gt;ⓑ&lt;/f&gt; pe Pavel, apoi l-au târât în afara cetății, crezând că a murit.');
INSERT INTO &quot;verses&quot; VALUES (510,14,20,'Dar când l-au înconjurat ucenicii, &lt;i&gt;Pavel&lt;/i&gt; s-a ridicat și a intrat în cetate. &lt;pb/&gt;A doua zi, &lt;i&gt;Pavel&lt;/i&gt; a plecat cu Barnaba la Derbe.');
INSERT INTO &quot;verses&quot; VALUES (510,14,21,'După ce au propovăduit Evanghelia în cetatea aceasta și&lt;f&gt;ⓒ&lt;/f&gt; au făcut mulți ucenici, s-au întors în Listra, în Iconium și în Antiohia &lt;i&gt;Pisidiei&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (510,14,22,'întărind sufletele ucenicilor, îndemnându-i&lt;f&gt;ⓓ&lt;/f&gt; să rămână în credință și &lt;i&gt;spunându-le&lt;/i&gt;: „În Împărăția lui Dumnezeu trebuie&lt;f&gt;ⓔ&lt;/f&gt; să intrăm prin multe necazuri.”');
INSERT INTO &quot;verses&quot; VALUES (510,14,23,'Le-au&lt;f&gt;ⓕ&lt;/f&gt; rânduit prezbiteri în fiecare biserică, după ce s-au rugat și au postit, apoi i-au încredințat în mâna Domnului, în care crezuseră.');
INSERT INTO &quot;verses&quot; VALUES (510,14,24,'După aceea, au trecut prin Pisidia și au ajuns în Pamfilia;');
INSERT INTO &quot;verses&quot; VALUES (510,14,25,'apoi au vestit Cuvântul în Perga și au coborât la Atalia.');
INSERT INTO &quot;verses&quot; VALUES (510,14,26,'De&lt;f&gt;ⓖ&lt;/f&gt; acolo au mers pe mare la Antiohia, unde fuseseră încredințați&lt;f&gt;ⓗ&lt;/f&gt; în grija harului lui Dumnezeu pentru lucrarea pe care o îndepliniseră.');
INSERT INTO &quot;verses&quot; VALUES (510,14,27,'După ce au ajuns ei, au adunat biserica și au&lt;f&gt;ⓘ&lt;/f&gt; istorisit tot ce făcuse Dumnezeu prin ei și cum le-a deschis&lt;f&gt;ⓙ&lt;/f&gt; neamurilor ușa credinței.');
INSERT INTO &quot;verses&quot; VALUES (510,14,28,'Ei au rămas destul de multă vreme [acolo] cu ucenicii.');
INSERT INTO &quot;verses&quot; VALUES (510,15,1,'&lt;pb/&gt;Din Iudeea&lt;f&gt;ⓚ&lt;/f&gt; au coborât unii care îi învățau pe frați astfel: „Dacă&lt;f&gt;ⓛ&lt;/f&gt; nu sunteți circumciși după&lt;f&gt;ⓜ&lt;/f&gt; obiceiul lui Moise, nu puteți fi mântuiți.”');
INSERT INTO &quot;verses&quot; VALUES (510,15,2,'Pentru că Pavel și Barnaba au avut cu ei nu puține controverse și discuții aprinse, s-a luat hotărârea ca Pavel și Barnaba, împreună cu câțiva dintre ei, să&lt;f&gt;ⓝ&lt;/f&gt; urce la apostolii și prezbiterii din Ierusalim &lt;i&gt;ca să-i întrebe&lt;/i&gt; cu privire la această nelămurire.');
INSERT INTO &quot;verses&quot; VALUES (510,15,3,'După ce au fost petrecuți&lt;f&gt;ⓞ&lt;/f&gt; de biserică, au trecut prin Fenicia și Samaria și au istorisit&lt;f&gt;ⓟ&lt;/f&gt; întoarcerea neamurilor &lt;i&gt;la Dumnezeu&lt;/i&gt;, făcându-le &lt;i&gt;astfel&lt;/i&gt; o mare bucurie tuturor fraților.');
INSERT INTO &quot;verses&quot; VALUES (510,15,4,'Când au ajuns la Ierusalim, au fost primiți de biserică, de apostoli și de prezbiteri și au&lt;f&gt;ⓠ&lt;/f&gt; istorisit tot ce făcuse Dumnezeu prin ei.');
INSERT INTO &quot;verses&quot; VALUES (510,15,5,'Atunci, unii din gruparea fariseilor care crezuseră s-au ridicat și au zis ca&lt;f&gt;ⓡ&lt;/f&gt; &lt;i&gt;neamurilor&lt;/i&gt; să li se poruncească să se circumcidă și să păzească Legea lui Moise.');
INSERT INTO &quot;verses&quot; VALUES (510,15,6,'&lt;pb/&gt;Apostolii și prezbiterii s-au adunat ca să cerceteze acest lucru.');
INSERT INTO &quot;verses&quot; VALUES (510,15,7,'După multe discuții, Petru s-a ridicat și le-a zis: „Fraților&lt;f&gt;ⓢ&lt;/f&gt;, voi știți că încă de la început Dumnezeu m-a ales dintre voi ca prin gura mea neamurile să audă cuvântul Evangheliei și să creadă.');
INSERT INTO &quot;verses&quot; VALUES (510,15,8,'Și Dumnezeu, care cunoaște&lt;f&gt;ⓣ&lt;/f&gt; inimile, a depus mărturie pentru ei, dându-le&lt;f&gt;ⓤ&lt;/f&gt; Duhul Sfânt ca și nouă.');
INSERT INTO &quot;verses&quot; VALUES (510,15,9,'N-a făcut nicio&lt;f&gt;ⓥ&lt;/f&gt; deosebire între noi și ei, întrucât le-a curățit&lt;f&gt;ⓦ&lt;/f&gt; inimile prin credință.');
INSERT INTO &quot;verses&quot; VALUES (510,15,10,'Așadar, de ce-L ispitiți pe Dumnezeu și&lt;f&gt;ⓧ&lt;/f&gt; puneți pe grumazul ucenicilor un jug pe care nici părinții noștri, nici noi nu l-am putut purta?');
INSERT INTO &quot;verses&quot; VALUES (510,15,11,'Dar noi credem&lt;f&gt;ⓨ&lt;/f&gt; că prin harul Domnului Isus suntem mântuiți și noi, și ei.”');
INSERT INTO &quot;verses&quot; VALUES (510,15,12,'&lt;pb/&gt;Toată adunarea a tăcut&lt;f&gt;ⓩ&lt;/f&gt; și i-a ascultat pe Barnaba și pe Pavel, care au istorisit toate semnele și minunile pe care le făcuse Dumnezeu prin ei între neamuri.');
INSERT INTO &quot;verses&quot; VALUES (510,15,13,'Când au tăcut ei, a&lt;f&gt;ⓐ&lt;/f&gt; luat cuvântul Iacov, zicând: „Fraților, ascultați-mă!');
INSERT INTO &quot;verses&quot; VALUES (510,15,14,'Simon&lt;f&gt;ⓑ&lt;/f&gt; a spus cum mai întâi Dumnezeu s-a uitat să-Și ia dintre neamuri un popor pentru Numele Său.');
INSERT INTO &quot;verses&quot; VALUES (510,15,15,'Și cu faptul acesta se potrivesc cuvintele profeților, după cum este scris:');
INSERT INTO &quot;verses&quot; VALUES (510,15,16,'&lt;pb/&gt;&lt;t&gt;«După aceea, Mă voi întoarce&lt;/t&gt; &lt;t&gt;&lt;e&gt;și voi ridica din nou lăcașul prăbușit al lui David,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;îi voi zidi iarăși dărâmăturile&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și-l voi înălța la loc,&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,15,17,'&lt;t&gt;&lt;e&gt;pentru ca și ceilalți oameni să-L caute pe Domnul,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și toate neamurile peste care a fost chemat Numele Meu,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;zice Domnul, care face aceste lucruri&lt;/e&gt; &lt;f&gt;[47]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,15,18,'&lt;t&gt;cunoscute din veșnicie.»&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,15,19,'&lt;pb/&gt;De aceea, eu&lt;f&gt;ⓒ&lt;/f&gt; socotesc să nu-i mai împovărăm pe aceia dintre neamuri care se&lt;f&gt;ⓓ&lt;/f&gt; întorc la Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (510,15,20,'ci să li se scrie să se ferească de&lt;f&gt;ⓔ&lt;/f&gt; necurățiile idolilor, de&lt;f&gt;ⓕ&lt;/f&gt; desfrâu, de &lt;i&gt;animale&lt;/i&gt; sugrumate și&lt;f&gt;ⓖ&lt;/f&gt; de sânge.');
INSERT INTO &quot;verses&quot; VALUES (510,15,21,'Căci Moise, încă din timpuri străvechi, are în fiecare cetate &lt;i&gt;oameni&lt;/i&gt; care-l propovăduiesc și este citit&lt;f&gt;ⓗ&lt;/f&gt; în sinagogi în fiecare zi de sabat.”');
INSERT INTO &quot;verses&quot; VALUES (510,15,22,'&lt;pb/&gt;Atunci, apostolii și prezbiterii, împreună cu întreaga biserică, au găsit cu cale să aleagă dintre ei niște bărbați pe care să-i trimită la Antiohia cu Pavel și Barnaba: pe Iuda, zis și Barsaba&lt;f&gt;ⓘ&lt;/f&gt;, și pe Sila, bărbați cu vază între frați.');
INSERT INTO &quot;verses&quot; VALUES (510,15,23,'Și au scris &lt;i&gt;astfel&lt;/i&gt; prin ei: „Apostolii și frații prezbiteri, către frații dintre neamuri care &lt;i&gt;sunt&lt;/i&gt; în Antiohia, Siria și Cilicia: sănătate!');
INSERT INTO &quot;verses&quot; VALUES (510,15,24,'Fiindcă am auzit că unii&lt;f&gt;ⓙ&lt;/f&gt; — plecați dintre noi fără să fi primit poruncă — v-au tulburat prin cuvintele lor și v-au zdruncinat sufletele [zicându-vă să vă circumcideți și să păziți Legea,]');
INSERT INTO &quot;verses&quot; VALUES (510,15,25,'după ce ne-am adunat toți într-un cuget, am găsit cu cale să alegem niște bărbați pe care să-i trimitem la voi, împreună cu preaiubiții noștri Barnaba și Pavel,');
INSERT INTO &quot;verses&quot; VALUES (510,15,26,'oameni&lt;f&gt;ⓚ&lt;/f&gt; ce și-au pus în joc viețile pentru Numele Domnului nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (510,15,27,'I-am trimis așadar pe Iuda și pe Sila împreună cu ei, ca să vă spună prin viu grai aceleași &lt;i&gt;lucruri&lt;/i&gt;:');
INSERT INTO &quot;verses&quot; VALUES (510,15,28,'anume că Duhul Sfânt și noi am găsit cu cale să nu mai punem asupra voastră nicio altă povară pe lângă cele necesare —');
INSERT INTO &quot;verses&quot; VALUES (510,15,29,'adică să&lt;f&gt;ⓛ&lt;/f&gt; vă feriți de carnea jertfită idolilor, de sânge&lt;f&gt;ⓜ&lt;/f&gt;, de &lt;i&gt;animale&lt;/i&gt; sugrumate și de desfrâu; dacă vă veți feri de acestea, bine veți face. Fiți sănătoși!”');
INSERT INTO &quot;verses&quot; VALUES (510,15,30,'&lt;pb/&gt;Așadar, când au primit învoire, au coborât la Antiohia, unde au adunat mulțimea &lt;i&gt;credincioșilor&lt;/i&gt; și au înmânat scrisoarea.');
INSERT INTO &quot;verses&quot; VALUES (510,15,31,'Citind-o, &lt;i&gt;aceștia&lt;/i&gt; s-au bucurat de îmbărbătarea &lt;i&gt;primită&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,15,32,'Iuda și Sila, fiind profeți, i-au&lt;f&gt;ⓝ&lt;/f&gt; îmbărbătat și ei pe frați și i-au întărit cu multe cuvinte.');
INSERT INTO &quot;verses&quot; VALUES (510,15,33,'După o vreme, frații le-au&lt;f&gt;ⓞ&lt;/f&gt; dat învoire să se întoarcă în pace la cei care îi trimiseseră. [');
INSERT INTO &quot;verses&quot; VALUES (510,15,34,'Totuși, Sila a găsit cu cale să mai rămână.]');
INSERT INTO &quot;verses&quot; VALUES (510,15,35,'Pavel&lt;f&gt;ⓟ&lt;/f&gt; și Barnaba au rămas în Antiohia pentru a da învățătură și a propovădui, împreună cu mulți alții, Cuvântul Domnului.');
INSERT INTO &quot;verses&quot; VALUES (510,15,36,'&lt;pb/&gt;După câteva zile, Pavel i-a zis lui Barnaba: „Să ne întoarcem să-i cercetăm pe frații din&lt;f&gt;ⓠ&lt;/f&gt; fiecare cetate în care am vestit Cuvântul Domnului, &lt;i&gt;ca să vedem&lt;/i&gt; ce mai fac!”');
INSERT INTO &quot;verses&quot; VALUES (510,15,37,'Barnaba voia să-l ia și pe Ioan&lt;f&gt;ⓡ&lt;/f&gt;, numit Marcu.');
INSERT INTO &quot;verses&quot; VALUES (510,15,38,'Pavel considera însă că nu este potrivit să-l ia cu ei pe acela care îi&lt;f&gt;ⓢ&lt;/f&gt; părăsise în Pamfilia și nu-i însoțise în lucrarea &lt;i&gt;lor&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,15,39,'Și s-a iscat o așa neînțelegere, încât s-au despărțit unul de celălalt; Barnaba l-a luat pe Marcu și a plecat pe mare spre Cipru,');
INSERT INTO &quot;verses&quot; VALUES (510,15,40,'iar Pavel l-a ales pe Sila și a plecat, după ce a fost încredințat&lt;f&gt;ⓣ&lt;/f&gt; de frați harului Domnului.');
INSERT INTO &quot;verses&quot; VALUES (510,15,41,'Și a străbătut Siria și Cilicia, întărind&lt;f&gt;ⓤ&lt;/f&gt; bisericile.');
INSERT INTO &quot;verses&quot; VALUES (510,16,1,'&lt;pb/&gt;După aceea, &lt;i&gt;Pavel&lt;/i&gt; a ajuns la Derbe&lt;f&gt;ⓥ&lt;/f&gt; și la Listra. Și iată, acolo era un ucenic numit&lt;f&gt;ⓦ&lt;/f&gt; Timotei, fiul&lt;f&gt;ⓧ&lt;/f&gt; unei iudeice credincioase și al unui tată grec.');
INSERT INTO &quot;verses&quot; VALUES (510,16,2,'Frații&lt;f&gt;ⓨ&lt;/f&gt; din Listra și Iconium îl vorbeau de bine.');
INSERT INTO &quot;verses&quot; VALUES (510,16,3,'Pavel a vrut să-l ia cu el și, după ce l-a luat&lt;f&gt;ⓩ&lt;/f&gt;, l-a circumcis, din pricina iudeilor care erau în acele locuri, fiindcă toți știau că tatăl lui este grec.');
INSERT INTO &quot;verses&quot; VALUES (510,16,4,'Străbătând cetățile, îi învățau &lt;i&gt;pe credincioși&lt;/i&gt; să păzească hotărârile&lt;f&gt;ⓐ&lt;/f&gt; luate de apostoli și de prezbiterii din Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (510,16,5,'Bisericile&lt;f&gt;ⓑ&lt;/f&gt; se întăreau în credință și sporeau la număr în fiecare zi.');
INSERT INTO &quot;verses&quot; VALUES (510,16,6,'&lt;pb/&gt;Au străbătut ținutul Frigiei și al Galatiei, fiindcă au fost opriți de Duhul Sfânt să vestească Cuvântul în Asia.');
INSERT INTO &quot;verses&quot; VALUES (510,16,7,'Ajunși lângă Misia, au încercat să intre în Bitinia, dar Duhul lui Isus nu le-a dat voie.');
INSERT INTO &quot;verses&quot; VALUES (510,16,8,'Atunci, au trecut prin Misia și au&lt;f&gt;ⓒ&lt;/f&gt; coborât la Troas.');
INSERT INTO &quot;verses&quot; VALUES (510,16,9,'Noaptea, Pavel a avut o vedenie: i s-a arătat un bărbat&lt;f&gt;ⓓ&lt;/f&gt; din&lt;f&gt;ⓔ&lt;/f&gt; Macedonia care îl chema, zicând: „Vino în Macedonia și ajută-ne!”');
INSERT INTO &quot;verses&quot; VALUES (510,16,10,'După ce a avut Pavel vedenia, am căutat îndată să plecăm în Macedonia, fiindcă înțelegeam că Domnul ne cheamă să le propovăduim &lt;i&gt;Evanghelia&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,16,11,'&lt;pb/&gt;Ne-am îmbarcat în Troas și ne-am dus direct la Samotrace, iar în ziua următoare la Neapolis.');
INSERT INTO &quot;verses&quot; VALUES (510,16,12,'De acolo ne-am dus la Filipi&lt;f&gt;ⓕ&lt;/f&gt;, care este cea mai însemnată cetate a Macedoniei și colonie &lt;i&gt;romană&lt;/i&gt;. În această cetate am rămas câteva zile.');
INSERT INTO &quot;verses&quot; VALUES (510,16,13,'În ziua sabatului, am ieșit pe poarta cetății, lângă un râu, unde credeam noi că este un loc de rugăciune. Ne-am așezat și le-am vorbit femeilor care erau adunate &lt;i&gt;acolo&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,16,14,'O femeie numită Lidia, vânzătoare de purpură din cetatea Tiatira, o temătoare de Dumnezeu&lt;f&gt;[48]&lt;/f&gt;, asculta; iar Domnul i-a&lt;f&gt;ⓖ&lt;/f&gt; deschis inima să ia aminte la cele spuse de Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,16,15,'După ce a fost botezată împreună cu &lt;i&gt;cei din&lt;/i&gt; casa ei, ne-a rugat, zicând: „Dacă mă socotiți credincioasă Domnului, intrați și rămâneți în casa mea!” Și ne-a&lt;f&gt;ⓗ&lt;/f&gt; înduplecat.');
INSERT INTO &quot;verses&quot; VALUES (510,16,16,'&lt;pb/&gt;Pe când mergeam la locul de rugăciune, ne-a ieșit înainte o sclavă care&lt;f&gt;ⓘ&lt;/f&gt; avea un duh de ghicire&lt;f&gt;[49]&lt;/f&gt;. Prin ghicire, le aducea mult câștig&lt;f&gt;ⓙ&lt;/f&gt; stăpânilor ei.');
INSERT INTO &quot;verses&quot; VALUES (510,16,17,'Ea venea după Pavel și după noi, strigând: „Oamenii aceștia sunt robi ai Dumnezeului celui Preaînalt și vă vestesc calea mântuirii.”');
INSERT INTO &quot;verses&quot; VALUES (510,16,18,'Și a făcut așa vreme de multe zile. Necăjit&lt;f&gt;ⓚ&lt;/f&gt;, Pavel s-a întors și i-a zis duhului: „În Numele lui Isus Hristos, îți poruncesc să ieși din ea!” Și&lt;f&gt;ⓛ&lt;/f&gt; &lt;i&gt;duhul&lt;/i&gt; a ieșit chiar în clipa aceea.');
INSERT INTO &quot;verses&quot; VALUES (510,16,19,'Când au văzut&lt;f&gt;ⓜ&lt;/f&gt; stăpânii ei că s-a dus nădejdea câștigului lor, au pus mâna&lt;f&gt;ⓝ&lt;/f&gt; pe Pavel și pe Sila și i-au&lt;f&gt;ⓞ&lt;/f&gt; târât în piață, înaintea magistraților.');
INSERT INTO &quot;verses&quot; VALUES (510,16,20,'I-au adus la pretori&lt;f&gt;[50]&lt;/f&gt; și au zis: „Oamenii aceștia ne tulbură&lt;f&gt;ⓟ&lt;/f&gt; cetatea; sunt iudei');
INSERT INTO &quot;verses&quot; VALUES (510,16,21,'și vestesc niște obiceiuri pe care noi, romani fiind, nu trebuie nici să le primim, nici să le înfăptuim.”');
INSERT INTO &quot;verses&quot; VALUES (510,16,22,'Mulțimea s-a ridicat și ea împotriva lor, iar pretorii au&lt;f&gt;ⓠ&lt;/f&gt; poruncit să li se rupă hainele și să fie bătuți cu nuiele.');
INSERT INTO &quot;verses&quot; VALUES (510,16,23,'După multe lovituri, i-au aruncat în temniță și i-au poruncit temnicerului să-i păzească bine.');
INSERT INTO &quot;verses&quot; VALUES (510,16,24,'Acesta, fiindcă primise o asemenea poruncă, i-a aruncat în adâncul temniței și le-a pus picioarele în butuci.');
INSERT INTO &quot;verses&quot; VALUES (510,16,25,'&lt;pb/&gt;Pe la miezul nopții, Pavel și Sila se rugau și Îi cântau imnuri lui Dumnezeu, iar cei închiși îi ascultau.');
INSERT INTO &quot;verses&quot; VALUES (510,16,26,'Deodată&lt;f&gt;ⓡ&lt;/f&gt;, s-a făcut un cutremur mare, încât s-au zguduit temeliile temniței. Îndată s-au deschis toate ușile și lanțurile tuturor s-au&lt;f&gt;ⓢ&lt;/f&gt; desfăcut.');
INSERT INTO &quot;verses&quot; VALUES (510,16,27,'Temnicerul s-a trezit și, văzând ușile temniței deschise, a scos sabia și era gata să se omoare, căci își închipuia că cei închiși fugiseră.');
INSERT INTO &quot;verses&quot; VALUES (510,16,28,'Dar Pavel a strigat cu glas tare, zicând: „Să nu-ți faci niciun rău, fiindcă suntem toți aici!”');
INSERT INTO &quot;verses&quot; VALUES (510,16,29,'Temnicerul a cerut o făclie, a sărit înăuntru și, tremurând, a căzut înaintea lui Pavel și a lui Sila.');
INSERT INTO &quot;verses&quot; VALUES (510,16,30,'&lt;i&gt;Apoi&lt;/i&gt; i-a scos afară și le-a zis: „Domnilor&lt;f&gt;ⓣ&lt;/f&gt;, ce trebuie să fac ca să fiu mântuit?”');
INSERT INTO &quot;verses&quot; VALUES (510,16,31,'Ei i-au răspuns: „Crede&lt;f&gt;ⓤ&lt;/f&gt; în Domnul Isus, și veți fi mântuiți tu și casa ta!”');
INSERT INTO &quot;verses&quot; VALUES (510,16,32,'Și le-au vestit Cuvântul Domnului lui și tuturor celor din casa lui.');
INSERT INTO &quot;verses&quot; VALUES (510,16,33,'Temnicerul i-a luat &lt;i&gt;la el&lt;/i&gt; chiar în ceasul acela din noapte, le-a spălat rănile și au fost botezați îndată el și toți ai lui.');
INSERT INTO &quot;verses&quot; VALUES (510,16,34,'I-a dus în casă, le-a&lt;f&gt;ⓥ&lt;/f&gt; pus masa și s-a bucurat cu toată casa &lt;i&gt;lui&lt;/i&gt; că a crezut în Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,16,35,'&lt;pb/&gt;Când s-a făcut ziuă, pretorii i-au trimis pe lictori&lt;f&gt;[51]&lt;/f&gt; să-i spună &lt;i&gt;temnicerului&lt;/i&gt;: „Dă-le drumul oamenilor acelora!”');
INSERT INTO &quot;verses&quot; VALUES (510,16,36,'Temnicerul i-a spus lui Pavel aceste cuvinte: „Pretorii au trimis &lt;i&gt;vorbă&lt;/i&gt; să vi se dea drumul; acum, ieșiți afară și mergeți în pace!”');
INSERT INTO &quot;verses&quot; VALUES (510,16,37,'Dar Pavel le-a zis &lt;i&gt;lictorilor&lt;/i&gt;: „După ce ne-au bătut în public fără să fim judecați, deși&lt;f&gt;ⓦ&lt;/f&gt; suntem romani, ne-au aruncat în temniță; acum ne scot afară pe ascuns? Nu! Să vină ei înșiși să ne scoată afară!”');
INSERT INTO &quot;verses&quot; VALUES (510,16,38,'Lictorii le-au spus cuvintele acestea pretorilor, iar acelora li s-a făcut teamă când au auzit că sunt romani.');
INSERT INTO &quot;verses&quot; VALUES (510,16,39,'Au venit să-i îmbuneze, i-au scos afară și i-au&lt;f&gt;ⓧ&lt;/f&gt; rugat să părăsească cetatea.');
INSERT INTO &quot;verses&quot; VALUES (510,16,40,'Ei au ieșit din temniță și au&lt;f&gt;ⓨ&lt;/f&gt; mers &lt;i&gt;în casa&lt;/i&gt; Lidiei. După ce i-au văzut pe frați și i-au îmbărbătat, au plecat.');
INSERT INTO &quot;verses&quot; VALUES (510,17,1,'&lt;pb/&gt;După ce au trecut prin Amfipolis și Apolonia, au venit la Tesalonic, unde era o sinagogă a iudeilor.');
INSERT INTO &quot;verses&quot; VALUES (510,17,2,'După obiceiul său, Pavel a&lt;f&gt;ⓩ&lt;/f&gt; intrat acolo și trei sabate la rând a vorbit cu ei din Scripturi,');
INSERT INTO &quot;verses&quot; VALUES (510,17,3,'explicând și dovedind că&lt;f&gt;ⓐ&lt;/f&gt; Hristosul trebuia să pătimească și să învie din morți. „Acesta este Hristosul: Isus, Cel pe care vi-L vestesc eu”, &lt;i&gt;zicea el&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,17,4,'Unii&lt;f&gt;ⓑ&lt;/f&gt; dintre ei, o mare mulțime de greci temători &lt;i&gt;de Dumnezeu&lt;/i&gt; și nu puține femei cu vază, au crezut și au trecut de partea lui Pavel și a lui Sila&lt;f&gt;ⓒ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,17,5,'Dar iudeii [care nu crezuseră], plini de invidie, au adunat de prin târg niște oameni de nimic, au făcut o gloată și au stârnit cetatea. S-au năpustit asupra casei lui Iason și-i căutau &lt;i&gt;pe Pavel și pe Sila&lt;/i&gt; ca să-i aducă înaintea poporului.');
INSERT INTO &quot;verses&quot; VALUES (510,17,6,'Negăsindu-i pe ei, i-au târât pe Iason&lt;f&gt;ⓓ&lt;/f&gt; și pe câțiva frați înaintea cârmuitorilor cetății, strigând: „Acești oameni&lt;f&gt;ⓔ&lt;/f&gt;, care au întors pe dos lumea, au venit și aici,');
INSERT INTO &quot;verses&quot; VALUES (510,17,7,'iar Iason i-a găzduit. Ei toți lucrează împotriva poruncilor cezarului și spun&lt;f&gt;ⓕ&lt;/f&gt; că este un alt împărat: Isus.”');
INSERT INTO &quot;verses&quot; VALUES (510,17,8,'&lt;i&gt;Iudeii&lt;/i&gt; au tulburat mulțimea și pe conducătorii cetății, care au auzit aceste &lt;i&gt;cuvinte&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,17,9,'&lt;i&gt;Conducătorii&lt;/i&gt;, după ce au luat o garanție în bani de la Iason și de la ceilalți, le-au dat drumul.');
INSERT INTO &quot;verses&quot; VALUES (510,17,10,'&lt;pb/&gt;Frații&lt;f&gt;ⓖ&lt;/f&gt; i-au trimis îndată pe Pavel și pe Sila noaptea la Bereea. Când au ajuns, au intrat în sinagoga iudeilor.');
INSERT INTO &quot;verses&quot; VALUES (510,17,11,'Aceștia erau mai deschiși la minte decât cei din Tesalonic. Au primit cuvântul cu toată bunăvoința și cercetau&lt;f&gt;ⓗ&lt;/f&gt; Scripturile în fiecare zi &lt;i&gt;ca să vadă&lt;/i&gt; dacă lucrurile erau așa &lt;i&gt;cum li se spunea&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,17,12,'Mulți dintre ei și dintre greci — femei cu vază și bărbați — au crezut.');
INSERT INTO &quot;verses&quot; VALUES (510,17,13,'Dar iudeii din Tesalonic, când au aflat că prin Pavel Cuvântul lui Dumnezeu a fost vestit și în Bereea, s-au dus acolo ca să stârnească și să tulbure mulțimile.');
INSERT INTO &quot;verses&quot; VALUES (510,17,14,'Atunci, frații l-au trimis îndată&lt;f&gt;ⓘ&lt;/f&gt; pe Pavel spre mare, iar Sila și Timotei au rămas acolo.');
INSERT INTO &quot;verses&quot; VALUES (510,17,15,'Însoțitorii lui Pavel l-au dus până la Atena; apoi au plecat, ducându-le poruncă&lt;f&gt;ⓙ&lt;/f&gt; lui Sila și lui Timotei să vină la el cât mai curând.');
INSERT INTO &quot;verses&quot; VALUES (510,17,16,'&lt;pb/&gt;În timp ce îi aștepta în Atena, Pavel era&lt;f&gt;ⓚ&lt;/f&gt; tot mai indignat în duhul său când vedea că cetatea era plină de idoli.');
INSERT INTO &quot;verses&quot; VALUES (510,17,17,'În sinagogă stătea de vorbă cu iudeii și cu &lt;i&gt;grecii&lt;/i&gt; temători de Dumnezeu, iar în piață &lt;i&gt;discuta&lt;/i&gt; în fiecare zi cu cei ce erau acolo.');
INSERT INTO &quot;verses&quot; VALUES (510,17,18,'Câțiva dintre filozofii epicurieni și stoici&lt;f&gt;[52]&lt;/f&gt; au intrat în vorbă cu el. Unii ziceau: „Ce vrea să spună palavragiul acesta?” Alții &lt;i&gt;răspundeau&lt;/i&gt;: „Pare a fi vestitorul unor zei străini”, fiindcă propovăduia despre Isus și despre înviere.');
INSERT INTO &quot;verses&quot; VALUES (510,17,19,'Atunci l-au luat, l-au dus în Areopag și i-au zis: „Putem să știm ce este această învățătură nouă despre care vorbești tu?');
INSERT INTO &quot;verses&quot; VALUES (510,17,20,'Căci suntem nedumeriți de ceea ce ne aud urechile. Vrem să știm ce înseamnă aceste lucruri.”');
INSERT INTO &quot;verses&quot; VALUES (510,17,21,'Toți atenienii și străinii stabiliți &lt;i&gt;în cetate&lt;/i&gt; nu-și petreceau timpul altfel decât spunând sau ascultând ceva nou.');
INSERT INTO &quot;verses&quot; VALUES (510,17,22,'&lt;pb/&gt;Pavel s-a ridicat în mijlocul Areopagului și a zis: „Bărbați atenieni! În toate privințele vă găsesc foarte religioși.');
INSERT INTO &quot;verses&quot; VALUES (510,17,23,'Străbătând &lt;i&gt;cetatea&lt;/i&gt; și privind locurile voastre de închinare, am găsit și un altar pe care este scris: «Unui dumnezeu necunoscut». Pe Acesta, pe care voi Îl venerați fără să-L cunoașteți, vi-L vestesc eu.');
INSERT INTO &quot;verses&quot; VALUES (510,17,24,'Dumnezeul&lt;f&gt;ⓛ&lt;/f&gt; care a făcut lumea și tot ce cuprinde ea este Domnul&lt;f&gt;ⓜ&lt;/f&gt; cerului și al pământului; El nu&lt;f&gt;ⓝ&lt;/f&gt; locuiește în temple făcute de mâini &lt;i&gt;omenești&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (510,17,25,'nici nu este slujit de mâini omenești, ca&lt;f&gt;ⓞ&lt;/f&gt; și când ar avea nevoie de ceva; căci El&lt;f&gt;ⓟ&lt;/f&gt; le dă tuturor viață, suflare și toate &lt;i&gt;lucrurile&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,17,26,'El a făcut dintr-unul singur toate neamurile de oameni, ca să locuiască pe toată fața pământului; le-a rânduit vremuri și a pus hotare&lt;f&gt;ⓠ&lt;/f&gt; locuirii lor,');
INSERT INTO &quot;verses&quot; VALUES (510,17,27,'ca&lt;f&gt;ⓡ&lt;/f&gt; ei să-L caute pe Dumnezeu și chiar și bâjbâind să-L găsească, deși&lt;f&gt;ⓢ&lt;/f&gt; nu este departe de niciunul dintre noi.');
INSERT INTO &quot;verses&quot; VALUES (510,17,28,'&lt;pb/&gt;În&lt;f&gt;ⓣ&lt;/f&gt; El trăim, ne mișcăm și existăm, precum&lt;f&gt;ⓤ&lt;/f&gt; a zis și unul dintre poeții voștri: &lt;pb/&gt;&lt;t&gt;«Din neamul Lui suntem și noi...»&lt;f&gt;[53]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,17,29,'&lt;pb/&gt;Așadar, fiindcă suntem din neam de Dumnezeu, nu&lt;f&gt;ⓥ&lt;/f&gt; se cade să credem că dumnezeirea este asemănătoare cu aurul, cu argintul sau cu piatra, chip făurit după meșteșugul și închipuirea omului.');
INSERT INTO &quot;verses&quot; VALUES (510,17,30,'Dumnezeu trece cu vederea vremurile&lt;f&gt;ⓦ&lt;/f&gt; de neștiință, dar poruncește acum&lt;f&gt;ⓧ&lt;/f&gt; tuturor oamenilor de pretutindeni să se pocăiască,');
INSERT INTO &quot;verses&quot; VALUES (510,17,31,'fiindcă a stabilit o zi în care va&lt;f&gt;ⓨ&lt;/f&gt; judeca lumea cu dreptate prin Omul pe care L-a rânduit și despre care a dat tuturor mărturie neîndoielnică prin&lt;f&gt;ⓩ&lt;/f&gt; faptul că L-a înviat din morți…”');
INSERT INTO &quot;verses&quot; VALUES (510,17,32,'&lt;pb/&gt;Când au auzit despre învierea morților, unii au început să-l ia în râs, iar alții i-au zis: „Cu privire la aceasta te vom asculta altădată.”');
INSERT INTO &quot;verses&quot; VALUES (510,17,33,'Așa că Pavel a plecat din mijlocul lor.');
INSERT INTO &quot;verses&quot; VALUES (510,17,34,'Totuși, unii au trecut de partea lui și au crezut; printre aceștia erau Dionisie, areopagitul, o femeie cu numele Damaris și alții împreună cu ei.');
INSERT INTO &quot;verses&quot; VALUES (510,18,1,'&lt;pb/&gt;După aceea, [Pavel] a plecat din Atena și a mers la Corint.');
INSERT INTO &quot;verses&quot; VALUES (510,18,2,'&lt;i&gt;Acolo&lt;/i&gt; l-a găsit pe un iudeu numit Aquila&lt;f&gt;ⓐ&lt;/f&gt;, originar din Pont, venit de curând din Italia împreună cu soția sa, Priscila, deoarece Claudiu poruncise ca toți iudeii să plece din Roma. A venit la ei');
INSERT INTO &quot;verses&quot; VALUES (510,18,3,'și, fiindcă aveau aceeași meserie, a rămas la ei și&lt;f&gt;ⓑ&lt;/f&gt; lucrau. Erau făcători de corturi.');
INSERT INTO &quot;verses&quot; VALUES (510,18,4,'&lt;i&gt;Pavel&lt;/i&gt;&lt;f&gt;ⓒ&lt;/f&gt; discuta în sinagogă în fiecare zi de sabat și căuta să-i înduplece și pe iudei, și pe greci.');
INSERT INTO &quot;verses&quot; VALUES (510,18,5,'După ce&lt;f&gt;ⓓ&lt;/f&gt; Sila și Timotei au venit din Macedonia, Pavel s-a&lt;f&gt;ⓔ&lt;/f&gt; dedicat &lt;i&gt;întru totul&lt;/i&gt; Cuvântului și le propovăduia iudeilor că Isus este Hristosul.');
INSERT INTO &quot;verses&quot; VALUES (510,18,6,'Dar, cum iudeii i se împotriveau&lt;f&gt;ⓕ&lt;/f&gt; și îl ponegreau, Pavel și-a scuturat&lt;f&gt;ⓖ&lt;/f&gt; hainele și le-a zis: „Sângele&lt;f&gt;ⓗ&lt;/f&gt; vostru &lt;i&gt;să fie&lt;/i&gt; asupra capului vostru; eu&lt;f&gt;ⓘ&lt;/f&gt; sunt fără vină. De acum&lt;f&gt;ⓙ&lt;/f&gt; voi merge la neamuri.”');
INSERT INTO &quot;verses&quot; VALUES (510,18,7,'Și după ce a ieșit de acolo, a intrat în casa unui om temător de Dumnezeu, numit Titius Iustus, care locuia chiar lângă sinagogă.');
INSERT INTO &quot;verses&quot; VALUES (510,18,8,'Dar&lt;f&gt;ⓚ&lt;/f&gt; Crisp, mai-marele sinagogii, a crezut în Domnul împreună cu toată casa lui. Și mulți dintre corinteni, auzindu-l &lt;i&gt;pe Pavel&lt;/i&gt;, au crezut și au fost botezați.');
INSERT INTO &quot;verses&quot; VALUES (510,18,9,'Noaptea, Domnul&lt;f&gt;ⓛ&lt;/f&gt; i-a zis lui Pavel într-o vedenie: &lt;J&gt;„Nu te teme! Vorbește și nu tăcea,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,18,10,'&lt;J&gt;fiindcă&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;Eu sunt cu tine și nimeni nu va pune &lt;i&gt;mâna&lt;/i&gt; pe tine ca să-ți facă rău, căci am mult popor în cetatea aceasta!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,18,11,'A rămas &lt;i&gt;acolo&lt;/i&gt; un an și șase luni, învățându-i &lt;i&gt;pe corinteni&lt;/i&gt; Cuvântul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,18,12,'&lt;pb/&gt;Pe când era Galion proconsul al Ahaiei, iudeii s-au ridicat cu un cuget împotriva lui Pavel, l-au dus înaintea scaunului de judecată');
INSERT INTO &quot;verses&quot; VALUES (510,18,13,'și au zis: „&lt;i&gt;Omul&lt;/i&gt; acesta îi înduplecă pe alții să I se închine lui Dumnezeu &lt;i&gt;într-un fel&lt;/i&gt; potrivnic Legii.”');
INSERT INTO &quot;verses&quot; VALUES (510,18,14,'Când să-și înceapă Pavel vorbirea, Galion le-a zis iudeilor: „Dacă&lt;f&gt;ⓝ&lt;/f&gt; ar fi vorba despre o nelegiuire sau o faptă rea, v-aș asculta plângerea, iudeilor!');
INSERT INTO &quot;verses&quot; VALUES (510,18,15,'Dar, dacă sunt neînțelegeri legate de cuvinte, de nume și de Legea voastră, vă privește; eu nu vreau să fiu judecător în asemenea lucruri.”');
INSERT INTO &quot;verses&quot; VALUES (510,18,16,'Și i-a alungat dinaintea scaunului de judecată.');
INSERT INTO &quot;verses&quot; VALUES (510,18,17,'Atunci, au pus cu toții mâna pe Sosten&lt;f&gt;ⓞ&lt;/f&gt;, mai-marele sinagogii, și au început să-l bată înaintea scaunului de judecată; dar lui Galion nu-i păsa de toate acestea.');
INSERT INTO &quot;verses&quot; VALUES (510,18,18,'&lt;pb/&gt;Pavel a mai rămas în Corint multă vreme. &lt;i&gt;Apoi&lt;/i&gt;, și-a luat rămas-bun de la frați și s-a îmbarcat spre Siria, împreună cu Priscila și Aquila, după ce în Chencreea&lt;f&gt;ⓟ&lt;/f&gt; își tunsese&lt;f&gt;ⓠ&lt;/f&gt; capul, căci făcuse o juruință.');
INSERT INTO &quot;verses&quot; VALUES (510,18,19,'Au ajuns în Efes, iar Pavel i-a lăsat pe &lt;i&gt;Aquila și Priscila&lt;/i&gt; și a intrat în sinagogă, unde a stat de vorbă cu iudeii.');
INSERT INTO &quot;verses&quot; VALUES (510,18,20,'&lt;i&gt;Aceștia&lt;/i&gt; l-au rugat să rămână [la ei] mai multă vreme, dar el n-a vrut,');
INSERT INTO &quot;verses&quot; VALUES (510,18,21,'ci și-a luat rămas-bun de la ei și a zis: „[Trebuie&lt;f&gt;ⓡ&lt;/f&gt; neapărat ca sărbătoarea ce vine s-o petrec în Ierusalim.] Mă voi întoarce la voi dacă&lt;f&gt;ⓢ&lt;/f&gt; va vrea Dumnezeu.” Și a plecat din Efes.');
INSERT INTO &quot;verses&quot; VALUES (510,18,22,'A debarcat la Cezareea, s-a suit &lt;i&gt;la Ierusalim&lt;/i&gt;&lt;f&gt;[54]&lt;/f&gt; și a salutat biserica, apoi s-a coborât la Antiohia.');
INSERT INTO &quot;verses&quot; VALUES (510,18,23,'&lt;pb/&gt;&lt;i&gt;Pavel&lt;/i&gt; a stat o vreme &lt;i&gt;în Antiohia&lt;/i&gt;, apoi a plecat și a străbătut ținuturile Galatiei&lt;f&gt;ⓣ&lt;/f&gt; și Frigiei, întărindu-i&lt;f&gt;ⓤ&lt;/f&gt; pe toți ucenicii.');
INSERT INTO &quot;verses&quot; VALUES (510,18,24,'&lt;pb/&gt;La Efes a venit un iudeu numit Apolo, originar din Alexandria — bărbat iscusit în vorbire, tare în Scripturi');
INSERT INTO &quot;verses&quot; VALUES (510,18,25,'și învățat în ce privește calea Domnului. Fiind înflăcărat în duh,&lt;f&gt;ⓥ&lt;/f&gt; vorbea și-i învăța amănunțit &lt;i&gt;pe oameni&lt;/i&gt; despre Isus, deși cunoștea&lt;f&gt;ⓦ&lt;/f&gt; doar botezul lui Ioan.');
INSERT INTO &quot;verses&quot; VALUES (510,18,26,'El a început să vorbească plin de îndrăzneală în sinagogă. După ce l-au auzit, Priscila și Aquila l-au luat deoparte și i-au explicat mai amănunțit calea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,18,27,'Fiindcă el voia să treacă în Ahaia, frații l-au încurajat și le-au scris ucenicilor să-l primească bine. Când a ajuns &lt;i&gt;acolo&lt;/i&gt;, le-a&lt;f&gt;ⓧ&lt;/f&gt; fost de mare ajutor celor ce crezuseră prin har,');
INSERT INTO &quot;verses&quot; VALUES (510,18,28,'fiindcă îi înfrunta cu putere pe iudei, înaintea tuturor, și le dovedea&lt;f&gt;ⓨ&lt;/f&gt; din Scripturi că Isus este Hristosul.');
INSERT INTO &quot;verses&quot; VALUES (510,19,1,'&lt;pb/&gt;În timp ce Apolo&lt;f&gt;ⓩ&lt;/f&gt; era în Corint, Pavel, după ce a străbătut ținuturile de sus, a coborât la Efes. A găsit câțiva ucenici');
INSERT INTO &quot;verses&quot; VALUES (510,19,2,'și le-a zis: „Ați primit Duhul Sfânt după ce ați crezut?” Ei i-au &lt;i&gt;răspuns&lt;/i&gt;: „Nici n-am&lt;f&gt;ⓐ&lt;/f&gt; auzit că este un Duh Sfânt.”');
INSERT INTO &quot;verses&quot; VALUES (510,19,3,'„Dar ce botez ați primit?”, le-a zis el. Și ei au răspuns: „Botezul&lt;f&gt;ⓑ&lt;/f&gt; lui Ioan.”');
INSERT INTO &quot;verses&quot; VALUES (510,19,4,'&lt;i&gt;Atunci,&lt;/i&gt; Pavel le-a spus: „Ioan&lt;f&gt;ⓒ&lt;/f&gt; a botezat cu botezul pocăinței, zicând poporului să creadă în Cel ce venea după el, adică în Isus.”');
INSERT INTO &quot;verses&quot; VALUES (510,19,5,'Când au auzit &lt;i&gt;aceste cuvinte&lt;/i&gt;, au fost botezați în&lt;f&gt;ⓓ&lt;/f&gt; Numele Domnului Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,19,6,'Când&lt;f&gt;ⓔ&lt;/f&gt; și-a pus Pavel mâinile peste ei, Duhul Sfânt a venit asupra lor și au început să vorbească&lt;f&gt;ⓕ&lt;/f&gt; în limbi și să profețească.');
INSERT INTO &quot;verses&quot; VALUES (510,19,7,'Erau cam doisprezece bărbați în total.');
INSERT INTO &quot;verses&quot; VALUES (510,19,8,'&lt;pb/&gt;&lt;i&gt;Pavel&lt;/i&gt; a intrat în sinagogă&lt;f&gt;ⓖ&lt;/f&gt; și vorbea cu îndrăzneală. Timp de trei luni a discutat cu ei și căuta să-i înduplece cu privire la&lt;f&gt;ⓗ&lt;/f&gt; &lt;i&gt;lucrurile&lt;/i&gt; Împărăției lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,19,9,'Dar, fiindcă&lt;f&gt;ⓘ&lt;/f&gt; unii erau împietriți, nu credeau și vorbeau de rău&lt;f&gt;ⓙ&lt;/f&gt; calea &lt;i&gt;Domnului&lt;/i&gt; înaintea mulțimii, &lt;i&gt;Pavel&lt;/i&gt; s-a despărțit de ei, luându-i cu sine pe ucenici și învățându-i în fiecare zi în școala [unuia] &lt;i&gt;numit&lt;/i&gt; Tiran.');
INSERT INTO &quot;verses&quot; VALUES (510,19,10,'Lucrul acesta&lt;f&gt;ⓚ&lt;/f&gt; a durat doi ani, așa că toți locuitorii Asiei, iudei și greci, au auzit Cuvântul Domnului.');
INSERT INTO &quot;verses&quot; VALUES (510,19,11,'Dumnezeu făcea&lt;f&gt;ⓛ&lt;/f&gt; minuni nemaipomenite prin mâinile lui Pavel,');
INSERT INTO &quot;verses&quot; VALUES (510,19,12,'astfel încât&lt;f&gt;ⓜ&lt;/f&gt; puneau peste bolnavi ștergare sau șorțuri ce atinseseră trupul lui, și bolile îi părăseau, iar duhurile rele ieșeau [din ei].');
INSERT INTO &quot;verses&quot; VALUES (510,19,13,'&lt;pb/&gt;Niște&lt;f&gt;ⓝ&lt;/f&gt; exorciști iudei care umblau din loc în loc au&lt;f&gt;ⓞ&lt;/f&gt; încercat să cheme Numele Domnului Isus peste cei ce aveau duhuri rele, zicând: „Vă conjur pe Isus, pe care-L propovăduiește Pavel!”');
INSERT INTO &quot;verses&quot; VALUES (510,19,14,'Lucrul acesta îl făceau șapte fii ai lui Sceva, un preot iudeu din cei de seamă.');
INSERT INTO &quot;verses&quot; VALUES (510,19,15,'Duhul rău le-a răspuns: „Pe Isus Îl cunosc și pe Pavel îl știu, dar voi cine sunteți?”');
INSERT INTO &quot;verses&quot; VALUES (510,19,16,'Și omul acela în care era duhul rău s-a năpustit asupra lor, i-a doborât pe toți și i-a bătut, așa încât &lt;i&gt;aceștia&lt;/i&gt; au fugit goi și răniți din casa aceea.');
INSERT INTO &quot;verses&quot; VALUES (510,19,17,'Faptul acesta a ajuns cunoscut de toți iudeii și grecii care locuiau în Efes; i-a cuprins frica pe toți&lt;f&gt;ⓟ&lt;/f&gt; și Numele Domnului Isus era preamărit.');
INSERT INTO &quot;verses&quot; VALUES (510,19,18,'Mulți dintre cei ce crezuseră veneau să-și mărturisească&lt;f&gt;ⓠ&lt;/f&gt; și să-și dea pe față faptele.');
INSERT INTO &quot;verses&quot; VALUES (510,19,19,'Nu puțini dintre cei ce făcuseră vrăjitorii și-au adus cărțile și le-au ars înaintea tuturor; prețul lor s-a socotit la cincizeci de mii de arginți.');
INSERT INTO &quot;verses&quot; VALUES (510,19,20,'Astfel, Cuvântul se răspândea și avea izbândă prin puterea Domnului&lt;f&gt;ⓡ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,19,21,'&lt;pb/&gt;După ce&lt;f&gt;ⓢ&lt;/f&gt; s-au petrecut aceste lucruri, Pavel a&lt;f&gt;ⓣ&lt;/f&gt; hotărât să meargă la Ierusalim trecând prin Macedonia și Ahaia. „După ce voi merge acolo”, își zicea el, „trebuie&lt;f&gt;ⓤ&lt;/f&gt; să văd și Roma.”');
INSERT INTO &quot;verses&quot; VALUES (510,19,22,'I-a trimis în Macedonia pe doi dintre&lt;f&gt;ⓥ&lt;/f&gt; cei ce-l ajutau: pe Timotei și pe Erast&lt;f&gt;ⓦ&lt;/f&gt;, iar el a mai rămas câtva timp în Asia.');
INSERT INTO &quot;verses&quot; VALUES (510,19,23,'&lt;pb/&gt;Cam pe vremea aceea&lt;f&gt;ⓧ&lt;/f&gt;, s-a făcut o mare tulburare cu privire la calea&lt;f&gt;ⓨ&lt;/f&gt; &lt;i&gt;Domnului&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,19,24,'Un argintar numit Demetrios făcea temple de argint ale &lt;i&gt;zeiței&lt;/i&gt; Artemis și astfel aducea un câștig însemnat meșterilor săi&lt;f&gt;ⓩ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,19,25,'&lt;i&gt;Așa că&lt;/i&gt; i-a adunat, împreună cu alții de aceeași meserie, și le-a zis: „Oameni buni, știți că din meșteșugul acesta ne vine bunăstarea;');
INSERT INTO &quot;verses&quot; VALUES (510,19,26,'voi vedeți și auziți că Pavel acesta nu numai în Efes, ci aproape în toată Asia i-a înduplecat și i-a dus în rătăcire pe mulți, zicând că cei făcuți de mâna &lt;i&gt;omului&lt;/i&gt; nu&lt;f&gt;ⓐ&lt;/f&gt; sunt zei.');
INSERT INTO &quot;verses&quot; VALUES (510,19,27,'Nu doar că breasla noastră riscă să ajungă disprețuită, dar și templul măreței zeițe Artemis este socotit un nimic, iar măreția aceleia care este venerată în toată Asia și în toată lumea e pe cale să fie nimicită.”');
INSERT INTO &quot;verses&quot; VALUES (510,19,28,'&lt;pb/&gt;Când au auzit ei &lt;i&gt;toate acestea&lt;/i&gt;, s-au umplut de mânie și au început să strige: „Mare este &lt;i&gt;zeița&lt;/i&gt; Artemis a efesenilor!”');
INSERT INTO &quot;verses&quot; VALUES (510,19,29,'Toată cetatea s-a tulburat. I-au înșfăcat pe macedonenii Gaius&lt;f&gt;ⓑ&lt;/f&gt; și Aristarh&lt;f&gt;ⓒ&lt;/f&gt;, însoțitori ai lui Pavel, și au năvălit toți într-un gând în teatru.');
INSERT INTO &quot;verses&quot; VALUES (510,19,30,'Pavel voia să vină înaintea mulțimii, dar ucenicii nu l-au lăsat.');
INSERT INTO &quot;verses&quot; VALUES (510,19,31,'Chiar și unii dintre mai-marii Asiei, care-i erau prieteni, au trimis la el să-l roage să nu intre în teatru.');
INSERT INTO &quot;verses&quot; VALUES (510,19,32,'Unii strigau una, alții alta, căci adunarea era în învălmășeală și cei mai mulți nici nu știau de ce se adunaseră.');
INSERT INTO &quot;verses&quot; VALUES (510,19,33,'Atunci, l-au scos din gloată pe Alexandru, pe care iudeii îl împingeau înainte. Alexandru&lt;f&gt;ⓓ&lt;/f&gt; a făcut&lt;f&gt;ⓔ&lt;/f&gt; semn cu mâna și voia să se apere înaintea mulțimii.');
INSERT INTO &quot;verses&quot; VALUES (510,19,34,'Dar când l-au recunoscut că este iudeu, au strigat toți într-un glas, timp de aproape două ore: „Mare este &lt;i&gt;zeița&lt;/i&gt; Artemis a efesenilor!”');
INSERT INTO &quot;verses&quot; VALUES (510,19,35,'Totuși, logofătul a potolit mulțimea și a zis: „Bărbați efeseni, cine nu știe oare că cetatea efesenilor este păzitoarea templului măreței Artemis și a &lt;i&gt;chipului ei&lt;/i&gt; căzut din cer?');
INSERT INTO &quot;verses&quot; VALUES (510,19,36,'Fiindcă lucrul acesta este de netăgăduit, trebuie să vă potoliți și să nu faceți vreun lucru nesăbuit.');
INSERT INTO &quot;verses&quot; VALUES (510,19,37,'I-ați adus aici pe oamenii aceștia, care nu sunt nici jefuitori de temple, nici hulitori ai zeiței noastre.');
INSERT INTO &quot;verses&quot; VALUES (510,19,38,'Dacă Demetrios și meșterii care sunt cu el au vreo plângere împotriva cuiva, există adunări de judecată și proconsuli; să se învinuiască unii pe alții!');
INSERT INTO &quot;verses&quot; VALUES (510,19,39,'Dacă mai aveți și altă pricină, se va hotărî &lt;i&gt;cu privire la ea&lt;/i&gt; într-o adunare legitimă.');
INSERT INTO &quot;verses&quot; VALUES (510,19,40,'&lt;i&gt;Dar acum&lt;/i&gt; suntem în primejdie să fim învinuiți de răscoală pentru &lt;i&gt;cele petrecute&lt;/i&gt; astăzi, căci n-avem niciun temei prin care să justificăm această răzvrătire.”');
INSERT INTO &quot;verses&quot; VALUES (510,19,41,'Și zicând acestea, a dat drumul adunării.');
INSERT INTO &quot;verses&quot; VALUES (510,20,1,'&lt;pb/&gt;După încetarea zarvei, Pavel i-a chemat pe ucenici și, după ce le-a dat sfaturi, și-a luat rămas-bun de la ei, a ieșit &lt;i&gt;din cetate&lt;/i&gt; și a&lt;f&gt;ⓕ&lt;/f&gt; plecat în Macedonia.');
INSERT INTO &quot;verses&quot; VALUES (510,20,2,'Străbătând ținutul acela, le-a dat &lt;i&gt;ucenicilor&lt;/i&gt; multe învățături; apoi a mers în Grecia,');
INSERT INTO &quot;verses&quot; VALUES (510,20,3,'unde a rămas trei luni. Avea de gând să plece pe mare spre Siria, dar&lt;f&gt;ⓖ&lt;/f&gt; iudeii au uneltit împotriva lui, de aceea a hotărât să se întoarcă prin Macedonia.');
INSERT INTO &quot;verses&quot; VALUES (510,20,4,'L-au însoțit Sopater, fiul lui Pirus, din Bereea, Aristarh&lt;f&gt;ⓗ&lt;/f&gt; și Secundus din Tesalonic, Gaius&lt;f&gt;ⓘ&lt;/f&gt; din Derbe, împreună cu Timotei&lt;f&gt;ⓙ&lt;/f&gt;, Tihic&lt;f&gt;ⓚ&lt;/f&gt; și Trofim&lt;f&gt;ⓛ&lt;/f&gt; din Asia.');
INSERT INTO &quot;verses&quot; VALUES (510,20,5,'Aceștia au mers înainte și ne-au așteptat în Troas.');
INSERT INTO &quot;verses&quot; VALUES (510,20,6,'După Sărbătoarea&lt;f&gt;ⓜ&lt;/f&gt; Azimelor, am plecat și noi cu corabia din Filipi, iar după cinci zile am ajuns la ei în&lt;f&gt;ⓝ&lt;/f&gt; Troas, unde am rămas șapte zile.');
INSERT INTO &quot;verses&quot; VALUES (510,20,7,'&lt;pb/&gt;În prima zi a săptămânii&lt;f&gt;ⓞ&lt;/f&gt;, când eram adunați ca să&lt;f&gt;ⓟ&lt;/f&gt; frângem pâinea, Pavel a început o cuvântare și, fiindcă avea de gând să plece a doua zi, și-a prelungit vorbirea până la miezul nopții.');
INSERT INTO &quot;verses&quot; VALUES (510,20,8,'În&lt;f&gt;ⓠ&lt;/f&gt; odaia de sus, unde eram adunați, erau multe candele.');
INSERT INTO &quot;verses&quot; VALUES (510,20,9,'Un tânăr numit Eutih, care era așezat pe fereastră, a adormit adânc în timpul îndelungii cuvântări a lui Pavel; biruit de somn, a căzut de la catul al treilea și a fost ridicat mort.');
INSERT INTO &quot;verses&quot; VALUES (510,20,10,'Dar Pavel a coborât, s-a repezit la el și, luându-l în brațe, a zis: „Nu&lt;f&gt;ⓡ&lt;/f&gt; vă tulburați, căci sufletul lui este în el!”');
INSERT INTO &quot;verses&quot; VALUES (510,20,11,'După ce a urcat, a frânt pâinea, a mâncat și a vorbit îndelung, până în zori; apoi a plecat.');
INSERT INTO &quot;verses&quot; VALUES (510,20,12,'Băiatul a fost adus viu, iar ei au fost mângâiați nespus.');
INSERT INTO &quot;verses&quot; VALUES (510,20,13,'&lt;pb/&gt;Noi ne-am dus la corabie și am plecat spre Asos, de unde urma să-l luăm pe Pavel, pentru că el hotărâse să călătorească &lt;i&gt;până acolo&lt;/i&gt; pe uscat.');
INSERT INTO &quot;verses&quot; VALUES (510,20,14,'Când ne-am întâlnit în Asos, l-am luat și ne-am dus la Mitilene.');
INSERT INTO &quot;verses&quot; VALUES (510,20,15,'De aici am călătorit pe mare și a doua zi am ajuns în dreptul &lt;i&gt;insulei&lt;/i&gt; Chios. În ziua următoare am trecut pe lângă Samos și, [după o oprire în Troghilion], am ajuns a doua zi la Milet.');
INSERT INTO &quot;verses&quot; VALUES (510,20,16,'Pavel se hotărâse să treacă pe lângă Efes, ca să nu mai zăbovească în Asia; căci&lt;f&gt;ⓢ&lt;/f&gt; se grăbea ca, dacă era cu putință, să&lt;f&gt;ⓣ&lt;/f&gt; fie la Ierusalim în Ziua&lt;f&gt;ⓤ&lt;/f&gt; Cincizecimii.');
INSERT INTO &quot;verses&quot; VALUES (510,20,17,'&lt;pb/&gt;Din Milet, Pavel a trimis la Efes și i-a chemat pe prezbiterii bisericii.');
INSERT INTO &quot;verses&quot; VALUES (510,20,18,'Când au ajuns la el, le-a zis: „Știți cum m-am purtat cu voi în tot acest timp, din&lt;f&gt;ⓥ&lt;/f&gt; prima zi în care am pus piciorul în Asia;');
INSERT INTO &quot;verses&quot; VALUES (510,20,19,'cum I-am slujit Domnului cu toată smerenia, cu lacrimi și în încercările de&lt;f&gt;ⓦ&lt;/f&gt; care am avut parte prin uneltirile iudeilor;');
INSERT INTO &quot;verses&quot; VALUES (510,20,20,'cum nu&lt;f&gt;ⓧ&lt;/f&gt; am trecut cu vederea nimic din ce vă era de folos, ci v-am propovăduit și v-am dat învățătură, fie în public, fie prin case,');
INSERT INTO &quot;verses&quot; VALUES (510,20,21,'vestindu-le&lt;f&gt;ⓨ&lt;/f&gt; iudeilor și grecilor întoarcerea&lt;f&gt;ⓩ&lt;/f&gt; la Dumnezeu și credința în Domnul nostru Isus [Hristos].');
INSERT INTO &quot;verses&quot; VALUES (510,20,22,'Și acum iată că, silit&lt;f&gt;ⓐ&lt;/f&gt; de Duhul, mă duc la Ierusalim, fără să știu ce mi se va întâmpla acolo;');
INSERT INTO &quot;verses&quot; VALUES (510,20,23,'numai că Duhul Sfânt&lt;f&gt;ⓑ&lt;/f&gt; mă înștiințează în fiecare cetate că mă așteaptă lanțuri și necazuri.');
INSERT INTO &quot;verses&quot; VALUES (510,20,24,'Dar eu nu&lt;f&gt;ⓒ&lt;/f&gt; pun niciun preț pe viața mea, ci&lt;f&gt;ⓓ&lt;/f&gt; vreau să-mi sfârșesc [cu bucurie] alergarea și&lt;f&gt;ⓔ&lt;/f&gt; slujirea pe&lt;f&gt;ⓕ&lt;/f&gt; care am primit-o de la Domnul Isus — aceea de a vesti Evanghelia harului lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,20,25,'&lt;pb/&gt;Și acum, știu&lt;f&gt;ⓖ&lt;/f&gt; că voi, toți cei în mijlocul cărora am umblat propovăduind Împărăția [lui Dumnezeu], nu-mi veți mai vedea fața.');
INSERT INTO &quot;verses&quot; VALUES (510,20,26,'De aceea vă mărturisesc astăzi că sunt curat de sângele&lt;f&gt;ⓗ&lt;/f&gt; tuturor,');
INSERT INTO &quot;verses&quot; VALUES (510,20,27,'căci nu m-am ferit să vă vestesc tot planul&lt;f&gt;ⓘ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (510,20,28,'Luați seama&lt;f&gt;ⓙ&lt;/f&gt; la voi înșivă și la toată turma peste care v-a&lt;f&gt;ⓚ&lt;/f&gt; pus Duhul Sfânt episcopi&lt;f&gt;[55]&lt;/f&gt;, ca să păstoriți Biserica lui Dumnezeu, pe care&lt;f&gt;ⓛ&lt;/f&gt; a dobândit-o cu&lt;f&gt;ⓜ&lt;/f&gt; însuși sângele Său.');
INSERT INTO &quot;verses&quot; VALUES (510,20,29,'Știu că după plecarea mea se vor&lt;f&gt;ⓝ&lt;/f&gt; vârî între voi lupi înverșunați, care nu vor cruța turma,');
INSERT INTO &quot;verses&quot; VALUES (510,20,30,'și se vor ridica din&lt;f&gt;ⓞ&lt;/f&gt; mijlocul vostru bărbați care vor da învățături strâmbe, ca să-i atragă pe ucenici după ei.');
INSERT INTO &quot;verses&quot; VALUES (510,20,31,'De aceea vegheați și aduceți-vă aminte că, timp de trei&lt;f&gt;ⓟ&lt;/f&gt; ani, zi și noapte, n-am încetat să vă sfătuiesc cu lacrimi pe fiecare dintre voi!');
INSERT INTO &quot;verses&quot; VALUES (510,20,32,'Iar acum, [fraților,] vă încredințez lui Dumnezeu și Cuvântului&lt;f&gt;ⓠ&lt;/f&gt; harului Său, care poate să vă&lt;f&gt;ⓡ&lt;/f&gt; zidească și să vă dea moștenirea&lt;f&gt;ⓢ&lt;/f&gt; împreună cu toți cei sfințiți.');
INSERT INTO &quot;verses&quot; VALUES (510,20,33,'N-am poftit nici argintul&lt;f&gt;ⓣ&lt;/f&gt;, nici aurul, nici îmbrăcămintea cuiva.');
INSERT INTO &quot;verses&quot; VALUES (510,20,34,'Voi înșivă știți că&lt;f&gt;ⓤ&lt;/f&gt; mâinile acestea au muncit pentru nevoile mele și ale celor ce erau cu mine.');
INSERT INTO &quot;verses&quot; VALUES (510,20,35,'Prin toate acestea v-am arătat că, trudind&lt;f&gt;ⓥ&lt;/f&gt; astfel, trebuie să-i ajutați pe cei neputincioși și să vă aduceți aminte de cuvintele Domnului Isus, căci El Însuși a zis: &lt;J&gt;«Este mai ferice să dai decât să primești.»&lt;/J&gt;”');
INSERT INTO &quot;verses&quot; VALUES (510,20,36,'&lt;pb/&gt;După ce a vorbit astfel, a îngenuncheat&lt;f&gt;ⓦ&lt;/f&gt; împreună cu ei toți și s-a rugat.');
INSERT INTO &quot;verses&quot; VALUES (510,20,37,'Au izbucnit toți în plâns, l-au îmbrățișat pe Pavel și l-au sărutat,');
INSERT INTO &quot;verses&quot; VALUES (510,20,38,'fiind îndurerați mai ales din pricina vorbei pe care le-o spusese, cum că nu-i vor mai vedea fața. Și l-au însoțit până la corabie.');
INSERT INTO &quot;verses&quot; VALUES (510,21,1,'&lt;pb/&gt;După ce ne-am smuls &lt;i&gt;din brațele&lt;/i&gt; lor, ne-am îmbarcat și am plecat direct la Cos, a doua zi la Rodos, iar de acolo la Patara.');
INSERT INTO &quot;verses&quot; VALUES (510,21,2,'Am găsit o corabie care avea să treacă &lt;i&gt;marea&lt;/i&gt; spre Fenicia, ne-am urcat în ea și am plecat.');
INSERT INTO &quot;verses&quot; VALUES (510,21,3,'Am trecut prin fața &lt;i&gt;insulei&lt;/i&gt; Cipru și, lăsând-o în partea stângă, ne-am urmat drumul spre Siria și am coborât în Tir, căci acolo trebuia să fie descărcată marfa corabiei.');
INSERT INTO &quot;verses&quot; VALUES (510,21,4,'I-am găsit pe ucenici și am rămas șapte zile acolo. Ei&lt;f&gt;ⓧ&lt;/f&gt; îi ziceau lui Pavel, prin Duhul, să nu urce la Ierusalim,');
INSERT INTO &quot;verses&quot; VALUES (510,21,5,'dar, când ni s-au împlinit zilele, am plecat mai departe. &lt;i&gt;Ucenicii&lt;/i&gt; ne-au însoțit toți, cu femeile și copiii, până afară din cetate. Am îngenuncheat&lt;f&gt;ⓨ&lt;/f&gt; pe țărm și ne-am rugat,');
INSERT INTO &quot;verses&quot; VALUES (510,21,6,'apoi ne-am luat rămas-bun; noi am urcat în corabie, iar ei s-au întors la casele lor&lt;f&gt;ⓩ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,21,7,'&lt;pb/&gt;După ce ne-am sfârșit călătoria pe mare, am plecat din Tir la Ptolemais. I-am salutat pe frați și am rămas la ei o zi.');
INSERT INTO &quot;verses&quot; VALUES (510,21,8,'A doua zi am plecat și am ajuns la Cezareea. Am intrat în casa lui Filip, evanghelistul&lt;f&gt;ⓐ&lt;/f&gt;, unul&lt;f&gt;ⓑ&lt;/f&gt; din cei șapte, și am rămas la el.');
INSERT INTO &quot;verses&quot; VALUES (510,21,9,'El avea patru fete fecioare, care&lt;f&gt;ⓒ&lt;/f&gt; profețeau.');
INSERT INTO &quot;verses&quot; VALUES (510,21,10,'Eram de mai multe zile acolo, când s-a coborât din Iudeea un profet numit Agab&lt;f&gt;ⓓ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,21,11,'A venit la noi, a luat brâul lui Pavel, și-a legat picioarele și mâinile și a zis: „Iată ce zice Duhul Sfânt: «Așa&lt;f&gt;ⓔ&lt;/f&gt; îl vor lega iudeii în Ierusalim pe bărbatul al căruia este brâul acesta și-l vor da în mâinile neamurilor.»”');
INSERT INTO &quot;verses&quot; VALUES (510,21,12,'Când am auzit aceste &lt;i&gt;cuvinte&lt;/i&gt;, atât noi, cât și cei din partea locului l-am rugat pe Pavel să nu se suie la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (510,21,13,'Atunci, Pavel a răspuns: „Ce&lt;f&gt;ⓕ&lt;/f&gt; faceți? De ce plângeți și-mi sfâșiați inima? Eu sunt gata nu numai să fiu legat, ci chiar să și mor în Ierusalim pentru Numele Domnului Isus!”');
INSERT INTO &quot;verses&quot; VALUES (510,21,14,'Fiindcă el nu se lăsa înduplecat, noi n-am mai stăruit, ci am zis: „Facă-se voia&lt;f&gt;ⓖ&lt;/f&gt; Domnului!”');
INSERT INTO &quot;verses&quot; VALUES (510,21,15,'&lt;pb/&gt;După acele zile, ne-am pregătit și am urcat la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (510,21,16,'Câțiva ucenici din Cezareea ne-au însoțit și ne-au dus să fim găzduiți la un anume Mnason din Cipru, un vechi ucenic.');
INSERT INTO &quot;verses&quot; VALUES (510,21,17,'&lt;pb/&gt;Când&lt;f&gt;ⓗ&lt;/f&gt; am ajuns la Ierusalim, frații ne-au primit cu bucurie.');
INSERT INTO &quot;verses&quot; VALUES (510,21,18,'A doua zi, Pavel a mers cu noi la Iacov&lt;f&gt;ⓘ&lt;/f&gt;, și toți prezbiterii au mers &lt;i&gt;acolo&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,21,19,'După ce i-a salutat, le-a istorisit&lt;f&gt;ⓙ&lt;/f&gt; amănunțit ce făcuse Dumnezeu printre neamuri prin&lt;f&gt;ⓚ&lt;/f&gt; slujirea lui.');
INSERT INTO &quot;verses&quot; VALUES (510,21,20,'Când au auzit ei &lt;i&gt;acestea&lt;/i&gt;, au dat slavă lui Dumnezeu, apoi i-au zis: „Vezi, frate, câte mii de iudei au crezut, și toți sunt zeloși&lt;f&gt;ⓛ&lt;/f&gt; pentru Lege!');
INSERT INTO &quot;verses&quot; VALUES (510,21,21,'Aceștia au auzit despre tine că-i înveți pe toți iudeii &lt;i&gt;care trăiesc&lt;/i&gt; printre neamuri să se lepede de Moise, zicându-le să nu-și circumcidă copiii și să nu mai țină tradițiile.');
INSERT INTO &quot;verses&quot; VALUES (510,21,22,'Ce este &lt;i&gt;de făcut&lt;/i&gt;? Negreșit, [se vor aduna mulți, fiindcă] vor auzi că ai venit.');
INSERT INTO &quot;verses&quot; VALUES (510,21,23,'Fă ce-ți spunem: sunt aici, cu noi, patru bărbați care au făcut o juruință!');
INSERT INTO &quot;verses&quot; VALUES (510,21,24,'Ia-i cu tine, curățește-te împreună cu ei și cheltuiește tu pentru ei ca să-și radă&lt;f&gt;ⓜ&lt;/f&gt; capul! Și astfel toți vor ști că nimic din ce au auzit despre tine nu este &lt;i&gt;adevărat&lt;/i&gt;, ci că și tu păzești Legea.');
INSERT INTO &quot;verses&quot; VALUES (510,21,25,'Cu privire la neamurile care au crezut, noi&lt;f&gt;ⓝ&lt;/f&gt; am hotărât și le-am scris să se ferească de carnea jertfită idolilor, de sânge, de &lt;i&gt;animale&lt;/i&gt; sugrumate și de desfrâu.”');
INSERT INTO &quot;verses&quot; VALUES (510,21,26,'A doua zi, Pavel i-a luat pe bărbații &lt;i&gt;aceia&lt;/i&gt; și, după ce s-a curățit împreună cu ei, a&lt;f&gt;ⓞ&lt;/f&gt; intrat în Templu ca să anunțe&lt;f&gt;ⓟ&lt;/f&gt; împlinirea zilelor curățirii, când avea să fie adusă jertfa pentru fiecare dintre ei.');
INSERT INTO &quot;verses&quot; VALUES (510,21,27,'&lt;pb/&gt;Când cele șapte zile erau pe sfârșite, iudeii&lt;f&gt;ⓠ&lt;/f&gt; din Asia, văzându-l &lt;i&gt;pe Pavel&lt;/i&gt; în Templu, au întărâtat toată mulțimea, au&lt;f&gt;ⓡ&lt;/f&gt; pus mâna pe el');
INSERT INTO &quot;verses&quot; VALUES (510,21,28,'și au început să strige: „Bărbați israeliți, dați o mână de ajutor! Iată-l pe omul care&lt;f&gt;ⓢ&lt;/f&gt; pretutindeni și tuturor dă învățături împotriva poporului, împotriva Legii și împotriva locului acestuia; ba încă a adus și greci în Templu și a pângărit acest loc sfânt.”');
INSERT INTO &quot;verses&quot; VALUES (510,21,29,'Căci îl văzuseră mai înainte pe Trofim&lt;f&gt;ⓣ&lt;/f&gt;, efeseanul, împreună cu el în cetate și credeau că Pavel îl adusese în Templu.');
INSERT INTO &quot;verses&quot; VALUES (510,21,30,'Toată cetatea s-a pus în mișcare&lt;f&gt;ⓤ&lt;/f&gt; și mulțimea a dat năvală din toate părțile. Au pus mâna pe Pavel, l-au târât afară din Templu și au zăvorât porțile îndată.');
INSERT INTO &quot;verses&quot; VALUES (510,21,31,'Pe când încercau să-l omoare, a ajuns vestea la tribunul cohortei că tot Ierusalimul este în fierbere.');
INSERT INTO &quot;verses&quot; VALUES (510,21,32,'Acesta&lt;f&gt;ⓥ&lt;/f&gt; a luat îndată soldați și centurioni și a alergat la ei. Văzându-i pe tribun și pe soldați, au încetat să-l mai bată pe Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,21,33,'Atunci, tribunul s-a apropiat, l-a&lt;f&gt;ⓦ&lt;/f&gt; arestat și a poruncit să fie legat cu două lanțuri. Apoi a întrebat cine este și ce a făcut.');
INSERT INTO &quot;verses&quot; VALUES (510,21,34,'Dar în mulțime unii strigau una, alții alta. Fiindcă nu putea afla nimic sigur din pricina zarvei, a poruncit să fie dus în fortăreață.');
INSERT INTO &quot;verses&quot; VALUES (510,21,35,'Când a ajuns pe trepte, a trebuit să fie dus de soldați din pricina mulțimii dezlănțuite,');
INSERT INTO &quot;verses&quot; VALUES (510,21,36,'căci puhoiul de oameni venea din urmă, strigând: „Omoară-l&lt;f&gt;ⓧ&lt;/f&gt;!”');
INSERT INTO &quot;verses&quot; VALUES (510,21,37,'&lt;pb/&gt;Când să intre în fortăreață, Pavel i-a spus tribunului: „Îmi îngădui să-ți spun ceva?” Acesta i-a zis: „Știi grecește?');
INSERT INTO &quot;verses&quot; VALUES (510,21,38,'Nu&lt;f&gt;ⓨ&lt;/f&gt; cumva ești tu egipteanul care s-a răsculat cu câteva zile în urmă și care i-a scos în pustiu pe cei patru mii de sicari&lt;f&gt;[56]&lt;/f&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (510,21,39,'Pavel a spus: „Eu&lt;f&gt;ⓩ&lt;/f&gt; sunt iudeu din Tarsul Ciliciei, cetățean al unei cetăți nu fără însemnătate. Te rog, dă-mi voie să vorbesc poporului!”');
INSERT INTO &quot;verses&quot; VALUES (510,21,40,'După ce i-a dat voie, Pavel a stat în picioare pe trepte și a&lt;f&gt;ⓐ&lt;/f&gt; făcut semn poporului cu mâna. S-a făcut liniște deplină și Pavel le-a vorbit în limba aramaică&lt;f&gt;[57]&lt;/f&gt;, zicând:');
INSERT INTO &quot;verses&quot; VALUES (510,22,1,'&lt;pb/&gt;„Fraților&lt;f&gt;ⓑ&lt;/f&gt; și părinților, ascultați acum cuvântul meu de apărare față de voi!”');
INSERT INTO &quot;verses&quot; VALUES (510,22,2,'Când au auzit că le vorbește în limba aramaică, au făcut mai multă liniște. El a continuat:');
INSERT INTO &quot;verses&quot; VALUES (510,22,3,'„Eu&lt;f&gt;ⓒ&lt;/f&gt; sunt iudeu, născut în Tarsul Ciliciei, dar am fost crescut în cetatea aceasta. Am fost educat cu strictețe în&lt;f&gt;ⓓ&lt;/f&gt; Legea strămoșească, la&lt;f&gt;ⓔ&lt;/f&gt; picioarele lui Gamaliel&lt;f&gt;ⓕ&lt;/f&gt;, și am fost&lt;f&gt;ⓖ&lt;/f&gt; plin de râvnă pentru Dumnezeu, cum&lt;f&gt;ⓗ&lt;/f&gt; sunteți voi toți astăzi.');
INSERT INTO &quot;verses&quot; VALUES (510,22,4,'Am&lt;f&gt;ⓘ&lt;/f&gt; prigonit până la moarte această Cale, am legat și am aruncat în temniță bărbați și femei;');
INSERT INTO &quot;verses&quot; VALUES (510,22,5,'marele-preot și tot&lt;f&gt;ⓙ&lt;/f&gt; sfatul bătrânilor îmi sunt martori. Am luat de la&lt;f&gt;ⓚ&lt;/f&gt; ei scrisori și pentru frații din Damasc și am plecat să-i aduc legați la Ierusalim pe cei ce erau acolo, ca să fie pedepsiți.');
INSERT INTO &quot;verses&quot; VALUES (510,22,6,'&lt;pb/&gt;Pe drum&lt;f&gt;ⓛ&lt;/f&gt;, când mă apropiam de Damasc, pe la amiază, o lumină puternică din cer a strălucit deodată în jurul meu.');
INSERT INTO &quot;verses&quot; VALUES (510,22,7,'Am căzut la pământ și am auzit un glas care-mi zicea: &lt;J&gt;«Saule, Saule, de ce Mă prigonești?»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,22,8,'«Cine ești, Doamne?», am răspuns eu. Și El mi-a zis: &lt;J&gt;«Eu sunt Isus din Nazaret, pe care-L prigonești.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,22,9,'Cei&lt;f&gt;ⓜ&lt;/f&gt; ce erau cu mine au văzut lumina [și au fost cuprinși de teamă], dar glasul Celui ce-mi vorbea nu l-au auzit.');
INSERT INTO &quot;verses&quot; VALUES (510,22,10,'Atunci, am întrebat: «Ce să fac, Doamne?» &lt;J&gt;«Ridică-te»&lt;/J&gt;, mi-a răspuns Domnul, &lt;J&gt;«du-te în Damasc și acolo ți se va spune tot ce ți-a fost rânduit să faci!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,22,11,'Cum nu vedeam nimic din pricina strălucirii acelei lumini, am ajuns în Damasc dus de mână de către cei ce erau cu mine.');
INSERT INTO &quot;verses&quot; VALUES (510,22,12,'&lt;pb/&gt;Un&lt;f&gt;ⓝ&lt;/f&gt; anume Anania, bărbat evlavios după Lege, cu nume bun&lt;f&gt;ⓞ&lt;/f&gt; printre toți iudeii&lt;f&gt;ⓟ&lt;/f&gt; care locuiesc [în Damasc],');
INSERT INTO &quot;verses&quot; VALUES (510,22,13,'a venit, a stat lângă mine și mi-a zis: «Frate Saul, recapătă-ți vederea!» Chiar în clipa aceea mi-am recăpătat vederea și &lt;i&gt;am putut&lt;/i&gt; să-l văd.');
INSERT INTO &quot;verses&quot; VALUES (510,22,14,'El mi-a zis: «Dumnezeul&lt;f&gt;ⓠ&lt;/f&gt; părinților noștri te-a&lt;f&gt;ⓡ&lt;/f&gt; ales să cunoști voia Lui, să-L&lt;f&gt;ⓢ&lt;/f&gt; vezi pe&lt;f&gt;ⓣ&lt;/f&gt; Cel Drept și să&lt;f&gt;ⓤ&lt;/f&gt; auzi cuvânt din gura Lui;');
INSERT INTO &quot;verses&quot; VALUES (510,22,15,'căci&lt;f&gt;ⓥ&lt;/f&gt; Îi vei fi martor înaintea tuturor oamenilor cu privire la cele pe care&lt;f&gt;ⓦ&lt;/f&gt; le-ai văzut și le-ai auzit.');
INSERT INTO &quot;verses&quot; VALUES (510,22,16,'Și acum, ce mai aștepți? Ridică-te, primește botezul și fii spălat&lt;f&gt;ⓧ&lt;/f&gt; de păcatele tale, chemând&lt;f&gt;ⓨ&lt;/f&gt; Numele [Domnului]!»');
INSERT INTO &quot;verses&quot; VALUES (510,22,17,'&lt;pb/&gt;După ce&lt;f&gt;ⓩ&lt;/f&gt; m-am întors la Ierusalim, pe când mă rugam în Templu, am căzut în extaz');
INSERT INTO &quot;verses&quot; VALUES (510,22,18,'și L-am&lt;f&gt;ⓐ&lt;/f&gt; văzut &lt;i&gt;pe Domnul&lt;/i&gt;, care-mi zicea: &lt;J&gt;«Repede! Ieși degrabă&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;din Ierusalim, căci nu vor primi mărturisirea ta despre Mine!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,22,19,'Și am zis: «Doamne, ei&lt;f&gt;ⓒ&lt;/f&gt; știu că eu îi întemnițam și-i băteam&lt;f&gt;ⓓ&lt;/f&gt; prin sinagogi pe cei ce cred în Tine');
INSERT INTO &quot;verses&quot; VALUES (510,22,20,'și că&lt;f&gt;ⓔ&lt;/f&gt;, atunci când era vărsat sângele lui Ștefan, martorul Tău, eram și eu de față, încuviințând&lt;f&gt;ⓕ&lt;/f&gt; și păzind hainele celor ce-l omorau!»');
INSERT INTO &quot;verses&quot; VALUES (510,22,21,'Iar El mi-a zis: &lt;J&gt;«Du-te, căci&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;te voi trimite departe, la neamuri…»&lt;/J&gt;”');
INSERT INTO &quot;verses&quot; VALUES (510,22,22,'&lt;pb/&gt;Ei l-au ascultat până la cuvântul acesta, iar apoi și-au ridicat glasul, zicând: „Ia&lt;f&gt;ⓗ&lt;/f&gt; de pe fața pământului pe un asemenea om! Nu merită să&lt;f&gt;ⓘ&lt;/f&gt; trăiască!”');
INSERT INTO &quot;verses&quot; VALUES (510,22,23,'Fiindcă ei strigau, își smulgeau hainele &lt;i&gt;de pe ei&lt;/i&gt; și aruncau țărână în văzduh,');
INSERT INTO &quot;verses&quot; VALUES (510,22,24,'tribunul a poruncit să-l ducă pe Pavel în fortăreață și să fie cercetat sub lovituri de bici, ca să afle de ce strigau astfel împotriva lui.');
INSERT INTO &quot;verses&quot; VALUES (510,22,25,'În timp ce-l întindeau ca să fie biciuit, Pavel i-a zis centurionului care era de față: „Vă&lt;f&gt;ⓙ&lt;/f&gt; este îngăduit să bateți un &lt;i&gt;cetățean&lt;/i&gt; roman care nici măcar nu a fost judecat?”');
INSERT INTO &quot;verses&quot; VALUES (510,22,26,'Auzind &lt;i&gt;aceasta&lt;/i&gt;, centurionul a mers să-i dea de știre tribunului, zicând: „Ce ai de gând să faci? Omul acesta este &lt;i&gt;cetățean&lt;/i&gt; roman.”');
INSERT INTO &quot;verses&quot; VALUES (510,22,27,'Tribunul s-a apropiat și i-a zis lui Pavel: „Spune-mi, ești &lt;i&gt;cetățean&lt;/i&gt; roman?” „Da”, i-a răspuns el.');
INSERT INTO &quot;verses&quot; VALUES (510,22,28,'Tribunul a zis: „Eu am dobândit cetățenia aceasta cu o sumă mare de bani.” Pavel a spus: „Eu o am chiar prin naștere.”');
INSERT INTO &quot;verses&quot; VALUES (510,22,29,'Îndată, cei ce urmau să-l cerceteze l-au lăsat în pace. Tribunului chiar i s-a făcut teamă când a aflat că &lt;i&gt;Pavel&lt;/i&gt; este &lt;i&gt;cetățean&lt;/i&gt; roman, fiindcă îl ținuse legat.');
INSERT INTO &quot;verses&quot; VALUES (510,22,30,'&lt;pb/&gt;A doua zi, fiindcă voia să știe adevărul despre învinuirile pe care i le aduceau iudeii, l-a dezlegat și a poruncit să se adune preoții de seamă și tot Sinedriul; apoi, l-a adus pe Pavel și l-a înfățișat înaintea lor.');
INSERT INTO &quot;verses&quot; VALUES (510,23,1,'&lt;pb/&gt;Pavel a privit țintă către Sinedriu și a zis: „Fraților, eu&lt;f&gt;ⓚ&lt;/f&gt; am trăit cu conștiința curată înaintea lui Dumnezeu până în ziua aceasta…”');
INSERT INTO &quot;verses&quot; VALUES (510,23,2,'Marele-preot Anania le-a poruncit celor ce stăteau lângă el să-l&lt;f&gt;ⓛ&lt;/f&gt; lovească peste gură.');
INSERT INTO &quot;verses&quot; VALUES (510,23,3,'Atunci, Pavel i-a zis: „Te va lovi pe tine Dumnezeu, perete văruit! Tu stai și mă judeci după Lege și poruncești&lt;f&gt;ⓜ&lt;/f&gt; să mă lovească împotriva Legii?”');
INSERT INTO &quot;verses&quot; VALUES (510,23,4,'Cei ce stăteau lângă el i-au zis: „&lt;i&gt;Îndrăznești&lt;/i&gt; să-l insulți pe marele-preot al lui Dumnezeu?”');
INSERT INTO &quot;verses&quot; VALUES (510,23,5,'Și Pavel a răspuns: „N-am&lt;f&gt;ⓝ&lt;/f&gt; știut, fraților, că este marele-preot, căci este scris: &lt;e&gt;Pe&lt;/e&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;mai-marele poporului tău să nu-l vorbești de rău.&lt;/e&gt;&lt;f&gt;[58]&lt;/f&gt;”');
INSERT INTO &quot;verses&quot; VALUES (510,23,6,'&lt;pb/&gt;Pavel, știind că unii erau saduchei, iar alții farisei, a strigat în Sinedriu: „Fraților, eu&lt;f&gt;ⓟ&lt;/f&gt; sunt fariseu, fiu de fariseu; din pricina nădejdii&lt;f&gt;ⓠ&lt;/f&gt; în învierea morților sunt judecat!”');
INSERT INTO &quot;verses&quot; VALUES (510,23,7,'Când a spus aceasta, s-a iscat o ceartă între farisei și saduchei, iar adunarea s-a dezbinat;');
INSERT INTO &quot;verses&quot; VALUES (510,23,8,'căci&lt;f&gt;ⓡ&lt;/f&gt; saducheii zic că nu există nici înviere, nici înger, nici duh, pe când fariseii le mărturisesc pe toate.');
INSERT INTO &quot;verses&quot; VALUES (510,23,9,'S-a făcut o mare larmă, iar câțiva cărturari din gruparea fariseilor s-au ridicat și au început o dezbatere aprinsă, zicând: „Noi nu găsim nimic rău&lt;f&gt;ⓢ&lt;/f&gt; la omul acesta; dacă&lt;f&gt;ⓣ&lt;/f&gt; i-a vorbit un duh sau un înger&lt;f&gt;ⓤ&lt;/f&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (510,23,10,'Fiindcă cearta se întețea, iar tribunul se temea ca nu cumva Pavel să fie sfâșiat de ei, le-a poruncit soldaților să coboare, să-l smulgă din mijlocul lor și să-l ducă în fortăreață.');
INSERT INTO &quot;verses&quot; VALUES (510,23,11,'&lt;pb/&gt;În&lt;f&gt;ⓥ&lt;/f&gt; noaptea următoare, Domnul i S-a arătat &lt;i&gt;lui Pavel&lt;/i&gt; și i-a zis: &lt;J&gt;„Curaj, [Pavele,] căci așa cum ai mărturisit despre Mine în Ierusalim, la fel trebuie să mărturisești și în Roma!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,23,12,'&lt;pb/&gt;Când s-a făcut ziuă, iudeii au&lt;f&gt;ⓦ&lt;/f&gt; uneltit și s-au legat cu blestem să nu mănânce, nici să nu bea până când nu-l vor omorî pe Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,23,13,'Cei ce făcuseră uneltirea aceasta erau peste patruzeci.');
INSERT INTO &quot;verses&quot; VALUES (510,23,14,'Ei au mers la preoții de seamă și la bătrâni și le-au zis: „Noi ne-am legat cu blestem să nu gustăm nimic până nu-l vom omorî pe Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,23,15,'Acum deci voi, împreună cu Sinedriul, cereți-i tribunului să-l aducă [mâine] înaintea voastră, ca și cum ați vrea să-i cercetați mai amănunțit &lt;i&gt;pricina&lt;/i&gt;! Noi suntem pregătiți să-l ucidem înainte să ajungă el &lt;i&gt;aici&lt;/i&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (510,23,16,'Auzind de această cursă, fiul surorii lui Pavel s-a dus în fortăreață și i-a dat de știre lui Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,23,17,'Pavel l-a chemat pe unul dintre centurioni și i-a zis: „Condu-l pe tânărul acesta la tribun, fiindcă are ceva să-i spună!”');
INSERT INTO &quot;verses&quot; VALUES (510,23,18,'Acesta l-a luat, l-a dus la tribun și a zis: „Prizonierul Pavel m-a chemat și m-a rugat să-l aduc la tine pe acest tânăr, întrucât are ceva să-ți spună.”');
INSERT INTO &quot;verses&quot; VALUES (510,23,19,'Tribunul l-a luat de mână, l-a tras deoparte și l-a întrebat: „Ce ai să-mi spui?”');
INSERT INTO &quot;verses&quot; VALUES (510,23,20,'El a răspuns: „Iudeii&lt;f&gt;ⓧ&lt;/f&gt; s-au sfătuit să te roage să-l aduci mâine pe Pavel înaintea Sinedriului, ca și cum ar vrea să-i cerceteze &lt;i&gt;pricina&lt;/i&gt; mai amănunțit.');
INSERT INTO &quot;verses&quot; VALUES (510,23,21,'Tu să nu le dai crezare, pentru că peste patruzeci dintre ei îl pândesc și s-au legat cu blestem să nu mănânce și să nu bea nimic până nu-l vor ucide; acum sunt pregătiți și așteaptă numai încuviințarea ta.”');
INSERT INTO &quot;verses&quot; VALUES (510,23,22,'Tribunul l-a lăsat pe tânăr &lt;i&gt;să plece&lt;/i&gt; și i-a poruncit: „Să nu spui nimănui că mi-ai dezvăluit aceste lucruri.”');
INSERT INTO &quot;verses&quot; VALUES (510,23,23,'&lt;pb/&gt;Apoi, a chemat doi centurioni și le-a zis: „Până la ceasul al treilea din noapte&lt;f&gt;[59]&lt;/f&gt;, să pregătiți două sute de soldați, șaptezeci de călăreți și două sute de lăncieri ca să meargă până la Cezareea!');
INSERT INTO &quot;verses&quot; VALUES (510,23,24,'Să fie și cai pentru Pavel, ca să încalece și să fie dus teafăr la procuratorul Felix!”');
INSERT INTO &quot;verses&quot; VALUES (510,23,25,'Acestuia i-a scris o scrisoare cu următoarele cuvinte:');
INSERT INTO &quot;verses&quot; VALUES (510,23,26,'„Claudius Lisias către preaalesul procurator Felix, sănătate!');
INSERT INTO &quot;verses&quot; VALUES (510,23,27,'Pe acest&lt;f&gt;ⓨ&lt;/f&gt; bărbat iudeii l-au prins și erau gata să-l omoare, dar eu m-am dus cu soldați și l-am scos din mâinile lor, fiindcă am aflat că este &lt;i&gt;cetățean&lt;/i&gt; roman.');
INSERT INTO &quot;verses&quot; VALUES (510,23,28,'Întrucât am&lt;f&gt;ⓩ&lt;/f&gt; vrut să știu pricina pentru care-l învinuiau, l-am dus înaintea Sinedriului lor.');
INSERT INTO &quot;verses&quot; VALUES (510,23,29,'Am aflat că este învinuit&lt;f&gt;ⓐ&lt;/f&gt; în legătură cu niște lucruri din Legea lor, dar&lt;f&gt;ⓑ&lt;/f&gt; &lt;i&gt;n-am găsit&lt;/i&gt; nicio vină vrednică de moarte sau de lanțuri.');
INSERT INTO &quot;verses&quot; VALUES (510,23,30,'Mi s-a adus&lt;f&gt;ⓒ&lt;/f&gt; însă la cunoștință că [iudeii] au făcut o uneltire împotriva lui; l-am trimis îndată la tine și le-am poruncit&lt;f&gt;ⓓ&lt;/f&gt; acuzatorilor să spună înaintea ta ce au împotriva lui. [Sănătate!]”');
INSERT INTO &quot;verses&quot; VALUES (510,23,31,'&lt;pb/&gt;Soldații, după cum li s-a poruncit, l-au luat pe Pavel și l-au dus noaptea până la Antipatris.');
INSERT INTO &quot;verses&quot; VALUES (510,23,32,'A doua zi, i-au lăsat pe călăreți să-l ducă mai departe, iar ei s-au întors în fortăreață.');
INSERT INTO &quot;verses&quot; VALUES (510,23,33,'Ajunși în Cezareea, &lt;i&gt;călăreții&lt;/i&gt; i-au înmânat procuratorului scrisoarea și i l-au înfățișat și pe Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,23,34,'După ce a citit &lt;i&gt;scrisoarea, procuratorul&lt;/i&gt; a întrebat din ce provincie este &lt;i&gt;Pavel&lt;/i&gt;. Când a aflat că este din Cilicia&lt;f&gt;ⓔ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (510,23,35,'a zis: „Te&lt;f&gt;ⓕ&lt;/f&gt; voi asculta când vor veni acuzatorii tăi.” Și a poruncit să fie închis în palatul&lt;f&gt;ⓖ&lt;/f&gt; lui Irod.');
INSERT INTO &quot;verses&quot; VALUES (510,24,1,'&lt;pb/&gt;După cinci&lt;f&gt;ⓗ&lt;/f&gt; zile, a venit marele-preot&lt;f&gt;ⓘ&lt;/f&gt; Anania cu unii dintre bătrâni și cu un orator &lt;i&gt;numit&lt;/i&gt; Tertullus și a făcut plângere la procurator împotriva lui Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,24,2,'&lt;i&gt;Pavel&lt;/i&gt; a fost chemat, iar Tertullus a început să-l învinuiască, zicând: „Datorită ție, preaalesule Felix, ne bucurăm de o pace deplină și multe reforme s-au făcut pentru neamul acesta prin grija ta.');
INSERT INTO &quot;verses&quot; VALUES (510,24,3,'Întotdeauna și pretutindeni noi le-am primit cu toată recunoștința.');
INSERT INTO &quot;verses&quot; VALUES (510,24,4,'Dar, ca să nu te mai ostenesc, te rog, în bunătatea ta, să asculți puținele noastre &lt;i&gt;cuvinte&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,24,5,'Ne-am dat seama&lt;f&gt;ⓙ&lt;/f&gt; că omul acesta este o ciumă: stârnește răzvrătiri printre toți iudeii din toată lumea, este căpetenia sectei nazarinenilor');
INSERT INTO &quot;verses&quot; VALUES (510,24,6,'și&lt;f&gt;ⓚ&lt;/f&gt; a încercat să pângărească până și Templul. Dar noi am pus mâna pe el. [Am vrut să-l judecăm&lt;f&gt;ⓛ&lt;/f&gt; după Legea noastră,');
INSERT INTO &quot;verses&quot; VALUES (510,24,7,'însă&lt;f&gt;ⓜ&lt;/f&gt; a venit tribunul Lisias, l-a smuls din mâinile noastre cu forța');
INSERT INTO &quot;verses&quot; VALUES (510,24,8,'și le-a poruncit&lt;f&gt;ⓝ&lt;/f&gt; acuzatorilor lui să vină la tine.] Dacă-l vei cerceta, tu însuți vei putea afla de la el toate lucrurile de care îl învinuim noi.”');
INSERT INTO &quot;verses&quot; VALUES (510,24,9,'Iudeii i s-au alăturat, susținând că așa stau lucrurile.');
INSERT INTO &quot;verses&quot; VALUES (510,24,10,'&lt;pb/&gt;Când procuratorul i-a făcut semn să vorbească, Pavel a răspuns: „Fiindcă știu că de mulți ani ești judecător peste neamul acesta, mă voi apăra cu încredere.');
INSERT INTO &quot;verses&quot; VALUES (510,24,11,'După cum poți afla &lt;i&gt;tu însuți&lt;/i&gt;, n-au trecut mai mult de douăsprezece zile de când am urcat la Ierusalim să&lt;f&gt;ⓞ&lt;/f&gt; mă închin.');
INSERT INTO &quot;verses&quot; VALUES (510,24,12,'Nu&lt;f&gt;ⓟ&lt;/f&gt; m-au găsit purtând discuții aprinse sau stârnind mulțimea nici în Templu, nici în sinagogi, nici în cetate.');
INSERT INTO &quot;verses&quot; VALUES (510,24,13,'Așa că nu ți-ar putea dovedi lucrurile de care mă învinuiesc acum.');
INSERT INTO &quot;verses&quot; VALUES (510,24,14,'Îți mărturisesc că slujesc Dumnezeului&lt;f&gt;ⓠ&lt;/f&gt; părinților mei după Calea&lt;f&gt;ⓡ&lt;/f&gt; pe care ei o numesc sectă; eu cred tot ce este scris în Lege&lt;f&gt;ⓢ&lt;/f&gt; și în Profeți');
INSERT INTO &quot;verses&quot; VALUES (510,24,15,'și am&lt;f&gt;ⓣ&lt;/f&gt; aceeași nădejde în Dumnezeu pe care o au și ei — că&lt;f&gt;ⓤ&lt;/f&gt; va fi o înviere a celor drepți și a celor nedrepți.');
INSERT INTO &quot;verses&quot; VALUES (510,24,16,'De aceea mă&lt;f&gt;ⓥ&lt;/f&gt; străduiesc să am tot timpul conștiința curată înaintea lui Dumnezeu și înaintea oamenilor.');
INSERT INTO &quot;verses&quot; VALUES (510,24,17,'După mai mulți ani, am&lt;f&gt;ⓦ&lt;/f&gt; venit să fac milostenii neamului meu și &lt;i&gt;să aduc&lt;/i&gt; jertfe.');
INSERT INTO &quot;verses&quot; VALUES (510,24,18,'Tocmai atunci&lt;f&gt;ⓧ&lt;/f&gt; m-au găsit curățit în Templu, nu cu gloată, nici cu zarvă.');
INSERT INTO &quot;verses&quot; VALUES (510,24,19,'Dar sunt niște iudei din Asia — aceștia&lt;f&gt;ⓨ&lt;/f&gt; ar trebui să se înfățișeze înaintea ta și să mă învinuiască, dacă au ceva împotriva mea.');
INSERT INTO &quot;verses&quot; VALUES (510,24,20,'Sau să spună aceștia de față ce nelegiuire au găsit în mine când am stat înaintea Sinedriului,');
INSERT INTO &quot;verses&quot; VALUES (510,24,21,'în afară de cuvintele pe care le-am strigat în mijlocul lor: «Pentru&lt;f&gt;ⓩ&lt;/f&gt; învierea morților sunt eu judecat astăzi înaintea voastră!»”');
INSERT INTO &quot;verses&quot; VALUES (510,24,22,'&lt;pb/&gt;Felix, care știa în amănunt despre Cale, i-a amânat, zicând: „Mă voi pronunța cu privire la pricina voastră când va veni tribunul&lt;f&gt;ⓐ&lt;/f&gt; Lisias.”');
INSERT INTO &quot;verses&quot; VALUES (510,24,23,'Și i-a poruncit centurionului să-l păzească [pe Pavel], dar să-i lase puțină libertate și să nu oprească pe&lt;f&gt;ⓑ&lt;/f&gt; nimeni dintre ai lui să-i poarte de grijă [sau să vină la el].');
INSERT INTO &quot;verses&quot; VALUES (510,24,24,'&lt;pb/&gt;După câteva zile, a venit Felix cu soția sa, Drusila, care era iudeică; l-a chemat pe Pavel și a ascultat despre credința în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (510,24,25,'Dar, pe când vorbea &lt;i&gt;Pavel&lt;/i&gt; despre dreptate, despre înfrânare și despre judecata viitoare, Felix s-a îngrozit și a zis: „Acum du-te; când voi mai avea timp, te voi chema.”');
INSERT INTO &quot;verses&quot; VALUES (510,24,26,'Totodată, nădăjduia că Pavel îi va da bani&lt;f&gt;ⓒ&lt;/f&gt;, de aceea trimitea și mai des să-l cheme, ca să stea de vorbă cu el.');
INSERT INTO &quot;verses&quot; VALUES (510,24,27,'&lt;pb/&gt;După doi ani, lui Felix i-a urmat Porcius Festus. Felix a vrut&lt;f&gt;ⓓ&lt;/f&gt; să le facă iudeilor o favoare și l-a lăsat pe Pavel în temniță.');
INSERT INTO &quot;verses&quot; VALUES (510,25,1,'&lt;pb/&gt;Festus a venit în provincia sa, iar după trei zile s-a suit de la Cezareea la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (510,25,2,'Preoții de seamă&lt;f&gt;ⓔ&lt;/f&gt; și mai-marii iudeilor au adus plângere împotriva lui Pavel și l-au rugat &lt;i&gt;pe Festus&lt;/i&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,25,3,'să le facă o favoare și să-l trimită &lt;i&gt;pe Pavel&lt;/i&gt; la Ierusalim. Îi întindeau astfel&lt;f&gt;ⓕ&lt;/f&gt; o cursă, ca să-l omoare pe drum.');
INSERT INTO &quot;verses&quot; VALUES (510,25,4,'Festus a răspuns că Pavel este ținut sub pază în Cezareea și că el însuși va merge acolo în curând.');
INSERT INTO &quot;verses&quot; VALUES (510,25,5,'„Deci”, a zis el, „cei mai însemnați dintre voi să coboare împreună cu mine și, dacă&lt;f&gt;ⓖ&lt;/f&gt; este ceva nepotrivit în omul acesta, să-l învinuiască!”');
INSERT INTO &quot;verses&quot; VALUES (510,25,6,'&lt;pb/&gt;Festus a rămas între ei doar opt sau zece zile, apoi s-a coborât la Cezareea. A doua zi a stat pe scaunul de judecată și a poruncit să fie adus Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,25,7,'Când a sosit el, iudeii care veniseră din Ierusalim l-au înconjurat și au adus împotriva lui multe&lt;f&gt;ⓗ&lt;/f&gt; și grave învinuiri, pe care însă nu le puteau dovedi.');
INSERT INTO &quot;verses&quot; VALUES (510,25,8,'În cuvântul &lt;i&gt;său&lt;/i&gt; de apărare, Pavel &lt;i&gt;a zis&lt;/i&gt;: „N-am păcătuit cu nimic nici&lt;f&gt;ⓘ&lt;/f&gt; împotriva Legii iudeilor, nici împotriva Templului, nici împotriva cezarului.”');
INSERT INTO &quot;verses&quot; VALUES (510,25,9,'Festus, care voia&lt;f&gt;ⓙ&lt;/f&gt; să le facă iudeilor o favoare, i-a răspuns lui Pavel: „Vrei&lt;f&gt;ⓚ&lt;/f&gt; să urci la Ierusalim și acolo să fii judecat de mine pentru acestea?”');
INSERT INTO &quot;verses&quot; VALUES (510,25,10,'Pavel a zis: „Eu stau înaintea instanței cezarului și de el trebuie să fiu judecat. Față de iudei n-am săvârșit nicio nedreptate, cum știi foarte bine.');
INSERT INTO &quot;verses&quot; VALUES (510,25,11,'Dacă&lt;f&gt;ⓛ&lt;/f&gt; am făcut vreo nedreptate sau ceva vrednic de moarte, nu mă feresc să mor. Dar dacă nu este &lt;i&gt;adevărat&lt;/i&gt; nimic din învinuirile acestora, nimeni nu poate să mă dea &lt;i&gt;pe mâna&lt;/i&gt; lor. Fac apel la cezar&lt;f&gt;ⓜ&lt;/f&gt;!”');
INSERT INTO &quot;verses&quot; VALUES (510,25,12,'Atunci, Festus, după ce a stat de vorbă cu sfetnicii, a răspuns: „La cezar ai făcut apel, la cezar vei merge!”');
INSERT INTO &quot;verses&quot; VALUES (510,25,13,'&lt;pb/&gt;După câteva zile, regele Agripa și Berenice au sosit la Cezareea, ca să-l salute pe Festus.');
INSERT INTO &quot;verses&quot; VALUES (510,25,14,'Fiindcă au rămas acolo mai multe zile, Festus i-a povestit regelui cazul lui Pavel, zicând: „Felix a lăsat în temniță un&lt;f&gt;ⓝ&lt;/f&gt; bărbat');
INSERT INTO &quot;verses&quot; VALUES (510,25,15,'împotriva&lt;f&gt;ⓞ&lt;/f&gt; căruia, când eram eu în Ierusalim, s-au plâns preoții de seamă și bătrânii iudeilor, cerându-mi să-l osândesc.');
INSERT INTO &quot;verses&quot; VALUES (510,25,16,'Le-am&lt;f&gt;ⓟ&lt;/f&gt; răspuns că la romani nu este obiceiul să fie predat un om învinuit înainte să stea față în față cu acuzatorii lui și să aibă prilejul de a răspunde la învinuiri.');
INSERT INTO &quot;verses&quot; VALUES (510,25,17,'Ei au venit aici și, fără vreo &lt;i&gt;altă&lt;/i&gt; amânare&lt;f&gt;ⓠ&lt;/f&gt;, eu am stat a doua zi pe scaunul de judecată și am poruncit ca omul să fie adus.');
INSERT INTO &quot;verses&quot; VALUES (510,25,18,'Când s-au înfățișat, acuzatorii nu i-au adus nicio vină din lucrurile rele pe care le bănuiam eu.');
INSERT INTO &quot;verses&quot; VALUES (510,25,19,'Aveau cu el doar&lt;f&gt;ⓡ&lt;/f&gt; controverse privitoare la religia lor și la un oarecare Isus, care a murit și despre care Pavel spune că este viu.');
INSERT INTO &quot;verses&quot; VALUES (510,25,20,'Fiindcă nu știam ce să fac în privința acestei controverse, l-am întrebat dacă vrea să meargă la Ierusalim și să fie judecat acolo pentru aceste &lt;i&gt;lucruri&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,25,21,'Dar Pavel a cerut să rămână sub pază, &lt;i&gt;ca să ajungă&lt;/i&gt; la judecata Maiestății Sale&lt;f&gt;[60]&lt;/f&gt;, așa că am poruncit să fie păzit până îl voi trimite la cezar.”');
INSERT INTO &quot;verses&quot; VALUES (510,25,22,'Agripa&lt;f&gt;ⓢ&lt;/f&gt; &lt;i&gt;i-a zis&lt;/i&gt; lui Festus: „Aș vrea să-l aud și eu pe omul &lt;i&gt;acesta&lt;/i&gt;.” „Mâine”, a răspuns &lt;i&gt;el&lt;/i&gt;, „îl vei auzi.”');
INSERT INTO &quot;verses&quot; VALUES (510,25,23,'&lt;pb/&gt;A doua zi, Agripa și Berenice au venit cu mare fast și au intrat în sala de judecată, împreună cu tribunii și cu bărbații cei mai de seamă ai cetății, iar la porunca lui Festus a fost adus Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,25,24,'Festus a zis: „Rege Agripa și voi toți cei adunați împreună cu noi, uitați-vă la &lt;i&gt;omul&lt;/i&gt; acesta, de care toată&lt;f&gt;ⓣ&lt;/f&gt; mulțimea iudeilor mi s-a plâns și în Ierusalim, și aici, strigând că nu trebuie să&lt;f&gt;ⓤ&lt;/f&gt; mai trăiască!');
INSERT INTO &quot;verses&quot; VALUES (510,25,25,'Eu însă înțeleg că n-a făcut&lt;f&gt;ⓥ&lt;/f&gt; nimic vrednic de moarte, dar, fiindcă&lt;f&gt;ⓦ&lt;/f&gt; el însuși a făcut apel la Augustus, am hotărât să-l trimit.');
INSERT INTO &quot;verses&quot; VALUES (510,25,26,'N-am nimic temeinic de scris suveranului cu privire la el, de aceea l-am adus înaintea voastră, și mai ales înaintea ta, rege Agripa, pentru ca în urma cercetării să am ce scrie.');
INSERT INTO &quot;verses&quot; VALUES (510,25,27,'Căci mi se pare fără noimă să-i trimit un întemnițat fără să arăt acuzele care i se aduc.”');
INSERT INTO &quot;verses&quot; VALUES (510,26,1,'&lt;pb/&gt;Agripa i-a zis lui Pavel: „Îți este îngăduit să te aperi.” Atunci, Pavel a întins mâna și a început să se apere &lt;i&gt;astfel&lt;/i&gt;:');
INSERT INTO &quot;verses&quot; VALUES (510,26,2,'„Mă socotesc fericit, rege Agripa, că pot să mă apăr astăzi înaintea ta cu privire la toate cele de care mă învinuiesc iudeii,');
INSERT INTO &quot;verses&quot; VALUES (510,26,3,'căci tu cunoști foarte bine toate obiceiurile și disputele lor; de aceea, te rog să mă asculți cu răbdare.');
INSERT INTO &quot;verses&quot; VALUES (510,26,4,'&lt;pb/&gt;Viața mea, petrecută de la început, încă din tinerețe, în mijlocul neamului meu, în Ierusalim, le este cunoscută tuturor iudeilor.');
INSERT INTO &quot;verses&quot; VALUES (510,26,5,'Ei mă știu de multă vreme și, dacă vor, &lt;i&gt;pot&lt;/i&gt; da mărturie că am trăit după cea mai strictă grupare&lt;f&gt;ⓧ&lt;/f&gt; a religiei noastre, ca fariseu.');
INSERT INTO &quot;verses&quot; VALUES (510,26,6,'Și&lt;f&gt;ⓨ&lt;/f&gt; acum sunt dat în judecată pentru nădejdea făgăduinței&lt;f&gt;ⓩ&lt;/f&gt; pe care a făcut-o Dumnezeu părinților noștri');
INSERT INTO &quot;verses&quot; VALUES (510,26,7,'și pe care cele douăsprezece&lt;f&gt;ⓐ&lt;/f&gt; seminții ale noastre, slujind cu râvnă lui Dumnezeu zi&lt;f&gt;ⓑ&lt;/f&gt; și noapte, nădăjduiesc să o vadă împlinită. Pentru această nădejde&lt;f&gt;ⓒ&lt;/f&gt;, o, rege, sunt învinuit eu de iudei!');
INSERT INTO &quot;verses&quot; VALUES (510,26,8,'De ce vi se pare de necrezut că Dumnezeu învie morții?');
INSERT INTO &quot;verses&quot; VALUES (510,26,9,'Și&lt;f&gt;ⓓ&lt;/f&gt; eu credeam că trebuie să fac multe împotriva Numelui lui Isus din Nazaret,');
INSERT INTO &quot;verses&quot; VALUES (510,26,10,'ceea ce&lt;f&gt;ⓔ&lt;/f&gt; am și făcut în Ierusalim. După ce am primit autoritate de la&lt;f&gt;ⓕ&lt;/f&gt; preoții de seamă, i-am aruncat în temniță pe mulți sfinți, iar când erau osândiți la moarte, îmi dădeam votul &lt;i&gt;împotriva lor&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,26,11,'I-am pedepsit adesea în toate sinagogile&lt;f&gt;ⓖ&lt;/f&gt; și mă străduiam să-i fac să hulească. În furia mea nebună împotriva lor, îi prigoneam până și în cetățile din afară&lt;f&gt;[61]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,26,12,'&lt;pb/&gt;Pentru aceasta&lt;f&gt;ⓗ&lt;/f&gt;, am mers la Damasc cu autoritate și însărcinare de la preoții de seamă.');
INSERT INTO &quot;verses&quot; VALUES (510,26,13,'Pe la amiază, o, rege, am văzut pe drum o lumină din cer, mai strălucitoare decât lumina soarelui, care ne-a învăluit pe mine și pe cei ce mă însoțeau.');
INSERT INTO &quot;verses&quot; VALUES (510,26,14,'Am căzut cu toții la pământ, iar eu am auzit un glas care-mi zicea în limba aramaică: &lt;J&gt;«Saule, Saule, de ce Mă prigonești? E în zadar să dai cu piciorul într-o țepușă!»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,26,15,'Eu am întrebat: «Cine ești, Doamne?» Și Domnul a zis: &lt;J&gt;«Eu sunt Isus, pe care-L prigonești.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,26,16,'&lt;J&gt;Dar ridică-te și stai în picioare, căci pentru aceasta M-am arătat ție: ca&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;să te rânduiesc slujitor și martor al celor pe care le-ai văzut și al celor pe care ți le voi arăta!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,26,17,'&lt;J&gt;Eu te izbăvesc de poporul și de neamurile la&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;care te trimit,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,26,18,'&lt;J&gt;ca să&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;le deschizi ochii, să&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;se întoarcă de la întuneric la lumină și de sub stăpânirea Satanei la Dumnezeu și să&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;primească iertare de păcate și parte&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;împreună cu cei&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;sfințiți prin credința în Mine.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,26,19,'&lt;pb/&gt;Și astfel, rege Agripa, n-am fost neascultător față de vedenia cerească,');
INSERT INTO &quot;verses&quot; VALUES (510,26,20,'ci, mai întâi celor din Damasc și din Ierusalim, apoi în tot ținutul Iudeei și neamurilor, le-am&lt;f&gt;ⓟ&lt;/f&gt; propovăduit să se pocăiască, să se întoarcă la Dumnezeu și să facă fapte&lt;f&gt;ⓠ&lt;/f&gt; vrednice de pocăință.');
INSERT INTO &quot;verses&quot; VALUES (510,26,21,'Iată de ce iudeii&lt;f&gt;ⓡ&lt;/f&gt; au pus mâna pe mine în Templu și au încercat să mă omoare.');
INSERT INTO &quot;verses&quot; VALUES (510,26,22,'Dar, cu ajutorul lui Dumnezeu, am ajuns până în ziua aceasta să mărturisesc la mic și mare și nu spun altceva decât ce&lt;f&gt;ⓢ&lt;/f&gt; au spus profeții și Moise&lt;f&gt;ⓣ&lt;/f&gt; că se va împlini:');
INSERT INTO &quot;verses&quot; VALUES (510,26,23,'anume că&lt;f&gt;ⓤ&lt;/f&gt; Hristosul va pătimi și că&lt;f&gt;ⓥ&lt;/f&gt;, fiind cel dintâi care va învia din morți, va&lt;f&gt;ⓦ&lt;/f&gt; vesti lumină atât poporului, cât și neamurilor.”');
INSERT INTO &quot;verses&quot; VALUES (510,26,24,'&lt;pb/&gt;Pe când se apăra el astfel, Festus a zis cu glas tare: „Pavele, ești&lt;f&gt;ⓧ&lt;/f&gt; nebun! Învățătura ta cea multă te face să dai în nebunie!”');
INSERT INTO &quot;verses&quot; VALUES (510,26,25,'„Nu sunt nebun, preaalesule Festus”, a răspuns Pavel, „dimpotrivă, rostesc cuvinte adevărate și înțelepte.');
INSERT INTO &quot;verses&quot; VALUES (510,26,26,'Regele știe aceste lucruri și de aceea îi vorbesc deschis, căci sunt convins că nimic nu i-a rămas ascuns, fiindcă ele nu s-au petrecut într-un ungher.');
INSERT INTO &quot;verses&quot; VALUES (510,26,27,'Crezi în profeți, rege Agripa? Știu că da.”');
INSERT INTO &quot;verses&quot; VALUES (510,26,28,'Și Agripa &lt;i&gt;i-a zis&lt;/i&gt; lui Pavel: „Lesne &lt;i&gt;mai vrei&lt;/i&gt; tu să mă convingi să devin creștin!”');
INSERT INTO &quot;verses&quot; VALUES (510,26,29,'Pavel a răspuns: „Mai lesne sau mai cu anevoie&lt;f&gt;ⓨ&lt;/f&gt;, eu mă rog lui Dumnezeu ca nu numai tu, ci toți cei ce mă ascultă astăzi să fie și ei cum sunt eu — desigur, fără lanțurile acestea.”');
INSERT INTO &quot;verses&quot; VALUES (510,26,30,'&lt;pb/&gt;Regele, procuratorul, Berenice și toți cei ce stătuseră împreună cu ei s-au ridicat.');
INSERT INTO &quot;verses&quot; VALUES (510,26,31,'Pe când se îndepărtau, vorbeau&lt;f&gt;ⓩ&lt;/f&gt; unii cu alții, zicând: „Omul acesta n-a făcut nimic vrednic de moarte sau de închisoare.”');
INSERT INTO &quot;verses&quot; VALUES (510,26,32,'Agripa i-a zis lui Festus: „Omul acesta putea fi eliberat dacă&lt;f&gt;ⓐ&lt;/f&gt; n-ar fi făcut apel la cezar.”');
INSERT INTO &quot;verses&quot; VALUES (510,27,1,'&lt;pb/&gt;După ce s-a luat hotărârea&lt;f&gt;ⓑ&lt;/f&gt; să plecăm pe mare în Italia, Pavel și alți câțiva întemnițați au fost predați în grija unui centurion numit Iulius, din cohorta Augusta.');
INSERT INTO &quot;verses&quot; VALUES (510,27,2,'Ne-am îmbarcat pe o corabie de la Adramitis, care urma să navigheze de-a lungul coastei Asiei, și am ieșit în larg. Cu noi era și Aristarh&lt;f&gt;ⓒ&lt;/f&gt;, un macedonean din Tesalonic.');
INSERT INTO &quot;verses&quot; VALUES (510,27,3,'A doua zi am ajuns la Sidon, iar Iulius, care îl trata&lt;f&gt;ⓓ&lt;/f&gt; pe Pavel cu omenie, i-a îngăduit să meargă la prieteni ca să primească îngrijiri.');
INSERT INTO &quot;verses&quot; VALUES (510,27,4,'După ce am plecat de acolo, am plutit la adăpostul &lt;i&gt;insulei&lt;/i&gt; Cipru, pentru că vânturile erau potrivnice.');
INSERT INTO &quot;verses&quot; VALUES (510,27,5,'După ce am străbătut marea prin dreptul Ciliciei și Pamfiliei, am ajuns la Mira, în Licia.');
INSERT INTO &quot;verses&quot; VALUES (510,27,6,'Acolo, centurionul a găsit o corabie din Alexandria care mergea spre Italia și ne-a urcat în ea.');
INSERT INTO &quot;verses&quot; VALUES (510,27,7,'Timp de mai multe zile am navigat încet și am ajuns cu greu aproape de Cnidos; fiindcă vântul &lt;i&gt;potrivnic&lt;/i&gt; nu contenea, am plutit la adăpostul &lt;i&gt;insulei&lt;/i&gt; Creta, pe lângă Salmone.');
INSERT INTO &quot;verses&quot; VALUES (510,27,8,'Am trecut anevoie pe lângă ea și am ajuns într-un loc numit Limanuri Bune, de care era aproape cetatea Lasaia.');
INSERT INTO &quot;verses&quot; VALUES (510,27,9,'&lt;pb/&gt;Trecuse destul de multă vreme și călătoria pe mare devenea primejdioasă, pentru că&lt;f&gt;ⓔ&lt;/f&gt; trecuse chiar și postul&lt;f&gt;[62]&lt;/f&gt;. De aceea, Pavel &lt;i&gt;le&lt;/i&gt;-a dat un sfat,');
INSERT INTO &quot;verses&quot; VALUES (510,27,10,'zicând: „Oameni &lt;i&gt;buni&lt;/i&gt;, după cum văd eu, călătoria va fi cu vătămare și cu multă pagubă nu doar pentru încărcătură și corabie, ci și pentru viețile noastre.”');
INSERT INTO &quot;verses&quot; VALUES (510,27,11,'Dar centurionul s-a încrezut mai mult în căpitan și în proprietarul corabiei decât în spusele lui Pavel.');
INSERT INTO &quot;verses&quot; VALUES (510,27,12,'Și fiindcă limanul nu era prielnic pentru iernat, cei mai mulți au fost de părere să plece de acolo pentru ca, dacă va fi cu putință, să ajungă să ierneze la Fenix, un liman al Cretei, cu vedere spre nord-vest și sud-vest.');
INSERT INTO &quot;verses&quot; VALUES (510,27,13,'&lt;pb/&gt;Începuse să sufle ușor vântul din sud și, crezând că pot ajunge la țintă, au ridicat ancorele și s-au ținut de coasta Cretei.');
INSERT INTO &quot;verses&quot; VALUES (510,27,14,'Dar nu după multă vreme, s-a dezlănțuit asupra &lt;i&gt;insulei&lt;/i&gt; un vânt aducător de furtună, numit Eurachilon.');
INSERT INTO &quot;verses&quot; VALUES (510,27,15,'Deoarece corabia a fost învăluită și n-a mai putut ține piept vântului, ne-am lăsat purtați &lt;i&gt;în derivă&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,27,16,'Trecând repede la adăpostul unei insule numite Cauda, abia am putut să legăm strâns barca de salvare.');
INSERT INTO &quot;verses&quot; VALUES (510,27,17,'După ce au ridicat-o, au folosit funii pentru a încinge corabia pe dedesubt și, de teamă să nu eșueze spre Sirtis&lt;f&gt;[63]&lt;/f&gt;, au coborât pânzele și s-au lăsat duși &lt;i&gt;de vânt&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,27,18,'Fiindcă eram bătuți foarte tare de furtună, a doua zi au început să arunce încărcătura peste bord,');
INSERT INTO &quot;verses&quot; VALUES (510,27,19,'iar a treia zi au aruncat cu mâinile lor și uneltele corabiei&lt;f&gt;ⓕ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,27,20,'Soarele și stelele nu s-au văzut mai multe zile, iar furtuna care ne împresura era așa de cumplită, încât pierduserăm orice speranță de scăpare.');
INSERT INTO &quot;verses&quot; VALUES (510,27,21,'&lt;pb/&gt;Fiindcă nu mâncaseră de multă vreme, Pavel s-a ridicat în mijlocul lor și a zis: „Oameni &lt;i&gt;buni&lt;/i&gt;, trebuia să mă ascultați și să nu fi plecat cu corabia din Creta, și ați fi fost scutiți de această vătămare și de pagube.');
INSERT INTO &quot;verses&quot; VALUES (510,27,22,'Acum, vă sfătuiesc să prindeți curaj, pentru că nu se va pierde nicio viață, ci numai corabia.');
INSERT INTO &quot;verses&quot; VALUES (510,27,23,'Un înger&lt;f&gt;ⓖ&lt;/f&gt; al Dumnezeului căruia&lt;f&gt;ⓗ&lt;/f&gt; Îi aparțin și pe care-L slujesc mi s-a arătat azi-noapte');
INSERT INTO &quot;verses&quot; VALUES (510,27,24,'și mi-a zis: «Nu te teme, Pavele! Tu trebuie să te înfățișezi înaintea cezarului și iată că Dumnezeu ți i-a dăruit pe toți cei ce călătoresc împreună cu tine.»');
INSERT INTO &quot;verses&quot; VALUES (510,27,25,'De aceea, oameni &lt;i&gt;buni&lt;/i&gt;, prindeți curaj, căci&lt;f&gt;ⓘ&lt;/f&gt; am încredere în Dumnezeu că va fi așa cum mi s-a spus!');
INSERT INTO &quot;verses&quot; VALUES (510,27,26,'Dar trebuie să dăm peste o insulă&lt;f&gt;ⓙ&lt;/f&gt;.”');
INSERT INTO &quot;verses&quot; VALUES (510,27,27,'&lt;pb/&gt;În a paisprezecea noapte de când eram purtați pe &lt;i&gt;Marea&lt;/i&gt; Adriatică, pe la miezul nopții, corăbierii au bănuit că pământul este aproape.');
INSERT INTO &quot;verses&quot; VALUES (510,27,28,'Au măsurat &lt;i&gt;adâncimea apei&lt;/i&gt; și au găsit douăzeci de stânjeni&lt;f&gt;[64]&lt;/f&gt;; au mers puțin mai departe, au măsurat din nou și au găsit cincisprezece stânjeni.');
INSERT INTO &quot;verses&quot; VALUES (510,27,29,'De teamă să nu eșueze în locuri stâncoase, au lăsat patru ancore la pupa și se rugau să se facă ziuă.');
INSERT INTO &quot;verses&quot; VALUES (510,27,30,'Dar cum corăbierii încercau să fugă de pe corabie și coborau barca de salvare la apă, pe motiv că trebuie să lase în jos ancorele de la proră,');
INSERT INTO &quot;verses&quot; VALUES (510,27,31,'Pavel le-a zis centurionului și soldaților: „Dacă oamenii aceștia nu rămân în corabie, nu puteți fi salvați!”');
INSERT INTO &quot;verses&quot; VALUES (510,27,32,'Atunci, soldații au tăiat funiile bărcii și au lăsat-o să cadă.');
INSERT INTO &quot;verses&quot; VALUES (510,27,33,'&lt;pb/&gt;Înainte de ivirea zorilor, Pavel i-a îndemnat pe toți să mănânce, zicând: „Astăzi este a paisprezecea zi de când vegheați fără încetare și n-ați mâncat nimic.');
INSERT INTO &quot;verses&quot; VALUES (510,27,34,'De aceea, vă îndemn să mâncați, căci lucrul acesta este pentru salvarea voastră, și niciun&lt;f&gt;ⓚ&lt;/f&gt; fir de păr din cap nu vi se va pierde.”');
INSERT INTO &quot;verses&quot; VALUES (510,27,35,'După ce a spus aceste &lt;i&gt;cuvinte&lt;/i&gt;, a luat pâine, I-a&lt;f&gt;ⓛ&lt;/f&gt; mulțumit lui Dumnezeu înaintea tuturor, a frânt-o și a început să mănânce.');
INSERT INTO &quot;verses&quot; VALUES (510,27,36,'Atunci, toți au prins curaj și au mâncat și ei.');
INSERT INTO &quot;verses&quot; VALUES (510,27,37,'În corabie eram în total două sute șaptezeci și șase de suflete&lt;f&gt;ⓜ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,27,38,'După ce s-au săturat, au ușurat corabia aruncând grâul în mare.');
INSERT INTO &quot;verses&quot; VALUES (510,27,39,'&lt;pb/&gt;Când s-a făcut ziuă, n-au recunoscut ținutul, dar au zărit un golf care avea țărmul nisipos și au hotărât să îndrepte corabia într-acolo, dacă era cu putință.');
INSERT INTO &quot;verses&quot; VALUES (510,27,40,'Au tăiat ancorele și le-au dat drumul în mare; au slăbit în același timp funiile cârmelor&lt;f&gt;[65]&lt;/f&gt;, apoi au ridicat pânza din față în bătaia vântului și s-au îndreptat spre țărm.');
INSERT INTO &quot;verses&quot; VALUES (510,27,41,'Au dat însă peste un banc de nisip și corabia s-a împotmolit&lt;f&gt;ⓝ&lt;/f&gt;; în vreme ce prora s-a înțepenit &lt;i&gt;în nisip&lt;/i&gt; și a rămas nemișcată, pupa a început să se sfărâme sub loviturile valurilor.');
INSERT INTO &quot;verses&quot; VALUES (510,27,42,'Soldații aveau de gând să-i omoare pe întemnițați, ca să nu scape vreunul înot.');
INSERT INTO &quot;verses&quot; VALUES (510,27,43,'Centurionul însă, care voia să-l salveze pe Pavel, le-a împiedicat planul. A poruncit ca cei ce pot înota să sară primii peste bord și să se îndrepte spre țărm,');
INSERT INTO &quot;verses&quot; VALUES (510,27,44,'iar ceilalți &lt;i&gt;să scape cum pot:&lt;/i&gt; unii pe scânduri, iar alții pe bucăți de corabie. Așa s-a făcut că au&lt;f&gt;ⓞ&lt;/f&gt; ajuns toți teferi la țărm.');
INSERT INTO &quot;verses&quot; VALUES (510,28,1,'&lt;pb/&gt;După ce am scăpat &lt;i&gt;de primejdie&lt;/i&gt;, am aflat că insula&lt;f&gt;ⓟ&lt;/f&gt; se numea Malta.');
INSERT INTO &quot;verses&quot; VALUES (510,28,2,'Barbarii&lt;f&gt;ⓠ&lt;/f&gt; ne-au arătat o bunăvoință neobișnuită; au aprins un foc și ne-au primit pe toți, căci începuse să plouă și era frig.');
INSERT INTO &quot;verses&quot; VALUES (510,28,3,'Pavel a adunat o grămadă de vreascuri și, când a pus-o pe foc, o viperă a ieșit din cauza căldurii și s-a prins de mâna lui.');
INSERT INTO &quot;verses&quot; VALUES (510,28,4,'Barbarii, când au văzut jivina atârnată de mâna lui, au zis unii către alții: „Fără îndoială că omul acesta este un ucigaș, căci, deși a scăpat teafăr din mare, Dreptatea&lt;f&gt;[66]&lt;/f&gt; nu-i îngăduie să trăiască.”');
INSERT INTO &quot;verses&quot; VALUES (510,28,5,'Pavel a scuturat jivina în foc și n-a pățit nimic,');
INSERT INTO &quot;verses&quot; VALUES (510,28,6,'deși ei așteptau să se umfle sau să cadă pe dată mort. Dar după ce au așteptat mult și au văzut că nu i se întâmplă nimic&lt;f&gt;ⓡ&lt;/f&gt; rău, și-au schimbat părerea și ziceau&lt;f&gt;ⓢ&lt;/f&gt; că este un zeu.');
INSERT INTO &quot;verses&quot; VALUES (510,28,7,'&lt;pb/&gt;În împrejurimi erau moșiile mai-marelui insulei, numit Publius. El ne-a primit și ne-a găzduit cu multă omenie trei zile.');
INSERT INTO &quot;verses&quot; VALUES (510,28,8,'Tatăl lui Publius zăcea cuprins de febră și dizenterie. Pavel a mers la el, s-a rugat&lt;f&gt;ⓣ&lt;/f&gt;, și-a pus&lt;f&gt;ⓤ&lt;/f&gt; mâinile peste el și l-a vindecat.');
INSERT INTO &quot;verses&quot; VALUES (510,28,9,'Atunci au început să vină și ceilalți bolnavi de pe insulă și au fost vindecați.');
INSERT INTO &quot;verses&quot; VALUES (510,28,10,'Ne-au arătat multă cinste&lt;f&gt;ⓥ&lt;/f&gt; și, la îmbarcarea noastră, ne-au dat tot ce aveam nevoie.');
INSERT INTO &quot;verses&quot; VALUES (510,28,11,'&lt;pb/&gt;După trei luni, am plecat cu o corabie venită din Alexandria care iernase pe insulă și care purta semnul Dioscurilor&lt;f&gt;[67]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (510,28,12,'Am ajuns la Siracuza, unde am rămas trei zile.');
INSERT INTO &quot;verses&quot; VALUES (510,28,13,'De acolo, am ridicat ancora și am ajuns la Regium. Fiindcă a doua zi a început să bată vântul din sud, după două zile am ajuns la Puteoli,');
INSERT INTO &quot;verses&quot; VALUES (510,28,14,'unde i-am întâlnit pe câțiva frați, care ne-au rugat să rămânem cu ei șapte zile. Și astfel am ajuns la Roma.');
INSERT INTO &quot;verses&quot; VALUES (510,28,15,'Acolo, frații care auziseră despre noi au venit în întâmpinarea noastră până la Forul lui Apius și până la Trei Taverne. Văzându-i, Pavel I-a mulțumit lui Dumnezeu și s-a îmbărbătat.');
INSERT INTO &quot;verses&quot; VALUES (510,28,16,'Când am ajuns la Roma, [centurionul i-a predat pe întemnițați prefectului gărzilor pretoriene, iar] lui&lt;f&gt;ⓦ&lt;/f&gt; Pavel i s-a îngăduit să locuiască separat, cu un soldat care-l păzea.');
INSERT INTO &quot;verses&quot; VALUES (510,28,17,'&lt;pb/&gt;După trei zile, Pavel i-a chemat pe mai-marii iudeilor și, după ce s-au adunat, le-a zis: „Fraților, fără să fi făcut&lt;f&gt;ⓧ&lt;/f&gt; ceva împotriva poporului sau obiceiurilor noastre strămoșești, în Ierusalim am&lt;f&gt;ⓨ&lt;/f&gt; fost predat romanilor și &lt;i&gt;pus&lt;/i&gt; în lanțuri.');
INSERT INTO &quot;verses&quot; VALUES (510,28,18,'După ce m-au cercetat&lt;f&gt;ⓩ&lt;/f&gt;, ei voiau să-mi dea drumul, pentru că nu era în mine nicio vină de moarte.');
INSERT INTO &quot;verses&quot; VALUES (510,28,19,'Dar iudeii s-au împotrivit și am&lt;f&gt;ⓐ&lt;/f&gt; fost nevoit să fac apel la cezar, dar nu fiindcă aș fi avut de gând să-mi învinuiesc neamul.');
INSERT INTO &quot;verses&quot; VALUES (510,28,20,'De aceea v-am chemat să vă văd și să vorbim, căci din pricina&lt;f&gt;ⓑ&lt;/f&gt; nădejdii lui Israel port eu acest&lt;f&gt;ⓒ&lt;/f&gt; lanț.”');
INSERT INTO &quot;verses&quot; VALUES (510,28,21,'Ei i-au răspuns: „Noi n-am primit din Iudeea nicio scrisoare cu privire la tine și nici n-a venit vreun frate care să ne anunțe sau să spună ceva rău despre tine.');
INSERT INTO &quot;verses&quot; VALUES (510,28,22,'Dar am vrea să auzim de la tine ce gândești, deoarece știm că gruparea aceasta pretutindeni stârnește&lt;f&gt;ⓓ&lt;/f&gt; împotrivire.”');
INSERT INTO &quot;verses&quot; VALUES (510,28,23,'&lt;pb/&gt;Au hotărât cu el o zi și au venit mai mulți la locuința lui. Pavel le-a dat lămuriri și mărturie&lt;f&gt;ⓔ&lt;/f&gt; despre Împărăția lui Dumnezeu încercând, de dimineața până seara, să-i convingă cu privire la Isus pe baza&lt;f&gt;ⓕ&lt;/f&gt; Legii lui Moise și a Profeților.');
INSERT INTO &quot;verses&quot; VALUES (510,28,24,'Unii&lt;f&gt;ⓖ&lt;/f&gt; s-au lăsat înduplecați de spusele lui, dar alții n-au crezut.');
INSERT INTO &quot;verses&quot; VALUES (510,28,25,'Fiindcă s-au iscat neînțelegeri între ei, au plecat, iar Pavel le-a mai zis doar atât: „Bine a zis Duhul Sfânt despre părinții voștri prin profetul Isaia,');
INSERT INTO &quot;verses&quot; VALUES (510,28,26,'când a spus: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Du-te&lt;/e&gt; &lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;la poporul acesta și spune-i:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;«De auzit veți auzi, și nu veți înțelege;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;de privit veți privi, și nu veți vedea.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,28,27,'&lt;t&gt;&lt;e&gt;Căci inima acestui popor s-a împietrit;&lt;/e&gt; &lt;f&gt;[68]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;cu urechile greu au auzit,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;iar ochii și i-au închis,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca nu cumva să vadă cu ochii,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;să audă cu urechile,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;să înțeleagă cu inima&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și să se întoarcă la Mine ca să-i vindec.»&lt;/e&gt; &lt;f&gt;[69]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (510,28,28,'&lt;pb/&gt;Să știți că mântuirea aceasta a lui Dumnezeu le-a fost trimisă neamurilor&lt;f&gt;ⓘ&lt;/f&gt; și ele vor asculta.”');
INSERT INTO &quot;verses&quot; VALUES (510,28,29,'[După ce le-a zis aceste &lt;i&gt;cuvinte&lt;/i&gt;, iudeii au plecat purtând discuții aprinse unii cu alții.]');
INSERT INTO &quot;verses&quot; VALUES (510,28,30,'&lt;pb/&gt;&lt;i&gt;Pavel&lt;/i&gt; a rămas doi ani într-o casă închiriată și îi primea pe toți cei care veneau la el.');
INSERT INTO &quot;verses&quot; VALUES (510,28,31,'Propovăduia&lt;f&gt;ⓙ&lt;/f&gt; Împărăția lui Dumnezeu și-i învăța pe oameni despre Domnul Isus Hristos cu toată îndrăzneala și fără nicio piedică.');
INSERT INTO &quot;verses&quot; VALUES (520,1,1,'&lt;pb/&gt;Pavel, rob al lui Hristos Isus, chemat&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;să fie&lt;/i&gt; apostol, pus deoparte&lt;f&gt;ⓑ&lt;/f&gt; pentru Evanghelia lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (520,1,2,'pe care o făgăduise mai înainte prin&lt;f&gt;ⓒ&lt;/f&gt; profeții Săi în Sfintele Scripturi');
INSERT INTO &quot;verses&quot; VALUES (520,1,3,'și care &lt;i&gt;Îl înfățișează&lt;/i&gt; pe Fiul Său, născut&lt;f&gt;ⓓ&lt;/f&gt; din sămânța lui David, în ce privește trupul,');
INSERT INTO &quot;verses&quot; VALUES (520,1,4,'iar în ce privește duhul&lt;f&gt;ⓔ&lt;/f&gt; sfințeniei, rânduit Fiul lui Dumnezeu cu putere, prin învierea din morți, anume pe Isus Hristos, Domnul nostru.');
INSERT INTO &quot;verses&quot; VALUES (520,1,5,'Prin El am&lt;f&gt;ⓕ&lt;/f&gt; primit har și apostolie, astfel încât, pentru&lt;f&gt;ⓖ&lt;/f&gt; Numele Lui, &lt;i&gt;să ducem&lt;/i&gt; ascultarea&lt;f&gt;ⓗ&lt;/f&gt; de credință la toate neamurile,');
INSERT INTO &quot;verses&quot; VALUES (520,1,6,'între care sunteți și voi, cei chemați ai lui Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (520,1,7,'&lt;i&gt;Vouă&lt;/i&gt; tuturor care sunteți în Roma, preaiubiți ai lui Dumnezeu, chemați&lt;f&gt;ⓘ&lt;/f&gt; &lt;i&gt;să fiți&lt;/i&gt; sfinți: har&lt;f&gt;ⓙ&lt;/f&gt; și pace de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (520,1,8,'&lt;pb/&gt;Mai întâi, Îi mulțumesc&lt;f&gt;ⓚ&lt;/f&gt; Dumnezeului meu, prin Isus Hristos, pentru voi toți, căci despre credința&lt;f&gt;ⓛ&lt;/f&gt; voastră s-a dus vestea în toată lumea.');
INSERT INTO &quot;verses&quot; VALUES (520,1,9,'Martor îmi este Dumnezeu&lt;f&gt;ⓜ&lt;/f&gt;, căruia Îi slujesc&lt;f&gt;ⓝ&lt;/f&gt; în duhul meu, prin &lt;i&gt;vestirea&lt;/i&gt; Evangheliei Fiului Său, că neîncetat vă pomenesc');
INSERT INTO &quot;verses&quot; VALUES (520,1,10,'și cer totdeauna în rugăciunile mele&lt;f&gt;ⓞ&lt;/f&gt; să reușesc în sfârșit, prin voia lui Dumnezeu, să ajung la voi.');
INSERT INTO &quot;verses&quot; VALUES (520,1,11,'Căci doresc să vă văd ca să vă dau vreun dar duhovnicesc pentru întărirea voastră');
INSERT INTO &quot;verses&quot; VALUES (520,1,12,'sau, mai degrabă, ca să fim &lt;i&gt;și noi&lt;/i&gt; îmbărbătați în mijlocul vostru, fiecare prin credința&lt;f&gt;ⓟ&lt;/f&gt; celuilalt, a voastră și a mea.');
INSERT INTO &quot;verses&quot; VALUES (520,1,13,'Nu vreau să nu știți, fraților, că de&lt;f&gt;ⓠ&lt;/f&gt; multe ori am avut de gând să vin la voi, ca să am vreun rod și printre voi, precum în rândul celorlalte neamuri, dar&lt;f&gt;ⓡ&lt;/f&gt; am fost împiedicat până acum.');
INSERT INTO &quot;verses&quot; VALUES (520,1,14,'Eu le sunt dator și grecilor&lt;f&gt;[1]&lt;/f&gt;, și barbarilor, și celor înțelepți, și celor neînvățați,');
INSERT INTO &quot;verses&quot; VALUES (520,1,15,'de unde și dorința mea vie ca și vouă, celor din Roma, să vă vestesc Evanghelia.');
INSERT INTO &quot;verses&quot; VALUES (520,1,16,'&lt;pb/&gt;Mie&lt;f&gt;ⓢ&lt;/f&gt; nu mi-e rușine de Evanghelia [lui Hristos], fiindcă&lt;f&gt;ⓣ&lt;/f&gt; ea este puterea lui Dumnezeu pentru mântuirea fiecăruia care crede: întâi a&lt;f&gt;ⓤ&lt;/f&gt; iudeului, apoi a grecului;');
INSERT INTO &quot;verses&quot; VALUES (520,1,17,'căci în&lt;f&gt;ⓥ&lt;/f&gt; ea se descoperă dreptatea&lt;f&gt;[2]&lt;/f&gt; lui Dumnezeu, prin credință spre credință&lt;f&gt;[3]&lt;/f&gt;, după cum este scris: &lt;e&gt;Cel drept&lt;/e&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;prin credință va trăi.&lt;/e&gt;&lt;f&gt;[4]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,1,18,'&lt;pb/&gt;Mânia&lt;f&gt;ⓧ&lt;/f&gt; lui Dumnezeu se descoperă din cer împotriva oricărei impietăți și împotriva oricărei nedreptăți a oamenilor care înăbușă adevărul în nelegiuirea lor,');
INSERT INTO &quot;verses&quot; VALUES (520,1,19,'pentru că&lt;f&gt;ⓨ&lt;/f&gt; ce se poate cunoaște despre Dumnezeu le este descoperit în ei înșiși, fiindu-le descoperit de Dumnezeu&lt;f&gt;ⓩ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,1,20,'Într-adevăr, însușirile Sale nevăzute&lt;f&gt;ⓐ&lt;/f&gt; — puterea Sa veșnică și dumnezeirea Sa — se văd lămurit de la crearea lumii, fiind înțelese prin cele create, astfel încât nu se pot dezvinovăți,');
INSERT INTO &quot;verses&quot; VALUES (520,1,21,'fiindcă, deși L-au cunoscut pe Dumnezeu, nu L-au proslăvit ca Dumnezeu, nici nu I-au mulțumit, ci au ajuns să aibă gânduri deșarte&lt;f&gt;ⓑ&lt;/f&gt;, și inima lor nepricepută s-a întunecat.');
INSERT INTO &quot;verses&quot; VALUES (520,1,22,'Au&lt;f&gt;ⓒ&lt;/f&gt; pretins că sunt înțelepți, dar au ajuns nebuni');
INSERT INTO &quot;verses&quot; VALUES (520,1,23,'și au preschimbat slava Dumnezeului&lt;f&gt;ⓓ&lt;/f&gt; nepieritor într-un chip care seamănă cu omul pieritor, cu păsări, dobitoace și târâtoare.');
INSERT INTO &quot;verses&quot; VALUES (520,1,24,'De aceea&lt;f&gt;ⓔ&lt;/f&gt;, Dumnezeu i-a dat &lt;i&gt;pradă&lt;/i&gt; poftelor inimilor lor, în necurăție, astfel încât&lt;f&gt;ⓕ&lt;/f&gt; își necinstesc&lt;f&gt;ⓖ&lt;/f&gt; trupurile între ei.');
INSERT INTO &quot;verses&quot; VALUES (520,1,25,'Au preschimbat adevărul&lt;f&gt;ⓗ&lt;/f&gt; lui Dumnezeu în minciună și s-au închinat și au slujit creaturii în&lt;f&gt;ⓘ&lt;/f&gt; locul Creatorului, care este binecuvântat în veci! Amin!');
INSERT INTO &quot;verses&quot; VALUES (520,1,26,'&lt;pb/&gt;Din pricina aceasta, Dumnezeu i-a dat &lt;i&gt;pradă&lt;/i&gt; unor patimi&lt;f&gt;ⓙ&lt;/f&gt; josnice; căci femeile lor au preschimbat menirea lor firească într-una împotriva firii;');
INSERT INTO &quot;verses&quot; VALUES (520,1,27,'tot astfel, și bărbații au părăsit menirea firească a femeii, s-au aprins în poftele lor unii pentru alții, au săvârșit bărbați cu bărbați &lt;i&gt;faptele&lt;/i&gt; rușinii și au primit în ei înșiși răsplata cuvenită pentru rătăcirea lor.');
INSERT INTO &quot;verses&quot; VALUES (520,1,28,'Și, fiindcă n-au găsit de cuviință să-L aibă pe Dumnezeu în cunoștința lor, Dumnezeu i-a dat &lt;i&gt;pradă&lt;/i&gt; unei gândiri stricate, ca să facă lucruri neîngăduite&lt;f&gt;ⓚ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,1,29,'Sunt plini de orice fel de nedreptate, ticăloșie, lăcomie, răutate; plini de invidie, ucidere, ceartă, înșelăciune, porniri răutăcioase; sunt bârfitori,');
INSERT INTO &quot;verses&quot; VALUES (520,1,30,'defăimători, urâtori de Dumnezeu, obraznici, trufași, lăudăroși, născocitori de rele, neascultători de părinți,');
INSERT INTO &quot;verses&quot; VALUES (520,1,31,'fără pricepere, fără de cuvânt, fără dragoste firească, [neînduplecați,] fără milă.');
INSERT INTO &quot;verses&quot; VALUES (520,1,32,'Deși cunosc&lt;f&gt;ⓛ&lt;/f&gt; dreapta hotărâre a lui Dumnezeu, anume că cei ce săvârșesc asemenea fapte sunt&lt;f&gt;ⓜ&lt;/f&gt; vrednici de moarte, nu numai că le fac, ci sunt și de acord&lt;f&gt;ⓝ&lt;/f&gt; cu cei ce le fac.');
INSERT INTO &quot;verses&quot; VALUES (520,2,1,'&lt;pb/&gt;Așadar, omule, oricine ai fi tu, care judeci &lt;i&gt;pe altul&lt;/i&gt;, nu te poți dezvinovăți&lt;f&gt;ⓞ&lt;/f&gt;, căci&lt;f&gt;ⓟ&lt;/f&gt;, prin faptul că judeci pe altul, pe tine însuți te osândești, fiindcă tu, care judeci, faci aceleași lucruri.');
INSERT INTO &quot;verses&quot; VALUES (520,2,2,'Știm că judecata lui Dumnezeu împotriva celor ce fac astfel de lucruri este potrivită cu adevărul.');
INSERT INTO &quot;verses&quot; VALUES (520,2,3,'Și crezi tu, omule, care judeci pe cei ce fac astfel de lucruri și care faci la fel, că vei scăpa de judecata lui Dumnezeu?');
INSERT INTO &quot;verses&quot; VALUES (520,2,4,'Sau disprețuiești &lt;i&gt;tu&lt;/i&gt; bogățiile&lt;f&gt;ⓠ&lt;/f&gt; bunătății, îngăduinței&lt;f&gt;ⓡ&lt;/f&gt; și îndelungii&lt;f&gt;ⓢ&lt;/f&gt; Sale răbdări? Nu vezi&lt;f&gt;ⓣ&lt;/f&gt; &lt;i&gt;tu&lt;/i&gt; că bunătatea lui Dumnezeu te îndeamnă la pocăință?');
INSERT INTO &quot;verses&quot; VALUES (520,2,5,'Dar cu împietrirea ta și cu inima ta nepocăită îți&lt;f&gt;ⓤ&lt;/f&gt; aduni o comoară de mânie pentru ziua mâniei și a arătării dreptei judecăți a lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (520,2,6,'care&lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;îi va da fiecăruia după faptele lui&lt;/e&gt;&lt;f&gt;[5]&lt;/f&gt;:');
INSERT INTO &quot;verses&quot; VALUES (520,2,7,'pe de o parte, viață veșnică celor ce, prin stăruința în fapte bune, caută slava, cinstea și nemurirea&lt;f&gt;[6]&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (520,2,8,'pe de altă parte, mânie și urgie celor ce, din duh de ceartă, se împotrivesc&lt;f&gt;ⓦ&lt;/f&gt; adevărului și se lasă înduplecați de nelegiuire.');
INSERT INTO &quot;verses&quot; VALUES (520,2,9,'Necaz și strâmtorare &lt;i&gt;va veni&lt;/i&gt; peste orice suflet omenesc care face răul: întâi&lt;f&gt;ⓧ&lt;/f&gt; peste iudeu, apoi peste grec.');
INSERT INTO &quot;verses&quot; VALUES (520,2,10,'Slavă&lt;f&gt;ⓨ&lt;/f&gt;, cinste și pace &lt;i&gt;va veni&lt;/i&gt; însă peste oricine face binele: întâi peste iudeu, apoi peste grec.');
INSERT INTO &quot;verses&quot; VALUES (520,2,11,'Căci&lt;f&gt;ⓩ&lt;/f&gt; Dumnezeu nu caută la fața omului.');
INSERT INTO &quot;verses&quot; VALUES (520,2,12,'&lt;pb/&gt;Toți cei ce au păcătuit fără Lege vor pieri fără Lege, și toți cei ce au păcătuit având Legea vor fi judecați după Lege.');
INSERT INTO &quot;verses&quot; VALUES (520,2,13,'Pentru că nu&lt;f&gt;ⓐ&lt;/f&gt; cei ce aud Legea sunt drepți înaintea lui Dumnezeu, ci cei ce împlinesc Legea vor fi îndreptățiți');
INSERT INTO &quot;verses&quot; VALUES (520,2,14,'(când neamurile, care nu au Legea, împlinesc din fire &lt;i&gt;cerințele&lt;/i&gt; Legii, ele, măcar că nu au Legea, își sunt singure lege');
INSERT INTO &quot;verses&quot; VALUES (520,2,15,'și dovedesc că lucrarea Legii este scrisă în inimile lor, în vreme ce conștiința lor dă și ea mărturie, iar gândurile lor se învinovățesc sau se dezvinovățesc între ele)');
INSERT INTO &quot;verses&quot; VALUES (520,2,16,'în&lt;f&gt;ⓑ&lt;/f&gt; ziua când va judeca Dumnezeu lucrurile ascunse ale oamenilor, după Evanghelia&lt;f&gt;ⓒ&lt;/f&gt; mea, prin&lt;f&gt;ⓓ&lt;/f&gt; Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (520,2,17,'&lt;pb/&gt;Tu&lt;f&gt;ⓔ&lt;/f&gt; însă, care te numești iudeu, te&lt;f&gt;ⓕ&lt;/f&gt; bizui pe Lege, te lauzi&lt;f&gt;ⓖ&lt;/f&gt; cu Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (520,2,18,'cunoști&lt;f&gt;ⓗ&lt;/f&gt; voia &lt;i&gt;Lui&lt;/i&gt; și știi să faci deosebire între lucruri&lt;f&gt;ⓘ&lt;/f&gt;, pentru că ești învățat de Lege,');
INSERT INTO &quot;verses&quot; VALUES (520,2,19,'ba încă te crezi&lt;f&gt;ⓙ&lt;/f&gt; călăuza orbilor, lumina celor ce sunt în întuneric,');
INSERT INTO &quot;verses&quot; VALUES (520,2,20,'povățuitorul celor fără minte, învățătorul celor neștiutori, pentru că în Lege ai&lt;f&gt;ⓚ&lt;/f&gt; întruchiparea cunoașterii și a adevărului,');
INSERT INTO &quot;verses&quot; VALUES (520,2,21,'tu deci, care înveți pe altul, pe tine însuți nu te înveți? Tu, care propovăduiești: „Să nu furi”, furi?');
INSERT INTO &quot;verses&quot; VALUES (520,2,22,'Tu&lt;f&gt;ⓛ&lt;/f&gt;, care zici: „Să nu săvârșești adulter”, săvârșești adulter?&lt;f&gt;ⓜ&lt;/f&gt; Tu, care ai oroare de idoli, le jefuiești templele?');
INSERT INTO &quot;verses&quot; VALUES (520,2,23,'Tu&lt;f&gt;ⓝ&lt;/f&gt;, care te lauzi cu Legea, Îl necinstești pe Dumnezeu prin călcarea Legii?');
INSERT INTO &quot;verses&quot; VALUES (520,2,24,'Căci &lt;e&gt;din pricina voastră este hulit Numele lui Dumnezeu între neamuri&lt;/e&gt;&lt;f&gt;[7]&lt;/f&gt;, după cum este&lt;f&gt;ⓞ&lt;/f&gt; scris.');
INSERT INTO &quot;verses&quot; VALUES (520,2,25,'&lt;pb/&gt;Circumcizia&lt;f&gt;ⓟ&lt;/f&gt;, negreșit, este de folos dacă împlinești Legea; dar dacă tu calci Legea, circumcizia ta ajunge necircumcizie.');
INSERT INTO &quot;verses&quot; VALUES (520,2,26,'Și dacă&lt;f&gt;ⓠ&lt;/f&gt; cel necircumcis păzește cerințele Legii, lipsa circumciziei nu i se va socoti oare ca circumcizie?');
INSERT INTO &quot;verses&quot; VALUES (520,2,27,'&lt;i&gt;Astfel&lt;/i&gt;, cel necircumcis din naștere, care împlinește Legea, te&lt;f&gt;ⓡ&lt;/f&gt; va osândi pe tine, care — cu &lt;i&gt;toată&lt;/i&gt; litera &lt;i&gt;Legii&lt;/i&gt; și cu circumcizie — &lt;i&gt;ajungi să&lt;/i&gt; calci Legea.');
INSERT INTO &quot;verses&quot; VALUES (520,2,28,'Nu&lt;f&gt;ⓢ&lt;/f&gt; cel care se arată &lt;i&gt;astfel&lt;/i&gt; pe dinafară este iudeu, nici circumcizia nu este cea exterioară, în carne.');
INSERT INTO &quot;verses&quot; VALUES (520,2,29,'Ci iudeu este &lt;i&gt;cineva&lt;/i&gt; în lăuntrul&lt;f&gt;ⓣ&lt;/f&gt; &lt;i&gt;său&lt;/i&gt;, și circumcizia&lt;f&gt;ⓤ&lt;/f&gt; este cea a inimii, prin Duhul&lt;f&gt;ⓥ&lt;/f&gt;, nu în literă; lauda&lt;f&gt;ⓦ&lt;/f&gt; lui vine nu de la oameni, ci de la Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,3,1,'&lt;pb/&gt;Care este deci avantajul iudeului, sau care este folosul circumciziei?');
INSERT INTO &quot;verses&quot; VALUES (520,3,2,'Este mare, în toate privințele. Mai întâi, fiindcă&lt;f&gt;ⓧ&lt;/f&gt; lor le-au fost încredințate cuvintele lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,3,3,'Dar oare, dacă unii&lt;f&gt;ⓨ&lt;/f&gt; n-au crezut, necredința&lt;f&gt;ⓩ&lt;/f&gt; lor va nimici credincioșia lui Dumnezeu?');
INSERT INTO &quot;verses&quot; VALUES (520,3,4,'Nicidecum&lt;f&gt;ⓐ&lt;/f&gt;! Dimpotrivă, Dumnezeu&lt;f&gt;ⓑ&lt;/f&gt; să fie &lt;i&gt;găsit&lt;/i&gt; adevărat și tot&lt;f&gt;ⓒ&lt;/f&gt; omul să fie &lt;i&gt;găsit&lt;/i&gt; mincinos, după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Ca să fii îndreptățit prin cuvintele Tale&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și să ieși biruitor când vei fi judecat.&lt;/e&gt; &lt;f&gt;[8]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,5,'&lt;pb/&gt;Dar dacă nedreptatea noastră pune în lumină dreptatea lui Dumnezeu, ce vom zice? Nu cumva este nedrept Dumnezeu când Își dezlănțuie mânia? (Omenește vorbesc&lt;f&gt;ⓓ&lt;/f&gt;.)');
INSERT INTO &quot;verses&quot; VALUES (520,3,6,'Nicidecum! Pentru că, altfel, cum&lt;f&gt;ⓔ&lt;/f&gt; va judeca Dumnezeu lumea?');
INSERT INTO &quot;verses&quot; VALUES (520,3,7,'Dar dacă, prin minciuna mea, adevărul lui Dumnezeu a prisosit și mai mult spre slava Lui, de ce mai sunt judecat ca păcătos?');
INSERT INTO &quot;verses&quot; VALUES (520,3,8,'Și oare „să&lt;f&gt;ⓕ&lt;/f&gt; facem răul ca &lt;i&gt;din el&lt;/i&gt; să vină binele”, cum suntem vorbiți de rău și cum pretind unii că spunem noi? Osânda acestor oameni este dreaptă.');
INSERT INTO &quot;verses&quot; VALUES (520,3,9,'&lt;pb/&gt;Ce urmează atunci? Suntem noi mai buni &lt;i&gt;decât ei&lt;/i&gt;? Nicidecum! Fiindcă i-am învinuit deja pe toți&lt;f&gt;ⓖ&lt;/f&gt;, atât iudei, cât și greci, că sunt sub păcat,');
INSERT INTO &quot;verses&quot; VALUES (520,3,10,'după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Nu este niciun om drept, niciunul măcar.&lt;/e&gt; &lt;f&gt;[9]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,11,'&lt;t&gt;&lt;e&gt;Nu este niciunul care să aibă pricepere.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Nu este niciunul care să-L caute pe Dumnezeu.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,12,'&lt;t&gt;&lt;e&gt;Toți s-au abătut și au ajuns netrebnici.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Nu este niciunul care să facă binele,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nici măcar unul.&lt;/e&gt; &lt;f&gt;[10]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,13,'&lt;t&gt;&lt;e&gt;Mormânt deschis este gâtlejul lor;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;limbile lor rostesc înșelăciune;&lt;/e&gt; &lt;f&gt;[11]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;venin de viperă este sub buzele lor;&lt;/e&gt; &lt;f&gt;[12]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,14,'&lt;t&gt;&lt;e&gt;gura le este plină de blestem și de amărăciune;&lt;/e&gt; &lt;f&gt;[13]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,15,'&lt;t&gt;&lt;e&gt;grabnice sunt picioarele lor la vărsare de sânge;&lt;/e&gt; &lt;f&gt;[14]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,16,'&lt;t&gt;&lt;e&gt;prăpăd și pustiire este în căile lor;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,17,'&lt;t&gt;&lt;e&gt;și calea păcii n-au cunoscut-o:&lt;/e&gt; &lt;f&gt;[15]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,18,'&lt;t&gt;&lt;e&gt;nu este frică de Dumnezeu înaintea ochilor lor.&lt;/e&gt; &lt;f&gt;[16]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,3,19,'&lt;pb/&gt;Știm însă că, ce&lt;f&gt;ⓗ&lt;/f&gt; spune Legea, pentru cei de sub Lege spune, pentru ca orice&lt;f&gt;ⓘ&lt;/f&gt; gură să fie astupată și întreaga&lt;f&gt;ⓙ&lt;/f&gt; lume să fie găsită vinovată înaintea lui Dumnezeu:');
INSERT INTO &quot;verses&quot; VALUES (520,3,20,'căci prin&lt;f&gt;ⓚ&lt;/f&gt; faptele Legii nu va fi îndreptățit nimeni înaintea Lui, fiindcă prin&lt;f&gt;ⓛ&lt;/f&gt; Lege vine cunoștința &lt;i&gt;deplină&lt;/i&gt; a păcatului.');
INSERT INTO &quot;verses&quot; VALUES (520,3,21,'&lt;pb/&gt;Acum însă, fără Lege, s-a arătat dreptatea&lt;f&gt;ⓜ&lt;/f&gt; lui Dumnezeu — despre care dau mărturie&lt;f&gt;ⓝ&lt;/f&gt; Legea și&lt;f&gt;ⓞ&lt;/f&gt; Profeții —');
INSERT INTO &quot;verses&quot; VALUES (520,3,22,'și anume dreptatea lui Dumnezeu prin&lt;f&gt;ⓟ&lt;/f&gt; credința în Isus Hristos, pentru toți cei ce cred. (Nu&lt;f&gt;ⓠ&lt;/f&gt; este nicio deosebire,');
INSERT INTO &quot;verses&quot; VALUES (520,3,23,'căci toți&lt;f&gt;ⓡ&lt;/f&gt; au păcătuit și sunt lipsiți de slava lui Dumnezeu.)');
INSERT INTO &quot;verses&quot; VALUES (520,3,24,'Ei sunt îndreptățiți fără plată, prin&lt;f&gt;ⓢ&lt;/f&gt; harul Său, prin&lt;f&gt;ⓣ&lt;/f&gt; răscumpărarea care este în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (520,3,25,'Pe El, Dumnezeu L-a&lt;f&gt;ⓤ&lt;/f&gt; înfățișat&lt;f&gt;[17]&lt;/f&gt; ca mijloc de ispășire, prin sângele&lt;f&gt;ⓥ&lt;/f&gt; Lui — prin credință — ca să-Și dovedească dreptatea, căci Dumnezeu trecuse cu vederea păcatele săvârșite odinioară&lt;f&gt;ⓦ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (520,3,26,'în vremea îngăduinței lui Dumnezeu, ca să-Și dovedească dreptatea în vremea de acum, astfel încât să fie drept și să-l îndreptățească pe cel ce are credință în Isus.');
INSERT INTO &quot;verses&quot; VALUES (520,3,27,'&lt;pb/&gt;Unde este deci&lt;f&gt;ⓧ&lt;/f&gt; temeiul de laudă? S-a dus! Prin ce fel de lege? A faptelor? Nu, ci prin legea credinței!');
INSERT INTO &quot;verses&quot; VALUES (520,3,28,'Pentru că noi socotim că prin&lt;f&gt;ⓨ&lt;/f&gt; credință este îndreptățit omul, fără faptele Legii.');
INSERT INTO &quot;verses&quot; VALUES (520,3,29,'Oare Dumnezeu este numai al iudeilor? Nu este El și al neamurilor? Ba da, și al neamurilor,');
INSERT INTO &quot;verses&quot; VALUES (520,3,30,'de vreme ce Dumnezeu este unul&lt;f&gt;ⓩ&lt;/f&gt;, și El îi va îndreptăți prin credință pe cei circumciși și tot prin credință pe cei necircumciși.');
INSERT INTO &quot;verses&quot; VALUES (520,3,31,'Așadar, prin credință desființăm Legea? Nicidecum! Dimpotrivă, întărim Legea.');
INSERT INTO &quot;verses&quot; VALUES (520,4,1,'&lt;pb/&gt;Ce vom zice deci că a căpătat Avraam, strămoșul nostru după trup&lt;f&gt;ⓐ&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (520,4,2,'Dacă Avraam a fost îndreptățit&lt;f&gt;ⓑ&lt;/f&gt; prin fapte, are temei de laudă, dar nu înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,4,3,'Căci ce zice Scriptura? &lt;e&gt;Avraam&lt;/e&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;L-a crezut pe Dumnezeu, și &lt;i&gt;aceasta&lt;/i&gt; i s-a socotit ca dreptate.&lt;/e&gt;&lt;f&gt;[18]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,4,4,'Celui&lt;f&gt;ⓓ&lt;/f&gt; ce lucrează, plata &lt;i&gt;cuvenită&lt;/i&gt; nu i se socotește ca har, ci ca ceva datorat;');
INSERT INTO &quot;verses&quot; VALUES (520,4,5,'pe când celui ce nu lucrează, dar care se încrede în Cel care-l îndreptățește pe cel nelegiuit&lt;f&gt;ⓔ&lt;/f&gt;, credința lui i se socotește ca dreptate.');
INSERT INTO &quot;verses&quot; VALUES (520,4,6,'Tot astfel și David vorbește de fericirea omului căruia Dumnezeu îi socotește dreptatea fără fapte:');
INSERT INTO &quot;verses&quot; VALUES (520,4,7,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Ferice de aceia ale căror fărădelegi au fost iertate&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și ale căror păcate au fost acoperite!&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,4,8,'&lt;t&gt;&lt;e&gt;Ferice de omul căruia nu-i pune Domnul la socoteală păcatul!&lt;/e&gt; &lt;f&gt;[19]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,4,9,'&lt;pb/&gt;Fericirea aceasta este &lt;i&gt;numai&lt;/i&gt; pentru cei circumciși sau și pentru cei necircumciși? Căci zicem că lui Avraam credința i-a fost socotită ca dreptate.');
INSERT INTO &quot;verses&quot; VALUES (520,4,10,'Dar cum i-a fost socotită? Pe când era circumcis sau pe când era necircumcis? Nu când era circumcis, ci când era necircumcis.');
INSERT INTO &quot;verses&quot; VALUES (520,4,11,'Apoi a&lt;f&gt;ⓕ&lt;/f&gt; primit semnul circumciziei, ca pecete a dreptății &lt;i&gt;căpătate&lt;/i&gt; prin credință pe când era &lt;i&gt;încă&lt;/i&gt; necircumcis, ca să&lt;f&gt;ⓖ&lt;/f&gt; fie &lt;i&gt;atât&lt;/i&gt; tatăl tuturor celor care cred fără a fi circumciși — și să li se socotească și lor o astfel de dreptate —');
INSERT INTO &quot;verses&quot; VALUES (520,4,12,'&lt;i&gt;cât&lt;/i&gt; și tatăl celor circumciși, adică al celor care nu numai că sunt circumciși, ci și calcă pe urmele credinței pe care o avea tatăl nostru Avraam când nu era circumcis.');
INSERT INTO &quot;verses&quot; VALUES (520,4,13,'&lt;pb/&gt;Într-adevăr, nu pe temeiul Legii li s-a dat lui Avraam sau seminței lui făgăduința că vor moșteni&lt;f&gt;ⓗ&lt;/f&gt; lumea, ci pe temeiul dreptății &lt;i&gt;care se capătă&lt;/i&gt; prin credință.');
INSERT INTO &quot;verses&quot; VALUES (520,4,14,'Căci&lt;f&gt;ⓘ&lt;/f&gt;, dacă moștenitori sunt cei ce &lt;i&gt;se bizuie&lt;/i&gt; pe Lege, credința este zădărnicită și făgăduința este desființată,');
INSERT INTO &quot;verses&quot; VALUES (520,4,15,'pentru că Legea&lt;f&gt;ⓙ&lt;/f&gt; aduce mânie; și unde nu este Lege, acolo nu este nici călcare de Lege.');
INSERT INTO &quot;verses&quot; VALUES (520,4,16,'De aceea este prin credință, ca să fie prin har&lt;f&gt;ⓚ&lt;/f&gt; și pentru ca&lt;f&gt;ⓛ&lt;/f&gt; făgăduința să fie întărită pentru toată sămânța lui Avraam: nu numai pentru cei care &lt;i&gt;se bizuie&lt;/i&gt; pe Lege, ci și pentru cei care&lt;f&gt;ⓜ&lt;/f&gt; au credința lui Avraam, tatăl nostru al tuturor,');
INSERT INTO &quot;verses&quot; VALUES (520,4,17,'după cum este scris: &lt;e&gt;Te-am rânduit să fii tatăl multor neamuri.&lt;/e&gt;&lt;f&gt;[20]&lt;/f&gt; &lt;i&gt;El este tatăl nostru&lt;/i&gt; înaintea lui Dumnezeu, în care a crezut, care&lt;f&gt;ⓝ&lt;/f&gt; aduce la viață pe cei morți și care cheamă la ființă cele&lt;f&gt;ⓞ&lt;/f&gt; ce nu sunt.');
INSERT INTO &quot;verses&quot; VALUES (520,4,18,'Nădăjduind împotriva oricărei nădejdi, el a crezut și, &lt;i&gt;astfel&lt;/i&gt;, a ajuns tatăl multor neamuri, după cum i se spusese: &lt;e&gt;Așa va fi sămânța ta.&lt;/e&gt;&lt;f&gt;[21]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,4,19,'Și n-a slăbit în credință când s-a&lt;f&gt;ⓟ&lt;/f&gt; uitat la trupul său, care era &lt;i&gt;aproape&lt;/i&gt; mort (căci avea cam o sută de ani), și la pântecele fără viață al Sarei&lt;f&gt;[22]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,4,20,'El nu s-a îndoit de făgăduința lui Dumnezeu, prin necredință, ci s-a întărit prin credință, dându-I slavă lui Dumnezeu');
INSERT INTO &quot;verses&quot; VALUES (520,4,21,'și fiind deplin încredințat că ce făgăduiește, El poate&lt;f&gt;ⓠ&lt;/f&gt; să și împlinească.');
INSERT INTO &quot;verses&quot; VALUES (520,4,22,'De aceea, &lt;i&gt;credința&lt;/i&gt; &lt;e&gt;i s-a socotit ca dreptate.&lt;/e&gt;&lt;f&gt;[23]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,4,23,'Însă nu&lt;f&gt;ⓡ&lt;/f&gt; numai pentru el este scris că &lt;e&gt;i s-a socotit [ca dreptate]&lt;/e&gt;,');
INSERT INTO &quot;verses&quot; VALUES (520,4,24,'ci este scris și pentru noi, cărora ne va fi &lt;i&gt;de asemenea&lt;/i&gt; socotită ca dreptate, anume celor ce se încred în Cel&lt;f&gt;ⓢ&lt;/f&gt; ce L-a înviat din morți pe Isus, Domnul nostru,');
INSERT INTO &quot;verses&quot; VALUES (520,4,25,'care&lt;f&gt;ⓣ&lt;/f&gt; a fost dat &lt;i&gt;la moarte&lt;/i&gt; pentru abaterile noastre și a înviat&lt;f&gt;ⓤ&lt;/f&gt; pentru îndreptățirea noastră.');
INSERT INTO &quot;verses&quot; VALUES (520,5,1,'&lt;pb/&gt;Deci, fiindcă am fost îndreptățiți prin credință&lt;f&gt;ⓥ&lt;/f&gt;, avem&lt;f&gt;[24]&lt;/f&gt; pace&lt;f&gt;ⓦ&lt;/f&gt; cu Dumnezeu prin&lt;f&gt;ⓧ&lt;/f&gt; Domnul nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (520,5,2,'Prin El, prin credință, am căpătat intrarea în acest har în care ne aflăm și ne lăudăm&lt;f&gt;ⓨ&lt;/f&gt; în nădejdea slavei lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,5,3,'Ba mai mult, ne&lt;f&gt;ⓩ&lt;/f&gt; lăudăm și în necazuri, căci știm&lt;f&gt;ⓐ&lt;/f&gt; că necazul aduce răbdare,');
INSERT INTO &quot;verses&quot; VALUES (520,5,4,'răbdarea&lt;f&gt;ⓑ&lt;/f&gt; formează un caracter încercat, iar acesta aduce nădejde.');
INSERT INTO &quot;verses&quot; VALUES (520,5,5,'Iar nădejdea&lt;f&gt;ⓒ&lt;/f&gt; &lt;i&gt;aceasta&lt;/i&gt; nu dezamăgește, pentru că&lt;f&gt;ⓓ&lt;/f&gt; dragostea lui Dumnezeu a fost turnată în inimile noastre prin Duhul Sfânt, care ne-a fost dat.');
INSERT INTO &quot;verses&quot; VALUES (520,5,6,'&lt;pb/&gt;Pe când eram noi încă slabi, Hristos, la vremea cuvenită, a&lt;f&gt;ⓔ&lt;/f&gt; murit pentru cei nelegiuiți.');
INSERT INTO &quot;verses&quot; VALUES (520,5,7,'Într-adevăr, pentru un om drept cu greu ar muri cineva, însă pentru un om bun poate că s-ar încumeta cineva să moară.');
INSERT INTO &quot;verses&quot; VALUES (520,5,8,'Dar Dumnezeu&lt;f&gt;ⓕ&lt;/f&gt; Își arată dragostea față de noi prin faptul că, pe când noi încă eram păcătoși, Hristos a murit pentru noi.');
INSERT INTO &quot;verses&quot; VALUES (520,5,9,'Deci cu atât mai mult acum, când am fost îndreptățiți prin&lt;f&gt;ⓖ&lt;/f&gt; sângele Lui, vom fi mântuiți prin El de mânia &lt;i&gt;lui Dumnezeu&lt;/i&gt;&lt;f&gt;ⓗ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,5,10,'Căci dacă&lt;f&gt;ⓘ&lt;/f&gt;, vrăjmași fiind, am fost împăcați&lt;f&gt;ⓙ&lt;/f&gt; cu Dumnezeu prin moartea Fiului Său, cu cât mai mult &lt;i&gt;acum&lt;/i&gt;, când suntem împăcați cu El, vom fi mântuiți prin viața Lui.');
INSERT INTO &quot;verses&quot; VALUES (520,5,11,'Și nu doar atât, dar ne și lăudăm în Dumnezeu prin Domnul nostru Isus Hristos, prin care am primit acum împăcarea.');
INSERT INTO &quot;verses&quot; VALUES (520,5,12,'&lt;pb/&gt;De aceea, după cum printr-un&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;singur&lt;/i&gt; om a intrat păcatul în lume, și prin&lt;f&gt;ⓛ&lt;/f&gt; păcat &lt;i&gt;a intrat&lt;/i&gt; moartea, și astfel moartea a trecut asupra tuturor oamenilor, deoarece toți au păcătuit…');
INSERT INTO &quot;verses&quot; VALUES (520,5,13,'(Căci până la &lt;i&gt;darea&lt;/i&gt; Legii păcatul era în lume, dar păcatul&lt;f&gt;ⓜ&lt;/f&gt; nu se pune la socoteală câtă vreme nu există Legea.');
INSERT INTO &quot;verses&quot; VALUES (520,5,14,'Totuși moartea a domnit de la Adam până la Moise și peste cei ce nu păcătuiseră printr-o călcare de lege asemănătoare cu a lui Adam, care&lt;f&gt;ⓝ&lt;/f&gt; este o imagine preînchipuitoare a Celui ce avea să vină.');
INSERT INTO &quot;verses&quot; VALUES (520,5,15,'Însă darul &lt;i&gt;fără plată&lt;/i&gt; nu este precum abaterea, căci dacă, prin abaterea unuia &lt;i&gt;singur&lt;/i&gt;, cei mulți au murit, cu cât mai mult harul lui Dumnezeu și darul &lt;i&gt;venit&lt;/i&gt; prin harul unui singur Om, Isus Hristos, s-au revărsat din belșug peste cei&lt;f&gt;ⓞ&lt;/f&gt; mulți.');
INSERT INTO &quot;verses&quot; VALUES (520,5,16,'În cazul darului &lt;i&gt;fără plată&lt;/i&gt;, urmările nu sunt precum cele pricinuite de păcatul unui singur om, căci judecata &lt;i&gt;cerută&lt;/i&gt; de o &lt;i&gt;singură abatere&lt;/i&gt; a adus osândă, pe când darul &lt;i&gt;fără plată venit&lt;/i&gt; în urma multor abateri &lt;i&gt;a adus&lt;/i&gt; îndreptățire.');
INSERT INTO &quot;verses&quot; VALUES (520,5,17,'Dacă deci, prin abaterea unuia singur, moartea a domnit prin el singur, cu mult mai mult cei ce primesc belșugul de har și darul dreptății vor domni în viață printr-unul singur, Isus Hristos!)');
INSERT INTO &quot;verses&quot; VALUES (520,5,18,'…Astfel dar, după cum printr-o singură abatere a venit osânda peste&lt;f&gt;ⓟ&lt;/f&gt; toți oamenii, tot așa printr-un singur act de dreptate a venit pentru toți oamenii îndreptățirea &lt;i&gt;care duce la&lt;/i&gt; viață.');
INSERT INTO &quot;verses&quot; VALUES (520,5,19,'Căci după cum, prin neascultarea unui singur om, cei mulți au fost făcuți păcătoși, tot astfel, prin ascultarea Unuia singur, cei mulți vor fi făcuți drepți.');
INSERT INTO &quot;verses&quot; VALUES (520,5,20,'Ba încă și Legea&lt;f&gt;ⓠ&lt;/f&gt; s-a adăugat ca să se înmulțească abaterile, dar unde s-a înmulțit păcatul, acolo harul s-a revărsat&lt;f&gt;ⓡ&lt;/f&gt; și mai îmbelșugat,');
INSERT INTO &quot;verses&quot; VALUES (520,5,21,'pentru ca, după cum păcatul a domnit aducând moartea, tot astfel și harul să domnească prin dreptate și &lt;i&gt;să dea&lt;/i&gt; viață veșnică prin Isus Hristos, Domnul nostru.');
INSERT INTO &quot;verses&quot; VALUES (520,6,1,'&lt;pb/&gt;Ce vom zice dar? Să stăruim&lt;f&gt;ⓢ&lt;/f&gt; în păcat ca să se înmulțească harul?');
INSERT INTO &quot;verses&quot; VALUES (520,6,2,'Nicidecum! Noi, care am&lt;f&gt;ⓣ&lt;/f&gt; murit față de păcat, cum să mai trăim în păcat?');
INSERT INTO &quot;verses&quot; VALUES (520,6,3,'Nu știți că noi, care&lt;f&gt;ⓤ&lt;/f&gt; am fost botezați în Hristos Isus, în&lt;f&gt;ⓥ&lt;/f&gt; moartea Lui am fost botezați?');
INSERT INTO &quot;verses&quot; VALUES (520,6,4,'Așadar, am fost îngropați&lt;f&gt;ⓦ&lt;/f&gt; odată cu El, prin botezul în moartea Lui, pentru ca, după cum&lt;f&gt;ⓧ&lt;/f&gt; a înviat Hristos din morți prin slava&lt;f&gt;ⓨ&lt;/f&gt; Tatălui, tot așa&lt;f&gt;ⓩ&lt;/f&gt; și noi să umblăm într-o viață nouă.');
INSERT INTO &quot;verses&quot; VALUES (520,6,5,'Într-adevăr, dacă&lt;f&gt;ⓐ&lt;/f&gt; ne-am făcut una cu El printr-o moarte asemănătoare cu a Lui, vom fi &lt;i&gt;una cu El&lt;/i&gt; și printr-o înviere &lt;i&gt;asemănătoare cu a Lui&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,6,6,'Știm &lt;i&gt;bine&lt;/i&gt; că omul&lt;f&gt;ⓑ&lt;/f&gt; nostru cel vechi a fost răstignit împreună &lt;i&gt;cu El&lt;/i&gt;, pentru ca trupul&lt;f&gt;ⓒ&lt;/f&gt; păcatului să fie dezbrăcat de putere, încât să nu mai fim robi ai păcatului;');
INSERT INTO &quot;verses&quot; VALUES (520,6,7,'căci cine&lt;f&gt;ⓓ&lt;/f&gt; a murit este izbăvit de păcat.');
INSERT INTO &quot;verses&quot; VALUES (520,6,8,'Dacă&lt;f&gt;ⓔ&lt;/f&gt; am murit împreună cu Hristos, credem că vom și trăi împreună cu El,');
INSERT INTO &quot;verses&quot; VALUES (520,6,9,'întrucât știm că Hristos&lt;f&gt;ⓕ&lt;/f&gt;, odată înviat din morți, nu mai moare — moartea nu mai are stăpânire asupra Lui.');
INSERT INTO &quot;verses&quot; VALUES (520,6,10,'Fiindcă, &lt;i&gt;prin moartea de&lt;/i&gt; care a murit, &lt;i&gt;El&lt;/i&gt; a&lt;f&gt;ⓖ&lt;/f&gt; murit pentru păcat o dată pentru totdeauna, iar &lt;i&gt;prin viața pe&lt;/i&gt; care o trăiește, trăiește&lt;f&gt;ⓗ&lt;/f&gt; pentru Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,6,11,'Tot așa și voi socotiți-vă morți&lt;f&gt;ⓘ&lt;/f&gt; față de păcat, dar vii&lt;f&gt;ⓙ&lt;/f&gt; pentru Dumnezeu, în Hristos Isus, [Domnul nostru].');
INSERT INTO &quot;verses&quot; VALUES (520,6,12,'&lt;pb/&gt;Așadar, păcatul să nu mai domnească&lt;f&gt;ⓚ&lt;/f&gt; în trupul vostru muritor și să nu mai ascultați de poftele lui.');
INSERT INTO &quot;verses&quot; VALUES (520,6,13,'Să nu mai dați păcatului mădularele&lt;f&gt;ⓛ&lt;/f&gt; voastre, ca unelte ale nedreptății, ci dați-vă&lt;f&gt;ⓜ&lt;/f&gt; pe voi înșivă lui Dumnezeu, ca înviați din morți, și dați-I lui Dumnezeu mădularele voastre, ca unelte ale dreptății!');
INSERT INTO &quot;verses&quot; VALUES (520,6,14,'Căci&lt;f&gt;ⓝ&lt;/f&gt; păcatul nu va mai stăpâni asupra voastră, pentru că nu sunteți sub Lege, ci sub har.');
INSERT INTO &quot;verses&quot; VALUES (520,6,15,'&lt;pb/&gt;Ce &lt;i&gt;urmează de aici&lt;/i&gt;? Să păcătuim pentru că&lt;f&gt;ⓞ&lt;/f&gt; nu mai suntem sub Lege, ci sub har? Nicidecum!');
INSERT INTO &quot;verses&quot; VALUES (520,6,16,'Nu știți că, dacă&lt;f&gt;ⓟ&lt;/f&gt; vă dați robi cuiva ca să-l ascultați, sunteți robii aceluia de care ascultați, fie ai păcatului, care duce la moarte, fie ai ascultării, care duce la dreptate?');
INSERT INTO &quot;verses&quot; VALUES (520,6,17,'Dar mulțumiri fie aduse lui Dumnezeu, pentru că, deși ați fost robi ai păcatului, v-ați supus din inimă modelului&lt;f&gt;ⓠ&lt;/f&gt; de învățătură care v-a fost încredințat&lt;f&gt;[25]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,6,18,'și, după ce ați fost eliberați&lt;f&gt;ⓡ&lt;/f&gt; de păcat, v-ați făcut robi ai dreptății.');
INSERT INTO &quot;verses&quot; VALUES (520,6,19,'Omenește vorbesc, din pricina slăbiciunii firii voastre pământești&lt;f&gt;[26]&lt;/f&gt;: după cum &lt;i&gt;odinioară&lt;/i&gt; ați făcut mădularele voastre roabe ale necurăției și ale fărădelegii, pentru fărădelege, tot astfel acum faceți mădularele voastre roabe ale dreptății, pentru sfințire!');
INSERT INTO &quot;verses&quot; VALUES (520,6,20,'Căci, pe când erați robi&lt;f&gt;ⓢ&lt;/f&gt; ai păcatului, erați liberi față de dreptate.');
INSERT INTO &quot;verses&quot; VALUES (520,6,21,'Și ce&lt;f&gt;ⓣ&lt;/f&gt; rod aduceați atunci? Rod de care acum vă este rușine, pentru că sfârșitul&lt;f&gt;ⓤ&lt;/f&gt; acelor &lt;i&gt;fapte&lt;/i&gt; este moartea.');
INSERT INTO &quot;verses&quot; VALUES (520,6,22,'Dar acum, după ce ați fost eliberați&lt;f&gt;ⓥ&lt;/f&gt; de păcat și v-ați făcut robi ai lui Dumnezeu, aveți ca rod sfințirea, al cărei sfârșit este viața veșnică.');
INSERT INTO &quot;verses&quot; VALUES (520,6,23,'Fiindcă plata&lt;f&gt;ⓦ&lt;/f&gt; păcatului este moartea, însă darul&lt;f&gt;ⓧ&lt;/f&gt; lui Dumnezeu este viața veșnică în Hristos Isus, Domnul nostru.');
INSERT INTO &quot;verses&quot; VALUES (520,7,1,'&lt;pb/&gt;Nu știți, fraților — vorbesc unor oameni care cunosc Legea — că Legea stăpânește asupra omului câtă vreme el trăiește?');
INSERT INTO &quot;verses&quot; VALUES (520,7,2,'De pildă, femeia&lt;f&gt;ⓨ&lt;/f&gt; măritată este legată prin Lege de bărbatul ei câtă vreme trăiește el, dar dacă-i&lt;f&gt;ⓩ&lt;/f&gt; moare bărbatul, este dezlegată de legea căsătoriei&lt;f&gt;[27]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,7,3,'Dacă deci ajunge a altuia cât încă îi trăiește bărbatul, se va chema adulteră; dar dacă-i moare bărbatul, este liberă față de Lege și nu mai este adulteră dacă ajunge a altuia.');
INSERT INTO &quot;verses&quot; VALUES (520,7,4,'Tot astfel, frații mei, și voi ați&lt;f&gt;ⓐ&lt;/f&gt; murit față de Lege prin trupul lui Hristos, ca să ajungeți ai Altuia, adică ai Celui ce a înviat din morți, ca să aducem rod pentru Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,7,5,'Căci pe când eram în firea pământească, patimile păcatelor, &lt;i&gt;date la iveală&lt;/i&gt; prin Lege, lucrau&lt;f&gt;ⓑ&lt;/f&gt; în mădularele noastre, ca să aducă&lt;f&gt;ⓒ&lt;/f&gt; rod pentru moarte.');
INSERT INTO &quot;verses&quot; VALUES (520,7,6,'Dar acum, am fost dezlegați de Lege, fiindcă am murit față de ceea ce ne ținea &lt;i&gt;în robie&lt;/i&gt;, ca să slujim prin înnoirea&lt;f&gt;ⓓ&lt;/f&gt; &lt;i&gt;lucrată de&lt;/i&gt; Duhul, și nu prin &lt;i&gt;trăirea&lt;/i&gt; veche, după literă.');
INSERT INTO &quot;verses&quot; VALUES (520,7,7,'&lt;pb/&gt;Așadar, ce vom zice? Legea este păcat? Nicidecum! Dimpotrivă, păcatul nu l-am&lt;f&gt;ⓔ&lt;/f&gt; cunoscut decât prin Lege. &lt;i&gt;De pildă&lt;/i&gt;, n-aș fi știut de poftă dacă Legea nu mi-ar fi spus: &lt;e&gt;Să nu poftești&lt;/e&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;e&gt;!&lt;/e&gt;&lt;f&gt;[28]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,7,8,'&lt;pb/&gt;Apoi, păcatul&lt;f&gt;ⓖ&lt;/f&gt; a folosit prilejul și a făcut să se nască în mine, prin poruncă, tot felul de pofte, căci fără Lege, păcatul este mort.');
INSERT INTO &quot;verses&quot; VALUES (520,7,9,'Odinioară, eram fără&lt;f&gt;ⓗ&lt;/f&gt; Lege și trăiam, dar când a venit porunca, păcatul a înviat,');
INSERT INTO &quot;verses&quot; VALUES (520,7,10,'iar eu am murit. Astfel, s-a dovedit că porunca aceea care&lt;f&gt;ⓘ&lt;/f&gt; trebuia să-mi dea viața mi-a adus moartea.');
INSERT INTO &quot;verses&quot; VALUES (520,7,11,'Pentru că păcatul s-a folosit de poruncă, m-a amăgit și, prin ea, mi-a adus moartea.');
INSERT INTO &quot;verses&quot; VALUES (520,7,12,'Așa că Legea&lt;f&gt;ⓙ&lt;/f&gt; este sfântă, iar porunca este sfântă, dreaptă și bună.');
INSERT INTO &quot;verses&quot; VALUES (520,7,13,'Atunci, un lucru bun mi-a adus moartea? Nicidecum! Dar păcatul, tocmai ca să iasă la iveală ca păcat, printr-un lucru bun mi-a pricinuit moartea, pentru ca, prin poruncă, păcatul să se arate din cale afară de păcătos.');
INSERT INTO &quot;verses&quot; VALUES (520,7,14,'&lt;pb/&gt;Știm că Legea este duhovnicească, dar eu sunt firesc, vândut&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;rob&lt;/i&gt; păcatului.');
INSERT INTO &quot;verses&quot; VALUES (520,7,15,'Căci nu știu ce fac: nu&lt;f&gt;ⓛ&lt;/f&gt; fac ce vreau, ci tocmai ce urăsc.');
INSERT INTO &quot;verses&quot; VALUES (520,7,16,'Dacă fac ce nu vreau, recunosc &lt;i&gt;prin aceasta&lt;/i&gt; că Legea este bună.');
INSERT INTO &quot;verses&quot; VALUES (520,7,17,'Dar atunci nu mai sunt eu cel ce lucrează &lt;i&gt;astfel&lt;/i&gt;, ci păcatul, care locuiește în mine.');
INSERT INTO &quot;verses&quot; VALUES (520,7,18,'Știu, &lt;i&gt;desigur&lt;/i&gt;, că nimic bun nu locuiește în&lt;f&gt;ⓜ&lt;/f&gt; mine, adică în firea mea pământească, pentru că, ce-i drept, vreau să fac binele, dar n-am puterea să-l fac.');
INSERT INTO &quot;verses&quot; VALUES (520,7,19,'Căci nu fac binele, pe care-l vreau, ci tocmai răul, pe care nu-l vreau!');
INSERT INTO &quot;verses&quot; VALUES (520,7,20,'Și dacă fac ce nu vreau, nu mai sunt eu cel ce lucrează &lt;i&gt;astfel&lt;/i&gt;, ci păcatul, care locuiește în mine.');
INSERT INTO &quot;verses&quot; VALUES (520,7,21,'Găsesc, așadar, legea aceasta: când vreau să fac binele, răul stă lipit &lt;i&gt;de mine&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,7,22,'Fiindcă, după omul dinăuntru, mă încântă&lt;f&gt;ⓝ&lt;/f&gt; Legea&lt;f&gt;ⓞ&lt;/f&gt; lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (520,7,23,'dar văd în&lt;f&gt;ⓟ&lt;/f&gt; mădularele mele o altă&lt;f&gt;ⓠ&lt;/f&gt; lege, care se luptă cu Legea &lt;i&gt;primită de&lt;/i&gt; mintea mea și care mă face rob legii păcatului din mădularele mele.');
INSERT INTO &quot;verses&quot; VALUES (520,7,24,'O, nenorocitul de mine! Cine mă va izbăvi de acest trup de moarte?');
INSERT INTO &quot;verses&quot; VALUES (520,7,25,'Mulțumiri&lt;f&gt;ⓡ&lt;/f&gt; &lt;i&gt;fie aduse&lt;/i&gt; lui Dumnezeu, prin Isus Hristos, Domnul nostru! Așadar, cu mintea eu slujesc Legii lui Dumnezeu, dar cu firea pământească &lt;i&gt;slujesc&lt;/i&gt; legii păcatului.');
INSERT INTO &quot;verses&quot; VALUES (520,8,1,'&lt;pb/&gt;Acum dar, nu este nicio osândire pentru cei ce sunt în Hristos Isus, [care nu&lt;f&gt;ⓢ&lt;/f&gt; trăiesc potrivit firii pământești, ci potrivit Duhului].');
INSERT INTO &quot;verses&quot; VALUES (520,8,2,'Într-adevăr, legea&lt;f&gt;ⓣ&lt;/f&gt; Duhului&lt;f&gt;ⓤ&lt;/f&gt; dătător de viață în Hristos Isus te-a eliberat de legea&lt;f&gt;ⓥ&lt;/f&gt; păcatului și a morții.');
INSERT INTO &quot;verses&quot; VALUES (520,8,3,'Căci — lucru&lt;f&gt;ⓦ&lt;/f&gt; cu neputință pentru Lege, întrucât firea pământească o făcea fără putere — Dumnezeu&lt;f&gt;ⓧ&lt;/f&gt; L-a trimis, din pricina păcatului, pe Însuși Fiul Său, într-o fire asemănătoare cu cea păcătoasă, și a osândit păcatul în firea pământească,');
INSERT INTO &quot;verses&quot; VALUES (520,8,4,'pentru ca cerința dreaptă a Legii să fie împlinită în noi, care&lt;f&gt;ⓨ&lt;/f&gt; umblăm nu potrivit firii pământești, ci potrivit Duhului.');
INSERT INTO &quot;verses&quot; VALUES (520,8,5,'Într-adevăr, cei ce&lt;f&gt;ⓩ&lt;/f&gt; trăiesc potrivit firii pământești năzuiesc la lucrurile firii pământești, pe când cei&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;ce trăiesc&lt;/i&gt; potrivit Duhului &lt;i&gt;năzuiesc&lt;/i&gt; la lucrurile Duhului.');
INSERT INTO &quot;verses&quot; VALUES (520,8,6,'Și năzuința&lt;f&gt;ⓑ&lt;/f&gt; firii pământești &lt;i&gt;duce la&lt;/i&gt; moarte, pe când năzuința Duhului &lt;i&gt;duce la&lt;/i&gt; viață și pace.');
INSERT INTO &quot;verses&quot; VALUES (520,8,7,'Fiindcă năzuința&lt;f&gt;ⓒ&lt;/f&gt; firii pământești este vrăjmășie împotriva lui Dumnezeu, căci ea nu se supune Legii lui Dumnezeu și nici&lt;f&gt;ⓓ&lt;/f&gt; nu se poate &lt;i&gt;supune&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,8,8,'Deci cei ce trăiesc în firea pământească nu pot să-I placă lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,8,9,'Voi însă nu mai trăiți în firea pământească, ci în Duhul, dacă Duhul&lt;f&gt;ⓔ&lt;/f&gt; lui Dumnezeu locuiește &lt;i&gt;într-adevăr&lt;/i&gt; în voi. Dacă cineva nu are Duhul&lt;f&gt;ⓕ&lt;/f&gt; lui Hristos, acela nu este al Lui.');
INSERT INTO &quot;verses&quot; VALUES (520,8,10,'Și dacă Hristos este în voi, trupul vostru, da, este mort din pricina păcatului, dar Duhul este viață datorită dreptății.');
INSERT INTO &quot;verses&quot; VALUES (520,8,11,'Și dacă Duhul Celui&lt;f&gt;ⓖ&lt;/f&gt; ce L-a înviat pe Isus din morți locuiește în voi, Cel&lt;f&gt;ⓗ&lt;/f&gt; ce L-a înviat pe Hristos Isus din morți va da viață și trupurilor voastre muritoare prin Duhul Său, care locuiește în voi.');
INSERT INTO &quot;verses&quot; VALUES (520,8,12,'&lt;pb/&gt;Așadar&lt;f&gt;ⓘ&lt;/f&gt;, fraților, noi nu suntem datori firii pământești, ca să &lt;i&gt;mai&lt;/i&gt; trăim potrivit firii pământești.');
INSERT INTO &quot;verses&quot; VALUES (520,8,13,'Dacă trăiți&lt;f&gt;ⓙ&lt;/f&gt; potrivit firii pământești, veți muri; dar dacă, prin Duhul, dați morții&lt;f&gt;ⓚ&lt;/f&gt; faptele trupului, veți trăi.');
INSERT INTO &quot;verses&quot; VALUES (520,8,14,'Căci toți cei ce sunt călăuziți&lt;f&gt;ⓛ&lt;/f&gt; de Duhul lui Dumnezeu sunt fii ai lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,8,15,'Și voi n-ați&lt;f&gt;ⓜ&lt;/f&gt; primit un duh de robie, ca să mai aveți frică&lt;f&gt;ⓝ&lt;/f&gt;, ci ați primit un duh&lt;f&gt;ⓞ&lt;/f&gt; de înfiere, prin care strigăm: „&lt;e&gt;Avva&lt;/e&gt;&lt;f&gt;ⓟ&lt;/f&gt;!”&lt;f&gt;[29]&lt;/f&gt;, adică: „Tată!”');
INSERT INTO &quot;verses&quot; VALUES (520,8,16,'Însuși&lt;f&gt;ⓠ&lt;/f&gt; Duhul dă mărturie împreună cu duhul nostru că suntem copiii lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,8,17,'Și, dacă suntem copii, suntem și moștenitori&lt;f&gt;ⓡ&lt;/f&gt;: moștenitori ai lui Dumnezeu și moștenitori împreună cu Hristos, dacă suferim&lt;f&gt;ⓢ&lt;/f&gt; împreună cu El, ca să fim &lt;i&gt;și&lt;/i&gt; proslăviți împreună cu El.');
INSERT INTO &quot;verses&quot; VALUES (520,8,18,'&lt;pb/&gt;Eu socotesc că suferințele&lt;f&gt;ⓣ&lt;/f&gt; din vremea de acum nu sunt vrednice &lt;i&gt;să fie puse alături&lt;/i&gt; de slava viitoare, care se va descoperi față de noi.');
INSERT INTO &quot;verses&quot; VALUES (520,8,19,'De asemenea, și creația&lt;f&gt;ⓤ&lt;/f&gt; așteaptă cu o dorință înfocată descoperirea&lt;f&gt;ⓥ&lt;/f&gt; fiilor lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,8,20,'Căci creația&lt;f&gt;ⓦ&lt;/f&gt; a fost supusă deșertăciunii — nu de bunăvoie, ci din pricina celui care a supus-o — cu nădejdea însă');
INSERT INTO &quot;verses&quot; VALUES (520,8,21,'că și ea va fi eliberată din robia stricăciunii, &lt;i&gt;ca să aibă parte de&lt;/i&gt; libertatea slavei copiilor lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,8,22,'Dar știm că, până acum, toată creația suspină și suferă chinurile facerii.');
INSERT INTO &quot;verses&quot; VALUES (520,8,23,'Și nu numai &lt;i&gt;ea&lt;/i&gt;, ci și cei care au cele dintâi roade&lt;f&gt;ⓧ&lt;/f&gt; ale Duhului: noi înșine suspinăm în sinea noastră și așteptăm înfierea, răscumpărarea trupului&lt;f&gt;ⓨ&lt;/f&gt; nostru.');
INSERT INTO &quot;verses&quot; VALUES (520,8,24,'Căci cu nădejdea&lt;f&gt;ⓩ&lt;/f&gt; aceasta am fost mântuiți. Dar o nădejde care se vede nu mai este nădejde, căci cine nădăjduiește ceea ce se vede?');
INSERT INTO &quot;verses&quot; VALUES (520,8,25,'În schimb, dacă nădăjduim ce nu vedem, așteptăm cu răbdare.');
INSERT INTO &quot;verses&quot; VALUES (520,8,26,'Tot astfel și Duhul ne ajută în slăbiciunea noastră, căci nu&lt;f&gt;ⓐ&lt;/f&gt; știm cum trebuie să ne rugăm. Dar însuși Duhul mijlocește &lt;i&gt;pentru noi&lt;/i&gt; cu suspine negrăite.');
INSERT INTO &quot;verses&quot; VALUES (520,8,27,'Și Cel&lt;f&gt;ⓑ&lt;/f&gt; care cercetează inimile știe care este năzuința Duhului, pentru că El mijlocește pentru sfinți potrivit&lt;f&gt;ⓒ&lt;/f&gt; &lt;i&gt;voii&lt;/i&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,8,28,'&lt;pb/&gt;Știm că toate lucrează împreună spre binele celor ce-L iubesc pe Dumnezeu, al celor care&lt;f&gt;ⓓ&lt;/f&gt; sunt chemați după planul &lt;i&gt;Său&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,8,29,'Căci pe aceia pe care i-a cunoscut mai&lt;f&gt;ⓔ&lt;/f&gt; dinainte i-a și hotărât mai&lt;f&gt;ⓕ&lt;/f&gt; dinainte să&lt;f&gt;ⓖ&lt;/f&gt; fie asemenea chipului Fiului Său, pentru ca El să fie Întâiul Născut dintre mai mulți frați.');
INSERT INTO &quot;verses&quot; VALUES (520,8,30,'Și pe aceia pe care i-a hotărât mai dinainte i-a și chemat&lt;f&gt;ⓗ&lt;/f&gt;, și pe aceia pe care i-a chemat i-a și îndreptățit&lt;f&gt;ⓘ&lt;/f&gt;, iar pe aceia pe care i-a îndreptățit i-a și proslăvit.');
INSERT INTO &quot;verses&quot; VALUES (520,8,31,'&lt;pb/&gt;Deci ce vom zice noi despre &lt;i&gt;toate&lt;/i&gt; acestea? Dacă Dumnezeu este pentru noi, cine &lt;i&gt;va fi&lt;/i&gt; împotriva noastră?');
INSERT INTO &quot;verses&quot; VALUES (520,8,32,'El&lt;f&gt;ⓙ&lt;/f&gt;, care nu L-a cruțat nici pe propriul Său Fiu, ci L-a&lt;f&gt;ⓚ&lt;/f&gt; dat pentru noi toți, cum nu ne va dărui totul &lt;i&gt;fără plată&lt;/i&gt; împreună cu El?');
INSERT INTO &quot;verses&quot; VALUES (520,8,33,'Cine le va aduce învinuiri aleșilor lui Dumnezeu? Dumnezeu&lt;f&gt;ⓛ&lt;/f&gt; este cel care îndreptățește!');
INSERT INTO &quot;verses&quot; VALUES (520,8,34,'Cine-i va osândi? Hristos Isus a murit, ba încă a și înviat, este&lt;f&gt;ⓜ&lt;/f&gt; la dreapta lui Dumnezeu și&lt;f&gt;ⓝ&lt;/f&gt; mijlocește pentru noi!');
INSERT INTO &quot;verses&quot; VALUES (520,8,35,'Cine ne va despărți pe noi de dragostea lui Hristos? Necazul, strâmtorarea, prigoana, foametea, lipsa de îmbrăcăminte, primejdia, sabia?');
INSERT INTO &quot;verses&quot; VALUES (520,8,36,'După cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Din pricina Ta&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;suntem dați morții toată ziua;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;suntem socotiți ca niște oi de tăiat.&lt;/e&gt; &lt;f&gt;[30]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,8,37,'&lt;pb/&gt;Totuși, în toate acestea suntem mai mult decât biruitori&lt;f&gt;ⓟ&lt;/f&gt; prin Acela care ne-a iubit.');
INSERT INTO &quot;verses&quot; VALUES (520,8,38,'Căci sunt încredințat că nici moartea, nici viața, nici îngerii, nici stăpânirile&lt;f&gt;ⓠ&lt;/f&gt;, nici lucrurile prezente, nici cele viitoare, nici puterile,');
INSERT INTO &quot;verses&quot; VALUES (520,8,39,'nici înălțimea, nici adâncimea, nici &lt;i&gt;vreo&lt;/i&gt; altă făptură nu vor putea să ne despartă de dragostea lui Dumnezeu, care este în Hristos Isus, Domnul nostru.');
INSERT INTO &quot;verses&quot; VALUES (520,9,1,'&lt;pb/&gt;Spun&lt;f&gt;ⓡ&lt;/f&gt; adevărul în Hristos, nu mint; conștiința îmi este martoră prin Duhul Sfânt');
INSERT INTO &quot;verses&quot; VALUES (520,9,2,'că&lt;f&gt;ⓢ&lt;/f&gt; am o mare tristețe și o durere necurmată în inimă.');
INSERT INTO &quot;verses&quot; VALUES (520,9,3,'Căci mi-aș fi dorit&lt;f&gt;ⓣ&lt;/f&gt; să fiu eu însumi anatema&lt;f&gt;[31]&lt;/f&gt;, &lt;i&gt;despărțit&lt;/i&gt; de Hristos, pentru frații mei, rudele mele trupești.');
INSERT INTO &quot;verses&quot; VALUES (520,9,4,'Ei sunt israeliți, au&lt;f&gt;ⓤ&lt;/f&gt; înfierea, slava, legămintele&lt;f&gt;ⓥ&lt;/f&gt;, darea Legii, slujirea &lt;i&gt;preoțească&lt;/i&gt;&lt;f&gt;ⓦ&lt;/f&gt;, făgăduințele,');
INSERT INTO &quot;verses&quot; VALUES (520,9,5,'se trag din patriarhi&lt;f&gt;ⓧ&lt;/f&gt; și tot din&lt;f&gt;ⓨ&lt;/f&gt; ei a ieșit, după trup, Hristosul, care&lt;f&gt;ⓩ&lt;/f&gt; este mai presus de toți, Dumnezeu binecuvântat în veci. Amin!');
INSERT INTO &quot;verses&quot; VALUES (520,9,6,'&lt;pb/&gt;Dar nu&lt;f&gt;ⓐ&lt;/f&gt; este ca și când ar fi dat greș Cuvântul lui Dumnezeu, căci nu&lt;f&gt;ⓑ&lt;/f&gt; toți cei din Israel sunt Israel.');
INSERT INTO &quot;verses&quot; VALUES (520,9,7,'Nici copiii &lt;i&gt;lui Avraam&lt;/i&gt; nu sunt toți sămânța lui Avraam, ci &lt;i&gt;este scris&lt;/i&gt;: &lt;e&gt;În Isaac&lt;/e&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;sămânța ta va avea un nume&lt;/e&gt;.&lt;f&gt;[32]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,8,'Aceasta înseamnă că nu copiii trupești sunt copiii lui Dumnezeu, ci copiii&lt;f&gt;ⓓ&lt;/f&gt; făgăduinței sunt socotiți sămânță.');
INSERT INTO &quot;verses&quot; VALUES (520,9,9,'Căci cuvântul acesta este o făgăduință: &lt;i&gt;&lt;e&gt;La anul&lt;/i&gt; pe vremea aceasta, Mă voi întoarce, și Sara va avea un fiu&lt;/e&gt;.&lt;f&gt;[33]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,10,'&lt;pb/&gt;Și nu doar atât; și cu Rebeca&lt;f&gt;ⓔ&lt;/f&gt; a fost la fel, după ce a rămas însărcinată de la un singur om, părintele nostru Isaac.');
INSERT INTO &quot;verses&quot; VALUES (520,9,11,'Căci, deși &lt;i&gt;gemenii&lt;/i&gt; nu se născuseră încă și nici nu făcuseră ceva bun sau rău — ca să rămână în picioare hotărârea și alegerea&lt;f&gt;[34]&lt;/f&gt; lui Dumnezeu, nu prin fapte, ci prin&lt;f&gt;ⓕ&lt;/f&gt; Cel ce cheamă —');
INSERT INTO &quot;verses&quot; VALUES (520,9,12,'&lt;i&gt;Rebecăi&lt;/i&gt; i s-a spus: &lt;e&gt;Cel mai mare va fi rob celui mai mic&lt;/e&gt;&lt;f&gt;[35]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (520,9,13,'după cum este scris: &lt;e&gt;Pe Iacov&lt;/e&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;l-am iubit, iar pe Esau l-am urât.&lt;/e&gt;&lt;f&gt;[36]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,14,'&lt;pb/&gt;Deci ce vom spune? Nu cumva&lt;f&gt;ⓗ&lt;/f&gt; este nedreptate la Dumnezeu? Nicidecum!');
INSERT INTO &quot;verses&quot; VALUES (520,9,15,'Căci El i-a zis lui Moise: &lt;e&gt;Voi avea milă de cine vreau să am milă și Mă voi îndura de cine vreau să Mă îndur.&lt;/e&gt;&lt;f&gt;[37]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,16,'Așadar, nu ține de cine vrea sau de cine aleargă, ci de Dumnezeu, care are milă.');
INSERT INTO &quot;verses&quot; VALUES (520,9,17,'Fiindcă Scriptura&lt;f&gt;ⓘ&lt;/f&gt; îi spune lui Faraon: &lt;e&gt;Tocmai de aceea te-am ridicat: ca să-Mi arăt în tine puterea și pentru ca Numele Meu să fie vestit în tot pământul&lt;/e&gt;.&lt;f&gt;[38]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,18,'Astfel, El are milă de cine vrea și împietrește pe cine vrea.');
INSERT INTO &quot;verses&quot; VALUES (520,9,19,'Dar îmi vei zice: „Atunci de ce mai aduce învinuire? Căci&lt;f&gt;ⓙ&lt;/f&gt; voinței Lui cine poate să-i stea împotrivă?”');
INSERT INTO &quot;verses&quot; VALUES (520,9,20,'Dar, mai degrabă, o, omule, cine ești tu, care răspunzi împotriva lui Dumnezeu? &lt;e&gt;Oare îi va zice vasul de lut&lt;/e&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;e&gt;olarului: „Pentru ce m-ai făcut așa?”&lt;/e&gt;&lt;f&gt;[39]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,21,'Nu are olarul&lt;f&gt;ⓛ&lt;/f&gt; stăpânire asupra lutului ca din același bulgăre&lt;f&gt;[40]&lt;/f&gt; să facă un&lt;f&gt;ⓜ&lt;/f&gt; vas de cinste și unul de ocară?');
INSERT INTO &quot;verses&quot; VALUES (520,9,22,'Și dacă Dumnezeu, vrând să-Și arate mânia și să-Și descopere puterea, a răbdat îndelung niște vase&lt;f&gt;ⓝ&lt;/f&gt; ale mâniei, hărăzite&lt;f&gt;ⓞ&lt;/f&gt; pieirii,');
INSERT INTO &quot;verses&quot; VALUES (520,9,23,'ca să-Și descopere bogăția&lt;f&gt;ⓟ&lt;/f&gt; slavei față de niște vase ale îndurării, pe care le-a pregătit&lt;f&gt;ⓠ&lt;/f&gt; mai dinainte pentru slavă?');
INSERT INTO &quot;verses&quot; VALUES (520,9,24,'&lt;i&gt;Aceste vase suntem noi&lt;/i&gt;, pe care ne-a chemat nu&lt;f&gt;ⓡ&lt;/f&gt; numai dintre iudei, ci și dintre neamuri,');
INSERT INTO &quot;verses&quot; VALUES (520,9,25,'după cum spune în Osea: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Voi&lt;/e&gt; &lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;chema popor al Meu pe cel ce nu era poporul Meu&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și preaiubită pe cea care nu era preaiubită.&lt;/e&gt; &lt;f&gt;[41]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,26,'&lt;t&gt;&lt;e&gt;Și în loc să li se zică:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;„Voi nu sunteți poporul Meu”,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;se vor chema fii ai Dumnezeului celui viu.&lt;/e&gt; &lt;f&gt;[42]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,27,'&lt;pb/&gt;Isaia, pe de altă parte, strigă cu privire la Israel: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Chiar&lt;/e&gt; &lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;dacă numărul fiilor lui Israel ar fi ca nisipul mării&lt;/e&gt;,&lt;/t&gt; &lt;t&gt;&lt;e&gt;numai rămășița va fi mântuită.&lt;/e&gt; &lt;f&gt;[43]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,28,'&lt;t&gt;&lt;e&gt;Căci Domnul va împlini pe deplin și repede,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;pe pământ, cuvântul Lui.&lt;/e&gt; &lt;f&gt;[44]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,29,'&lt;pb/&gt;Și cum zisese Isaia mai înainte: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Dacă&lt;/e&gt; &lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;nu ne-ar fi lăsat Domnul Savaot&lt;/e&gt;&lt;f&gt;[45]&lt;/f&gt; &lt;e&gt;o sămânță,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;am fi ajuns ca&lt;/e&gt; &lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;Sodoma&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și ne-am fi asemănat cu Gomora.&lt;/e&gt; &lt;f&gt;[46]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,9,30,'&lt;pb/&gt;Deci ce vom zice? Neamurile&lt;f&gt;ⓦ&lt;/f&gt;, care nu urmăreau să fie îndreptățite, au fost îndreptățite: au căpătat o dreptate care&lt;f&gt;ⓧ&lt;/f&gt; vine prin credință.');
INSERT INTO &quot;verses&quot; VALUES (520,9,31,'Israel însă, care urmărea&lt;f&gt;ⓨ&lt;/f&gt; o lege a dreptății, n-a&lt;f&gt;ⓩ&lt;/f&gt; ajuns la Lege.');
INSERT INTO &quot;verses&quot; VALUES (520,9,32,'De ce? Pentru că n&lt;i&gt;-a căutat-o&lt;/i&gt; prin credință, ci prin fapte. S-a&lt;f&gt;ⓐ&lt;/f&gt; lovit de piatra de poticnire,');
INSERT INTO &quot;verses&quot; VALUES (520,9,33,'după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată, pun în Sion o piatră&lt;/e&gt; &lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;de poticnire&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și o stâncă de cădere,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și cel care crede în El&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu va fi dat de rușine&lt;/e&gt; &lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;.&lt;/e&gt; &lt;f&gt;[47]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,1,'&lt;pb/&gt;Fraților, dorința inimii mele și rugăciunea mea către Dumnezeu pentru ei este să fie mântuiți.');
INSERT INTO &quot;verses&quot; VALUES (520,10,2,'Mărturisesc despre ei că&lt;f&gt;ⓓ&lt;/f&gt; au râvnă pentru Dumnezeu, dar lipsită de pricepere,');
INSERT INTO &quot;verses&quot; VALUES (520,10,3,'căci, fiindcă n-au cunoscut dreptatea lui Dumnezeu&lt;f&gt;ⓔ&lt;/f&gt; și au căutat să-și statornicească propria lor dreptate,&lt;f&gt;ⓕ&lt;/f&gt; nu s-au supus îndreptățirii de la Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,10,4,'Hristos&lt;f&gt;ⓖ&lt;/f&gt; este ținta Legii, în vederea îndreptățirii fiecăruia care crede.');
INSERT INTO &quot;verses&quot; VALUES (520,10,5,'Într-adevăr, Moise descrie &lt;i&gt;astfel&lt;/i&gt; dreptatea&lt;f&gt;ⓗ&lt;/f&gt; pe care o dă Legea: &lt;e&gt;Omul care face acestea va trăi prin ele.&lt;/e&gt;&lt;f&gt;[48]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,6,'Pe când dreptatea &lt;i&gt;care vine&lt;/i&gt; din credință vorbește astfel: &lt;e&gt;Să nu spui în inima ta&lt;/e&gt;&lt;f&gt;[49]&lt;/f&gt;: &lt;e&gt;„Cine se va sui în cer?”&lt;/e&gt;&lt;f&gt;[50]&lt;/f&gt; (adică să-L coboare pe Hristos)');
INSERT INTO &quot;verses&quot; VALUES (520,10,7,'sau: Cine se va coborî în adânc? (adică să-L scoale pe Hristos din morți).');
INSERT INTO &quot;verses&quot; VALUES (520,10,8,'Dar ce spune &lt;i&gt;ea&lt;/i&gt;? &lt;e&gt;Cuvântul este aproape de tine: în gura ta și în inima ta.&lt;/e&gt;&lt;f&gt;[51]&lt;/f&gt; &lt;i&gt;Acesta este&lt;/i&gt; cuvântul credinței, pe care-l propovăduim noi.');
INSERT INTO &quot;verses&quot; VALUES (520,10,9,'Dacă&lt;i&gt;-L&lt;/i&gt;&lt;f&gt;ⓘ&lt;/f&gt; mărturisești cu gura ta pe Isus ca Domn și crezi în inima ta că Dumnezeu L-a înviat din morți, vei fi mântuit;');
INSERT INTO &quot;verses&quot; VALUES (520,10,10,'căci cu inima se crede în vederea îndreptățirii, iar cu gura se mărturisește în vederea mântuirii.');
INSERT INTO &quot;verses&quot; VALUES (520,10,11,'După cum zice Scriptura: &lt;e&gt;Oricine&lt;/e&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;e&gt;crede în El nu va fi făcut de rușine.&lt;/e&gt;&lt;f&gt;[52]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,12,'Într-adevăr, nu&lt;f&gt;ⓚ&lt;/f&gt; este nicio deosebire între iudeu și grec, căci pentru toți este același&lt;f&gt;ⓛ&lt;/f&gt; Domn, care Își revarsă bogățiile&lt;f&gt;ⓜ&lt;/f&gt; peste toți cei ce-L cheamă.');
INSERT INTO &quot;verses&quot; VALUES (520,10,13,'Fiindcă&lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;oricine va chema Numele&lt;/e&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;Domnului va fi mântuit&lt;/e&gt;&lt;f&gt;[53]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,10,14,'&lt;pb/&gt;Dar cum Îl vor chema pe Acela în care n-au crezut? Și cum vor crede în Acela despre care n-au auzit? Și cum vor auzi despre El fără propovăduitor?');
INSERT INTO &quot;verses&quot; VALUES (520,10,15,'Și cum vor propovădui dacă nu sunt trimiși? După cum este scris: &lt;e&gt;Ce&lt;/e&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;frumoase sunt picioarele [celor ce vestesc pacea, ale] celor ce aduc vești bune!&lt;/e&gt;&lt;f&gt;[54]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,16,'Dar nu&lt;f&gt;ⓠ&lt;/f&gt; toți au ascultat de Evanghelie. Așa cum zice Isaia: &lt;e&gt;Doamne&lt;/e&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;e&gt;, cine a crezut propovăduirea noastră?&lt;/e&gt;&lt;f&gt;[55]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,17,'Astfel, credința vine în urma auzirii, iar auzirea &lt;i&gt;vine&lt;/i&gt; prin Cuvântul lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (520,10,18,'Dar eu întreb: „N-au auzit ei?” Dimpotrivă, &lt;pb/&gt;&lt;t&gt;&lt;e&gt;pe tot pământul a răsunat glasul lor&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și până la marginile lumii, cuvintele lor&lt;/e&gt; &lt;f&gt;[56]&lt;/f&gt;.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,19,'&lt;pb/&gt;Dar întreb iarăși: „Oare n-a cunoscut Israel &lt;i&gt;lucrul acesta&lt;/i&gt;?” Mai întâi spune Moise: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Vă voi întărâta la gelozie prin cei care nu sunt un neam,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;printr-un neam fără pricepere vă voi stârni mânia.&lt;/e&gt; &lt;f&gt;[57]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,20,'&lt;pb/&gt;Iar Isaia îndrăznește și spune: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Am fost găsit de cei ce nu Mă căutau;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;M-am făcut cunoscut celor ce nu întrebau de Mine.&lt;/e&gt; &lt;f&gt;[58]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,10,21,'&lt;pb/&gt;Pe când către Israel zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Toată ziua Mi-am întins mâinile&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;spre un popor neascultător și care întoarce vorba.&lt;/e&gt; &lt;f&gt;[59]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,1,'&lt;pb/&gt;Întreb atunci: Nu cumva Și-a lepădat Dumnezeu poporul? Nicidecum! Căci și&lt;f&gt;ⓢ&lt;/f&gt; eu sunt israelit, din sămânța lui Avraam, din seminția lui Beniamin.');
INSERT INTO &quot;verses&quot; VALUES (520,11,2,'&lt;e&gt;Dumnezeu nu Și-a lepădat poporul&lt;/e&gt;&lt;f&gt;[60]&lt;/f&gt;, pe care l-a&lt;f&gt;ⓣ&lt;/f&gt; cunoscut mai dinainte. Nu știți oare ce spune Scriptura despre Ilie? Cum se plânge el lui Dumnezeu împotriva lui Israel, când spune:');
INSERT INTO &quot;verses&quot; VALUES (520,11,3,'&lt;e&gt;Doamne, pe profeții Tăi i-au omorât, altarele Tale le-au dărâmat; am rămas numai eu și caută să-mi ia viața&lt;/e&gt;&lt;f&gt;[61]&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (520,11,4,'Dar ce-i răspunde Dumnezeu? &lt;e&gt;Mi-am păstrat șapte mii de bărbați, care nu și-au plecat genunchiul înaintea lui Baal.&lt;/e&gt;&lt;f&gt;[62]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,5,'Tot așa&lt;f&gt;ⓤ&lt;/f&gt;, și în vremea de acum este o rămășiță potrivit unei alegeri prin har.');
INSERT INTO &quot;verses&quot; VALUES (520,11,6,'Și dacă&lt;f&gt;ⓥ&lt;/f&gt; este prin har, atunci nu mai este prin fapte; altminteri, harul n-ar mai fi har. [Și dacă este prin fapte, nu mai este prin har; altminteri, fapta n-ar mai fi faptă.]');
INSERT INTO &quot;verses&quot; VALUES (520,11,7,'Ce urmează deci? Că Israel&lt;f&gt;ⓦ&lt;/f&gt; n-a dobândit ceea ce căuta, iar cei aleși au dobândit, pe când ceilalți au fost împietriți,');
INSERT INTO &quot;verses&quot; VALUES (520,11,8,'după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Dumnezeu le-a dat un duh de amorțire&lt;/e&gt; &lt;f&gt;[63]&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;&lt;e&gt;ochi cu care să nu vadă și urechi cu care să n-audă&lt;/e&gt; &lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;până în ziua de astăzi.&lt;/e&gt; &lt;f&gt;[64]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,9,'&lt;pb/&gt;Și David zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Facă-se masa lor cursă și laț,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;prilej de cădere și de plată dreaptă.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,10,'&lt;t&gt;&lt;e&gt;Să li se întunece ochii ca să nu vadă&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și spinarea să li se încovoaie mereu.&lt;/e&gt; &lt;f&gt;[65]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,11,'&lt;pb/&gt;Întreb deci: S-au poticnit ei ca să cadă? Nicidecum! Ci, prin&lt;f&gt;ⓨ&lt;/f&gt; abaterea lor, [a venit] mântuirea neamurilor, ca să stârnească gelozia &lt;i&gt;lui Israel&lt;/i&gt;;');
INSERT INTO &quot;verses&quot; VALUES (520,11,12,'și dacă abaterea lor a fost bogăție pentru lume, iar paguba lor, bogăție pentru neamuri, cu atât mai mult va fi plinătatea &lt;i&gt;întoarcerii&lt;/i&gt;&lt;f&gt;[66]&lt;/f&gt; lor!');
INSERT INTO &quot;verses&quot; VALUES (520,11,13,'V-o spun vouă, neamurilor: atât cât sunt&lt;f&gt;ⓩ&lt;/f&gt; eu însumi apostol al neamurilor, îmi slăvesc slujba mea,');
INSERT INTO &quot;verses&quot; VALUES (520,11,14,'doar-doar voi stârni gelozia celor din neamul meu și-i voi&lt;f&gt;ⓐ&lt;/f&gt; mântui pe unii din ei.');
INSERT INTO &quot;verses&quot; VALUES (520,11,15,'Căci dacă lepădarea lor &lt;i&gt;a însemnat&lt;/i&gt; împăcarea lumii, ce va fi primirea lor, dacă nu viață din morți?');
INSERT INTO &quot;verses&quot; VALUES (520,11,16,'Iar dacă pârga este sfântă, și aluatul este sfânt; și dacă rădăcina este sfântă, și ramurile sunt sfinte.');
INSERT INTO &quot;verses&quot; VALUES (520,11,17,'Dar dacă unele&lt;f&gt;ⓑ&lt;/f&gt; ramuri au fost rupte, iar tu&lt;f&gt;ⓒ&lt;/f&gt;, deși ești măslin sălbatic, ai fost altoit între cele &lt;i&gt;rămase&lt;/i&gt; și ai ajuns părtaș rădăcinii și grăsimii măslinului,');
INSERT INTO &quot;verses&quot; VALUES (520,11,18,'nu&lt;f&gt;ⓓ&lt;/f&gt; te făli față de ramuri! Dacă te fălești, &lt;i&gt;să știi&lt;/i&gt; că nu tu ții rădăcina, ci rădăcina &lt;i&gt;te ține&lt;/i&gt; pe tine.');
INSERT INTO &quot;verses&quot; VALUES (520,11,19,'Atunci vei spune: „Ramurile au fost rupte ca eu să fiu altoit.”');
INSERT INTO &quot;verses&quot; VALUES (520,11,20,'Bine &lt;i&gt;spus&lt;/i&gt;: din pricina necredinței au fost rupte, iar tu prin credință stai în picioare! Nu&lt;f&gt;ⓔ&lt;/f&gt; te îngâmfa, ci teme-te&lt;f&gt;ⓕ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (520,11,21,'Căci dacă n-a cruțat Dumnezeu ramurile firești, nici pe tine nu te va cruța.');
INSERT INTO &quot;verses&quot; VALUES (520,11,22,'Vezi deci bunătatea și asprimea lui Dumnezeu: asprime față de cei ce au căzut și bunătate față de tine, dacă&lt;f&gt;ⓖ&lt;/f&gt; vei stărui în această bunătate; altfel, vei fi tăiat și&lt;f&gt;ⓗ&lt;/f&gt; tu!');
INSERT INTO &quot;verses&quot; VALUES (520,11,23,'Dar și aceia, dacă&lt;f&gt;ⓘ&lt;/f&gt; nu stăruie în necredință, vor fi altoiți, căci Dumnezeu poate să-i altoiască din nou.');
INSERT INTO &quot;verses&quot; VALUES (520,11,24,'Fiindcă, dacă tu ai fost tăiat dintr-un măslin sălbatic din fire și ai fost altoit, peste fire, într-un măslin bun, cu atât mai mult vor fi altoiți aceștia, ramurile firești, în propriul lor măslin.');
INSERT INTO &quot;verses&quot; VALUES (520,11,25,'&lt;pb/&gt;Fraților, ca să nu vă socotiți singuri înțelepți&lt;f&gt;ⓙ&lt;/f&gt;, nu vreau să fiți în necunoștință cu privire la taina aceasta: o parte din Israel rămâne împietrită&lt;f&gt;ⓚ&lt;/f&gt; până&lt;f&gt;ⓛ&lt;/f&gt; la plinătatea întoarcerii neamurilor.');
INSERT INTO &quot;verses&quot; VALUES (520,11,26,'Și astfel, tot Israelul va fi mântuit, după cum stă scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Izbăvitorul va veni&lt;/e&gt; &lt;f&gt;ⓜ&lt;/f&gt; &lt;e&gt;din Sion&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și va îndepărta nelegiuirile de la Iacov.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,27,'&lt;t&gt;&lt;e&gt;Acesta &lt;i&gt;va fi&lt;/i&gt; legământul&lt;/e&gt; &lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;dintre mine și ei&lt;/e&gt;&lt;f&gt;[67]&lt;/f&gt;&lt;e&gt;,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;când voi înlătura păcatele lor.&lt;/e&gt; &lt;f&gt;[68]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,28,'&lt;pb/&gt;În ce privește Evanghelia, ei sunt vrăjmași pentru binele vostru, dar în ce privește alegerea, sunt iubiți&lt;f&gt;ⓞ&lt;/f&gt; de dragul părinților lor.');
INSERT INTO &quot;verses&quot; VALUES (520,11,29,'Căci darurile și chemarea lui Dumnezeu nu se schimbă&lt;f&gt;ⓟ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,11,30,'După cum voi, odinioară&lt;f&gt;ⓠ&lt;/f&gt;, n-ați ascultat de Dumnezeu, iar acum ați căpătat îndurare prin neascultarea acelora,');
INSERT INTO &quot;verses&quot; VALUES (520,11,31,'tot astfel, acum, ei n-au ascultat pentru ca, prin îndurarea arătată vouă, să aibă și ei parte de îndurare.');
INSERT INTO &quot;verses&quot; VALUES (520,11,32,'Fiindcă Dumnezeu&lt;f&gt;ⓡ&lt;/f&gt; i-a închis pe toți în neascultare, ca să Se îndure de toți.');
INSERT INTO &quot;verses&quot; VALUES (520,11,33,'&lt;pb/&gt;&lt;t&gt;O, adâncul bogăției, înțelepciunii și cunoașterii lui Dumnezeu!&lt;/t&gt; &lt;t&gt;Cât&lt;f&gt;ⓢ&lt;/f&gt; de nepătrunse sunt judecățile Sale&lt;/t&gt; &lt;t&gt;și cât de neînțelese&lt;f&gt;ⓣ&lt;/f&gt; sunt căile Sale!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,34,'&lt;t&gt;&lt;e&gt;Căci&lt;/e&gt; &lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;cine a cunoscut gândul Domnului?&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Sau cine&lt;/e&gt; &lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;a fost sfătuitorul Său?&lt;/e&gt;&lt;f&gt;[69]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,35,'&lt;t&gt;&lt;e&gt;Cine&lt;/e&gt; &lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;I-a dat ceva întâi,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca să primească înapoi de la El?&lt;/e&gt; &lt;f&gt;[70]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,11,36,'&lt;t&gt;Din El&lt;f&gt;ⓧ&lt;/f&gt;, prin El și pentru El&lt;/t&gt; &lt;t&gt;sunt toate lucrurile.&lt;/t&gt; &lt;t&gt;A Lui să fie slava în veci! Amin!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,12,1,'&lt;pb/&gt;Vă îndemn deci, fraților, pentru îndurarea lui Dumnezeu, să&lt;f&gt;ⓨ&lt;/f&gt; vă aduceți&lt;f&gt;ⓩ&lt;/f&gt; trupurile ca jertfă vie, sfântă, plăcută lui Dumnezeu; aceasta va fi închinarea voastră duhovnicească&lt;f&gt;[71]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,12,2,'Și să&lt;f&gt;ⓐ&lt;/f&gt; nu vă luați după chipul veacului acestuia, ci să vă transformați prin înnoirea minții, ca să puteți deosebi&lt;f&gt;ⓑ&lt;/f&gt; care este voia lui Dumnezeu: cea bună, plăcută și desăvârșită.');
INSERT INTO &quot;verses&quot; VALUES (520,12,3,'Prin harul care mi-a fost dat, îi spun fiecăruia dintre voi să&lt;f&gt;ⓒ&lt;/f&gt; nu aibă despre sine gânduri mai înalte decât se cuvine, ci să aibă gânduri cumpătate, după măsura de credință pe care a împărțit-o Dumnezeu fiecăruia&lt;f&gt;ⓓ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,12,4,'Căci, după cum într-un trup avem mai multe mădulare și mădularele n-au toate aceeași funcție,');
INSERT INTO &quot;verses&quot; VALUES (520,12,5,'tot astfel noi&lt;f&gt;ⓔ&lt;/f&gt;, deși suntem mulți, suntem un trup în Hristos, dar fiecare în parte suntem mădulare unii altora.');
INSERT INTO &quot;verses&quot; VALUES (520,12,6,'Fiindcă avem&lt;f&gt;ⓕ&lt;/f&gt; diferite daruri, după harul care ne-a fost dat, &lt;i&gt;fiecare să le întrebuințăm astfel&lt;/i&gt;: dacă este darul profeției&lt;f&gt;ⓖ&lt;/f&gt;, după măsura credinței;');
INSERT INTO &quot;verses&quot; VALUES (520,12,7,'dacă este slujirea, în slujire; cine este învățător&lt;f&gt;ⓗ&lt;/f&gt;, în învățătură;');
INSERT INTO &quot;verses&quot; VALUES (520,12,8,'cine îmbărbătează&lt;f&gt;ⓘ&lt;/f&gt;, în îmbărbătare; cine dăruiește, &lt;i&gt;să dea&lt;/i&gt; în chip dezinteresat; cine&lt;f&gt;ⓙ&lt;/f&gt; cârmuiește, &lt;i&gt;să arate&lt;/i&gt; sârguință; cine face milostenie, s-o facă&lt;f&gt;ⓚ&lt;/f&gt; bucuros.');
INSERT INTO &quot;verses&quot; VALUES (520,12,9,'&lt;pb/&gt;Dragostea&lt;f&gt;ⓛ&lt;/f&gt; să fie neprefăcută. Urâți răul și alipiți-vă de bine!');
INSERT INTO &quot;verses&quot; VALUES (520,12,10,'Iubiți-vă unii pe alții cu dragoste frățească&lt;f&gt;ⓜ&lt;/f&gt;! Dați-vă întâietate unii altora în ce privește cinstea!');
INSERT INTO &quot;verses&quot; VALUES (520,12,11,'În sârguință, nu fiți leneși, ci plini de râvnă cu duhul! Slujiți-I Domnului!');
INSERT INTO &quot;verses&quot; VALUES (520,12,12,'Bucurați-vă&lt;f&gt;ⓝ&lt;/f&gt; în nădejde! Fiți răbdători&lt;f&gt;ⓞ&lt;/f&gt; în necaz! Stăruiți&lt;f&gt;ⓟ&lt;/f&gt; în rugăciune!');
INSERT INTO &quot;verses&quot; VALUES (520,12,13,'Fiți părtași la nevoile&lt;f&gt;ⓠ&lt;/f&gt; sfinților! Fiți primitori&lt;f&gt;ⓡ&lt;/f&gt; de oaspeți!');
INSERT INTO &quot;verses&quot; VALUES (520,12,14,'Binecuvântați&lt;f&gt;ⓢ&lt;/f&gt; pe cei ce vă prigonesc, binecuvântați și nu blestemați!');
INSERT INTO &quot;verses&quot; VALUES (520,12,15,'Bucurați-vă cu cei ce se bucură; plângeți cu cei ce plâng!');
INSERT INTO &quot;verses&quot; VALUES (520,12,16,'Aveți&lt;f&gt;ⓣ&lt;/f&gt; aceleași gânduri unii față de alții! Să nu aveți&lt;f&gt;ⓤ&lt;/f&gt; gânduri de mărire, ci luați parte la cele smerite! Nu&lt;f&gt;ⓥ&lt;/f&gt; vă socotiți singuri înțelepți!');
INSERT INTO &quot;verses&quot; VALUES (520,12,17,'&lt;pb/&gt;Nu&lt;f&gt;ⓦ&lt;/f&gt; întoarceți nimănui rău pentru rău! Fiți preocupați de ceea ce este bine înaintea tuturor oamenilor!');
INSERT INTO &quot;verses&quot; VALUES (520,12,18,'Dacă este cu putință, atât cât ține de voi, trăiți&lt;f&gt;ⓧ&lt;/f&gt; în pace cu toți oamenii!');
INSERT INTO &quot;verses&quot; VALUES (520,12,19,'Nu&lt;f&gt;ⓨ&lt;/f&gt; vă răzbunați singuri, preaiubiților, ci lăsați aceasta în seama mâniei &lt;i&gt;lui Dumnezeu&lt;/i&gt;, căci este scris: &lt;e&gt;A Mea este răzbunarea&lt;/e&gt;&lt;f&gt;ⓩ&lt;/f&gt;&lt;e&gt;; Eu voi răsplăti&lt;/e&gt;&lt;f&gt;[72]&lt;/f&gt;, zice Domnul.');
INSERT INTO &quot;verses&quot; VALUES (520,12,20,'Dimpotrivă&lt;f&gt;ⓐ&lt;/f&gt;, &lt;e&gt;dacă îi este foame vrăjmașului tău, dă-i să mănânce; dacă-i este sete, dă-i să bea; căci, dacă vei face astfel, vei îngrămădi cărbuni aprinși pe capul lui.&lt;/e&gt;&lt;f&gt;[73]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,12,21,'Nu te lăsa biruit de rău, ci biruiește răul prin bine!');
INSERT INTO &quot;verses&quot; VALUES (520,13,1,'&lt;pb/&gt;Orice om&lt;f&gt;ⓑ&lt;/f&gt; să fie supus înaltelor autorități, căci nu există autoritate care să nu vină de la Dumnezeu. Și&lt;f&gt;ⓒ&lt;/f&gt; cele care sunt, de Dumnezeu sunt rânduite.');
INSERT INTO &quot;verses&quot; VALUES (520,13,2,'Astfel, cine se opune autorității se împotrivește rânduielii lui Dumnezeu, și cei ce se împotrivesc își vor lua osânda.');
INSERT INTO &quot;verses&quot; VALUES (520,13,3,'Conducătorii nu sunt de temut pentru o faptă bună, ci pentru una rea. Vrei să nu-ți fie teamă de autoritate? Fă&lt;f&gt;ⓓ&lt;/f&gt; binele și vei avea laudă de la ea!');
INSERT INTO &quot;verses&quot; VALUES (520,13,4,'Căci ea este slujitorul lui Dumnezeu pentru binele tău. Dar dacă faci răul, teme-te, căci nu degeaba poartă sabia! Căci ea este slujitorul lui Dumnezeu, care pedepsește cu mânie pe cel ce face răul.');
INSERT INTO &quot;verses&quot; VALUES (520,13,5,'De aceea trebuie să fiți supuși: însă nu doar din pricina acestei mânii, ci&lt;f&gt;ⓔ&lt;/f&gt; și din conștiință.');
INSERT INTO &quot;verses&quot; VALUES (520,13,6,'De aceea plătiți și dările! Căci &lt;i&gt;conducătorii&lt;/i&gt;&lt;f&gt;[74]&lt;/f&gt; sunt slujbași ai lui Dumnezeu, stăruind necurmat tocmai în aceasta.');
INSERT INTO &quot;verses&quot; VALUES (520,13,7,'Dați&lt;f&gt;ⓕ&lt;/f&gt; tuturor cele datorate: dările, cui &lt;i&gt;se cuvin&lt;/i&gt; dările; vama, cui datorați vama; teama, cui datorați teama; cinstea, cui datorați cinstea.');
INSERT INTO &quot;verses&quot; VALUES (520,13,8,'&lt;pb/&gt;Să nu datorați nimănui nimic, decât să vă iubiți unii pe alții, căci cine&lt;f&gt;ⓖ&lt;/f&gt; iubește pe celălalt a împlinit Legea.');
INSERT INTO &quot;verses&quot; VALUES (520,13,9,'Căci poruncile: &lt;e&gt;Să&lt;/e&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;nu săvârșești adulter, să nu ucizi, să nu furi, [să nu dai mărturie mincinoasă], să nu poftești&lt;/e&gt;&lt;f&gt;[75]&lt;/f&gt; și orice altă poruncă se cuprind în cuvântul acesta: &lt;e&gt;Să-l&lt;/e&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;e&gt;iubești pe aproapele tău ca pe tine însuți.&lt;/e&gt;&lt;f&gt;[76]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,13,10,'Dragostea nu face rău aproapelui: dragostea&lt;f&gt;ⓙ&lt;/f&gt; este deci împlinirea Legii.');
INSERT INTO &quot;verses&quot; VALUES (520,13,11,'Și aceasta fiindcă știți ce vremuri sunt: este deja ceasul să vă&lt;f&gt;ⓚ&lt;/f&gt; treziți din somn, căci acum mântuirea este mai aproape de noi decât atunci când am crezut.');
INSERT INTO &quot;verses&quot; VALUES (520,13,12,'Noaptea e pe sfârșite, se apropie ziua. Să&lt;f&gt;ⓛ&lt;/f&gt; ne dezbrăcăm deci de faptele întunericului și să&lt;f&gt;ⓜ&lt;/f&gt; ne îmbrăcăm cu armele luminii.');
INSERT INTO &quot;verses&quot; VALUES (520,13,13,'Să&lt;f&gt;ⓝ&lt;/f&gt; umblăm cum se cuvine, ca în timpul zilei, nu&lt;f&gt;ⓞ&lt;/f&gt; în chefuri și în beții; nu&lt;f&gt;ⓟ&lt;/f&gt; în desfrâu și în dezmăț; nu în certuri și în invidie.');
INSERT INTO &quot;verses&quot; VALUES (520,13,14,'Dimpotrivă, îmbrăcați-vă&lt;f&gt;ⓠ&lt;/f&gt; în Domnul Isus Hristos și nu vă îngrijiți de&lt;f&gt;ⓡ&lt;/f&gt; firea pământească pentru a-i îndeplini poftele!');
INSERT INTO &quot;verses&quot; VALUES (520,14,1,'&lt;pb/&gt;Acceptați-l pe cel slab&lt;f&gt;ⓢ&lt;/f&gt; în credință, fără certuri asupra părerilor îndoielnice!');
INSERT INTO &quot;verses&quot; VALUES (520,14,2,'Unul crede că &lt;i&gt;poate&lt;/i&gt; să mănânce&lt;f&gt;ⓣ&lt;/f&gt; de toate, pe când cel slab mănâncă verdețuri.');
INSERT INTO &quot;verses&quot; VALUES (520,14,3,'Cine mănâncă să nu-l disprețuiască pe cine&lt;f&gt;ⓤ&lt;/f&gt; nu mănâncă, iar cine nu mănâncă să nu-l judece pe cine mănâncă, fiindcă Dumnezeu l-a acceptat.');
INSERT INTO &quot;verses&quot; VALUES (520,14,4,'Cine ești tu&lt;f&gt;ⓥ&lt;/f&gt;, care judeci pe slujitorul altuia? Față de stăpânul său stă în picioare sau cade; și va sta în picioare, căci Domnul are putere să-l facă să stea.');
INSERT INTO &quot;verses&quot; VALUES (520,14,5,'Unul&lt;f&gt;ⓦ&lt;/f&gt; socotește o zi mai presus decât alta, dar pentru altul toate zilele &lt;i&gt;sunt la fel&lt;/i&gt;. Fiecare să fie deplin încredințat în cugetul lui.');
INSERT INTO &quot;verses&quot; VALUES (520,14,6,'Cine ține o zi, pentru Domnul o ține. Cine mănâncă, pentru Domnul mănâncă, pentru că Îi mulțumește&lt;f&gt;ⓧ&lt;/f&gt; lui Dumnezeu. Cine nu mănâncă, pentru Domnul nu mănâncă și Îi mulțumește &lt;i&gt;și el&lt;/i&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,14,7,'Fiindcă nimeni&lt;f&gt;ⓨ&lt;/f&gt; dintre noi nu trăiește pentru sine și nimeni nu moare pentru sine.');
INSERT INTO &quot;verses&quot; VALUES (520,14,8,'Dacă trăim, pentru Domnul trăim și, dacă murim, pentru Domnul murim. Deci, fie că trăim, fie că murim, ai Domnului suntem.');
INSERT INTO &quot;verses&quot; VALUES (520,14,9,'Căci Hristos&lt;f&gt;ⓩ&lt;/f&gt; pentru aceasta a murit și a înviat: ca să domnească și peste morți, și peste vii.');
INSERT INTO &quot;verses&quot; VALUES (520,14,10,'Dar tu de ce-l judeci pe fratele tău? Și tu de ce-l disprețuiești pe fratele tău? Fiindcă toți&lt;f&gt;ⓐ&lt;/f&gt; ne vom înfățișa înaintea scaunului de judecată al lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,14,11,'După cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;„Pe viața Mea”, zice Domnul, „că înaintea Mea se va pleca orice genunchi&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și orice limbă va mărturisi pe Dumnezeu.”&lt;/e&gt; &lt;f&gt;[77]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,14,12,'&lt;pb/&gt;Așa că fiecare&lt;f&gt;ⓑ&lt;/f&gt; din noi va da socoteală despre sine însuși lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,14,13,'&lt;pb/&gt;Să nu ne mai judecăm deci unii pe alții, ci mai bine să judecați așa: să nu puneți un prilej de cădere sau o piatră de poticnire&lt;f&gt;ⓒ&lt;/f&gt; înaintea unui frate.');
INSERT INTO &quot;verses&quot; VALUES (520,14,14,'Eu știu și sunt încredințat în Domnul Isus că&lt;f&gt;ⓓ&lt;/f&gt; nimic nu este spurcat în sine, ci &lt;i&gt;doar&lt;/i&gt; pentru cel&lt;f&gt;ⓔ&lt;/f&gt; ce socotește că ceva este spurcat, pentru acela este spurcat.');
INSERT INTO &quot;verses&quot; VALUES (520,14,15,'Căci, dacă din pricina unei mâncări se mâhnește fratele tău, nu mai umbli potrivit cu dragostea. Nu&lt;f&gt;ⓕ&lt;/f&gt; nimici, prin mâncarea ta, pe acela pentru care a murit Hristos!');
INSERT INTO &quot;verses&quot; VALUES (520,14,16,'Nu&lt;f&gt;ⓖ&lt;/f&gt; faceți ca binele vostru să fie vorbit de rău!');
INSERT INTO &quot;verses&quot; VALUES (520,14,17,'Căci&lt;f&gt;ⓗ&lt;/f&gt; Împărăția lui Dumnezeu nu este mâncare și băutură, ci dreptate, pace și bucurie în Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (520,14,18,'Cine Îi slujește astfel lui Hristos Îi este plăcut&lt;f&gt;ⓘ&lt;/f&gt; lui Dumnezeu și este cinstit de oameni.');
INSERT INTO &quot;verses&quot; VALUES (520,14,19,'Așadar&lt;f&gt;ⓙ&lt;/f&gt;, să urmărim lucrurile care aduc pacea și ne zidesc unii pe alții&lt;f&gt;ⓚ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,14,20,'Să nu nimicești lucrarea lui Dumnezeu pentru o mâncare. Într-adevăr, toate&lt;f&gt;ⓛ&lt;/f&gt; sunt curate; dar&lt;f&gt;ⓜ&lt;/f&gt; este rău când un om, din pricina unui prilej de cădere, ajunge să mănânce din ele&lt;f&gt;[78]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,14,21,'Bine este să nu mănânci carne&lt;f&gt;ⓝ&lt;/f&gt;, să nu bei vin și să te ferești de orice l-ar face pe fratele tău să cadă.');
INSERT INTO &quot;verses&quot; VALUES (520,14,22,'Încredințarea pe care o ai păstreaz-o pentru tine, înaintea lui Dumnezeu! Ferice&lt;f&gt;ⓞ&lt;/f&gt; de cel ce nu se osândește singur prin ceea ce încuviințează!');
INSERT INTO &quot;verses&quot; VALUES (520,14,23,'Dar cine are îndoieli, și totuși mănâncă, este osândit, pentru că nu &lt;i&gt;mănâncă&lt;/i&gt; din încredințare. Tot&lt;f&gt;ⓟ&lt;/f&gt; ce nu vine din încredințare este păcat.');
INSERT INTO &quot;verses&quot; VALUES (520,15,1,'&lt;pb/&gt;Noi&lt;f&gt;ⓠ&lt;/f&gt;, care suntem tari, suntem datori să purtăm slăbiciunile&lt;f&gt;ⓡ&lt;/f&gt; celor slabi și să nu ne facem pe plac nouă înșine.');
INSERT INTO &quot;verses&quot; VALUES (520,15,2,'Fiecare&lt;f&gt;ⓢ&lt;/f&gt; din noi să facă pe plac aproapelui în ce este bine, spre zidire&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (520,15,3,'Căci&lt;f&gt;ⓤ&lt;/f&gt; nici Hristos nu Și-a făcut pe plac Lui Însuși, ci, după cum este scris: &lt;e&gt;Ocările celor ce Te ocărăsc au căzut peste Mine.&lt;/e&gt;&lt;f&gt;[79]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,15,4,'Și tot ce a fost&lt;f&gt;ⓥ&lt;/f&gt; scris mai înainte a fost scris pentru învățătura noastră, pentru ca, prin răbdarea și prin mângâierea pe care le aduc Scripturile, să avem nădejde.');
INSERT INTO &quot;verses&quot; VALUES (520,15,5,'Dumnezeul&lt;f&gt;ⓦ&lt;/f&gt; răbdării și al mângâierii să vă dea aceleași gânduri unii față de alții ca și Hristos Isus,');
INSERT INTO &quot;verses&quot; VALUES (520,15,6,'pentru ca împreună, cu&lt;f&gt;ⓧ&lt;/f&gt; o singură gură, să-L slăviți pe Dumnezeu, Tatăl Domnului nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (520,15,7,'Așadar, acceptați-vă&lt;f&gt;ⓨ&lt;/f&gt; unii pe alții, cum&lt;f&gt;ⓩ&lt;/f&gt; și Hristos v-a acceptat pe voi, spre slava lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (520,15,8,'&lt;pb/&gt;Vă spun că Hristos&lt;f&gt;ⓐ&lt;/f&gt; S-a făcut slujitor al circumciziei pentru adevărul lui Dumnezeu, ca să&lt;f&gt;ⓑ&lt;/f&gt; întărească făgăduințele date părinților,');
INSERT INTO &quot;verses&quot; VALUES (520,15,9,'iar neamurile să-L slăvească&lt;f&gt;ⓒ&lt;/f&gt; pe Dumnezeu pentru îndurarea Lui, după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;De aceea Te voi lăuda printre neamuri&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și Numelui Tău Îi voi cânta.&lt;/e&gt; &lt;f&gt;[80]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,15,10,'&lt;pb/&gt;Și mai zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Veseliți-vă, neamuri, împreună cu poporul Său!&lt;/e&gt; &lt;f&gt;[81]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,15,11,'&lt;pb/&gt;Iar apoi: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Lăudați-L pe Domnul, toate neamurile;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;slăviți-L, toate popoarele!&lt;/e&gt; &lt;f&gt;[82]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,15,12,'&lt;pb/&gt;Tot astfel, și Isaia spune: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Se va ivi Rădăcina lui Ișai,&lt;/e&gt; &lt;f&gt;ⓓ&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Cel care Se va ridica să domnească peste neamuri;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;în El vor nădăjdui neamurile.&lt;/e&gt; &lt;f&gt;[83]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,15,13,'&lt;pb/&gt;Dumnezeul nădejdii să vă umple de toată bucuria&lt;f&gt;ⓔ&lt;/f&gt; și pacea în credință, ca să fiți tot mai bogați în nădejde, prin puterea Duhului Sfânt!');
INSERT INTO &quot;verses&quot; VALUES (520,15,14,'&lt;pb/&gt;Eu, unul, sunt&lt;f&gt;ⓕ&lt;/f&gt; încredințat cu privire la voi, frații mei, că sunteți plini de bunătate, plini&lt;f&gt;ⓖ&lt;/f&gt; de toată cunoștința și în stare să vă sfătuiți unii pe alții.');
INSERT INTO &quot;verses&quot; VALUES (520,15,15,'Totuși, pe alocuri, v-am scris cu mai multă îndrăzneală, ca să vă fie spre aducere aminte, prin harul&lt;f&gt;ⓗ&lt;/f&gt; pe care mi l-a dat Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (520,15,16,'ca să fiu&lt;f&gt;ⓘ&lt;/f&gt; slujitorul lui Hristos Isus între neamuri și să împlinesc slujba preoțească a Evangheliei lui Dumnezeu, pentru ca neamurile să fie o ofrandă&lt;f&gt;ⓙ&lt;/f&gt; bine primită, sfințită prin Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (520,15,17,'Deci mă pot lăuda în&lt;f&gt;ⓚ&lt;/f&gt; Hristos Isus cu lucrarea &lt;i&gt;mea&lt;/i&gt; pentru Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (520,15,18,'Căci n-aș îndrăzni să pomenesc altceva decât ce a lucrat&lt;f&gt;ⓛ&lt;/f&gt; Hristos prin mine, pentru&lt;f&gt;ⓜ&lt;/f&gt; aducerea neamurilor la ascultare prin cuvânt și faptă:');
INSERT INTO &quot;verses&quot; VALUES (520,15,19,'prin&lt;f&gt;ⓝ&lt;/f&gt; puterea semnelor și a minunilor și prin puterea Duhului lui Dumnezeu. Așa că, din Ierusalim și împrejurimi până la Iliric, am răspândit din plin Evanghelia lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (520,15,20,'Am dorit astfel cu orice preț să vestesc Evanghelia acolo unde Hristos nu era cunoscut, ca&lt;f&gt;ⓞ&lt;/f&gt; să nu zidesc pe temelia pusă de altul,');
INSERT INTO &quot;verses&quot; VALUES (520,15,21,'ci, după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Aceia cărora nu li se propovăduise despre El vor vedea,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și cei ce nu auziseră vor pricepe.&lt;/e&gt; &lt;f&gt;[84]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (520,15,22,'&lt;pb/&gt;De aceea am și&lt;f&gt;ⓟ&lt;/f&gt; fost adesea împiedicat să vin la voi.');
INSERT INTO &quot;verses&quot; VALUES (520,15,23,'Dar acum, fiindcă nu mai este niciun alt loc &lt;i&gt;de lucru&lt;/i&gt; pe aceste meleaguri și&lt;f&gt;ⓠ&lt;/f&gt; fiindcă de ani buni doresc fierbinte să vin la voi,');
INSERT INTO &quot;verses&quot; VALUES (520,15,24,'când mă voi duce în Spania, nădăjduiesc să trec să vă văd și&lt;f&gt;ⓡ&lt;/f&gt; să fiu ajutat de voi până acolo, odată ce întâi mă voi bucura de voi măcar o vreme.');
INSERT INTO &quot;verses&quot; VALUES (520,15,25,'Acum, mă duc&lt;f&gt;ⓢ&lt;/f&gt; la Ierusalim să duc ajutoare sfinților,');
INSERT INTO &quot;verses&quot; VALUES (520,15,26,'căci cei din Macedonia și Ahaia au&lt;f&gt;ⓣ&lt;/f&gt; găsit că este bine să facă o strângere de ajutoare pentru săracii dintre sfinții care sunt în Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (520,15,27,'Au găsit că este bine, dar sunt și datori față de ei, pentru că, dacă&lt;f&gt;ⓤ&lt;/f&gt; cei dintre neamuri au ajuns să aibă parte de &lt;i&gt;bunurile&lt;/i&gt; lor duhovnicești, au și ei datoria&lt;f&gt;ⓥ&lt;/f&gt; de a le sluji cu cele pământești.');
INSERT INTO &quot;verses&quot; VALUES (520,15,28,'Așadar, după ce voi împlini &lt;i&gt;misiunea&lt;/i&gt; aceasta și le voi încredința rodul&lt;f&gt;ⓦ&lt;/f&gt; &lt;i&gt;adunat&lt;/i&gt;, voi trece pe la voi în drum spre Spania.');
INSERT INTO &quot;verses&quot; VALUES (520,15,29,'Știu&lt;f&gt;ⓧ&lt;/f&gt; că, atunci când voi veni la voi, voi veni cu plinătatea binecuvântării lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (520,15,30,'&lt;pb/&gt;Vă îndemn, fraților, pentru Domnul nostru Isus Hristos și pentru&lt;f&gt;ⓨ&lt;/f&gt; dragostea Duhului, să&lt;f&gt;ⓩ&lt;/f&gt; vă luptați împreună cu mine în rugăciunile voastre către Dumnezeu pentru mine,');
INSERT INTO &quot;verses&quot; VALUES (520,15,31,'ca să&lt;f&gt;ⓐ&lt;/f&gt; fiu izbăvit de împotrivitorii din Iudeea și pentru ca slujba&lt;f&gt;ⓑ&lt;/f&gt; mea de la Ierusalim să fie bine primită de sfinți;');
INSERT INTO &quot;verses&quot; VALUES (520,15,32,'și astfel să ajung la voi cu bucurie, prin voia&lt;f&gt;ⓒ&lt;/f&gt; lui Dumnezeu, și să&lt;f&gt;ⓓ&lt;/f&gt; mă revigorez&lt;f&gt;ⓔ&lt;/f&gt; împreună cu voi.');
INSERT INTO &quot;verses&quot; VALUES (520,15,33,'Dumnezeul&lt;f&gt;ⓕ&lt;/f&gt; păcii să fie cu voi toți! Amin!');
INSERT INTO &quot;verses&quot; VALUES (520,16,1,'&lt;pb/&gt;V-o încredințez pe Fivi, sora noastră, care este diaconeasă&lt;f&gt;[85]&lt;/f&gt; a bisericii din Chencreea&lt;f&gt;ⓖ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (520,16,2,'să o&lt;f&gt;ⓗ&lt;/f&gt; primiți în Domnul, cum se cuvine sfinților, și să o sprijiniți în orice privință ar avea nevoie de voi, căci și ea le-a fost multora binefăcătoare, inclusiv mie însumi.');
INSERT INTO &quot;verses&quot; VALUES (520,16,3,'&lt;pb/&gt;Spuneți salutări Priscăi&lt;f&gt;[86]&lt;/f&gt; și lui Aquila, tovarășii mei de lucru în Hristos Isus,');
INSERT INTO &quot;verses&quot; VALUES (520,16,4,'care și-au pus capul în joc pentru viața mea! Le mulțumesc nu numai eu, ci și toate bisericile dintre neamuri.');
INSERT INTO &quot;verses&quot; VALUES (520,16,5,'Spuneți salutări și bisericii&lt;f&gt;ⓘ&lt;/f&gt; care se adună în casa lor! Salutați-l pe Epenet, preaiubitul meu, care este cel dintâi rod&lt;f&gt;ⓙ&lt;/f&gt; al Asiei pentru Hristos!');
INSERT INTO &quot;verses&quot; VALUES (520,16,6,'Salutați-o pe Maria, care s-a ostenit mult pentru voi!');
INSERT INTO &quot;verses&quot; VALUES (520,16,7,'Spuneți salutări lui Andronic și Iuniei, rudele mele și tovarășii mei de temniță, care sunt de seamă între apostoli și care au&lt;f&gt;ⓚ&lt;/f&gt; venit la Hristos înainte de mine!');
INSERT INTO &quot;verses&quot; VALUES (520,16,8,'Salutați-l pe Ampliat, preaiubitul meu în Domnul!');
INSERT INTO &quot;verses&quot; VALUES (520,16,9,'Salutați-l pe Urban, tovarășul nostru de lucru în Hristos, și pe Stahis, preaiubitul meu!');
INSERT INTO &quot;verses&quot; VALUES (520,16,10,'Spuneți salutări lui Apeles, cel încercat în Hristos! Salutări și celor ai lui Aristobul!');
INSERT INTO &quot;verses&quot; VALUES (520,16,11,'Salutări lui Irodion, ruda mea! Spuneți salutări celor din casa lui Narcis, care sunt în Domnul!');
INSERT INTO &quot;verses&quot; VALUES (520,16,12,'Salutări Trifenei și Trifosei, care se ostenesc pentru Domnul! Salutați-o pe Persida, cea preaiubită, care s-a ostenit mult în Domnul!');
INSERT INTO &quot;verses&quot; VALUES (520,16,13,'Spuneți salutări lui Rufus, cel ales&lt;f&gt;ⓛ&lt;/f&gt; în Domnul, și mamei lui, &lt;i&gt;care mi-a fost ca&lt;/i&gt; mamă!');
INSERT INTO &quot;verses&quot; VALUES (520,16,14,'Salutări lui Asincrit, lui Flegon, lui Hermes, lui Patrobas, lui Hermas și fraților care sunt împreună cu ei!');
INSERT INTO &quot;verses&quot; VALUES (520,16,15,'Spuneți salutări lui Filolog și Iuliei, lui Nereu și surorii lui, lui Olimpas și tuturor sfinților care sunt împreună cu ei!');
INSERT INTO &quot;verses&quot; VALUES (520,16,16,'Spuneți-vă&lt;f&gt;ⓜ&lt;/f&gt; salutări unii altora cu o sărutare sfântă! Toate bisericile lui Hristos vă trimit salutări.');
INSERT INTO &quot;verses&quot; VALUES (520,16,17,'&lt;pb/&gt;Vă îndemn, fraților, să vă păziți de cei ce&lt;f&gt;ⓝ&lt;/f&gt; fac dezbinări și pun piedici împotriva învățăturii pe care ați primit-o. Depărtați-vă&lt;f&gt;ⓞ&lt;/f&gt; de ei!');
INSERT INTO &quot;verses&quot; VALUES (520,16,18,'Căci astfel de oameni nu-I slujesc lui Hristos, Domnul nostru, ci pântecelui&lt;f&gt;ⓟ&lt;/f&gt; lor și, prin&lt;f&gt;ⓠ&lt;/f&gt; vorbe mieroase și măgulitoare, înșală inimile celor lesne crezători.');
INSERT INTO &quot;verses&quot; VALUES (520,16,19,'Cât despre voi, ascultarea&lt;f&gt;ⓡ&lt;/f&gt; voastră este cunoscută de toți. De aceea, mă bucur de voi și doresc să fiți înțelepți în ce privește binele, dar nepricepuți în ce privește răul.');
INSERT INTO &quot;verses&quot; VALUES (520,16,20,'Dumnezeul&lt;f&gt;ⓢ&lt;/f&gt; păcii îl va&lt;f&gt;ⓣ&lt;/f&gt; zdrobi pe Satana sub picioarele voastre în curând. Harul&lt;f&gt;ⓤ&lt;/f&gt; Domnului nostru Isus [Hristos] să fie cu voi!');
INSERT INTO &quot;verses&quot; VALUES (520,16,21,'&lt;pb/&gt;Timotei&lt;f&gt;ⓥ&lt;/f&gt;, tovarășul meu de lucru, vă salută; tot așa și Luciu&lt;f&gt;ⓦ&lt;/f&gt;, Iason&lt;f&gt;ⓧ&lt;/f&gt; și Sosipater&lt;f&gt;ⓨ&lt;/f&gt;, rudele mele.');
INSERT INTO &quot;verses&quot; VALUES (520,16,22,'Vă trimit salutări în Domnul eu, Tertius, care am scris epistola aceasta.');
INSERT INTO &quot;verses&quot; VALUES (520,16,23,'Gaius&lt;f&gt;ⓩ&lt;/f&gt;, gazda mea și a întregii biserici, vă salută. Erast&lt;f&gt;ⓐ&lt;/f&gt;, vistierul cetății, vă trimite salutări; tot așa și fratele Quartus.');
INSERT INTO &quot;verses&quot; VALUES (520,16,24,'&lt;pb/&gt;[Harul&lt;f&gt;ⓑ&lt;/f&gt; Domnului nostru Isus Hristos să fie cu voi cu toți! Amin!');
INSERT INTO &quot;verses&quot; VALUES (520,16,25,'Iar a Aceluia&lt;f&gt;ⓒ&lt;/f&gt; care poate să vă întărească, după&lt;f&gt;ⓓ&lt;/f&gt; Evanghelia mea și după predicarea lui Isus Hristos — potrivit cu&lt;f&gt;ⓔ&lt;/f&gt; descoperirea tainei care&lt;f&gt;ⓕ&lt;/f&gt; a fost ținută sub tăcere timp de veacuri,');
INSERT INTO &quot;verses&quot; VALUES (520,16,26,'dar&lt;f&gt;ⓖ&lt;/f&gt; care a fost arătată acum prin scrierile profeților, prin porunca Dumnezeului celui veșnic, fiind adusă la cunoștința tuturor neamurilor, spre ascultarea&lt;f&gt;ⓗ&lt;/f&gt; credinței —');
INSERT INTO &quot;verses&quot; VALUES (520,16,27,'a singurului&lt;f&gt;ⓘ&lt;/f&gt;, înțeleptului Dumnezeu, să fie slava, prin Isus Hristos, în vecii vecilor! Amin!]');
INSERT INTO &quot;verses&quot; VALUES (530,1,1,'&lt;pb/&gt;Pavel, chemat&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;să fie&lt;/i&gt; apostol al lui Hristos Isus prin&lt;f&gt;ⓑ&lt;/f&gt; voia lui Dumnezeu, și fratele Sosten&lt;f&gt;ⓒ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (530,1,2,'către biserica lui Dumnezeu care este în Corint, către cei&lt;f&gt;ⓓ&lt;/f&gt; sfințiți&lt;f&gt;ⓔ&lt;/f&gt; în Hristos Isus, chemați&lt;f&gt;ⓕ&lt;/f&gt; &lt;i&gt;să fie&lt;/i&gt; sfinți, împreună cu toți cei ce cheamă&lt;f&gt;ⓖ&lt;/f&gt; în vreun loc Numele lui Isus Hristos, Domnul&lt;f&gt;ⓗ&lt;/f&gt; lor și&lt;f&gt;ⓘ&lt;/f&gt; al nostru:');
INSERT INTO &quot;verses&quot; VALUES (530,1,3,'Har&lt;f&gt;ⓙ&lt;/f&gt; și pace vouă de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (530,1,4,'&lt;pb/&gt;Îi mulțumesc&lt;f&gt;ⓚ&lt;/f&gt; Dumnezeului meu totdeauna cu privire la voi, pentru harul lui Dumnezeu care v-a fost dat în Hristos Isus,');
INSERT INTO &quot;verses&quot; VALUES (530,1,5,'căci în El ați fost îmbogățiți în&lt;f&gt;ⓛ&lt;/f&gt; toate &lt;i&gt;privințele&lt;/i&gt;, cu orice vorbire și cu orice cunoștință,');
INSERT INTO &quot;verses&quot; VALUES (530,1,6,'iar mărturia&lt;f&gt;ⓜ&lt;/f&gt; lui Hristos a fost &lt;i&gt;bine&lt;/i&gt; întărită în mijlocul vostru,');
INSERT INTO &quot;verses&quot; VALUES (530,1,7,'încât nu duceți lipsă de niciun fel de dar în timp ce așteptați&lt;f&gt;ⓝ&lt;/f&gt; să Se arate Domnul nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,1,8,'El&lt;f&gt;ⓞ&lt;/f&gt; vă va întări până la sfârșit, &lt;i&gt;ca să fiți&lt;/i&gt; fără vină&lt;f&gt;ⓟ&lt;/f&gt; în ziua Domnului nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,1,9,'Credincios&lt;f&gt;ⓠ&lt;/f&gt; este Dumnezeu, prin care ați fost chemați la părtășia&lt;f&gt;ⓡ&lt;/f&gt; cu Fiul Său — Isus Hristos, Domnul nostru.');
INSERT INTO &quot;verses&quot; VALUES (530,1,10,'&lt;pb/&gt;Vă îndemn, fraților, pentru Numele Domnului nostru Isus Hristos, să&lt;f&gt;ⓢ&lt;/f&gt; vă puneți de acord în ceea ce spuneți și să nu aveți dezbinări între voi, ci să fiți uniți într-un gând și într-un cuget.');
INSERT INTO &quot;verses&quot; VALUES (530,1,11,'Căci, frații mei, ai Cloei mi-au povestit că între voi sunt certuri.');
INSERT INTO &quot;verses&quot; VALUES (530,1,12,'Ce spun eu este că fiecare&lt;f&gt;ⓣ&lt;/f&gt; din voi zice: „Eu sunt al lui Pavel!”, „Și eu, al lui Apolo!”&lt;f&gt;ⓤ&lt;/f&gt;, „Iar eu, al lui Chifa!”&lt;f&gt;ⓥ&lt;/f&gt;, „Și eu, al lui Hristos!”');
INSERT INTO &quot;verses&quot; VALUES (530,1,13,'A fost Hristos&lt;f&gt;ⓦ&lt;/f&gt; împărțit? Doar nu Pavel a fost răstignit pentru voi! Sau în numele lui Pavel ați fost voi botezați?');
INSERT INTO &quot;verses&quot; VALUES (530,1,14,'Îi mulțumesc lui Dumnezeu că n-am botezat pe niciunul dintre voi în afară de Crisp&lt;f&gt;ⓧ&lt;/f&gt; și de Gaius&lt;f&gt;ⓨ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (530,1,15,'ca să nu spună cineva că în numele meu ați fost botezați.');
INSERT INTO &quot;verses&quot; VALUES (530,1,16,'Am mai botezat și casa lui Stefanas&lt;f&gt;ⓩ&lt;/f&gt;; încolo, nu știu să mai fi botezat pe altcineva.');
INSERT INTO &quot;verses&quot; VALUES (530,1,17,'Căci Hristos nu m-a trimis să botez, ci să vestesc Evanghelia; dar nu&lt;f&gt;ⓐ&lt;/f&gt; cu înțelepciunea vorbirii, ca să nu fie zădărnicită crucea lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,1,18,'&lt;pb/&gt;Propovăduirea crucii&lt;f&gt;[1]&lt;/f&gt; este o nebunie&lt;f&gt;ⓑ&lt;/f&gt; pentru cei&lt;f&gt;ⓒ&lt;/f&gt; ce sunt pe calea pierzării, dar pentru noi, care&lt;f&gt;ⓓ&lt;/f&gt; suntem pe calea mântuirii, este puterea&lt;f&gt;ⓔ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,1,19,'Căci este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Voi prăpădi&lt;/e&gt; &lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;înțelepciunea celor înțelepți,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și priceperea celor pricepuți o voi zădărnici.&lt;/e&gt; &lt;f&gt;[2]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,1,20,'&lt;pb/&gt;Unde este înțeleptul? Unde este cărturarul? Unde este certărețul veacului acestuia? N-a&lt;f&gt;ⓖ&lt;/f&gt; arătat Dumnezeu că înțelepciunea lumii acesteia este nebunie?');
INSERT INTO &quot;verses&quot; VALUES (530,1,21,'Căci&lt;f&gt;ⓗ&lt;/f&gt;, întrucât, în înțelepciunea lui Dumnezeu, lumea nu L-a cunoscut pe Dumnezeu prin înțelepciune, Dumnezeu a găsit de cuviință să-i mântuiască pe credincioși prin nebunia propovăduirii.');
INSERT INTO &quot;verses&quot; VALUES (530,1,22,'Într-adevăr, iudeii&lt;f&gt;ⓘ&lt;/f&gt; cer semne, iar grecii caută înțelepciune;');
INSERT INTO &quot;verses&quot; VALUES (530,1,23,'noi însă propovăduim un Hristos răstignit, care pentru iudei&lt;f&gt;ⓙ&lt;/f&gt; este o pricină de poticnire, iar pentru neamuri, nebunie&lt;f&gt;ⓚ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (530,1,24,'dar pentru cei chemați, atât iudei, cât și greci, Hristos este puterea&lt;f&gt;ⓛ&lt;/f&gt; lui Dumnezeu și înțelepciunea&lt;f&gt;ⓜ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,1,25,'Căci nebunia lui Dumnezeu este mai înțeleaptă decât oamenii, și slăbiciunea lui Dumnezeu este mai tare decât oamenii.');
INSERT INTO &quot;verses&quot; VALUES (530,1,26,'&lt;pb/&gt;&lt;i&gt;De altfel&lt;/i&gt;, uitați-vă la chemarea voastră, fraților, căci &lt;i&gt;printre voi&lt;/i&gt; nu sunt mulți înțelepți în felul lumii, nici mulți puternici, nici mulți de neam ales!');
INSERT INTO &quot;verses&quot; VALUES (530,1,27,'Dimpotrivă, Dumnezeu a ales lucrurile&lt;f&gt;ⓝ&lt;/f&gt; nebune ale lumii ca să le facă de rușine pe cele înțelepte. Dumnezeu a ales lucrurile slabe ale lumii ca să le facă de rușine pe cele tari.');
INSERT INTO &quot;verses&quot; VALUES (530,1,28,'Dumnezeu a ales lucrurile umile&lt;f&gt;[3]&lt;/f&gt; ale lumii și lucrurile disprețuite, ba chiar lucrurile care nu sunt, ca&lt;f&gt;ⓞ&lt;/f&gt; să le nimicească pe cele ce sunt;');
INSERT INTO &quot;verses&quot; VALUES (530,1,29,'așa încât&lt;f&gt;ⓟ&lt;/f&gt; să nu se laude nimeni înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,1,30,'Prin El sunteți și voi în Hristos Isus, care pentru noi s-a făcut înțelepciune&lt;f&gt;ⓠ&lt;/f&gt; de la Dumnezeu, dreptate&lt;f&gt;ⓡ&lt;/f&gt;, sfințire&lt;f&gt;ⓢ&lt;/f&gt; și răscumpărare&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,1,31,'Astfel, după cum este scris: &lt;e&gt;Cine&lt;/e&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;se laudă să se laude în Domnul!&lt;/e&gt;&lt;f&gt;[4]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,2,1,'&lt;pb/&gt;Cât despre mine, fraților, când am venit la voi, n-am&lt;f&gt;ⓥ&lt;/f&gt; venit să vă vestesc taina&lt;f&gt;ⓦ&lt;/f&gt; lui Dumnezeu cu o vorbire sau o înțelepciune strălucită.');
INSERT INTO &quot;verses&quot; VALUES (530,2,2,'Căci n-am avut de gând să știu între voi altceva decât&lt;f&gt;ⓧ&lt;/f&gt; pe Isus Hristos, și încă răstignit.');
INSERT INTO &quot;verses&quot; VALUES (530,2,3,'Eu însumi, în slăbiciune&lt;f&gt;ⓨ&lt;/f&gt;, cu frică și cu mult tremur am&lt;f&gt;ⓩ&lt;/f&gt; venit la voi.');
INSERT INTO &quot;verses&quot; VALUES (530,2,4,'Și cuvântul meu și propovăduirea mea nu&lt;f&gt;ⓐ&lt;/f&gt; stăteau în vorbirile înduplecătoare ale înțelepciunii, ci&lt;f&gt;ⓑ&lt;/f&gt; într-o dovadă de Duh și de putere,');
INSERT INTO &quot;verses&quot; VALUES (530,2,5,'pentru ca credința voastră să fie &lt;i&gt;întemeiată&lt;/i&gt; nu pe înțelepciunea oamenilor, ci pe puterea&lt;f&gt;ⓒ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,2,6,'&lt;pb/&gt;O înțelepciune&lt;f&gt;ⓓ&lt;/f&gt; propovăduim și noi printre cei&lt;f&gt;ⓔ&lt;/f&gt; desăvârșiți, dar nu a veacului acestuia, nici a conducătorilor acestui veac, care&lt;f&gt;ⓕ&lt;/f&gt; sunt ca și nimiciți.');
INSERT INTO &quot;verses&quot; VALUES (530,2,7,'Dimpotrivă, propovăduim înțelepciunea lui Dumnezeu cea tainică și ținută ascunsă, pe care&lt;f&gt;ⓖ&lt;/f&gt; a rânduit-o Dumnezeu spre slava noastră, mai înainte de veci,');
INSERT INTO &quot;verses&quot; VALUES (530,2,8,'și pe care&lt;f&gt;ⓗ&lt;/f&gt; n-a cunoscut-o niciunul dintre conducătorii acestui veac; căci, dacă&lt;f&gt;ⓘ&lt;/f&gt; ar fi cunoscut-o, nu L-ar fi răstignit pe Domnul slavei.');
INSERT INTO &quot;verses&quot; VALUES (530,2,9,'Dar &lt;i&gt;s-a întâmplat&lt;/i&gt; după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Lucruri pe care ochiul nu le-a văzut, urechea nu le-a auzit&lt;/e&gt; &lt;f&gt;[5]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și la inima omului nu s-au suit&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;a pregătit Dumnezeu pentru cei ce-L iubesc.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,2,10,'&lt;pb/&gt;Nouă însă Dumnezeu ni&lt;f&gt;ⓙ&lt;/f&gt; le-a descoperit prin Duhul. Căci Duhul cercetează totul, chiar și lucrurile adânci ale lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,2,11,'Într-adevăr, cine dintre oameni cunoaște &lt;i&gt;lucrurile&lt;/i&gt; omului dacă nu&lt;f&gt;ⓚ&lt;/f&gt; duhul omului, care este în el? Tot așa&lt;f&gt;ⓛ&lt;/f&gt;, nimeni nu cunoaște &lt;i&gt;lucrurile&lt;/i&gt; lui Dumnezeu, ci numai Duhul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,2,12,'Iar noi n-am primit duhul lumii, ci Duhul&lt;f&gt;ⓜ&lt;/f&gt; care vine de la Dumnezeu, ca să cunoaștem lucrurile pe care ni le-a dăruit Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,2,13,'Și vorbim&lt;f&gt;ⓝ&lt;/f&gt; despre ele nu cu vorbiri învățate din înțelepciunea omenească, ci cu vorbiri învățate de la Duhul Sfânt, întrebuințând o vorbire duhovnicească pentru lucrurile duhovnicești.');
INSERT INTO &quot;verses&quot; VALUES (530,2,14,'Dar&lt;f&gt;ⓞ&lt;/f&gt; omul lumesc&lt;f&gt;[6]&lt;/f&gt; nu primește lucrurile Duhului lui Dumnezeu; pentru&lt;f&gt;ⓟ&lt;/f&gt; el sunt o nebunie și&lt;f&gt;ⓠ&lt;/f&gt; nu le poate înțelege, pentru că &lt;i&gt;ele&lt;/i&gt; trebuie judecate duhovnicește.');
INSERT INTO &quot;verses&quot; VALUES (530,2,15,'&lt;i&gt;Omul&lt;/i&gt; duhovnicesc&lt;f&gt;ⓡ&lt;/f&gt;, dimpotrivă, judecă totul, dar el însuși nu poate fi judecat de nimeni.');
INSERT INTO &quot;verses&quot; VALUES (530,2,16,'Căci&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;cine a cunoscut gândul Domnului, ca să-I poată da învățătură?&lt;/e&gt;&lt;f&gt;[7]&lt;/f&gt; Noi însă avem gândul lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,3,1,'&lt;pb/&gt;Eu unul, fraților, nu v-am putut vorbi ca unor &lt;i&gt;oameni&lt;/i&gt; duhovnicești, ci &lt;i&gt;v-am vorbit&lt;/i&gt; ca unor oameni firești&lt;f&gt;[8]&lt;/f&gt;, ca unor copilași&lt;f&gt;ⓣ&lt;/f&gt; în Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,3,2,'Cu lapte&lt;f&gt;ⓤ&lt;/f&gt; v-am hrănit, nu cu hrană &lt;i&gt;tare&lt;/i&gt;, căci nu o puteați &lt;i&gt;mânca&lt;/i&gt;, și nici acum nu sunteți în stare,');
INSERT INTO &quot;verses&quot; VALUES (530,3,3,'pentru că tot firești sunteți. Când&lt;f&gt;ⓥ&lt;/f&gt; între voi este invidie, ceartă [și dezbinare], nu sunteți voi firești și nu trăiți în felul &lt;i&gt;celorlalți&lt;/i&gt; oameni?');
INSERT INTO &quot;verses&quot; VALUES (530,3,4,'Când unul zice: „Eu&lt;f&gt;ⓦ&lt;/f&gt; sunt al lui Pavel!” și altul: „Eu sunt al lui Apolo”, nu sunteți voi &lt;i&gt;ca ceilalți&lt;/i&gt; oameni?');
INSERT INTO &quot;verses&quot; VALUES (530,3,5,'Cine este Apolo? Și cine este Pavel? Sunt slujitori&lt;f&gt;ⓧ&lt;/f&gt; prin care voi ați crezut, dar fiecare &lt;i&gt;slujește&lt;/i&gt; după&lt;f&gt;ⓨ&lt;/f&gt; cum i-a dat Domnul.');
INSERT INTO &quot;verses&quot; VALUES (530,3,6,'Eu&lt;f&gt;ⓩ&lt;/f&gt; am sădit, Apolo&lt;f&gt;ⓐ&lt;/f&gt; a udat, dar Dumnezeu a făcut să crească,');
INSERT INTO &quot;verses&quot; VALUES (530,3,7,'așadar, nici&lt;f&gt;ⓑ&lt;/f&gt; cel ce sădește, nici cel ce udă nu au vreo importanță, ci Dumnezeu, care face să crească.');
INSERT INTO &quot;verses&quot; VALUES (530,3,8,'Cel ce sădește și cel ce udă sunt una, dar&lt;f&gt;ⓒ&lt;/f&gt; fiecare își va lua răsplata după osteneala lui.');
INSERT INTO &quot;verses&quot; VALUES (530,3,9,'Căci noi&lt;f&gt;ⓓ&lt;/f&gt; suntem lucrători împreună cu Dumnezeu, &lt;i&gt;iar&lt;/i&gt; voi sunteți ogorul lui Dumnezeu, clădirea&lt;f&gt;ⓔ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,3,10,'&lt;pb/&gt;După&lt;f&gt;ⓕ&lt;/f&gt; harul lui Dumnezeu care mi-a fost dat, eu, ca un meșter-zidar înțelept, am pus temelia&lt;f&gt;ⓖ&lt;/f&gt; și un altul zidește deasupra. Dar fiecare&lt;f&gt;ⓗ&lt;/f&gt; să ia bine seama cum zidește deasupra.');
INSERT INTO &quot;verses&quot; VALUES (530,3,11,'Căci nimeni nu poate pune o altă temelie decât cea&lt;f&gt;ⓘ&lt;/f&gt; care a fost pusă și care&lt;f&gt;ⓙ&lt;/f&gt; este Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,3,12,'Iar dacă zidește cineva pe această temelie aur, argint, pietre scumpe, lemn, fân sau paie,');
INSERT INTO &quot;verses&quot; VALUES (530,3,13,'lucrarea fiecăruia&lt;f&gt;ⓚ&lt;/f&gt; va fi dată pe față: ziua &lt;i&gt;Domnului&lt;/i&gt; o va face cunoscută, căci se va descoperi&lt;f&gt;ⓛ&lt;/f&gt; în foc&lt;f&gt;ⓜ&lt;/f&gt;. Și focul va dovedi cum este lucrarea fiecăruia.');
INSERT INTO &quot;verses&quot; VALUES (530,3,14,'Dacă lucrarea cuiva rămâne cum a fost zidită, el va&lt;f&gt;ⓝ&lt;/f&gt; primi o răsplată.');
INSERT INTO &quot;verses&quot; VALUES (530,3,15,'Dacă lucrarea lui va fi arsă, își va pierde &lt;i&gt;răsplata&lt;/i&gt;. Cât despre el, va fi mântuit, dar ca prin foc.');
INSERT INTO &quot;verses&quot; VALUES (530,3,16,'Nu&lt;f&gt;ⓞ&lt;/f&gt; știți că voi sunteți templul lui Dumnezeu și că Duhul lui Dumnezeu locuiește în voi?');
INSERT INTO &quot;verses&quot; VALUES (530,3,17,'Dacă nimicește cineva templul lui Dumnezeu, pe acela îl va nimici Dumnezeu, căci templul lui Dumnezeu este sfânt, și așa sunteți voi.');
INSERT INTO &quot;verses&quot; VALUES (530,3,18,'&lt;pb/&gt;Nimeni&lt;f&gt;ⓟ&lt;/f&gt; să nu se înșele: dacă cineva dintre voi se crede înțelept în felul veacului acestuia, să se facă nebun, ca să ajungă înțelept!');
INSERT INTO &quot;verses&quot; VALUES (530,3,19,'Căci&lt;f&gt;ⓠ&lt;/f&gt; înțelepciunea lumii acesteia este o nebunie înaintea lui Dumnezeu, după cum este scris: &lt;e&gt;El îi prinde pe cei înțelepți în viclenia lor&lt;/e&gt;.&lt;f&gt;[9]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,3,20,'Și iarăși: &lt;e&gt;Domnul cunoaște gândurile celor înțelepți. &lt;i&gt;Știe&lt;/i&gt; că sunt deșarte&lt;/e&gt;.&lt;f&gt;[10]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,3,21,'Așadar, nimeni&lt;f&gt;ⓡ&lt;/f&gt; să nu se laude cu oameni, căci toate sunt ale voastre:');
INSERT INTO &quot;verses&quot; VALUES (530,3,22,'fie Pavel, fie Apolo, fie Chifa, fie lumea, fie viața, fie moartea, fie lucrurile prezente, fie cele viitoare — toate sunt ale voastre');
INSERT INTO &quot;verses&quot; VALUES (530,3,23,'și&lt;f&gt;ⓢ&lt;/f&gt; voi sunteți ai lui Hristos, iar Hristos este al lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,4,1,'&lt;pb/&gt;Iată cum trebuie să fim văzuți noi: ca&lt;f&gt;ⓣ&lt;/f&gt; slujitori ai lui Hristos și ca ispravnici&lt;f&gt;ⓤ&lt;/f&gt; ai tainelor lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,4,2,'În rest, ce se cere de la ispravnici este ca fiecare să fie găsit credincios.');
INSERT INTO &quot;verses&quot; VALUES (530,4,3,'Pentru mine contează prea puțin dacă sunt cercetat de voi sau de un tribunal omenesc. Ba încă nici eu nu mă mai cercetez pe mine.');
INSERT INTO &quot;verses&quot; VALUES (530,4,4,'Căci nu mă știu &lt;i&gt;vinovat&lt;/i&gt; cu nimic, dar nu în baza acestui fapt&lt;f&gt;ⓥ&lt;/f&gt; sunt îndreptățit&lt;f&gt;[11]&lt;/f&gt;: cel ce mă cercetează este Domnul.');
INSERT INTO &quot;verses&quot; VALUES (530,4,5,'De aceea&lt;f&gt;ⓦ&lt;/f&gt;, să nu judecați nimic înainte de vreme, până va veni Domnul, care&lt;f&gt;ⓧ&lt;/f&gt; va aduce la lumină lucrurile ascunse în întuneric și va da la iveală intențiile din inimile &lt;i&gt;oamenilor&lt;/i&gt;. Atunci, fiecare își va primi lauda de la Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,4,6,'&lt;pb/&gt;Aceste lucruri, fraților, le-am spus pentru voi în mod figurat cu privire la mine și la Apolo, pentru ca, prin &lt;i&gt;exemplul&lt;/i&gt; nostru, să învățați să nu &lt;i&gt;treceți&lt;/i&gt; peste ce este scris și nimeni să&lt;f&gt;ⓨ&lt;/f&gt; nu se îngâmfe &lt;i&gt;luând partea&lt;/i&gt; unuia împotriva celuilalt.');
INSERT INTO &quot;verses&quot; VALUES (530,4,7,'Căci cine te face deosebit? Ce&lt;f&gt;ⓩ&lt;/f&gt; lucru ai pe care să nu-l fi primit? Și dacă l-ai primit, de ce te lauzi ca și cum nu l-ai fi primit?');
INSERT INTO &quot;verses&quot; VALUES (530,4,8,'&lt;pb/&gt;O, iată-vă sătui! Deja sunteți&lt;f&gt;ⓐ&lt;/f&gt; bogați! Ați ajuns să împărățiți fără noi! Și măcar de ați împărăți cu adevărat, ca să împărățim și noi împreună cu voi!');
INSERT INTO &quot;verses&quot; VALUES (530,4,9,'Căci mi se pare că Dumnezeu ne-a rânduit pe noi, apostolii, cei mai de pe urmă, ca pe niște&lt;f&gt;ⓑ&lt;/f&gt; condamnați la moarte, fiindcă&lt;f&gt;ⓒ&lt;/f&gt; am ajuns o priveliște pentru lume, îngeri și oameni.');
INSERT INTO &quot;verses&quot; VALUES (530,4,10,'Noi&lt;f&gt;ⓓ&lt;/f&gt; suntem nebuni&lt;f&gt;ⓔ&lt;/f&gt; pentru Hristos; voi, înțelepți în Hristos! Noi&lt;f&gt;ⓕ&lt;/f&gt;, slabi; voi, tari! Voi, puși în cinste; noi, disprețuiți!');
INSERT INTO &quot;verses&quot; VALUES (530,4,11,'Până&lt;f&gt;ⓖ&lt;/f&gt; în clipa de față suferim de foame și de sete, suntem goi&lt;f&gt;ⓗ&lt;/f&gt;, chinuiți, &lt;i&gt;mereu&lt;/i&gt; pe drumuri,');
INSERT INTO &quot;verses&quot; VALUES (530,4,12,'trudim&lt;f&gt;ⓘ&lt;/f&gt; și lucrăm cu mâinile noastre; când suntem batjocoriți&lt;f&gt;ⓙ&lt;/f&gt;, binecuvântăm; când suntem prigoniți, răbdăm;');
INSERT INTO &quot;verses&quot; VALUES (530,4,13,'când suntem vorbiți de rău, răspundem cu blândețe. Am ajuns ca gunoiul lumii acesteia, ca ceva lepădat de toți, până în clipa de față.');
INSERT INTO &quot;verses&quot; VALUES (530,4,14,'&lt;pb/&gt;Nu ca să vă fac de rușine vă scriu aceste lucruri, ci ca să vă sfătuiesc ca&lt;f&gt;ⓚ&lt;/f&gt; pe niște copii ai mei preaiubiți.');
INSERT INTO &quot;verses&quot; VALUES (530,4,15,'Căci și dacă ați avea zece mii de pedagogi în&lt;f&gt;ⓛ&lt;/f&gt; Hristos, totuși nu aveți mai mulți părinți, pentru că eu v-am născut în Hristos Isus, prin Evanghelie.');
INSERT INTO &quot;verses&quot; VALUES (530,4,16,'Așadar, urmați pilda mea&lt;f&gt;ⓜ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,4,17,'De aceea vi l-am trimis pe Timotei&lt;f&gt;ⓝ&lt;/f&gt;, care este copilul meu preaiubit și credincios în Domnul. El vă va aduce aminte de purtarea mea în Hristos Isus și de felul în care îi învăț pe oameni pretutindeni în toate bisericile.');
INSERT INTO &quot;verses&quot; VALUES (530,4,18,'&lt;pb/&gt;&lt;i&gt;La gândul&lt;/i&gt; că n-aș mai veni la voi, unii s-au îngâmfat.');
INSERT INTO &quot;verses&quot; VALUES (530,4,19,'Voi veni în curând la voi&lt;f&gt;ⓞ&lt;/f&gt;, dacă Domnul&lt;f&gt;ⓟ&lt;/f&gt; va vrea, și voi vedea nu vorbele celor ce s-au îngâmfat, ci puterea lor.');
INSERT INTO &quot;verses&quot; VALUES (530,4,20,'Căci Împărăția&lt;f&gt;ⓠ&lt;/f&gt; lui Dumnezeu nu stă în vorbe, ci în putere.');
INSERT INTO &quot;verses&quot; VALUES (530,4,21,'Ce vreți? Să vin la voi cu&lt;f&gt;ⓡ&lt;/f&gt; nuiaua sau cu dragoste și cu duhul blândeții?');
INSERT INTO &quot;verses&quot; VALUES (530,5,1,'&lt;pb/&gt;Din toate părțile se aude că între voi este desfrâu, și încă un asemenea desfrâu cum nici între&lt;f&gt;ⓢ&lt;/f&gt; păgâni [nu se pomenește], până acolo că&lt;f&gt;ⓣ&lt;/f&gt; unul &lt;i&gt;din voi&lt;/i&gt; trăiește cu soția&lt;f&gt;ⓤ&lt;/f&gt; tatălui său.');
INSERT INTO &quot;verses&quot; VALUES (530,5,2,'Și voi v-ați fălit în loc să vă mâhniți, pentru ca cel ce a săvârșit fapta aceasta să fie dat afară din mijlocul vostru!');
INSERT INTO &quot;verses&quot; VALUES (530,5,3,'Eu, unul&lt;f&gt;ⓥ&lt;/f&gt;, deși n-am fost &lt;i&gt;la voi&lt;/i&gt; cu trupul, am fost de față cu duhul și l-am și judecat, ca și când aș fi fost de față &lt;i&gt;în trup&lt;/i&gt;, pe cel ce a făcut o astfel de faptă.');
INSERT INTO &quot;verses&quot; VALUES (530,5,4,'În Numele Domnului nostru Isus, în vreme ce voi și duhul meu suntem strânși laolaltă prin&lt;f&gt;ⓦ&lt;/f&gt; puterea Domnului nostru Isus,');
INSERT INTO &quot;verses&quot; VALUES (530,5,5,'un astfel de &lt;i&gt;om&lt;/i&gt; să&lt;f&gt;ⓧ&lt;/f&gt; fie dat pe mâna Satanei, pentru nimicirea cărnii, pentru ca duhul &lt;i&gt;lui&lt;/i&gt; să fie mântuit în ziua Domnului [Isus].');
INSERT INTO &quot;verses&quot; VALUES (530,5,6,'&lt;pb/&gt;Lauda voastră nu este bună. Nu știți că puțină&lt;f&gt;ⓨ&lt;/f&gt; drojdie&lt;f&gt;[12]&lt;/f&gt; dospește tot aluatul&lt;f&gt;[13]&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (530,5,7,'Îndepărtați drojdia cea veche, ca să fiți un aluat nou, cum și sunteți, fără drojdie, căci mielul nostru de Paște a fost jertfit, &lt;i&gt;și anume&lt;/i&gt; Hristos&lt;f&gt;ⓩ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,5,8,'Să&lt;f&gt;ⓐ&lt;/f&gt; sărbătorim deci nu cu drojdia veche, nici cu&lt;f&gt;ⓑ&lt;/f&gt; drojdia răutății și a vicleniei, ci cu azimele sincerității și ale adevărului.');
INSERT INTO &quot;verses&quot; VALUES (530,5,9,'&lt;pb/&gt;V-am scris în epistola &lt;i&gt;mea&lt;/i&gt; să&lt;f&gt;ⓒ&lt;/f&gt; nu aveți legătură cu desfrânații.');
INSERT INTO &quot;verses&quot; VALUES (530,5,10,'Însă&lt;f&gt;ⓓ&lt;/f&gt; nu &lt;i&gt;m-am referit la&lt;/i&gt; desfrânații acestei lumi sau la cei lacomi, hrăpăreți sau idolatri, fiindcă atunci ar trebui să ieșiți din&lt;f&gt;ⓔ&lt;/f&gt; lume.');
INSERT INTO &quot;verses&quot; VALUES (530,5,11,'Dimpotrivă, v-am scris să nu aveți legături cu&lt;f&gt;ⓕ&lt;/f&gt; cineva care, deși își zice „frate”, este desfrânat sau lacom sau idolatru sau batjocoritor sau bețiv sau hrăpăreț; cu un astfel &lt;i&gt;de om&lt;/i&gt; nici să nu mâncați.');
INSERT INTO &quot;verses&quot; VALUES (530,5,12,'Căci ce treabă am eu să-i judec pe cei&lt;f&gt;ⓖ&lt;/f&gt; din afară? Nu &lt;i&gt;este oare treaba&lt;/i&gt; voastră să-i judecați pe cei&lt;f&gt;ⓗ&lt;/f&gt; dinăuntru?');
INSERT INTO &quot;verses&quot; VALUES (530,5,13,'Pe cei din afară îi va judeca Dumnezeu. &lt;e&gt;Dați-l afară&lt;/e&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;e&gt;pe &lt;i&gt;omul&lt;/i&gt; cel rău din mijlocul vostru.&lt;/e&gt;&lt;f&gt;[14]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,6,1,'&lt;pb/&gt;Când unul din voi are un conflict cu altul, îndrăznește să se judece cu el la cei nedrepți, și nu la sfinți?');
INSERT INTO &quot;verses&quot; VALUES (530,6,2,'Oare nu știți că sfinții&lt;f&gt;ⓙ&lt;/f&gt; vor judeca lumea? Și dacă lumea va fi judecată de voi, sunteți nevrednici &lt;i&gt;să judecați&lt;/i&gt; pricini de foarte mică însemnătate?');
INSERT INTO &quot;verses&quot; VALUES (530,6,3,'Nu știți că noi îi vom judeca&lt;f&gt;ⓚ&lt;/f&gt; pe îngeri? Cu cât mai mult &lt;i&gt;pricini&lt;/i&gt; din viața aceasta!');
INSERT INTO &quot;verses&quot; VALUES (530,6,4,'Deci&lt;f&gt;ⓛ&lt;/f&gt;, când aveți pricini privitoare la lucrurile vieții acesteia, voi îi puneți judecători pe aceia care în biserică nu au nicio trecere&lt;f&gt;[15]&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (530,6,5,'Spre rușinea voastră zic lucrul acesta. Așadar, nu este între voi &lt;i&gt;nici măcar&lt;/i&gt; un om înțelept care să fie în stare să judece între frate și frate?');
INSERT INTO &quot;verses&quot; VALUES (530,6,6,'În loc să fie așa, un frate se duce la judecată împotriva altui frate, și încă înaintea necredincioșilor!');
INSERT INTO &quot;verses&quot; VALUES (530,6,7,'Chiar faptul că aveți judecăți între voi este deja o pierdere. Dar mai degrabă de ce&lt;f&gt;ⓜ&lt;/f&gt; nu acceptați să fiți nedreptățiți? De ce nu acceptați mai bine paguba?');
INSERT INTO &quot;verses&quot; VALUES (530,6,8,'În loc să fie așa, voi sunteți cei care nedreptățiți și păgubiți, și&lt;f&gt;ⓝ&lt;/f&gt; încă pe frați!');
INSERT INTO &quot;verses&quot; VALUES (530,6,9,'&lt;pb/&gt;Oare nu știți că cei nedrepți nu vor moșteni Împărăția lui Dumnezeu? Nu vă înșelați! Nici&lt;f&gt;ⓞ&lt;/f&gt; desfrânații, nici idolatrii, nici adulterii, nici efeminații&lt;f&gt;[16]&lt;/f&gt;, nici sodomiții&lt;f&gt;[17]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (530,6,10,'nici hoții, nici lacomii, nici bețivii, nici batjocoritorii, nici hrăpăreții nu vor moșteni Împărăția lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,6,11,'Și așa erați unii&lt;f&gt;ⓟ&lt;/f&gt; dintre voi! Dar&lt;f&gt;ⓠ&lt;/f&gt; ați fost spălați, ați fost sfințiți, ați fost îndreptățiți în Numele Domnului Isus Hristos și prin Duhul Dumnezeului nostru.');
INSERT INTO &quot;verses&quot; VALUES (530,6,12,'&lt;pb/&gt;„Toate&lt;f&gt;ⓡ&lt;/f&gt; îmi sunt îngăduite”&lt;f&gt;[18]&lt;/f&gt;, dar nu toate sunt de folos; „toate îmi sunt îngăduite”, dar nimic să nu pună stăpânire pe mine.');
INSERT INTO &quot;verses&quot; VALUES (530,6,13,'„Mâncărurile&lt;f&gt;ⓢ&lt;/f&gt; sunt pentru pântece, și pântecele este pentru mâncăruri”, dar Dumnezeu le va nimici și pe unul, și pe celelalte. Dar trupul nu este pentru desfrâu, ci este pentru&lt;f&gt;ⓣ&lt;/f&gt; Domnul, iar&lt;f&gt;ⓤ&lt;/f&gt; Domnul este pentru trup.');
INSERT INTO &quot;verses&quot; VALUES (530,6,14,'Și Dumnezeu&lt;f&gt;ⓥ&lt;/f&gt;, &lt;i&gt;care&lt;/i&gt; L-a înviat pe Domnul, ne va învia și pe noi prin&lt;f&gt;ⓦ&lt;/f&gt; puterea Sa.');
INSERT INTO &quot;verses&quot; VALUES (530,6,15,'Nu știți că trupurile&lt;f&gt;ⓧ&lt;/f&gt; voastre sunt mădulare ale lui Hristos? Voi lua deci mădularele lui Hristos și voi face din ele mădulare ale unei desfrânate? Nicidecum!');
INSERT INTO &quot;verses&quot; VALUES (530,6,16,'Nu știți că cine se lipește de o desfrânată este un singur trup &lt;i&gt;cu ea&lt;/i&gt;? Căci zice &lt;i&gt;Scriptura&lt;/i&gt;: &lt;e&gt;Cei doi vor&lt;/e&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;fi un singur trup&lt;/e&gt;.&lt;f&gt;[19]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,6,17,'Dar&lt;f&gt;ⓩ&lt;/f&gt; cine se lipește de Domnul este un singur duh &lt;i&gt;cu El&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,6,18,'Fugiți&lt;f&gt;ⓐ&lt;/f&gt; de desfrâu! Orice alt păcat pe care-l face omul este &lt;i&gt;săvârșit&lt;/i&gt; în afara trupului, dar cine se desfrânează păcătuiește&lt;f&gt;ⓑ&lt;/f&gt; împotriva propriului său trup.');
INSERT INTO &quot;verses&quot; VALUES (530,6,19,'Oare nu&lt;f&gt;ⓒ&lt;/f&gt; știți că trupul vostru este templul Duhului Sfânt, care locuiește în voi și pe care Îl aveți de la Dumnezeu? Și&lt;f&gt;ⓓ&lt;/f&gt; că voi nu sunteți ai voștri?');
INSERT INTO &quot;verses&quot; VALUES (530,6,20,'Ați&lt;f&gt;ⓔ&lt;/f&gt; fost cumpărați cu un preț. Slăviți-L deci pe Dumnezeu în trupul [și în duhul] vostru, [care sunt ale lui Dumnezeu]!');
INSERT INTO &quot;verses&quot; VALUES (530,7,1,'&lt;pb/&gt;Cu privire la lucrurile despre care mi-ați scris, &lt;i&gt;eu cred că&lt;/i&gt; este bine&lt;f&gt;ⓕ&lt;/f&gt; pentru om să nu se atingă de femeie.');
INSERT INTO &quot;verses&quot; VALUES (530,7,2,'Totuși, din pricina desfrâului, fiecare &lt;i&gt;bărbat&lt;/i&gt; să-și aibă soția lui și fiecare &lt;i&gt;femeie&lt;/i&gt; să-și aibă soțul ei!');
INSERT INTO &quot;verses&quot; VALUES (530,7,3,'Soțul&lt;f&gt;ⓖ&lt;/f&gt; să-și &lt;i&gt;împlinească&lt;/i&gt; față de soție datoria &lt;i&gt;de soț&lt;/i&gt; și tot așa &lt;i&gt;să facă&lt;/i&gt; și soția față de soț!');
INSERT INTO &quot;verses&quot; VALUES (530,7,4,'Soția nu este stăpână pe trupul ei, ci soțul &lt;i&gt;ei&lt;/i&gt;. Tot astfel, nici soțul nu este stăpân peste trupul lui, ci soția &lt;i&gt;lui&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,7,5,'Să&lt;f&gt;ⓗ&lt;/f&gt; nu vă lipsiți unul pe altul &lt;i&gt;de datoria de soți&lt;/i&gt; decât de comun acord, pentru o vreme, ca să vă îndeletniciți [cu postul și] cu rugăciunea, apoi să fiți iarăși împreună, ca să&lt;f&gt;ⓘ&lt;/f&gt; nu vă ispitească Satana din pricina nestăpânirii voastre!');
INSERT INTO &quot;verses&quot; VALUES (530,7,6,'Lucrul acesta îl spun ca un sfat, nu&lt;f&gt;ⓙ&lt;/f&gt; ca o poruncă.');
INSERT INTO &quot;verses&quot; VALUES (530,7,7,'Eu aș&lt;f&gt;ⓚ&lt;/f&gt; vrea ca toți oamenii să fie ca&lt;f&gt;ⓛ&lt;/f&gt; mine, dar&lt;f&gt;ⓜ&lt;/f&gt; fiecare are de la Dumnezeu darul lui: unul într-un fel, altul într-altul.');
INSERT INTO &quot;verses&quot; VALUES (530,7,8,'&lt;pb/&gt;Celor necăsătoriți și văduvelor le spun că este bine&lt;f&gt;ⓝ&lt;/f&gt; pentru ei dacă rămân ca mine.');
INSERT INTO &quot;verses&quot; VALUES (530,7,9,'Dar, dacă&lt;f&gt;ⓞ&lt;/f&gt; nu se înfrânează, să se căsătorească, pentru că este mai bine să se căsătorească decât să ardă&lt;f&gt;[20]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,7,10,'Celor căsătoriți le poruncesc nu&lt;f&gt;ⓟ&lt;/f&gt; eu, ci Domnul: soția&lt;f&gt;ⓠ&lt;/f&gt; să nu se despartă de soț!');
INSERT INTO &quot;verses&quot; VALUES (530,7,11,'(Dacă este despărțită, să rămână nemăritată sau să se împace cu soțul &lt;i&gt;ei&lt;/i&gt;!) Și &lt;i&gt;nici&lt;/i&gt; soțul să nu-și lase soția!');
INSERT INTO &quot;verses&quot; VALUES (530,7,12,'&lt;pb/&gt;Celorlalți le zic eu, nu&lt;f&gt;ⓡ&lt;/f&gt; Domnul: dacă un frate are o soție necredincioasă și ea este de acord să trăiască &lt;i&gt;mai departe&lt;/i&gt; cu el, să nu se despartă de ea!');
INSERT INTO &quot;verses&quot; VALUES (530,7,13,'Și dacă o femeie are un soț necredincios și el este de acord să trăiască &lt;i&gt;mai departe&lt;/i&gt; cu ea, să nu se despartă de soțul &lt;i&gt;ei&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,7,14,'Căci soțul necredincios este sfințit prin soția [credincioasă] și soția necredincioasă &lt;i&gt;este sfințită&lt;/i&gt; prin fratele &lt;i&gt;credincios&lt;/i&gt;; altminteri, copiii voștri ar&lt;f&gt;ⓢ&lt;/f&gt; fi necurați, dar, de fapt, sunt sfinți.');
INSERT INTO &quot;verses&quot; VALUES (530,7,15,'Dacă cel necredincios &lt;i&gt;vrea&lt;/i&gt; să se despartă, să se despartă; în astfel de &lt;i&gt;împrejurări&lt;/i&gt; fratele nu este legat, și nici sora: Dumnezeu ne-a chemat &lt;i&gt;să trăim&lt;/i&gt; în&lt;f&gt;ⓣ&lt;/f&gt; pace.');
INSERT INTO &quot;verses&quot; VALUES (530,7,16,'Căci ce știi tu, femeie, dacă îți vei mântui&lt;f&gt;ⓤ&lt;/f&gt; soțul? Sau ce știi tu, bărbate, dacă îți vei mântui soția?');
INSERT INTO &quot;verses&quot; VALUES (530,7,17,'&lt;pb/&gt;Încolo, fiecare să trăiască după cum i-a făcut Domnul parte și după cum l-a chemat Dumnezeu! Aceasta&lt;f&gt;ⓥ&lt;/f&gt; este rânduiala pe care am așezat-o în toate bisericile.');
INSERT INTO &quot;verses&quot; VALUES (530,7,18,'Dacă cineva a fost chemat pe când era circumcis, să nu-și ascundă semnele circumciziei! Dacă cineva a fost chemat pe când era necircumcis, să nu se circumcidă&lt;f&gt;ⓦ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,7,19,'Circumcizia&lt;f&gt;ⓧ&lt;/f&gt; nu înseamnă nimic și lipsa circumciziei nu înseamnă nimic; &lt;i&gt;importantă este&lt;/i&gt; păzirea&lt;f&gt;ⓨ&lt;/f&gt; poruncilor lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,7,20,'Fiecare să rămână în starea&lt;f&gt;[21]&lt;/f&gt; în care era când a fost chemat!');
INSERT INTO &quot;verses&quot; VALUES (530,7,21,'Ai fost chemat &lt;i&gt;când erai&lt;/i&gt; rob? Nu te îngrijora, dar, dacă poți să ajungi liber, folosește &lt;i&gt;prilejul&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,7,22,'Căci robul chemat în Domnul este un&lt;f&gt;ⓩ&lt;/f&gt; eliberat al Domnului. Tot astfel, cel liber care a fost chemat este un rob&lt;f&gt;ⓐ&lt;/f&gt; al lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,7,23,'Voi ați fost cumpărați cu&lt;f&gt;ⓑ&lt;/f&gt; un preț. Nu vă faceți robi oamenilor!');
INSERT INTO &quot;verses&quot; VALUES (530,7,24,'Fiecare&lt;f&gt;ⓒ&lt;/f&gt;, fraților, să rămână cu Dumnezeu &lt;i&gt;în starea&lt;/i&gt; în care era când a fost chemat!');
INSERT INTO &quot;verses&quot; VALUES (530,7,25,'&lt;pb/&gt;Cu privire la &lt;i&gt;feciori și&lt;/i&gt; fecioare&lt;f&gt;[22]&lt;/f&gt; n-am&lt;f&gt;ⓓ&lt;/f&gt; o poruncă de la Domnul. Le dau însă un sfat, fiindcă&lt;f&gt;ⓔ&lt;/f&gt; am primit de la Domnul harul să fiu&lt;f&gt;ⓕ&lt;/f&gt; vrednic de crezare.');
INSERT INTO &quot;verses&quot; VALUES (530,7,26,'Consider că este bine astfel, având în vedere strâmtorarea de acum: este&lt;f&gt;ⓖ&lt;/f&gt; bine pentru om să rămână cum este.');
INSERT INTO &quot;verses&quot; VALUES (530,7,27,'Ești legat de o soție? Nu căuta dezlegare! Nu ești legat de o soție? Nu căuta soție!');
INSERT INTO &quot;verses&quot; VALUES (530,7,28,'Dar nici dacă te însori nu păcătuiești. Dacă fecioara se mărită, nu păcătuiește. Dar unii ca aceștia vor avea necazuri pământești, iar eu aș vrea să vă cruț.');
INSERT INTO &quot;verses&quot; VALUES (530,7,29,'Ce spun eu, fraților, este că vremea s-a scurtat: de acum încolo, cei&lt;f&gt;ⓗ&lt;/f&gt; care au soții să fie ca și cum n-ar avea;');
INSERT INTO &quot;verses&quot; VALUES (530,7,30,'cei ce plâng, ca și cum n-ar plânge; cei ce se bucură, ca și cum nu s-ar bucura; cei ce cumpără, ca și cum n-ar stăpâni;');
INSERT INTO &quot;verses&quot; VALUES (530,7,31,'cei ce se folosesc de lumea aceasta, ca și cum nu s-ar&lt;f&gt;ⓘ&lt;/f&gt; folosi&lt;f&gt;[23]&lt;/f&gt; de ea, căci&lt;f&gt;ⓙ&lt;/f&gt; chipul lumii acesteia trece!');
INSERT INTO &quot;verses&quot; VALUES (530,7,32,'Dar eu aș vrea ca voi să fiți fără griji. Cel&lt;f&gt;ⓚ&lt;/f&gt; necăsătorit se îngrijește de lucrurile Domnului, cum să placă Domnului.');
INSERT INTO &quot;verses&quot; VALUES (530,7,33,'Dar cel căsătorit se îngrijește de lucrurile lumii, cum să placă soției,');
INSERT INTO &quot;verses&quot; VALUES (530,7,34,'și este împărțit. Atât femeia nemăritată, cât și fecioara se&lt;f&gt;ⓛ&lt;/f&gt; îngrijesc de lucrurile Domnului, ca să fie sfinte și cu trupul, și cu duhul, dar cea măritată se îngrijește de lucrurile lumii, cum să-i placă soțului.');
INSERT INTO &quot;verses&quot; VALUES (530,7,35,'Spun lucrul acesta pentru binele vostru, nu ca să vă întind o capcană, ci pentru ceea ce este frumos și ca să-I puteți sluji Domnului fără piedici.');
INSERT INTO &quot;verses&quot; VALUES (530,7,36,'Dacă cineva crede că se comportă rușinos față de trupul său fecior&lt;f&gt;[24]&lt;/f&gt;, dacă este sub presiunea instinctului și așa trebuie să fie, să facă ce vrea: nu greșește, să se căsătorească.');
INSERT INTO &quot;verses&quot; VALUES (530,7,37,'Dar cine stă ferm în inima sa și nu este sub presiune, ci are stăpânire asupra propriei voințe și așa a hotărât în inima sa — să-și păstreze trupul fecior — bine va face.');
INSERT INTO &quot;verses&quot; VALUES (530,7,38,'Astfel&lt;f&gt;ⓜ&lt;/f&gt;, cine își dă în căsătorie trupul fecior bine face, și cine nu și-l dă mai bine face.');
INSERT INTO &quot;verses&quot; VALUES (530,7,39,'O femeie&lt;f&gt;ⓝ&lt;/f&gt; este legată [de lege] câtă vreme îi trăiește soțul; dar dacă-i moare soțul, este liberă să se mărite cu cine vrea, numai &lt;i&gt;să fie&lt;/i&gt; în&lt;f&gt;ⓞ&lt;/f&gt; Domnul.');
INSERT INTO &quot;verses&quot; VALUES (530,7,40,'Dar mai fericită va fi dacă rămâne așa cum este, după&lt;f&gt;ⓟ&lt;/f&gt; părerea mea. Și cred&lt;f&gt;ⓠ&lt;/f&gt; că și eu am Duhul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,8,1,'&lt;pb/&gt;Cu privire&lt;f&gt;ⓡ&lt;/f&gt; la carnea jertfită idolilor, știm că „toți avem&lt;f&gt;ⓢ&lt;/f&gt; cunoaștere”&lt;f&gt;[25]&lt;/f&gt;. Dar cunoașterea&lt;f&gt;ⓣ&lt;/f&gt; îngâmfă, pe când dragostea zidește.');
INSERT INTO &quot;verses&quot; VALUES (530,8,2,'Dacă&lt;f&gt;ⓤ&lt;/f&gt; cineva crede că știe ceva, încă n-a cunoscut cum trebuie să cunoască.');
INSERT INTO &quot;verses&quot; VALUES (530,8,3,'Dar dacă Îl iubește cineva pe Dumnezeu, este&lt;f&gt;ⓥ&lt;/f&gt; cunoscut de Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,8,4,'Așadar, cu privire la mâncarea cărnii jertfite idolilor, știm că niciun idol&lt;f&gt;ⓦ&lt;/f&gt; din lume &lt;i&gt;nu există cu adevărat&lt;/i&gt; și&lt;f&gt;ⓧ&lt;/f&gt; că nu este decât un Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,8,5,'Căci chiar dacă ar fi așa-ziși&lt;f&gt;ⓨ&lt;/f&gt; „dumnezei”, fie în cer, fie pe pământ (după cum și sunt mulți „dumnezei” și mulți „domni”),');
INSERT INTO &quot;verses&quot; VALUES (530,8,6,'&lt;pb/&gt;&lt;t&gt;pentru noi&lt;f&gt;ⓩ&lt;/f&gt; există un singur Dumnezeu — Tatăl,&lt;/t&gt; &lt;t&gt;din&lt;f&gt;ⓐ&lt;/f&gt; care sunt toate lucrurile și pentru care &lt;i&gt;trăim&lt;/i&gt; și noi,&lt;/t&gt; &lt;t&gt;și un&lt;f&gt;ⓑ&lt;/f&gt; singur Domn — Isus Hristos,&lt;/t&gt; &lt;t&gt;prin&lt;f&gt;ⓒ&lt;/f&gt; care sunt toate lucrurile și, prin El, și noi.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,8,7,'&lt;pb/&gt;Dar nu toți au &lt;i&gt;ajuns la&lt;/i&gt; cunoașterea aceasta. Ci unii, fiindcă până acum au fost obișnuiți cu idolul, mănâncă un lucru ca fiind jertfit idolului, și conștiința lor, care este slabă, este întinată&lt;f&gt;ⓓ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,8,8,'Dar ce mâncăm&lt;f&gt;ⓔ&lt;/f&gt; nu ne face plăcuți lui Dumnezeu; nici nu pierdem ceva dacă nu mâncăm, nici nu câștigăm dacă mâncăm.');
INSERT INTO &quot;verses&quot; VALUES (530,8,9,'Luați seama&lt;f&gt;ⓕ&lt;/f&gt; însă ca nu cumva acest drept al vostru &lt;i&gt;de a mânca&lt;/i&gt; să ajungă o piatră de poticnire&lt;f&gt;ⓖ&lt;/f&gt; pentru cei slabi!');
INSERT INTO &quot;verses&quot; VALUES (530,8,10,'Căci dacă cineva te vede pe tine, care „ai cunoaștere”, că stai la masă într-un templu de idoli, conștiința&lt;f&gt;ⓗ&lt;/f&gt; lui, care este slabă, nu-l va îndemna&lt;f&gt;[26]&lt;/f&gt; să mănânce din carnea jertfită idolilor?');
INSERT INTO &quot;verses&quot; VALUES (530,8,11,'Astfel piere cel slab din&lt;f&gt;ⓘ&lt;/f&gt; pricina cunoașterii tale, &lt;i&gt;el,&lt;/i&gt; fratele &lt;i&gt;tău&lt;/i&gt; pentru care a murit Hristos!');
INSERT INTO &quot;verses&quot; VALUES (530,8,12,'Dacă păcătuiți astfel&lt;f&gt;ⓙ&lt;/f&gt; împotriva fraților &lt;i&gt;voștri&lt;/i&gt; și le răniți cugetul slab, împotriva lui Hristos păcătuiți.');
INSERT INTO &quot;verses&quot; VALUES (530,8,13,'De aceea, dacă&lt;f&gt;ⓚ&lt;/f&gt; ce mănânc eu îl face pe fratele meu să păcătuiască, nu voi mânca în veci carne, ca să nu-l fac pe fratele meu să păcătuiască.');
INSERT INTO &quot;verses&quot; VALUES (530,9,1,'&lt;pb/&gt;Nu&lt;f&gt;ⓛ&lt;/f&gt; sunt eu liber? Nu&lt;f&gt;ⓜ&lt;/f&gt; sunt eu apostol? Nu L-am văzut eu pe Isus, Domnul nostru? Nu&lt;f&gt;ⓝ&lt;/f&gt; sunteți voi lucrarea mea în Domnul?');
INSERT INTO &quot;verses&quot; VALUES (530,9,2,'Dacă pentru alții nu sunt apostol, sunt totuși pentru voi; căci voi sunteți pecetea&lt;f&gt;ⓞ&lt;/f&gt; apostoliei mele în Domnul.');
INSERT INTO &quot;verses&quot; VALUES (530,9,3,'Răspunsul meu de apărare împotriva celor ce mă cercetează este acesta:');
INSERT INTO &quot;verses&quot; VALUES (530,9,4,'Oare n-avem dreptul să mâncăm și să bem?');
INSERT INTO &quot;verses&quot; VALUES (530,9,5,'Oare n-avem dreptul să luăm &lt;i&gt;cu noi&lt;/i&gt; o soră, o soție, cum fac ceilalți apostoli și frații&lt;f&gt;ⓟ&lt;/f&gt; Domnului și Chifa?&lt;f&gt;ⓠ&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,9,6,'Sau numai eu și Barnaba avem&lt;f&gt;ⓡ&lt;/f&gt; obligația să lucrăm&lt;f&gt;[27]&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (530,9,7,'Cine merge la oaste pe cheltuiala sa? Cine sădește&lt;f&gt;ⓢ&lt;/f&gt; o vie și nu mănâncă din rodul ei? Sau cine paște o turmă și nu bea din laptele turmei?');
INSERT INTO &quot;verses&quot; VALUES (530,9,8,'Oare lucrurile acestea le spun &lt;i&gt;numai&lt;/i&gt; după mintea omului? Nu le spune și Legea?');
INSERT INTO &quot;verses&quot; VALUES (530,9,9,'În Legea lui Moise este scris: &lt;e&gt;Să&lt;/e&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;nu legi gura boului când treieră grâul!&lt;/e&gt;&lt;f&gt;[28]&lt;/f&gt; Oare pe boi îi are în vedere Dumnezeu aici?');
INSERT INTO &quot;verses&quot; VALUES (530,9,10,'Nu vorbește El mai cu seamă în folosul nostru? Da, pentru noi a fost scris, căci cine&lt;f&gt;ⓤ&lt;/f&gt; ară trebuie să are cu nădejde și cine treieră &lt;i&gt;grâul trebuie să-l treiere&lt;/i&gt; cu nădejdea că va avea parte de el.');
INSERT INTO &quot;verses&quot; VALUES (530,9,11,'Dacă&lt;f&gt;ⓥ&lt;/f&gt; noi am semănat printre voi bunurile duhovnicești, ar fi oare &lt;i&gt;prea&lt;/i&gt; mult dacă vom secera bunurile voastre materiale?');
INSERT INTO &quot;verses&quot; VALUES (530,9,12,'Dacă alții au acest drept asupra voastră, noi și mai mult, nu-i așa? Dar&lt;f&gt;ⓦ&lt;/f&gt; noi nu ne-am folosit de dreptul acesta, ci răbdăm totul, ca&lt;f&gt;ⓧ&lt;/f&gt; să nu punem vreo piedică Evangheliei lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,9,13,'&lt;pb/&gt;Nu&lt;f&gt;ⓨ&lt;/f&gt; știți că cei ce îndeplinesc slujbele sfinte se hrănesc din lucrurile de la Templu, iar cei ce slujesc altarului își iau partea de la altar?');
INSERT INTO &quot;verses&quot; VALUES (530,9,14,'Tot așa, Domnul&lt;f&gt;ⓩ&lt;/f&gt; a rânduit ca&lt;f&gt;ⓐ&lt;/f&gt; cei ce propovăduiesc Evanghelia să trăiască din Evanghelie.');
INSERT INTO &quot;verses&quot; VALUES (530,9,15,'Eu însă nu m-am&lt;f&gt;ⓑ&lt;/f&gt; folosit de niciunul din aceste &lt;i&gt;drepturi&lt;/i&gt;. Și nu vă scriu aceste lucruri ca să &lt;i&gt;cer&lt;/i&gt; să se facă așa cu mine, căci aș vrea&lt;f&gt;ⓒ&lt;/f&gt; mai bine să mor decât să-mi ia cineva motivul de laudă.');
INSERT INTO &quot;verses&quot; VALUES (530,9,16,'Dacă vestesc Evanghelia, nu este pentru mine un motiv de laudă, căci trebuie&lt;f&gt;ⓓ&lt;/f&gt; s-o vestesc; și vai de mine, dacă nu vestesc Evanghelia!');
INSERT INTO &quot;verses&quot; VALUES (530,9,17,'Dacă fac lucrul acesta de bunăvoie, am&lt;f&gt;ⓔ&lt;/f&gt; o răsplată, &lt;i&gt;dar&lt;/i&gt; dacă îl fac fiindcă sunt constrâns, este o isprăvnicie care mi-a&lt;f&gt;ⓕ&lt;/f&gt; fost încredințată.');
INSERT INTO &quot;verses&quot; VALUES (530,9,18,'Așadar, care este răsplata mea? Este&lt;f&gt;ⓖ&lt;/f&gt; să vestesc fără plată Evanghelia și să nu&lt;f&gt;ⓗ&lt;/f&gt; mă folosesc de dreptul meu în Evanghelie.');
INSERT INTO &quot;verses&quot; VALUES (530,9,19,'&lt;pb/&gt;Căci, deși sunt liber față de toți, m-am făcut robul tuturor&lt;f&gt;ⓘ&lt;/f&gt; ca&lt;f&gt;ⓙ&lt;/f&gt; să-i câștig pe cât mai mulți.');
INSERT INTO &quot;verses&quot; VALUES (530,9,20,'Cu iudeii&lt;f&gt;ⓚ&lt;/f&gt; m-am făcut ca un iudeu ca să-i câștig pe iudei, cu cei de sub Lege m-am făcut ca și când aș fi fost sub Lege (deși eu unul nu sunt sub Lege) ca să-i câștig pe cei ce sunt sub Lege,');
INSERT INTO &quot;verses&quot; VALUES (530,9,21,'cu cei ce&lt;f&gt;ⓛ&lt;/f&gt; sunt fără lege&lt;f&gt;ⓜ&lt;/f&gt; m-am făcut ca și cum aș fi fost fără lege (deși nu sunt fără&lt;f&gt;ⓝ&lt;/f&gt; o lege a lui Dumnezeu, ci sunt sub legea lui Hristos) ca să-i câștig pe cei fără lege.');
INSERT INTO &quot;verses&quot; VALUES (530,9,22,'M-am făcut slab cu cei&lt;f&gt;ⓞ&lt;/f&gt; slabi ca să-i câștig pe cei slabi. M-am făcut tuturor&lt;f&gt;ⓟ&lt;/f&gt; totul ca&lt;f&gt;ⓠ&lt;/f&gt; să-i mântuiesc măcar pe unii din ei.');
INSERT INTO &quot;verses&quot; VALUES (530,9,23,'Fac totul pentru Evanghelie ca să am și eu parte de ea.');
INSERT INTO &quot;verses&quot; VALUES (530,9,24,'&lt;pb/&gt;Nu știți că cei ce aleargă în arenă cu toții aleargă, dar numai unul ia premiul? Alergați în așa&lt;f&gt;ⓡ&lt;/f&gt; fel, încât să apucați premiul!');
INSERT INTO &quot;verses&quot; VALUES (530,9,25,'Toți atleții&lt;f&gt;ⓢ&lt;/f&gt; se supun la înfrânări de tot felul ca să capete o cunună pieritoare; noi &lt;i&gt;să facem lucrul acesta&lt;/i&gt; pentru &lt;i&gt;o cunună&lt;/i&gt; nepieritoare&lt;f&gt;ⓣ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,9,26,'Eu deci alerg, dar nu ca și când aș fi fără țintă. Lovesc cu pumnii, dar nu ca unul care lovește în vânt.');
INSERT INTO &quot;verses&quot; VALUES (530,9,27,'Dimpotrivă&lt;f&gt;ⓤ&lt;/f&gt;, mă port aspru cu trupul meu și îl țin rob ca nu cumva, după ce am propovăduit altora, eu însumi să fiu respins&lt;f&gt;ⓥ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,10,1,'&lt;pb/&gt;Fraților, nu vreau să nu știți că părinții noștri au fost cu toții sub nor&lt;f&gt;ⓦ&lt;/f&gt;, toți au trecut prin mare&lt;f&gt;ⓧ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (530,10,2,'toți au fost botezați în nor și în mare pentru Moise,');
INSERT INTO &quot;verses&quot; VALUES (530,10,3,'toți au mâncat aceeași mâncare&lt;f&gt;ⓨ&lt;/f&gt; duhovnicească');
INSERT INTO &quot;verses&quot; VALUES (530,10,4,'și toți au băut aceeași băutură&lt;f&gt;ⓩ&lt;/f&gt; duhovnicească, pentru că beau dintr-o Stâncă duhovnicească ce îi însoțea, și Stânca era Hristos&lt;f&gt;[29]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,10,5,'Totuși, cei mai mulți dintre ei n-au fost plăcuți lui Dumnezeu, căci au&lt;f&gt;ⓐ&lt;/f&gt; pierit în pustie.');
INSERT INTO &quot;verses&quot; VALUES (530,10,6,'Și aceste lucruri s-au întâmplat &lt;i&gt;ca să ne fie&lt;/i&gt; nouă pilde&lt;f&gt;[30]&lt;/f&gt;, așa încât să nu poftim după lucruri rele, cum&lt;f&gt;ⓑ&lt;/f&gt; au poftit ei.');
INSERT INTO &quot;verses&quot; VALUES (530,10,7,'Să nu fiți închinători la idoli, ca unii dintre ei, după cum este scris: &lt;e&gt;Poporul s-a așezat să mănânce și să bea și s-au sculat să joace&lt;/e&gt;&lt;f&gt;[31]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,10,8,'Să&lt;f&gt;ⓒ&lt;/f&gt; nu ne desfrânăm, cum au făcut unii din ei, încât într-o singură zi au căzut&lt;f&gt;[32]&lt;/f&gt; douăzeci și trei de mii&lt;f&gt;[33]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,10,9,'Să nu-L ispitim pe Hristos, cum L-au ispitit unii&lt;f&gt;ⓓ&lt;/f&gt; din ei, care au&lt;f&gt;ⓔ&lt;/f&gt; pierit &lt;i&gt;mușcați&lt;/i&gt; de șerpi!');
INSERT INTO &quot;verses&quot; VALUES (530,10,10,'Să nu cârtiți, cum au cârtit unii&lt;f&gt;ⓕ&lt;/f&gt; din ei, care au&lt;f&gt;ⓖ&lt;/f&gt; pierit &lt;i&gt;uciși&lt;/i&gt; de Nimicitorul&lt;f&gt;ⓗ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,10,11,'Aceste lucruri li s-au întâmplat &lt;i&gt;ca să ne fie&lt;/i&gt; pildă și au&lt;f&gt;ⓘ&lt;/f&gt; fost scrise ca învățătură pentru noi, cei peste care&lt;f&gt;ⓙ&lt;/f&gt; a venit sfârșitul veacurilor.');
INSERT INTO &quot;verses&quot; VALUES (530,10,12,'Așadar, cine&lt;f&gt;ⓚ&lt;/f&gt; crede că stă în picioare să ia seama să nu cadă.');
INSERT INTO &quot;verses&quot; VALUES (530,10,13,'Pe voi nu v-a ajuns nicio ispită care să nu fie &lt;i&gt;potrivită cu puterea&lt;/i&gt; omenească. Iar Dumnezeu este credincios și nu&lt;f&gt;ⓛ&lt;/f&gt; va îngădui să fiți ispitiți peste puterile voastre, ci, odată &lt;i&gt;venită&lt;/i&gt; ispita, va pregăti calea de scăpare, ca s-o puteți răbda.');
INSERT INTO &quot;verses&quot; VALUES (530,10,14,'&lt;pb/&gt;De aceea, preaiubiții mei, fugiți&lt;f&gt;ⓜ&lt;/f&gt; de idolatrie!');
INSERT INTO &quot;verses&quot; VALUES (530,10,15,'Vă vorbesc ca unor oameni înțelepți: judecați voi înșivă ce spun!');
INSERT INTO &quot;verses&quot; VALUES (530,10,16,'Paharul&lt;f&gt;ⓝ&lt;/f&gt; binecuvântat, pe care-l binecuvântăm, nu este el împărtășirea cu sângele lui Hristos? Pâinea&lt;f&gt;ⓞ&lt;/f&gt; pe care o frângem nu este ea împărtășirea cu trupul lui Hristos?');
INSERT INTO &quot;verses&quot; VALUES (530,10,17,'Fiindcă este o pâine, noi&lt;f&gt;ⓟ&lt;/f&gt;, cei mulți, &lt;i&gt;suntem&lt;/i&gt; un trup, căci toți luăm din aceeași pâine.');
INSERT INTO &quot;verses&quot; VALUES (530,10,18,'Uitați-vă la neamul lui Israel&lt;f&gt;[34]&lt;/f&gt;: cei ce mănâncă jertfele nu sunt ei&lt;f&gt;ⓠ&lt;/f&gt; în împărtășire cu altarul?');
INSERT INTO &quot;verses&quot; VALUES (530,10,19,'Așadar, ce zic eu? Că&lt;f&gt;ⓡ&lt;/f&gt; un lucru jertfit idolilor este ceva? Sau că un idol este ceva?');
INSERT INTO &quot;verses&quot; VALUES (530,10,20,'Dimpotrivă, &lt;i&gt;eu zic că&lt;/i&gt; ce jertfesc&lt;f&gt;ⓢ&lt;/f&gt; [neamurile], jertfesc demonilor, nu lui Dumnezeu. &lt;i&gt;Și eu&lt;/i&gt; nu vreau ca voi să fiți în împărtășire cu demonii.');
INSERT INTO &quot;verses&quot; VALUES (530,10,21,'Nu&lt;f&gt;ⓣ&lt;/f&gt; puteți bea paharul Domnului și paharul&lt;f&gt;ⓤ&lt;/f&gt; demonilor; nu puteți lua parte la masa Domnului și la masa demonilor.');
INSERT INTO &quot;verses&quot; VALUES (530,10,22,'Sau&lt;f&gt;ⓥ&lt;/f&gt; vrem să stârnim gelozia Domnului? Oare suntem&lt;f&gt;ⓦ&lt;/f&gt; noi mai tari decât El?');
INSERT INTO &quot;verses&quot; VALUES (530,10,23,'&lt;pb/&gt;„Toate&lt;f&gt;ⓧ&lt;/f&gt; îmi sunt îngăduite”, dar nu toate sunt de folos. „Toate îmi sunt îngăduite”, dar nu toate zidesc.');
INSERT INTO &quot;verses&quot; VALUES (530,10,24,'Nimeni&lt;f&gt;ⓨ&lt;/f&gt; să nu-și urmărească propriul interes, ci pe al celuilalt!');
INSERT INTO &quot;verses&quot; VALUES (530,10,25,'Să mâncați din tot ce&lt;f&gt;ⓩ&lt;/f&gt; se vinde în piața de carne, fără să cercetați ceva din pricina conștiinței!');
INSERT INTO &quot;verses&quot; VALUES (530,10,26,'Căci &lt;e&gt;al&lt;/e&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;e&gt;Domnului este pământul și tot ce cuprinde el&lt;/e&gt;&lt;f&gt;[35]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,10,27,'Dacă vă invită un necredincios [la masă] și vreți să vă duceți, să mâncați din tot ce&lt;f&gt;ⓑ&lt;/f&gt; vă pune înainte, fără să cercetați ceva din pricina conștiinței.');
INSERT INTO &quot;verses&quot; VALUES (530,10,28,'Dar, dacă vă spune cineva: „Aceasta este carne sacră&lt;f&gt;[36]&lt;/f&gt;”, să nu mâncați, din pricina celui ce v-a înștiințat și din pricina&lt;f&gt;ⓒ&lt;/f&gt; conștiinței, [căci &lt;e&gt;al&lt;/e&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;e&gt;Domnului este pământul și tot ce cuprinde el&lt;/e&gt;].');
INSERT INTO &quot;verses&quot; VALUES (530,10,29,'Vorbesc nu de conștiința proprie, ci de conștiința celuilalt. Căci de ce&lt;f&gt;ⓔ&lt;/f&gt; să fie judecată libertatea mea de conștiința altuia?');
INSERT INTO &quot;verses&quot; VALUES (530,10,30,'Dacă eu mănânc aducând mulțumiri, de ce să fiu vorbit de rău &lt;i&gt;pentru un lucru&lt;/i&gt; pentru&lt;f&gt;ⓕ&lt;/f&gt; care mulțumesc?');
INSERT INTO &quot;verses&quot; VALUES (530,10,31,'Deci&lt;f&gt;ⓖ&lt;/f&gt;, fie că mâncați, fie că beți, fie că faceți altceva, să faceți totul pentru slava lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (530,10,32,'Să nu&lt;f&gt;ⓗ&lt;/f&gt; fiți pricină de poticnire nici pentru iudei, nici pentru greci, nici pentru Biserica&lt;f&gt;ⓘ&lt;/f&gt; lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (530,10,33,'după cum și eu &lt;i&gt;încerc&lt;/i&gt;&lt;f&gt;ⓙ&lt;/f&gt; în toate privințele să plac tuturor, căutând nu&lt;f&gt;ⓚ&lt;/f&gt; folosul meu, ci al celor mai mulți, ca să fie mântuiți!');
INSERT INTO &quot;verses&quot; VALUES (530,11,1,'&lt;pb/&gt;Călcați pe urmele&lt;f&gt;ⓛ&lt;/f&gt; mele, întrucât și eu calc pe urmele lui Hristos!');
INSERT INTO &quot;verses&quot; VALUES (530,11,2,'&lt;pb/&gt;Vă laud că&lt;f&gt;ⓜ&lt;/f&gt; în toate &lt;i&gt;privințele&lt;/i&gt; vă aduceți aminte de mine și că țineți&lt;f&gt;ⓝ&lt;/f&gt; învățăturile&lt;f&gt;[37]&lt;/f&gt; întocmai cum vi le-am dat&lt;f&gt;[38]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,11,3,'Dar vreau să știți: capul oricărui bărbat este Hristos&lt;f&gt;ⓞ&lt;/f&gt;, capul&lt;f&gt;ⓟ&lt;/f&gt; femeii este bărbatul, iar capul&lt;f&gt;ⓠ&lt;/f&gt; lui Hristos este Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,11,4,'Orice bărbat care se roagă sau profețește&lt;f&gt;ⓡ&lt;/f&gt; cu capul acoperit își necinstește&lt;f&gt;[39]&lt;/f&gt; capul.');
INSERT INTO &quot;verses&quot; VALUES (530,11,5,'Dimpotrivă, orice femeie&lt;f&gt;ⓢ&lt;/f&gt; care se roagă sau profețește cu capul descoperit își necinstește capul, pentru că este ca și cum ar fi rasă&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,11,6,'Dacă o femeie nu își acoperă &lt;i&gt;capul&lt;/i&gt;, să se și tundă! Iar dacă este rușinos&lt;f&gt;ⓤ&lt;/f&gt; ca o femeie să fie tunsă ori rasă, să își acopere &lt;i&gt;capul&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,11,7,'Bărbatul nu trebuie să-și acopere capul, pentru că el este&lt;f&gt;ⓥ&lt;/f&gt; chipul și slava lui Dumnezeu, pe când femeia este slava bărbatului.');
INSERT INTO &quot;verses&quot; VALUES (530,11,8,'Căci nu&lt;f&gt;ⓦ&lt;/f&gt; bărbatul &lt;i&gt;a fost făcut&lt;/i&gt; din femeie, ci femeia din bărbat,');
INSERT INTO &quot;verses&quot; VALUES (530,11,9,'și nu&lt;f&gt;ⓧ&lt;/f&gt; bărbatul a fost făcut pentru femeie, ci femeia pentru bărbat.');
INSERT INTO &quot;verses&quot; VALUES (530,11,10,'De aceea, femeia trebuie să&lt;f&gt;ⓨ&lt;/f&gt; aibă pe cap &lt;i&gt;un semn al&lt;/i&gt; autorității, din pricina îngerilor&lt;f&gt;ⓩ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,11,11,'Totuși&lt;f&gt;ⓐ&lt;/f&gt;, în Domnul, nici femeia &lt;i&gt;nu este&lt;/i&gt; fără bărbat, nici bărbatul fără femeie.');
INSERT INTO &quot;verses&quot; VALUES (530,11,12,'Căci după cum femeia este din bărbat, tot așa și bărbatul este prin femeie, și toate&lt;f&gt;ⓑ&lt;/f&gt; sunt de la Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,11,13,'Judecați voi înșivă: se cuvine ca o femeie să se roage lui Dumnezeu &lt;i&gt;cu capul&lt;/i&gt; descoperit?');
INSERT INTO &quot;verses&quot; VALUES (530,11,14,'Nu vă învață chiar și ordinea firească a lucrurilor că este dezonorant pentru un bărbat să poarte părul lung,');
INSERT INTO &quot;verses&quot; VALUES (530,11,15,'dar că pentru o femeie purtarea părului lung este o cinste? Pentru că părul i-a fost dat ca acoperământ.');
INSERT INTO &quot;verses&quot; VALUES (530,11,16,'Dacă&lt;f&gt;ⓒ&lt;/f&gt; iubește cineva cearta de vorbe, noi nu avem un astfel de obicei și nici&lt;f&gt;ⓓ&lt;/f&gt; bisericile lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,11,17,'&lt;pb/&gt;Acestea fiind spuse, nu vă laud, fiindcă, atunci când vă adunați laolaltă, nu vă faceți mai buni, ci mai răi.');
INSERT INTO &quot;verses&quot; VALUES (530,11,18,'Mai întâi de toate, aud&lt;f&gt;ⓔ&lt;/f&gt; că, atunci când vă adunați ca biserică, între voi sunt dezbinări, și în parte o cred,');
INSERT INTO &quot;verses&quot; VALUES (530,11,19,'căci trebuie&lt;f&gt;ⓕ&lt;/f&gt; să fie și grupări între voi, ca&lt;f&gt;ⓖ&lt;/f&gt; să iasă la lumină cei găsiți buni.');
INSERT INTO &quot;verses&quot; VALUES (530,11,20,'Așadar, când vă adunați în același loc, nu &lt;i&gt;mai ajungeți&lt;/i&gt; să mâncați Cina Domnului.');
INSERT INTO &quot;verses&quot; VALUES (530,11,21,'Atunci când mâncați, fiecare se grăbește cu mâncarea proprie, așa că unul este flămând, iar altul&lt;f&gt;ⓗ&lt;/f&gt; este beat.');
INSERT INTO &quot;verses&quot; VALUES (530,11,22,'Oare n-aveți case unde să mâncați și să beți? Sau disprețuiți biserica&lt;f&gt;ⓘ&lt;/f&gt; lui Dumnezeu și vreți să-i faceți de rușine&lt;f&gt;ⓙ&lt;/f&gt; pe cei lipsiți? Ce să vă zic? Să vă laud? În privința aceasta nu vă laud.');
INSERT INTO &quot;verses&quot; VALUES (530,11,23,'&lt;pb/&gt;Eu am&lt;f&gt;ⓚ&lt;/f&gt; primit de la Domnul ce v-am învățat, anume că&lt;f&gt;ⓛ&lt;/f&gt; Domnul Isus, în noaptea în care a fost vândut, a luat o pâine');
INSERT INTO &quot;verses&quot; VALUES (530,11,24,'și, după ce a adus mulțumire, a frânt-o și a zis: &lt;J&gt;„&lt;e&gt;[Luați, mâncați;] acesta este trupul Meu [care se frânge] pentru voi; să faceți aceasta în amintirea Mea!&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[40]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,11,25,'Tot astfel, după cină, &lt;i&gt;a luat&lt;/i&gt; paharul și a zis: &lt;J&gt;„&lt;e&gt;Acest pahar este legământul cel nou în sângele Meu; să faceți aceasta ori de câte ori veți bea din el, în amintirea Mea!&lt;/e&gt;”&lt;/J&gt;&lt;f&gt;[41]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,11,26,'Pentru că, ori de câte ori mâncați din pâinea aceasta și beți din paharul acesta, vestiți moartea Domnului până&lt;f&gt;ⓜ&lt;/f&gt; va veni El.');
INSERT INTO &quot;verses&quot; VALUES (530,11,27,'&lt;pb/&gt;Astfel&lt;f&gt;ⓝ&lt;/f&gt;, oricine mănâncă pâinea &lt;i&gt;aceasta&lt;/i&gt; sau bea paharul Domnului în chip nevrednic va fi vinovat de trupul și sângele Domnului.');
INSERT INTO &quot;verses&quot; VALUES (530,11,28,'Fiecare&lt;f&gt;ⓞ&lt;/f&gt; deci să se cerceteze pe sine și așa să mănânce din pâinea &lt;i&gt;aceasta&lt;/i&gt; și să bea din paharul &lt;i&gt;acesta&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,11,29,'Căci cine mănâncă și bea își mănâncă și bea propria osândă dacă nu deosebește trupul &lt;i&gt;Domnului&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,11,30,'De aceea sunt între voi mulți suferinzi&lt;f&gt;[42]&lt;/f&gt; și bolnavi și nu puțini mor&lt;f&gt;[43]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,11,31,'Dacă&lt;f&gt;ⓟ&lt;/f&gt; ne-am judeca singuri, n-am fi judecați.');
INSERT INTO &quot;verses&quot; VALUES (530,11,32,'Dar când suntem judecați, suntem&lt;f&gt;ⓠ&lt;/f&gt; pedepsiți de Domnul, ca să nu fim osândiți odată cu lumea.');
INSERT INTO &quot;verses&quot; VALUES (530,11,33,'Așadar, frații mei, când vă adunați să mâncați, așteptați-vă unii pe alții!');
INSERT INTO &quot;verses&quot; VALUES (530,11,34,'Dacă îi este foame&lt;f&gt;ⓡ&lt;/f&gt; cuiva, să mănânce acasă&lt;f&gt;ⓢ&lt;/f&gt;, așa încât să nu vă adunați spre osândă! Celelalte lucruri le voi&lt;f&gt;ⓣ&lt;/f&gt; rândui când voi&lt;f&gt;ⓤ&lt;/f&gt; veni.');
INSERT INTO &quot;verses&quot; VALUES (530,12,1,'&lt;pb/&gt;Cu privire&lt;f&gt;ⓥ&lt;/f&gt; la &lt;i&gt;darurile&lt;/i&gt; duhovnicești, fraților, nu vreau să fiți în necunoștință.');
INSERT INTO &quot;verses&quot; VALUES (530,12,2,'&lt;i&gt;După cum&lt;/i&gt; știți, când erați păgâni&lt;f&gt;ⓦ&lt;/f&gt;, vă duceați la idolii cei muți, după cum erați atrași.');
INSERT INTO &quot;verses&quot; VALUES (530,12,3,'De aceea vă spun că&lt;f&gt;ⓧ&lt;/f&gt; nimeni, când vorbește prin Duhul lui Dumnezeu, nu zice: „Isus să fie anatema&lt;f&gt;[44]&lt;/f&gt;!” Și nimeni&lt;f&gt;ⓨ&lt;/f&gt; nu poate zice: „Isus este Domnul” decât prin Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (530,12,4,'&lt;pb/&gt;Sunt&lt;f&gt;ⓩ&lt;/f&gt; felurite daruri, dar este același&lt;f&gt;ⓐ&lt;/f&gt; Duh;');
INSERT INTO &quot;verses&quot; VALUES (530,12,5,'sunt&lt;f&gt;ⓑ&lt;/f&gt; felurite slujbe, dar este același Domn;');
INSERT INTO &quot;verses&quot; VALUES (530,12,6,'sunt felurite lucrări, dar este același Dumnezeu, care&lt;f&gt;ⓒ&lt;/f&gt; lucrează toate în toți.');
INSERT INTO &quot;verses&quot; VALUES (530,12,7,'Și&lt;f&gt;ⓓ&lt;/f&gt; fiecăruia i se dă manifestarea Duhului spre folosul &lt;i&gt;celorlalți&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,12,8,'De pildă, unuia îi este dat, prin Duhul, un cuvânt&lt;f&gt;ⓔ&lt;/f&gt; de înțelepciune; altuia, un cuvânt&lt;f&gt;ⓕ&lt;/f&gt; de cunoștință, potrivit aceluiași Duh;');
INSERT INTO &quot;verses&quot; VALUES (530,12,9,'altuia&lt;f&gt;ⓖ&lt;/f&gt;, credința, prin același Duh; altuia, daruri&lt;f&gt;ⓗ&lt;/f&gt; de vindecare, prin același Duh;');
INSERT INTO &quot;verses&quot; VALUES (530,12,10,'altuia&lt;f&gt;ⓘ&lt;/f&gt;, săvârșirea minunilor; altuia, profeția&lt;f&gt;ⓙ&lt;/f&gt;; altuia&lt;f&gt;ⓚ&lt;/f&gt;, deosebirea duhurilor; altuia, felurite&lt;f&gt;ⓛ&lt;/f&gt; limbi; și altuia, tălmăcirea limbilor.');
INSERT INTO &quot;verses&quot; VALUES (530,12,11,'Dar toate aceste lucruri le lucrează unul și același Duh, care împarte&lt;f&gt;ⓜ&lt;/f&gt; fiecăruia după cum&lt;f&gt;ⓝ&lt;/f&gt; vrea.');
INSERT INTO &quot;verses&quot; VALUES (530,12,12,'&lt;pb/&gt;Căci, după cum&lt;f&gt;ⓞ&lt;/f&gt; trupul este unul și are multe mădulare, iar toate mădularele trupului, deși sunt multe, &lt;i&gt;alcătuiesc&lt;/i&gt; un singur trup, tot așa&lt;f&gt;ⓟ&lt;/f&gt; este și Hristos.');
INSERT INTO &quot;verses&quot; VALUES (530,12,13,'Într-adevăr, noi toți am fost botezați într-un&lt;f&gt;ⓠ&lt;/f&gt; singur Duh, &lt;i&gt;ca să alcătuim&lt;/i&gt; un singur trup, fie&lt;f&gt;ⓡ&lt;/f&gt; iudei, fie greci, fie robi, fie liberi, și toți&lt;f&gt;ⓢ&lt;/f&gt; am fost adăpați dintr-un singur Duh.');
INSERT INTO &quot;verses&quot; VALUES (530,12,14,'Astfel, trupul nu este un singur mădular, ci mai multe.');
INSERT INTO &quot;verses&quot; VALUES (530,12,15,'Dacă piciorul ar zice: „Nu sunt mână, deci nu sunt din trup”, nu este &lt;i&gt;oare parte&lt;/i&gt; din trup?');
INSERT INTO &quot;verses&quot; VALUES (530,12,16,'Dacă urechea ar zice: „Nu sunt ochi, deci nu sunt din trup”, nu este &lt;i&gt;oare parte&lt;/i&gt; din trup?');
INSERT INTO &quot;verses&quot; VALUES (530,12,17,'Dacă tot trupul ar fi ochi, unde ar fi auzul? Dacă totul ar fi auz, unde ar fi mirosul?');
INSERT INTO &quot;verses&quot; VALUES (530,12,18,'Dimpotrivă, Dumnezeu a pus&lt;f&gt;ⓣ&lt;/f&gt; în trup mădularele — pe fiecare dintre ele — așa cum&lt;f&gt;ⓤ&lt;/f&gt; a vrut El.');
INSERT INTO &quot;verses&quot; VALUES (530,12,19,'Dacă ar fi toate un singur mădular, unde ar fi trupul?');
INSERT INTO &quot;verses&quot; VALUES (530,12,20,'În fapt, există multe mădulare, dar un singur trup.');
INSERT INTO &quot;verses&quot; VALUES (530,12,21,'Ochiul nu-i poate zice mâinii: „N-am nevoie de tine”; nici capul nu le poate zice picioarelor: „N-am nevoie de voi.”');
INSERT INTO &quot;verses&quot; VALUES (530,12,22,'Ba mai mult, mădularele trupului care par mai slabe sunt de neapărată trebuință.');
INSERT INTO &quot;verses&quot; VALUES (530,12,23,'Și &lt;i&gt;părțile&lt;/i&gt; trupului care par mai puțin onorabile le îmbrăcăm cu mai multă cinste. Așa că părțile rușinoase ale trupului le acoperim după buna-cuviință,');
INSERT INTO &quot;verses&quot; VALUES (530,12,24,'pe când celelalte n-au nevoie &lt;i&gt;să fie acoperite&lt;/i&gt;. Dar Dumnezeu a întocmit trupul ca să dea mai multă cinste mădularelor lipsite de cinste,');
INSERT INTO &quot;verses&quot; VALUES (530,12,25,'și astfel să nu fie dezbinare în trup, ci mădularele să se îngrijească în același fel unele de altele.');
INSERT INTO &quot;verses&quot; VALUES (530,12,26,'Și dacă suferă un mădular, toate mădularele suferă împreună cu el; dacă unul este lăudat, toate se bucură împreună cu el.');
INSERT INTO &quot;verses&quot; VALUES (530,12,27,'Voi&lt;f&gt;ⓥ&lt;/f&gt; sunteți trupul lui Hristos, și fiecare în parte, mădularele&lt;f&gt;ⓦ&lt;/f&gt; &lt;i&gt;Lui&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,12,28,'Și Dumnezeu&lt;f&gt;ⓧ&lt;/f&gt; a pus în Biserică întâi apostoli&lt;f&gt;ⓨ&lt;/f&gt;; în al doilea &lt;i&gt;rând&lt;/i&gt;, profeți&lt;f&gt;ⓩ&lt;/f&gt;; în al treilea &lt;i&gt;rând&lt;/i&gt;, învățători; apoi &lt;i&gt;pe cei ce au darul&lt;/i&gt; minunilor, apoi &lt;i&gt;pe cei ce au&lt;/i&gt; darul&lt;f&gt;ⓐ&lt;/f&gt; vindecării, &lt;i&gt;darul&lt;/i&gt; ajutorărilor&lt;f&gt;ⓑ&lt;/f&gt;, &lt;i&gt;darul&lt;/i&gt; conducerii&lt;f&gt;ⓒ&lt;/f&gt; și &lt;i&gt;darul vorbirii în&lt;/i&gt; felurite limbi.&lt;f&gt;[45]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,12,29,'Oare toți sunt apostoli? Toți sunt profeți? Toți sunt învățători? Toți săvârșesc minuni?');
INSERT INTO &quot;verses&quot; VALUES (530,12,30,'Toți au daruri de vindecare? Toți vorbesc în limbi? Toți tălmăcesc?');
INSERT INTO &quot;verses&quot; VALUES (530,12,31,'Căutați însă&lt;f&gt;ⓓ&lt;/f&gt; darurile cele mai bune, și vă voi arăta o cale nespus mai bună!');
INSERT INTO &quot;verses&quot; VALUES (530,13,1,'&lt;pb/&gt;Dacă aș vorbi în limbi omenești și îngerești, dar n-aș avea dragoste, n-aș fi &lt;i&gt;decât&lt;/i&gt; o aramă sunătoare sau un chimval zăngănitor.');
INSERT INTO &quot;verses&quot; VALUES (530,13,2,'Și dacă aș avea &lt;i&gt;darul&lt;/i&gt; profeției&lt;f&gt;ⓔ&lt;/f&gt; și aș pătrunde toate tainele și toată cunoașterea, dacă aș avea toată credința, încât&lt;f&gt;ⓕ&lt;/f&gt; să mut și munții, dar n-aș avea dragoste, n-aș fi nimic.');
INSERT INTO &quot;verses&quot; VALUES (530,13,3,'Iar dacă&lt;f&gt;ⓖ&lt;/f&gt; mi-aș împărți toată averea &lt;i&gt;pentru săraci&lt;/i&gt;, dacă mi-aș da trupul ca să fie ars&lt;f&gt;[46]&lt;/f&gt;, și n-aș avea dragoste, nu mi-ar folosi la nimic.');
INSERT INTO &quot;verses&quot; VALUES (530,13,4,'&lt;pb/&gt;Dragostea&lt;f&gt;ⓗ&lt;/f&gt; este îndelung răbdătoare, este plină de bunătate, dragostea nu invidiază, dragostea nu se laudă, nu se umflă &lt;i&gt;de mândrie&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (530,13,5,'nu se poartă necuviincios, nu-și&lt;f&gt;ⓘ&lt;/f&gt; urmărește propriile interese, nu are izbucniri de mânie, nu ține socoteală răului,');
INSERT INTO &quot;verses&quot; VALUES (530,13,6,'nu&lt;f&gt;ⓙ&lt;/f&gt; se bucură de nelegiuire, ci se&lt;f&gt;ⓚ&lt;/f&gt; bucură de adevăr,');
INSERT INTO &quot;verses&quot; VALUES (530,13,7,'acoperă totul, crede totul, nădăjduiește totul, suferă totul&lt;f&gt;ⓛ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,13,8,'&lt;pb/&gt;Dragostea nu va pieri niciodată. Profețiile se vor sfârși; limbile vor înceta; cunoașterea va avea sfârșit.');
INSERT INTO &quot;verses&quot; VALUES (530,13,9,'Căci&lt;f&gt;ⓜ&lt;/f&gt; cunoaștem în parte și profețim în parte,');
INSERT INTO &quot;verses&quot; VALUES (530,13,10,'dar, când va veni desăvârșirea, acest „în parte” se va sfârși.');
INSERT INTO &quot;verses&quot; VALUES (530,13,11,'Când eram copil, vorbeam ca un copil, simțeam ca un copil, gândeam ca un copil; când m-am făcut om mare, am sfârșit cu ce era copilăresc.');
INSERT INTO &quot;verses&quot; VALUES (530,13,12,'Acum vedem&lt;f&gt;ⓝ&lt;/f&gt; ca într-o oglindă&lt;f&gt;[47]&lt;/f&gt;, în chip nedeslușit&lt;f&gt;[48]&lt;/f&gt;, dar atunci vom vedea față&lt;f&gt;ⓞ&lt;/f&gt; către față. Acum cunosc în parte, dar atunci voi cunoaște deplin, după cum și eu am fost cunoscut pe deplin.');
INSERT INTO &quot;verses&quot; VALUES (530,13,13,'Așadar, rămân acestea trei: credința, nădejdea și dragostea, dar cea mai mare dintre ele este dragostea.');
INSERT INTO &quot;verses&quot; VALUES (530,14,1,'&lt;pb/&gt;Urmăriți&lt;f&gt;ⓟ&lt;/f&gt; dragostea, râvniți și după &lt;i&gt;darurile&lt;/i&gt; duhovnicești, dar&lt;f&gt;ⓠ&lt;/f&gt; mai ales să profețiți!');
INSERT INTO &quot;verses&quot; VALUES (530,14,2,'Cine vorbește&lt;f&gt;ⓡ&lt;/f&gt; în &lt;i&gt;altă&lt;/i&gt; limbă nu oamenilor le vorbește, ci lui Dumnezeu, căci nimeni nu-l înțelege, iar el cu duhul spune taine.');
INSERT INTO &quot;verses&quot; VALUES (530,14,3,'Dimpotrivă, cine profețește le vorbește oamenilor spre zidire, sfătuire și mângâiere.');
INSERT INTO &quot;verses&quot; VALUES (530,14,4,'Cine vorbește în &lt;i&gt;altă&lt;/i&gt; limbă se zidește pe sine, dar cine profețește zidește biserica.');
INSERT INTO &quot;verses&quot; VALUES (530,14,5,'Aș dori ca toți să vorbiți în limbi, dar mai ales să profețiți. Cine profețește este mai mare decât cine vorbește în limbi, dacă nu cumva le și tălmăcește, ca să capete biserica zidire &lt;i&gt;sufletească&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,14,6,'&lt;pb/&gt;Altminteri, fraților, cu ce v-aș fi de folos dacă aș veni la voi și aș vorbi în limbi, dar cuvântul meu nu v-ar aduce nici descoperire&lt;f&gt;ⓢ&lt;/f&gt;, nici cunoaștere, nici profeție, nici învățătură?');
INSERT INTO &quot;verses&quot; VALUES (530,14,7,'Chiar și lucrurile neînsuflețite care scot sunete (un fluier sau o harfă), dacă nu scot sunete deslușite, cum se va ști ce se cântă cu fluierul sau cu harfa?');
INSERT INTO &quot;verses&quot; VALUES (530,14,8,'Și dacă trâmbița scoate un sunet neclar, cine se va pregăti de luptă?');
INSERT INTO &quot;verses&quot; VALUES (530,14,9,'La fel și voi, dacă nu spuneți ceva inteligibil când vorbiți în altă limbă, cum se va ști ce spuneți? Este ca și când ați vorbi în vânt.');
INSERT INTO &quot;verses&quot; VALUES (530,14,10,'Oricât de multe feluri de limbi ar fi în lume, niciuna nu este fără sens.');
INSERT INTO &quot;verses&quot; VALUES (530,14,11,'Dar, dacă nu cunosc înțelesul sunetului, voi fi un străin pentru cel ce vorbește, și cel ce vorbește va fi un străin pentru mine.');
INSERT INTO &quot;verses&quot; VALUES (530,14,12,'Tot așa și voi, fiindcă râvniți după &lt;i&gt;daruri&lt;/i&gt; duhovnicești, căutați să le aveți din belșug, pentru zidirea bisericii.');
INSERT INTO &quot;verses&quot; VALUES (530,14,13,'&lt;pb/&gt;De aceea, cine vorbește în &lt;i&gt;altă&lt;/i&gt; limbă să se roage să &lt;i&gt;aibă și darul să o&lt;/i&gt; tălmăcească.');
INSERT INTO &quot;verses&quot; VALUES (530,14,14,'Fiindcă, dacă mă rog în &lt;i&gt;altă&lt;/i&gt; limbă, duhul meu se roagă, dar mintea mea este fără rod.');
INSERT INTO &quot;verses&quot; VALUES (530,14,15,'Ce este &lt;i&gt;de făcut&lt;/i&gt; atunci? Mă voi ruga cu duhul, dar mă voi ruga și cu mintea; voi&lt;f&gt;ⓣ&lt;/f&gt; cânta cu duhul, dar voi cânta și cu&lt;f&gt;ⓤ&lt;/f&gt; mintea.');
INSERT INTO &quot;verses&quot; VALUES (530,14,16,'Altminteri, dacă binecuvântezi cu duhul, cum va răspunde: „Amin!” la mulțumirile pe care le aduci tu omul simplu&lt;f&gt;[49]&lt;/f&gt;, când el nu știe ce spui?');
INSERT INTO &quot;verses&quot; VALUES (530,14,17,'Negreșit, tu mulțumești foarte frumos, dar celălalt nu este zidit.');
INSERT INTO &quot;verses&quot; VALUES (530,14,18,'Îi mulțumesc lui Dumnezeu că eu vorbesc în limbi mai mult decât voi toți.');
INSERT INTO &quot;verses&quot; VALUES (530,14,19,'Dar în biserică vreau mai bine să spun cinci cuvinte cu mintea, ca să-i învăț și pe alții, decât zece mii de cuvinte în &lt;i&gt;altă&lt;/i&gt; limbă.');
INSERT INTO &quot;verses&quot; VALUES (530,14,20,'&lt;pb/&gt;Fraților&lt;f&gt;ⓥ&lt;/f&gt;, nu fiți copii la minte; la răutate fiți&lt;f&gt;ⓦ&lt;/f&gt; prunci, iar la minte, fiți oameni mari!');
INSERT INTO &quot;verses&quot; VALUES (530,14,21,'În&lt;f&gt;ⓧ&lt;/f&gt; Lege este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Prin oameni de altă limbă și prin buze străine voi vorbi acestui popor,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și nici așa nu Mă vor asculta&lt;/e&gt; &lt;f&gt;[50]&lt;/f&gt;, zice Domnul.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,14,22,'&lt;pb/&gt;Prin urmare, limbile sunt un semn nu pentru cei credincioși, ci pentru cei necredincioși. Profeția, dimpotrivă, este nu pentru cei necredincioși, ci pentru cei credincioși.');
INSERT INTO &quot;verses&quot; VALUES (530,14,23,'Deci, dacă s-ar aduna toată biserica la un loc și toți ar vorbi în limbi și ar intra și oameni simpli sau necredincioși, n-ar&lt;f&gt;ⓨ&lt;/f&gt; zice ei că sunteți nebuni?');
INSERT INTO &quot;verses&quot; VALUES (530,14,24,'Dar dacă toți profețesc și intră un necredincios sau un om simplu, el este convins de toți, este cercetat de toți.');
INSERT INTO &quot;verses&quot; VALUES (530,14,25,'&lt;i&gt;Gândurile&lt;/i&gt; ascunse din inima lui sunt date la iveală, așa că va cădea cu fața la pământ, se va închina lui Dumnezeu și va mărturisi&lt;f&gt;ⓩ&lt;/f&gt;: &lt;e&gt;Cu adevărat Dumnezeu este în mijlocul vostru.&lt;/e&gt;&lt;f&gt;[51]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,14,26,'&lt;pb/&gt;Ce este &lt;i&gt;de făcut&lt;/i&gt; atunci, fraților? Când vă adunați laolaltă și unul are o cântare, &lt;i&gt;altul&lt;/i&gt; o învățătură&lt;f&gt;ⓐ&lt;/f&gt;, &lt;i&gt;altul&lt;/i&gt; o descoperire, &lt;i&gt;altul un cuvânt în altă&lt;/i&gt; limbă, &lt;i&gt;altul&lt;/i&gt; o tălmăcire, toate&lt;f&gt;ⓑ&lt;/f&gt; să se facă spre zidirea &lt;i&gt;sufletească&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,14,27,'Dacă se vorbește în limbi, să vorbească numai câte doi sau cel mult trei, pe rând, și unul să tălmăcească.');
INSERT INTO &quot;verses&quot; VALUES (530,14,28,'Dacă nu este cine să tălmăcească, să tacă în biserică și să-și vorbească &lt;i&gt;numai&lt;/i&gt; lui însuși și lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,14,29,'Profeții să vorbească doi sau trei, și&lt;f&gt;ⓒ&lt;/f&gt; ceilalți să judece.');
INSERT INTO &quot;verses&quot; VALUES (530,14,30,'Iar dacă unul care stă jos are o descoperire, cel&lt;f&gt;ⓓ&lt;/f&gt; dintâi să tacă.');
INSERT INTO &quot;verses&quot; VALUES (530,14,31,'Fiindcă puteți să profețiți toți, dar unul după altul, pentru ca toți să primească învățătură și toți să fie îmbărbătați.');
INSERT INTO &quot;verses&quot; VALUES (530,14,32,'Duhurile&lt;f&gt;ⓔ&lt;/f&gt; profeților sunt supuse profeților,');
INSERT INTO &quot;verses&quot; VALUES (530,14,33,'căci Dumnezeu nu este un Dumnezeu al neorânduielii, ci al păcii. &lt;pb/&gt;Ca&lt;f&gt;ⓕ&lt;/f&gt; în toate bisericile sfinților,');
INSERT INTO &quot;verses&quot; VALUES (530,14,34,'femeile&lt;f&gt;ⓖ&lt;/f&gt; să tacă în adunări, căci lor nu le este îngăduit să vorbească, ci &lt;i&gt;trebuie&lt;/i&gt; să&lt;f&gt;ⓗ&lt;/f&gt; fie supuse, după cum zice și Legea&lt;f&gt;ⓘ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,14,35,'Dacă vor să învețe ceva despre un lucru, să-i întrebe pe bărbații lor acasă, căci este rușinos ca o femeie să vorbească în biserică.');
INSERT INTO &quot;verses&quot; VALUES (530,14,36,'Ce? De la voi a pornit Cuvântul lui Dumnezeu? Sau numai până la voi a ajuns el?');
INSERT INTO &quot;verses&quot; VALUES (530,14,37,'&lt;pb/&gt;Dacă&lt;f&gt;ⓙ&lt;/f&gt; cineva se crede profet sau &lt;i&gt;om&lt;/i&gt; duhovnicesc, să priceapă că ce vă scriu eu este o poruncă a Domnului!');
INSERT INTO &quot;verses&quot; VALUES (530,14,38,'Și dacă cineva nu recunoaște, &lt;i&gt;nici el&lt;/i&gt; să nu aibă recunoașterea &lt;i&gt;voastră&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,14,39,'Așadar, frații mei, râvniți&lt;f&gt;ⓚ&lt;/f&gt; să profețiți, fără să împiedicați vorbirea în limbi!');
INSERT INTO &quot;verses&quot; VALUES (530,14,40,'Dar toate&lt;f&gt;ⓛ&lt;/f&gt; să se facă în chip cuviincios și cu rânduială.');
INSERT INTO &quot;verses&quot; VALUES (530,15,1,'&lt;pb/&gt;Vreau să vă reamintesc, fraților, Evanghelia pe care&lt;f&gt;ⓜ&lt;/f&gt; v-am propovăduit-o, pe care ați primit-o&lt;f&gt;ⓝ&lt;/f&gt;, în care ați rămas');
INSERT INTO &quot;verses&quot; VALUES (530,15,2,'și prin&lt;f&gt;ⓞ&lt;/f&gt; care sunteți mântuiți, dacă o țineți după cuvântul pe care vi l-am vestit; altfel, degeaba ați&lt;f&gt;ⓟ&lt;/f&gt; crezut.');
INSERT INTO &quot;verses&quot; VALUES (530,15,3,'V-am&lt;f&gt;ⓠ&lt;/f&gt; învățat, înainte de toate, ceea&lt;f&gt;ⓡ&lt;/f&gt; ce și eu am primit, &lt;i&gt;anume&lt;/i&gt; că Hristos a murit pentru păcatele noastre, după&lt;f&gt;ⓢ&lt;/f&gt; Scripturi,');
INSERT INTO &quot;verses&quot; VALUES (530,15,4,'că a fost îngropat și a înviat a treia zi, după Scripturi&lt;f&gt;ⓣ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (530,15,5,'și&lt;f&gt;ⓤ&lt;/f&gt; că i S-a arătat lui Chifa, apoi celor&lt;f&gt;ⓥ&lt;/f&gt; doisprezece.');
INSERT INTO &quot;verses&quot; VALUES (530,15,6,'După aceea, S-a arătat la peste cinci sute de frați deodată, dintre care cei mai mulți sunt încă în viață, iar unii au adormit.');
INSERT INTO &quot;verses&quot; VALUES (530,15,7,'Mai târziu, i S-a arătat lui Iacov&lt;f&gt;ⓦ&lt;/f&gt;, apoi tuturor&lt;f&gt;ⓧ&lt;/f&gt; apostolilor.');
INSERT INTO &quot;verses&quot; VALUES (530,15,8,'La urmă de tot&lt;f&gt;ⓨ&lt;/f&gt;, ca unei stârpituri, mi S-a arătat și mie.');
INSERT INTO &quot;verses&quot; VALUES (530,15,9,'Căci eu sunt cel mai neînsemnat dintre apostoli; nu sunt vrednic să mă numesc apostol, fiindcă am&lt;f&gt;ⓩ&lt;/f&gt; prigonit biserica lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (530,15,10,'Prin&lt;f&gt;ⓐ&lt;/f&gt; harul lui Dumnezeu sunt ce sunt, și harul Lui față de mine n-a fost zadarnic; ba încă am&lt;f&gt;ⓑ&lt;/f&gt; trudit mai mult decât toți; totuși&lt;f&gt;ⓒ&lt;/f&gt;, nu eu &lt;i&gt;am trudit&lt;/i&gt;, ci harul lui Dumnezeu care este cu mine.');
INSERT INTO &quot;verses&quot; VALUES (530,15,11,'Așadar, fie eu, fie ceilalți, noi așa propovăduim și voi așa ați crezut.');
INSERT INTO &quot;verses&quot; VALUES (530,15,12,'&lt;pb/&gt;Iar dacă se propovăduiește că Hristos a înviat din morți, cum zic unii dintre voi că nu este o înviere a morților?');
INSERT INTO &quot;verses&quot; VALUES (530,15,13,'Dacă nu este o înviere a morților, nici Hristos n-a înviat.');
INSERT INTO &quot;verses&quot; VALUES (530,15,14,'Și dacă Hristos n-a înviat, zadarnică este propovăduirea noastră, zadarnică este și credința voastră.');
INSERT INTO &quot;verses&quot; VALUES (530,15,15,'Ba încă suntem găsiți martori mincinoși ai lui Dumnezeu, fiindcă am mărturisit&lt;f&gt;ⓓ&lt;/f&gt; despre Dumnezeu că L-a înviat pe Hristos, pe care, &lt;i&gt;de fapt,&lt;/i&gt; nu L-a înviat, dacă este adevărat că morții nu învie.');
INSERT INTO &quot;verses&quot; VALUES (530,15,16,'Căci, dacă nu învie morții, nici Hristos n-a înviat.');
INSERT INTO &quot;verses&quot; VALUES (530,15,17,'Iar dacă Hristos n-a înviat, credința voastră este zadarnică, voi sunteți încă în păcatele voastre');
INSERT INTO &quot;verses&quot; VALUES (530,15,18,'și, prin urmare, cei ce au adormit în Hristos sunt pierduți.');
INSERT INTO &quot;verses&quot; VALUES (530,15,19,'Dacă numai pentru viața aceasta ne-am pus nădejdea în Hristos, suntem cei mai de plâns dintre toți oamenii!');
INSERT INTO &quot;verses&quot; VALUES (530,15,20,'&lt;pb/&gt;Dar, de fapt, Hristos&lt;f&gt;ⓔ&lt;/f&gt; a înviat din morți, primul rod&lt;f&gt;ⓕ&lt;/f&gt; dintre cei adormiți.');
INSERT INTO &quot;verses&quot; VALUES (530,15,21,'Căci, dacă&lt;f&gt;ⓖ&lt;/f&gt; moartea &lt;i&gt;a venit&lt;/i&gt; printr-un om, tot printr-un&lt;f&gt;ⓗ&lt;/f&gt; om &lt;i&gt;a venit&lt;/i&gt; și învierea morților.');
INSERT INTO &quot;verses&quot; VALUES (530,15,22,'Și după cum toți mor în Adam, tot așa toți vor fi aduși la viață în Hristos,');
INSERT INTO &quot;verses&quot; VALUES (530,15,23,'dar fiecare&lt;f&gt;ⓘ&lt;/f&gt; la rândul cetei lui. Hristos — cel dintâi rod, apoi cei ce sunt ai lui Hristos, la venirea Lui.');
INSERT INTO &quot;verses&quot; VALUES (530,15,24,'Apoi va veni sfârșitul, când El va da Împărăția&lt;f&gt;ⓙ&lt;/f&gt; în mâinile lui Dumnezeu Tatăl, după ce va fi nimicit orice căpetenie, orice stăpânire și orice putere.');
INSERT INTO &quot;verses&quot; VALUES (530,15,25,'Căci trebuie ca El să împărățească până&lt;f&gt;ⓚ&lt;/f&gt; &lt;e&gt;va pune pe&lt;/e&gt; toți &lt;e&gt;vrăjmașii sub picioarele&lt;/e&gt; Sale&lt;f&gt;[52]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,15,26,'Ultimul vrăjmaș&lt;f&gt;ⓛ&lt;/f&gt; care va fi nimicit va fi moartea,');
INSERT INTO &quot;verses&quot; VALUES (530,15,27,'căci Dumnezeu&lt;f&gt;ⓜ&lt;/f&gt; &lt;e&gt;a pus totul sub picioarele Lui&lt;/e&gt;&lt;f&gt;[53]&lt;/f&gt;. Dar când zice că totul I-a fost supus, se înțelege că în afară de Cel ce I-a supus totul.');
INSERT INTO &quot;verses&quot; VALUES (530,15,28,'Și când&lt;f&gt;ⓝ&lt;/f&gt; toate lucrurile Îi vor fi supuse, atunci Însuși&lt;f&gt;ⓞ&lt;/f&gt; Fiul Se va supune Celui ce I-a supus toate lucrurile, pentru ca Dumnezeu să fie totul în toți.');
INSERT INTO &quot;verses&quot; VALUES (530,15,29,'&lt;pb/&gt;Altfel, ce vor face cei care se botează pentru cei morți? Dacă morții nu învie nicidecum, de ce se mai botează ei pentru &lt;i&gt;cei morți&lt;/i&gt;?');
INSERT INTO &quot;verses&quot; VALUES (530,15,30,'Și de ce&lt;f&gt;ⓟ&lt;/f&gt; suntem noi în primejdie în orice clipă?');
INSERT INTO &quot;verses&quot; VALUES (530,15,31,'Jur că, &lt;i&gt;după cum am motiv să mă&lt;/i&gt; laud&lt;f&gt;ⓠ&lt;/f&gt; cu voi în Hristos Isus, &lt;i&gt;tot astfel&lt;/i&gt; sunt zilnic în primejdie&lt;f&gt;ⓡ&lt;/f&gt; de moarte, fraților!&lt;f&gt;[54]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,15,32,'Dacă, omenește &lt;i&gt;vorbind&lt;/i&gt;, m-am&lt;f&gt;ⓢ&lt;/f&gt; luptat cu fiarele în Efes, care-mi este folosul? Dacă nu învie morții, &lt;e&gt;să&lt;/e&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;mâncăm și să bem, căci mâine vom muri&lt;/e&gt;&lt;f&gt;[55]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,15,33,'Nu vă înșelați: „Tovărășiile&lt;f&gt;ⓤ&lt;/f&gt; rele strică obiceiurile bune”!');
INSERT INTO &quot;verses&quot; VALUES (530,15,34,'&lt;pb/&gt;Veniți-vă&lt;f&gt;ⓥ&lt;/f&gt; în fire cum se cuvine și nu păcătuiți! Căci&lt;f&gt;ⓦ&lt;/f&gt; sunt între voi unii care nu-L cunosc pe Dumnezeu — spre&lt;f&gt;ⓧ&lt;/f&gt; rușinea voastră o spun!');
INSERT INTO &quot;verses&quot; VALUES (530,15,35,'&lt;pb/&gt;Dar va zice cineva: „Cum&lt;f&gt;ⓨ&lt;/f&gt; învie morții? Și cu ce trup vor veni &lt;i&gt;la viață&lt;/i&gt;?”');
INSERT INTO &quot;verses&quot; VALUES (530,15,36,'Om fără minte! Ce&lt;f&gt;ⓩ&lt;/f&gt; semeni tu nu revine la viață dacă nu moare mai întâi.');
INSERT INTO &quot;verses&quot; VALUES (530,15,37,'Iar când semeni nu semeni trupul care va fi, ci un simplu grăunte, fie de grâu, fie de altceva.');
INSERT INTO &quot;verses&quot; VALUES (530,15,38,'Apoi, Dumnezeu îi dă un trup, după cum vrea: fiecărei semințe îi dă un trup al ei.');
INSERT INTO &quot;verses&quot; VALUES (530,15,39,'Nu toate trupurile sunt la fel, ci unul este trupul oamenilor, altul este trupul dobitoacelor, altul este trupul păsărilor, altul al peștilor.');
INSERT INTO &quot;verses&quot; VALUES (530,15,40,'Există corpuri cerești și corpuri pământești, dar una este strălucirea corpurilor cerești și alta, a corpurilor pământești.');
INSERT INTO &quot;verses&quot; VALUES (530,15,41,'Una este strălucirea soarelui, alta, strălucirea lunii și alta, strălucirea stelelor; chiar o stea se deosebește în strălucire de altă stea.');
INSERT INTO &quot;verses&quot; VALUES (530,15,42,'Așa&lt;f&gt;ⓐ&lt;/f&gt; este și învierea morților. &lt;i&gt;Trupul&lt;/i&gt; este semănat în putrezire și învie în neputrezire;');
INSERT INTO &quot;verses&quot; VALUES (530,15,43,'este&lt;f&gt;ⓑ&lt;/f&gt; semănat în ocară și învie în slavă; este semănat în slăbiciune și învie în putere.');
INSERT INTO &quot;verses&quot; VALUES (530,15,44,'Este semănat trup fizic&lt;f&gt;[56]&lt;/f&gt; și învie trup duhovnicesc. Dacă există trup fizic, există și trup duhovnicesc.');
INSERT INTO &quot;verses&quot; VALUES (530,15,45,'&lt;i&gt;De altfel&lt;/i&gt;, chiar așa este scris: &lt;e&gt;Omul&lt;/e&gt; dintâi, Adam, &lt;e&gt;a devenit un suflet viu.&lt;/e&gt;&lt;f&gt;[57]&lt;/f&gt; Ultimul Adam&lt;f&gt;ⓒ&lt;/f&gt; &lt;i&gt;a devenit&lt;/i&gt; duh&lt;f&gt;ⓓ&lt;/f&gt; dătător de viață.');
INSERT INTO &quot;verses&quot; VALUES (530,15,46,'Dar întâi vine ce este fizic, nu ce este duhovnicesc; ce este duhovnicesc vine pe urmă.');
INSERT INTO &quot;verses&quot; VALUES (530,15,47,'Omul&lt;f&gt;ⓔ&lt;/f&gt; dintâi este din pământ&lt;f&gt;ⓕ&lt;/f&gt;, pământesc; omul al doilea este din&lt;f&gt;ⓖ&lt;/f&gt; cer.');
INSERT INTO &quot;verses&quot; VALUES (530,15,48,'Cum este cel pământesc, așa sunt și cei pământești; cum&lt;f&gt;ⓗ&lt;/f&gt; este Cel ceresc, așa sunt și cei cerești.');
INSERT INTO &quot;verses&quot; VALUES (530,15,49,'Și după cum&lt;f&gt;ⓘ&lt;/f&gt; am purtat chipul celui pământesc, vom&lt;f&gt;ⓙ&lt;/f&gt; purta și chipul Celui ceresc.');
INSERT INTO &quot;verses&quot; VALUES (530,15,50,'&lt;pb/&gt;Ce spun eu, fraților, este că nu pot carnea&lt;f&gt;ⓚ&lt;/f&gt; și sângele să moștenească Împărăția lui Dumnezeu, nici putrezirea nu poate moșteni neputrezirea.');
INSERT INTO &quot;verses&quot; VALUES (530,15,51,'Iată, vă spun o taină: nu&lt;f&gt;ⓛ&lt;/f&gt; vom adormi toți, dar&lt;f&gt;ⓜ&lt;/f&gt; toți vom fi schimbați,');
INSERT INTO &quot;verses&quot; VALUES (530,15,52,'dintr-odată, cât ai clipi din ochi, la cea din urmă trâmbiță. Trâmbița&lt;f&gt;ⓝ&lt;/f&gt; va suna, morții vor învia nesupuși putrezirii și noi vom fi schimbați.');
INSERT INTO &quot;verses&quot; VALUES (530,15,53,'Căci trebuie ca &lt;i&gt;trupul&lt;/i&gt; acesta supus putrezirii să se îmbrace în neputrezire și &lt;i&gt;trupul&lt;/i&gt;&lt;f&gt;ⓞ&lt;/f&gt; acesta muritor să se îmbrace în nemurire.');
INSERT INTO &quot;verses&quot; VALUES (530,15,54,'Când &lt;i&gt;trupul&lt;/i&gt; acesta supus putrezirii se va îmbrăca în neputrezire și &lt;i&gt;trupul&lt;/i&gt; acesta muritor se va îmbrăca în nemurire, atunci se va împlini cuvântul care este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Moartea&lt;/e&gt; &lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;a fost înghițită de biruință&lt;/e&gt;.&lt;f&gt;[58]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,15,55,'&lt;t&gt;&lt;e&gt;Unde îți este biruința, moarte?&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Unde îți este boldul, moarte?&lt;/e&gt; &lt;f&gt;[59]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,15,56,'&lt;pb/&gt;Boldul morții este păcatul și puterea&lt;f&gt;ⓠ&lt;/f&gt; păcatului este Legea.');
INSERT INTO &quot;verses&quot; VALUES (530,15,57,'Dar&lt;f&gt;ⓡ&lt;/f&gt; mulțumiri fie aduse lui Dumnezeu, care ne dă biruința&lt;f&gt;ⓢ&lt;/f&gt; prin Domnul nostru Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (530,15,58,'De aceea&lt;f&gt;ⓣ&lt;/f&gt;, preaiubiții mei frați, fiți tari, neclintiți, sporiți întotdeauna în lucrul Domnului, căci știți că&lt;f&gt;ⓤ&lt;/f&gt; osteneala voastră în Domnul nu este zadarnică!');
INSERT INTO &quot;verses&quot; VALUES (530,16,1,'&lt;pb/&gt;Cu privire la colecta&lt;f&gt;ⓥ&lt;/f&gt; pentru sfinți, să faceți și voi cum am rânduit în bisericile Galatiei.');
INSERT INTO &quot;verses&quot; VALUES (530,16,2,'În&lt;f&gt;ⓦ&lt;/f&gt; prima zi a săptămânii, fiecare din voi să pună deoparte acasă ce va putea, după câștigul lui, ca să nu faceți colecte &lt;i&gt;abia&lt;/i&gt; când vin eu!');
INSERT INTO &quot;verses&quot; VALUES (530,16,3,'Când sosesc &lt;i&gt;eu&lt;/i&gt;, pe&lt;f&gt;ⓧ&lt;/f&gt; cei pe care îi veți socoti vrednici îi voi trimite, cu scrisori &lt;i&gt;de recomandare&lt;/i&gt;, să ducă darul vostru la Ierusalim.');
INSERT INTO &quot;verses&quot; VALUES (530,16,4,'Dacă&lt;f&gt;ⓨ&lt;/f&gt; merită să mă duc și eu, vor merge cu mine.');
INSERT INTO &quot;verses&quot; VALUES (530,16,5,'&lt;pb/&gt;La voi am să vin după ce&lt;f&gt;ⓩ&lt;/f&gt; voi trece prin Macedonia, căci prin Macedonia voi trece.');
INSERT INTO &quot;verses&quot; VALUES (530,16,6,'Poate mă voi opri pe la voi sau poate chiar voi ierna la voi, ca apoi să mă&lt;f&gt;ⓐ&lt;/f&gt; trimiteți&lt;f&gt;[60]&lt;/f&gt; acolo unde am de mers.');
INSERT INTO &quot;verses&quot; VALUES (530,16,7,'De data aceasta nu vreau să vă văd în treacăt, ci nădăjduiesc să mai rămân cu voi o vreme, dacă&lt;f&gt;ⓑ&lt;/f&gt; va îngădui Domnul.');
INSERT INTO &quot;verses&quot; VALUES (530,16,8,'Voi rămâne totuși în Efes până la Cincizecime,');
INSERT INTO &quot;verses&quot; VALUES (530,16,9,'căci mi&lt;f&gt;ⓒ&lt;/f&gt; s-a deschis aici o ușă largă și promițătoare, dar sunt&lt;f&gt;ⓓ&lt;/f&gt; și mulți împotrivitori.');
INSERT INTO &quot;verses&quot; VALUES (530,16,10,'Dacă&lt;f&gt;ⓔ&lt;/f&gt; sosește Timotei, aveți grijă să se simtă în largul lui la voi, căci el, asemenea mie, face lucrarea&lt;f&gt;ⓕ&lt;/f&gt; Domnului!');
INSERT INTO &quot;verses&quot; VALUES (530,16,11,'Nimeni&lt;f&gt;ⓖ&lt;/f&gt; să nu-l disprețuiască! Să-l trimiteți în pace pentru călătorie&lt;f&gt;ⓗ&lt;/f&gt;, ca să vină la mine, pentru că îl aștept cu frații!');
INSERT INTO &quot;verses&quot; VALUES (530,16,12,'&lt;pb/&gt;Cu privire la fratele Apolo&lt;f&gt;ⓘ&lt;/f&gt;: l-am rugat mult să vină la voi cu frații, dar nu a dorit nicidecum să vină acum; va veni însă când va avea înlesnire.');
INSERT INTO &quot;verses&quot; VALUES (530,16,13,'&lt;pb/&gt;Vegheați&lt;f&gt;ⓙ&lt;/f&gt;, fiți tari în credință, îmbărbătați-vă&lt;f&gt;ⓚ&lt;/f&gt;, întăriți-vă&lt;f&gt;ⓛ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (530,16,14,'Tot ce&lt;f&gt;ⓜ&lt;/f&gt; faceți să fie făcut cu dragoste!');
INSERT INTO &quot;verses&quot; VALUES (530,16,15,'&lt;pb/&gt;Încă un îndemn, fraților. Îi cunoașteți &lt;i&gt;pe cei din&lt;/i&gt; casa&lt;f&gt;ⓝ&lt;/f&gt; lui Stefanas; știți că ei sunt primul rod&lt;f&gt;ⓞ&lt;/f&gt; al Ahaiei și că s-au pus cu totul în slujba sfinților&lt;f&gt;ⓟ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (530,16,16,'Fiți și voi supuși&lt;f&gt;ⓠ&lt;/f&gt; unor astfel de oameni și fiecăruia care ajută la lucru și se&lt;f&gt;ⓡ&lt;/f&gt; ostenește!');
INSERT INTO &quot;verses&quot; VALUES (530,16,17,'Mă bucur de venirea lui Stefanas, Fortunat și Ahaic; ei&lt;f&gt;ⓢ&lt;/f&gt; au împlinit ce lipsea din partea voastră,');
INSERT INTO &quot;verses&quot; VALUES (530,16,18,'căci&lt;f&gt;ⓣ&lt;/f&gt; mi-au răcorit duhul meu și al vostru. Să-i&lt;f&gt;ⓤ&lt;/f&gt; prețuiți pe astfel de oameni!');
INSERT INTO &quot;verses&quot; VALUES (530,16,19,'&lt;pb/&gt;Bisericile din Asia&lt;f&gt;[61]&lt;/f&gt; vă trimit salutări. Aquila și Prisca, împreună&lt;f&gt;ⓥ&lt;/f&gt; cu biserica din casa lor, vă trimit multe salutări în Domnul.');
INSERT INTO &quot;verses&quot; VALUES (530,16,20,'Toți frații vă trimit salutări. Spuneți-vă salutări&lt;f&gt;ⓦ&lt;/f&gt; unii altora cu o sărutare sfântă!');
INSERT INTO &quot;verses&quot; VALUES (530,16,21,'Salutul&lt;f&gt;ⓧ&lt;/f&gt; este scris cu mâna mea: Pavel.');
INSERT INTO &quot;verses&quot; VALUES (530,16,22,'Dacă nu-L&lt;f&gt;ⓨ&lt;/f&gt; iubește cineva pe Domnul nostru Isus Hristos, să&lt;f&gt;ⓩ&lt;/f&gt; fie anatema! &lt;e&gt;Marána tha!&lt;/e&gt;&lt;f&gt;[62]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (530,16,23,'Harul&lt;f&gt;ⓐ&lt;/f&gt; Domnului Isus [Hristos] să fie cu voi!');
INSERT INTO &quot;verses&quot; VALUES (530,16,24,'Dragostea mea este cu voi cu toți în Hristos Isus. [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (540,1,1,'&lt;pb/&gt;Pavel, apostol&lt;f&gt;ⓐ&lt;/f&gt; al lui Hristos Isus prin voia lui Dumnezeu, și fratele Timotei, către biserica lui Dumnezeu care este în Corint și&lt;f&gt;ⓑ&lt;/f&gt; către toți sfinții care sunt în toată Ahaia:');
INSERT INTO &quot;verses&quot; VALUES (540,1,2,'Har&lt;f&gt;ⓒ&lt;/f&gt; și pace vouă de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (540,1,3,'&lt;pb/&gt;Binecuvântat&lt;f&gt;ⓓ&lt;/f&gt; să fie Dumnezeu, Tatăl Domnului nostru Isus Hristos, Părintele îndurărilor și Dumnezeul oricărei mângâieri,');
INSERT INTO &quot;verses&quot; VALUES (540,1,4,'care ne mângâie în toate necazurile noastre, pentru ca, prin mângâierea cu care noi înșine suntem mângâiați de Dumnezeu, să-i putem mângâia pe cei ce se află în vreun necaz!');
INSERT INTO &quot;verses&quot; VALUES (540,1,5,'Căci, după cum avem parte din belșug de suferințele&lt;f&gt;ⓔ&lt;/f&gt; lui Hristos, tot așa, prin Hristos, avem parte din belșug și de mângâiere.');
INSERT INTO &quot;verses&quot; VALUES (540,1,6,'Dar dacă suntem în necaz, &lt;i&gt;suntem&lt;/i&gt; pentru mângâierea și mântuirea voastră; dacă suntem mângâiați, &lt;i&gt;suntem&lt;/i&gt; pentru mângâierea voastră, care vă ajută să răbdați aceleași suferințe&lt;f&gt;ⓕ&lt;/f&gt; ca și noi.');
INSERT INTO &quot;verses&quot; VALUES (540,1,7,'Și nădejdea noastră cu privire la voi este neclintită, pentru că știm că, dacă&lt;f&gt;ⓖ&lt;/f&gt; aveți parte de suferințe, aveți parte și de mângâiere.');
INSERT INTO &quot;verses&quot; VALUES (540,1,8,'&lt;pb/&gt;Fraților, nu vrem să fiți în necunoștință despre necazul&lt;f&gt;ⓗ&lt;/f&gt; care ne-a lovit în Asia, de care am fost apăsați peste măsură de mult, mai presus de puterile noastre, încât nu știam dacă vom mai scăpa cu viață.');
INSERT INTO &quot;verses&quot; VALUES (540,1,9,'Credeam că suntem condamnați la moarte, ca să nu ne punem încrederea&lt;f&gt;ⓘ&lt;/f&gt; în noi înșine, ci în Dumnezeu, care învie morții.');
INSERT INTO &quot;verses&quot; VALUES (540,1,10,'El&lt;f&gt;ⓙ&lt;/f&gt; ne-a izbăvit și ne izbăvește dintr-un mare &lt;i&gt;pericol de&lt;/i&gt; moarte și avem nădejde că ne va izbăvi și mai departe.');
INSERT INTO &quot;verses&quot; VALUES (540,1,11,'Voi înșivă ne&lt;f&gt;ⓚ&lt;/f&gt; ajutați cu rugăciunile voastre, pentru&lt;f&gt;ⓛ&lt;/f&gt; ca darul pe care l-am primit prin &lt;i&gt;rugăciunile&lt;/i&gt; multora să fie pentru mulți &lt;i&gt;un prilej&lt;/i&gt; de mulțumire către Dumnezeu pentru noi.');
INSERT INTO &quot;verses&quot; VALUES (540,1,12,'&lt;pb/&gt;Lauda noastră este aceasta: mărturia pe care ne-o dă conștiința, &lt;i&gt;anume&lt;/i&gt; că ne-am purtat în lume, și mai ales față de voi, cu o sinceritate și o curăție&lt;f&gt;ⓜ&lt;/f&gt; date de Dumnezeu, nu&lt;f&gt;ⓝ&lt;/f&gt; cu înțelepciune firească, ci prin harul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,1,13,'Nu vă scriem altceva decât ce citiți și cunoașteți. Și trag nădejde că în cele din urmă veți recunoaște,');
INSERT INTO &quot;verses&quot; VALUES (540,1,14,'cum ați și recunoscut în parte, că&lt;f&gt;ⓞ&lt;/f&gt; noi suntem lauda voastră, după cum și voi&lt;f&gt;ⓟ&lt;/f&gt; &lt;i&gt;veți fi&lt;/i&gt; lauda noastră în ziua Domnului Isus.');
INSERT INTO &quot;verses&quot; VALUES (540,1,15,'&lt;pb/&gt;Cu această încredințare voiam&lt;f&gt;ⓠ&lt;/f&gt; să vin mai înainte la voi, ca să aveți un&lt;f&gt;ⓡ&lt;/f&gt; al doilea har,');
INSERT INTO &quot;verses&quot; VALUES (540,1,16,'iar de la voi să trec prin Macedonia; apoi&lt;f&gt;ⓢ&lt;/f&gt; din Macedonia să mă întorc la voi, ca să-mi oferiți sprijinul &lt;i&gt;necesar&lt;/i&gt; pentru călătoria în Iudeea.');
INSERT INTO &quot;verses&quot; VALUES (540,1,17,'Când am făcut acest plan, am lucrat oare în chip ușuratic? &lt;i&gt;Credeți că&lt;/i&gt; hotărârile pe care le iau sunt în felul lumii&lt;f&gt;ⓣ&lt;/f&gt;, ca să zic, &lt;i&gt;în același timp,&lt;/i&gt; „da, da” și „nu, nu”?');
INSERT INTO &quot;verses&quot; VALUES (540,1,18,'Credincios este Dumnezeu că vorbirea noastră față de voi n-a fost și „da”, și „nu”.');
INSERT INTO &quot;verses&quot; VALUES (540,1,19,'Căci Fiul&lt;f&gt;ⓤ&lt;/f&gt; lui Dumnezeu, Isus Hristos, care a fost propovăduit de noi în mijlocul vostru (prin mine, prin Silvan și prin Timotei) n-a fost &lt;i&gt;și&lt;/i&gt; „da”, și „nu”, ci&lt;f&gt;ⓥ&lt;/f&gt; în El nu este decât „da”.');
INSERT INTO &quot;verses&quot; VALUES (540,1,20,'Toate&lt;f&gt;ⓦ&lt;/f&gt; făgăduințele lui Dumnezeu sunt „da” în El; de aceea și „amin”-ul pe care-l spunem noi prin El este spre slava lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,1,21,'Iar Cel ce ne întărește împreună cu voi în Hristos și care ne-a&lt;f&gt;ⓧ&lt;/f&gt; uns este Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,1,22,'Tot El ne-a&lt;f&gt;ⓨ&lt;/f&gt; pecetluit și ne-a pus în inimi Duhul ca arvună&lt;f&gt;[1]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,1,23,'&lt;pb/&gt;Îl chem&lt;f&gt;ⓩ&lt;/f&gt; pe Dumnezeu ca martor față de sufletul meu că n-am mai venit &lt;i&gt;până acum&lt;/i&gt; la Corint tocmai ca&lt;f&gt;ⓐ&lt;/f&gt; să vă cruț.');
INSERT INTO &quot;verses&quot; VALUES (540,1,24,'Nu spun că&lt;f&gt;ⓑ&lt;/f&gt; am avea stăpânire peste credința voastră; din contră, lucrăm și noi la bucuria voastră, fiindcă stați tari în&lt;f&gt;ⓒ&lt;/f&gt; credință.');
INSERT INTO &quot;verses&quot; VALUES (540,2,1,'&lt;pb/&gt;Am hotărât deci în sinea mea să&lt;f&gt;ⓓ&lt;/f&gt; nu mă întorc la voi cu întristare&lt;f&gt;[2]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,2,2,'Căci, dacă eu vă întristez, cine îmi va aduce bucurie dacă nu cel întristat de mine?');
INSERT INTO &quot;verses&quot; VALUES (540,2,3,'Și v-am scris tocmai din acest motiv, pentru ca la venirea mea să&lt;f&gt;ⓔ&lt;/f&gt; n-am parte de întristare de la cei ce ar fi trebuit să-mi aducă bucurie, căci sunt&lt;f&gt;ⓕ&lt;/f&gt; încredințat, cu privire la voi toți, că bucuria mea este bucuria voastră, a tuturor.');
INSERT INTO &quot;verses&quot; VALUES (540,2,4,'Din multă mâhnire și strângere de inimă v-am scris, &lt;i&gt;cu ochii scăldați&lt;/i&gt; în lacrimi, nu&lt;f&gt;ⓖ&lt;/f&gt; ca să vă întristez, ci ca să ajungeți să cunoașteți dragostea nespus de mare pe care o am față de voi.');
INSERT INTO &quot;verses&quot; VALUES (540,2,5,'&lt;pb/&gt;Dacă&lt;f&gt;ⓗ&lt;/f&gt; a adus cineva întristare, nu pe mine m-a&lt;f&gt;ⓘ&lt;/f&gt; întristat, ci — într-o anumită măsură, ca să nu exagerez — pe voi toți.');
INSERT INTO &quot;verses&quot; VALUES (540,2,6,'Pentru un asemenea &lt;i&gt;om&lt;/i&gt; este de ajuns chiar pedeapsa care i-a fost dată de&lt;f&gt;ⓙ&lt;/f&gt; cei mai mulți &lt;i&gt;dintre voi&lt;/i&gt;;');
INSERT INTO &quot;verses&quot; VALUES (540,2,7,'așa că &lt;i&gt;acum&lt;/i&gt;&lt;f&gt;ⓚ&lt;/f&gt; este mai bine să-l iertați și să-l mângâiați, ca să nu fie doborât de prea multă întristare.');
INSERT INTO &quot;verses&quot; VALUES (540,2,8,'De aceea, vă rog să vă arătați iarăși dragostea față de el,');
INSERT INTO &quot;verses&quot; VALUES (540,2,9,'căci v-am scris și ca să vă pun la încercare și să văd dacă sunteți&lt;f&gt;ⓛ&lt;/f&gt; ascultători în toate.');
INSERT INTO &quot;verses&quot; VALUES (540,2,10,'Dar pe cine iertați voi, iert și eu. Într-adevăr, ce am iertat eu — dacă a fost ceva de iertat — am iertat pentru voi în fața lui Hristos,');
INSERT INTO &quot;verses&quot; VALUES (540,2,11,'ca să nu-l lăsăm pe Satana să aibă vreun câștig de la noi, căci nu suntem în necunoștință de planurile lui.');
INSERT INTO &quot;verses&quot; VALUES (540,2,12,'&lt;pb/&gt;Când am ajuns&lt;f&gt;ⓜ&lt;/f&gt; la Troas pentru Evanghelia lui Hristos, deși mi se deschisese acolo o ușă&lt;f&gt;ⓝ&lt;/f&gt; în Domnul,');
INSERT INTO &quot;verses&quot; VALUES (540,2,13,'n-am&lt;f&gt;ⓞ&lt;/f&gt; avut liniște în duhul meu fiindcă nu l-am găsit pe Tit, fratele meu; de aceea mi-am luat rămas bun de la frați și am plecat în Macedonia.');
INSERT INTO &quot;verses&quot; VALUES (540,2,14,'&lt;pb/&gt;Mulțumiri fie aduse lui Dumnezeu, care, în Hristos, ne poartă mereu în triumf și care prin noi răspândește în orice loc mireasma&lt;f&gt;ⓟ&lt;/f&gt; cunoașterii Lui!');
INSERT INTO &quot;verses&quot; VALUES (540,2,15,'Noi suntem mireasma lui Hristos înaintea lui Dumnezeu, atât printre&lt;f&gt;ⓠ&lt;/f&gt; cei ce sunt pe calea mântuirii, cât și printre&lt;f&gt;ⓡ&lt;/f&gt; cei ce sunt pe calea pierzării:');
INSERT INTO &quot;verses&quot; VALUES (540,2,16,'pentru unii&lt;f&gt;ⓢ&lt;/f&gt;, o mireasmă de moarte, spre moarte; pentru alții, o mireasmă de viață, spre viață. Și cine&lt;f&gt;ⓣ&lt;/f&gt; este destoinic pentru aceste lucruri?');
INSERT INTO &quot;verses&quot; VALUES (540,2,17,'Căci noi nu facem negoț cu Cuvântul&lt;f&gt;ⓤ&lt;/f&gt; lui Dumnezeu, cum fac cei mai mulți, ci vorbim dintr-o motivație sinceră, ca din partea lui Dumnezeu, înaintea lui Dumnezeu, în Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,3,1,'&lt;pb/&gt;Începem&lt;f&gt;ⓥ&lt;/f&gt; iarăși să ne vorbim singuri de bine&lt;f&gt;[3]&lt;/f&gt;? Avem cumva trebuință, ca unii, de scrisori&lt;f&gt;ⓦ&lt;/f&gt; de recomandare către voi sau de la voi?');
INSERT INTO &quot;verses&quot; VALUES (540,3,2,'Scrisoarea noastră sunteți voi&lt;f&gt;ⓧ&lt;/f&gt;, scrisă în inimile noastre, cunoscută și citită de toți oamenii.');
INSERT INTO &quot;verses&quot; VALUES (540,3,3,'Este limpede că sunteți o scrisoare a lui Hristos, trimisă&lt;f&gt;ⓨ&lt;/f&gt; de noi, scrisă nu cu cerneală, ci cu Duhul Dumnezeului celui viu; nu pe niște&lt;f&gt;ⓩ&lt;/f&gt; table de piatră, ci pe niște&lt;f&gt;ⓐ&lt;/f&gt; table care sunt inimi de carne.');
INSERT INTO &quot;verses&quot; VALUES (540,3,4,'&lt;pb/&gt;Avem încredințarea aceasta în Dumnezeu, prin Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,3,5,'Nu că noi, prin noi înșine, suntem destoinici să socotim ceva ca &lt;i&gt;venind&lt;/i&gt; de la noi. Dimpotrivă, destoinicia&lt;f&gt;ⓑ&lt;/f&gt; noastră &lt;i&gt;vine&lt;/i&gt; de la Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (540,3,6,'care ne-a și învrednicit să fim slujitori&lt;f&gt;ⓒ&lt;/f&gt; ai unui legământ&lt;f&gt;ⓓ&lt;/f&gt; nou, care nu este al literei&lt;f&gt;ⓔ&lt;/f&gt;, ci al Duhului, căci litera&lt;f&gt;ⓕ&lt;/f&gt; omoară, dar&lt;f&gt;ⓖ&lt;/f&gt; Duhul dă viață.');
INSERT INTO &quot;verses&quot; VALUES (540,3,7,'&lt;pb/&gt;Acum, dacă slujba &lt;i&gt;aducătoare&lt;/i&gt; de moarte, &lt;i&gt;scrisă&lt;/i&gt; cu litere săpate&lt;f&gt;ⓗ&lt;/f&gt; în piatră, era &lt;i&gt;însoțită&lt;/i&gt; de slavă, încât&lt;f&gt;ⓘ&lt;/f&gt; fiii lui Israel nu-și puteau aținti privirea asupra feței lui Moise din pricina strălucirii&lt;f&gt;[4]&lt;/f&gt; chipului său, &lt;i&gt;deși era o strălucire&lt;/i&gt; trecătoare,');
INSERT INTO &quot;verses&quot; VALUES (540,3,8,'cu cât mai mult slujba&lt;f&gt;ⓙ&lt;/f&gt; Duhului va fi însoțită de slavă?');
INSERT INTO &quot;verses&quot; VALUES (540,3,9,'Dacă slujba &lt;i&gt;aducătoare&lt;/i&gt; de osândă a fost &lt;i&gt;însoțită&lt;/i&gt; de slavă, cu cât mai mult o întrece în slavă slujba&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;aducătoare&lt;/i&gt; de dreptate?');
INSERT INTO &quot;verses&quot; VALUES (540,3,10,'În acest caz, &lt;i&gt;prima&lt;/i&gt;, deși a fost însoțită de slavă, pălește în comparație &lt;i&gt;cu a doua&lt;/i&gt;, a cărei slavă o întrece cu mult.&lt;f&gt;[5]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,3,11,'Dacă ceea ce este trecător &lt;i&gt;a fost însoțit&lt;/i&gt; de slavă, cu atât mai mult &lt;i&gt;este însoțit&lt;/i&gt; de slavă ceea ce dăinuie!');
INSERT INTO &quot;verses&quot; VALUES (540,3,12,'&lt;pb/&gt;Fiindcă avem deci o astfel de nădejde, noi lucrăm&lt;f&gt;ⓛ&lt;/f&gt; cu multă îndrăzneală');
INSERT INTO &quot;verses&quot; VALUES (540,3,13,'și nu &lt;i&gt;facem&lt;/i&gt; ca Moise, care&lt;f&gt;ⓜ&lt;/f&gt; își punea un văl pe față pentru ca fiii lui Israel să nu-și ațintească privirea asupra sfârșitului &lt;i&gt;slavei&lt;/i&gt; trecătoare.');
INSERT INTO &quot;verses&quot; VALUES (540,3,14,'Dar mintea lor s-a împietrit&lt;f&gt;ⓝ&lt;/f&gt;, căci, până în ziua de astăzi, la citirea vechiului legământ rămâne același văl, neridicat, fiindcă vălul este dat la o parte în Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,3,15,'Da, până astăzi, când se citește &lt;i&gt;din&lt;/i&gt; Moise, peste inimile lor se află un văl.');
INSERT INTO &quot;verses&quot; VALUES (540,3,16,'Dar când&lt;f&gt;ⓞ&lt;/f&gt; &lt;i&gt;vreunul&lt;/i&gt; se întoarce &lt;e&gt;la Domnul, vălul este luat&lt;/e&gt;&lt;f&gt;[6]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,3,17,'Or, &lt;i&gt;acest&lt;/i&gt; Domn&lt;f&gt;ⓟ&lt;/f&gt; este Duhul, și unde este Duhul Domnului, &lt;i&gt;acolo&lt;/i&gt; este libertate.');
INSERT INTO &quot;verses&quot; VALUES (540,3,18,'Noi toți privim cu fața descoperită, ca&lt;f&gt;ⓠ&lt;/f&gt; într-o oglindă, slava&lt;f&gt;ⓡ&lt;/f&gt; Domnului&lt;f&gt;[7]&lt;/f&gt; și suntem transformați&lt;f&gt;ⓢ&lt;/f&gt; în același chip al Lui, din slavă în slavă, prin Domnul, &lt;i&gt;adică prin&lt;/i&gt; Duhul.');
INSERT INTO &quot;verses&quot; VALUES (540,4,1,'&lt;pb/&gt;De aceea, fiindcă avem slujba aceasta&lt;f&gt;ⓣ&lt;/f&gt;, după îndurarea&lt;f&gt;ⓤ&lt;/f&gt; pe care am primit-o, noi nu cădem de oboseală,');
INSERT INTO &quot;verses&quot; VALUES (540,4,2,'ci am lepădat dedesubturile rușinoase, nu umblăm cu vicleșug și nu falsificăm&lt;f&gt;ⓥ&lt;/f&gt; Cuvântul lui Dumnezeu. Dimpotrivă&lt;f&gt;ⓦ&lt;/f&gt;, arătăm adevărul și ne facem vrednici &lt;i&gt;să fim primiți&lt;/i&gt; de orice cuget omenesc înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,4,3,'Iar dacă Evanghelia noastră este acoperită, ea este acoperită&lt;f&gt;ⓧ&lt;/f&gt; pentru cei ce sunt pe calea pierzării,');
INSERT INTO &quot;verses&quot; VALUES (540,4,4,'anume pentru necredincioși, a căror&lt;f&gt;ⓨ&lt;/f&gt; minte a orbit-o dumnezeul&lt;f&gt;ⓩ&lt;/f&gt; veacului acestuia ca să nu vadă strălucind lumina&lt;f&gt;ⓐ&lt;/f&gt; Evangheliei glorioase a lui Hristos, care&lt;f&gt;ⓑ&lt;/f&gt; este chipul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,4,5,'Căci&lt;f&gt;ⓒ&lt;/f&gt; noi nu ne propovăduim pe noi înșine, ci pe Hristos Isus, Domnul. Noi&lt;f&gt;ⓓ&lt;/f&gt; &lt;i&gt;suntem&lt;/i&gt; robii voștri pentru Isus.');
INSERT INTO &quot;verses&quot; VALUES (540,4,6,'Căci Dumnezeu, care a zis: „Să strălucească lumina din întuneric”&lt;f&gt;[8]&lt;/f&gt;, a strălucit în inimile noastre ca să fim luminați&lt;f&gt;ⓔ&lt;/f&gt; prin cunoașterea slavei dumnezeiești de pe chipul lui Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,4,7,'&lt;pb/&gt;Comoara aceasta o purtăm în niște vase&lt;f&gt;ⓕ&lt;/f&gt; de lut, pentru ca&lt;f&gt;ⓖ&lt;/f&gt; această putere extraordinară să fie de la Dumnezeu, și nu de la noi.');
INSERT INTO &quot;verses&quot; VALUES (540,4,8,'Suntem apăsați&lt;f&gt;ⓗ&lt;/f&gt;, dar nu strâmtorați; descumpăniți, dar nu deznădăjduiți;');
INSERT INTO &quot;verses&quot; VALUES (540,4,9,'prigoniți, dar nu părăsiți; doborâți&lt;f&gt;ⓘ&lt;/f&gt;, dar nu omorâți.');
INSERT INTO &quot;verses&quot; VALUES (540,4,10,'Purtăm întotdeauna&lt;f&gt;ⓙ&lt;/f&gt; în trupul nostru omorârea Domnului Isus, pentru ca&lt;f&gt;ⓚ&lt;/f&gt; și viața lui Isus să se arate în trupul nostru.');
INSERT INTO &quot;verses&quot; VALUES (540,4,11,'Căci noi, cei vii, suntem neîncetat&lt;f&gt;ⓛ&lt;/f&gt; dați la moarte din pricina lui Isus, pentru ca și viața lui Isus să se arate în trupul nostru muritor.');
INSERT INTO &quot;verses&quot; VALUES (540,4,12,'Astfel că în noi lucrează moartea&lt;f&gt;ⓜ&lt;/f&gt;, iar în voi, viața.');
INSERT INTO &quot;verses&quot; VALUES (540,4,13,'Însă fiindcă avem același&lt;f&gt;ⓝ&lt;/f&gt; duh de credință, potrivit cu ceea ce este scris — &lt;e&gt;am crezut, de aceea am vorbit&lt;/e&gt;&lt;f&gt;[9]&lt;/f&gt; — și noi credem, de aceea și vorbim.');
INSERT INTO &quot;verses&quot; VALUES (540,4,14,'Și știm că Cel&lt;f&gt;ⓞ&lt;/f&gt; ce L-a înviat pe Domnul Isus ne va învia și pe noi împreună cu Isus și ne va înfățișa împreună cu voi.');
INSERT INTO &quot;verses&quot; VALUES (540,4,15,'Căci toate&lt;f&gt;ⓟ&lt;/f&gt; se petrec în folosul vostru, pentru ca harul&lt;f&gt;ⓠ&lt;/f&gt; mare, &lt;i&gt;căpătat&lt;/i&gt; de &lt;i&gt;tot&lt;/i&gt; mai mulți, să sporească mulțumirile spre slava lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,4,16,'&lt;pb/&gt;De aceea, nu cădem de oboseală, ci, chiar dacă omul nostru din afară se trece, totuși &lt;i&gt;omul&lt;/i&gt; nostru dinăuntru&lt;f&gt;ⓡ&lt;/f&gt; se înnoiește din zi în zi.');
INSERT INTO &quot;verses&quot; VALUES (540,4,17,'Căci necazurile noastre ușoare&lt;f&gt;ⓢ&lt;/f&gt; de o clipă lucrează pentru noi, peste măsură și peste așteptări, o greutate veșnică de slavă.');
INSERT INTO &quot;verses&quot; VALUES (540,4,18,'Pentru că noi nu ne uităm&lt;f&gt;ⓣ&lt;/f&gt; la ce se vede, ci la ce nu se vede, căci ce se vede este trecător, pe când ce nu se vede este veșnic.');
INSERT INTO &quot;verses&quot; VALUES (540,5,1,'&lt;pb/&gt;Știm că, dacă se desface&lt;f&gt;ⓤ&lt;/f&gt; cortul care este locuința noastră pământească&lt;f&gt;[10]&lt;/f&gt;, avem o clădire de la Dumnezeu, o locuință veșnică, nefăcută de mână &lt;i&gt;omenească&lt;/i&gt;, în ceruri.');
INSERT INTO &quot;verses&quot; VALUES (540,5,2,'Și suspinăm&lt;f&gt;ⓥ&lt;/f&gt; în &lt;i&gt;cortul&lt;/i&gt; acesta, dornici să ne îmbrăcăm cu locașul nostru ceresc');
INSERT INTO &quot;verses&quot; VALUES (540,5,3,'(desigur, dacă nu cumva vom fi găsiți goi&lt;f&gt;ⓦ&lt;/f&gt; atunci când ne vom dezbrăca &lt;i&gt;de cel vechi&lt;/i&gt;&lt;f&gt;[11]&lt;/f&gt;).');
INSERT INTO &quot;verses&quot; VALUES (540,5,4,'Într-adevăr, câtă vreme suntem în cortul &lt;i&gt;pământesc&lt;/i&gt;, suspinăm sub povară, fiindcă nu dorim să ne dezbrăcăm &lt;i&gt;de trupul acesta&lt;/i&gt;, ci să ne îmbrăcăm&lt;f&gt;ⓧ&lt;/f&gt; &lt;i&gt;cu celălalt trup&lt;/i&gt; peste acesta, pentru ca ceea ce este muritor să fie înghițit de viață&lt;f&gt;[12]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,5,5,'Iar Cel&lt;f&gt;ⓨ&lt;/f&gt; ce ne-a pregătit chiar în vederea acestui &lt;i&gt;lucru&lt;/i&gt; este Dumnezeu, care ne-a&lt;f&gt;ⓩ&lt;/f&gt; dat Duhul ca arvună.');
INSERT INTO &quot;verses&quot; VALUES (540,5,6,'&lt;pb/&gt;Așadar, suntem întotdeauna plini de încredere și știm că, dacă suntem acasă în trup, suntem în pribegie, &lt;i&gt;departe&lt;/i&gt; de Domnul,');
INSERT INTO &quot;verses&quot; VALUES (540,5,7,'pentru că umblăm&lt;f&gt;ⓐ&lt;/f&gt; prin credință, nu prin vedere.');
INSERT INTO &quot;verses&quot; VALUES (540,5,8,'Suntem plini de încredere și am prefera&lt;f&gt;ⓑ&lt;/f&gt; să nu mai fim acasă în acest trup, ci să fim acasă la Domnul.');
INSERT INTO &quot;verses&quot; VALUES (540,5,9,'De aceea ne și străduim să-I fim plăcuți, fie că suntem acasă, fie că pribegim departe de casă.');
INSERT INTO &quot;verses&quot; VALUES (540,5,10,'Căci&lt;f&gt;ⓒ&lt;/f&gt; toți trebuie să ne înfățișăm înaintea scaunului de judecată al lui Hristos, pentru ca&lt;f&gt;ⓓ&lt;/f&gt; fiecare să-și primească &lt;i&gt;răsplata&lt;/i&gt; după binele sau răul pe care-l va fi făcut când trăia în trup.');
INSERT INTO &quot;verses&quot; VALUES (540,5,11,'&lt;pb/&gt;Fiindcă știm &lt;i&gt;ce este&lt;/i&gt; frica&lt;f&gt;ⓔ&lt;/f&gt; de Domnul, căutăm să-i înduplecăm pe oameni, dar Dumnezeu&lt;f&gt;ⓕ&lt;/f&gt; ne cunoaște bine și sper că și voi ne cunoașteți bine în cugetele voastre.');
INSERT INTO &quot;verses&quot; VALUES (540,5,12,'Nu&lt;f&gt;ⓖ&lt;/f&gt; &lt;i&gt;vrem să&lt;/i&gt; ne vorbim iarăși de bine înaintea voastră, ci să vă dăm un temei de laudă&lt;f&gt;ⓗ&lt;/f&gt; cu privire la noi, ca să aveți ce să răspundeți celor care se laudă cu înfățișarea, nu cu ce este în inimă.');
INSERT INTO &quot;verses&quot; VALUES (540,5,13,'Dacă&lt;f&gt;ⓘ&lt;/f&gt; ne-am ieșit din minți, pentru Dumnezeu ne-am ieșit; dacă suntem întregi la minte, pentru voi suntem.');
INSERT INTO &quot;verses&quot; VALUES (540,5,14,'Căci dragostea lui Hristos ne constrânge&lt;f&gt;[13]&lt;/f&gt;, fiindcă socotim că, &lt;i&gt;dacă&lt;/i&gt; Unul singur a murit&lt;f&gt;ⓙ&lt;/f&gt; pentru toți, atunci toți au murit.');
INSERT INTO &quot;verses&quot; VALUES (540,5,15,'El a murit pentru toți, pentru ca&lt;f&gt;ⓚ&lt;/f&gt; cei ce trăiesc să nu mai trăiască pentru ei înșiși, ci pentru Cel ce a murit și a înviat pentru ei.');
INSERT INTO &quot;verses&quot; VALUES (540,5,16,'&lt;pb/&gt;Așa că&lt;f&gt;ⓛ&lt;/f&gt;, de acum încolo, nu mai cunoaștem pe nimeni în felul lumii; chiar dacă L-am cunoscut pe Hristos în felul lumii, totuși&lt;f&gt;ⓜ&lt;/f&gt; acum nu-L mai cunoaștem &lt;i&gt;în felul acesta&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,5,17,'Căci, dacă este&lt;f&gt;ⓝ&lt;/f&gt; cineva în Hristos, este&lt;f&gt;ⓞ&lt;/f&gt; o creație nouă. Cele&lt;f&gt;ⓟ&lt;/f&gt; vechi s-au dus, iată că &lt;i&gt;toate&lt;/i&gt; s-au făcut noi.');
INSERT INTO &quot;verses&quot; VALUES (540,5,18,'Și toate &lt;i&gt;acestea&lt;/i&gt; sunt de la Dumnezeu, care&lt;f&gt;ⓠ&lt;/f&gt; ne-a împăcat cu El prin Hristos și ne-a încredințat slujba împăcării:');
INSERT INTO &quot;verses&quot; VALUES (540,5,19,'că, adică, Dumnezeu&lt;f&gt;ⓡ&lt;/f&gt; era în Hristos, împăcând lumea cu Sine, fără să țină seama de abaterile ei, și ne-a încredințat nouă propovăduirea acestei împăcări.');
INSERT INTO &quot;verses&quot; VALUES (540,5,20,'Noi, așadar, suntem&lt;f&gt;ⓢ&lt;/f&gt; ambasadori ai lui Hristos; și, ca și cum&lt;f&gt;ⓣ&lt;/f&gt; Dumnezeu ar îndemna prin noi, &lt;i&gt;vă&lt;/i&gt; rugăm fierbinte, în Numele lui Hristos: Împăcați-vă cu Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (540,5,21,'Pe&lt;f&gt;ⓤ&lt;/f&gt; Cel ce n-a cunoscut niciun păcat, El L-a făcut păcat pentru noi, pentru ca noi să fim dreptatea lui Dumnezeu în El&lt;f&gt;ⓥ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,6,1,'&lt;pb/&gt;Ca unii care lucrează împreună &lt;i&gt;cu Dumnezeu&lt;/i&gt;&lt;f&gt;ⓦ&lt;/f&gt;, vă&lt;f&gt;ⓧ&lt;/f&gt; sfătuim să&lt;f&gt;ⓨ&lt;/f&gt; &lt;i&gt;faceți în așa fel încât&lt;/i&gt; să nu fi primit în zadar harul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,6,2,'Căci El zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;La vremea potrivită, te-am ascultat;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;în ziua mântuirii, te-am ajutat.&lt;/e&gt; &lt;f&gt;[14]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;Iată că acum este vremea potrivită; iată că acum este ziua mântuirii.');
INSERT INTO &quot;verses&quot; VALUES (540,6,3,'Noi&lt;f&gt;ⓩ&lt;/f&gt; nu dăm nimănui niciun prilej de poticnire, pentru ca slujba &lt;i&gt;noastră&lt;/i&gt; să nu fie criticată.');
INSERT INTO &quot;verses&quot; VALUES (540,6,4,'Ci, în toate, arătăm că suntem slujitori&lt;f&gt;ⓐ&lt;/f&gt; ai lui Dumnezeu, prin multă răbdare în necazuri, în nevoi, în strâmtorări,');
INSERT INTO &quot;verses&quot; VALUES (540,6,5,'în&lt;f&gt;ⓑ&lt;/f&gt; bătăi, în temnițe, în mijlocul tulburărilor &lt;i&gt;din cetăți&lt;/i&gt;, în osteneli, în nopți de nesomn, în posturi,');
INSERT INTO &quot;verses&quot; VALUES (540,6,6,'în curăție, în cunoaștere, cu îndelungă răbdare, cu bunătate, prin Duhul Sfânt, cu o dragoste neprefăcută,');
INSERT INTO &quot;verses&quot; VALUES (540,6,7,'prin&lt;f&gt;ⓒ&lt;/f&gt; cuvântul adevărului, prin puterea&lt;f&gt;ⓓ&lt;/f&gt; lui Dumnezeu, cu armele&lt;f&gt;ⓔ&lt;/f&gt; dreptății, cele de atac și de apărare,');
INSERT INTO &quot;verses&quot; VALUES (540,6,8,'în slavă și în ocară, defăimați sau vorbiți de bine. &lt;i&gt;Suntem priviți&lt;/i&gt; ca niște înșelători, cu toate&lt;f&gt;ⓕ&lt;/f&gt; că spunem adevărul;');
INSERT INTO &quot;verses&quot; VALUES (540,6,9,'ca niște necunoscuți, cu toate că suntem bine cunoscuți; ca&lt;f&gt;ⓖ&lt;/f&gt; unii care mor, și iată că trăim; ca&lt;f&gt;ⓗ&lt;/f&gt; niște pedepsiți, dar nu omorâți;');
INSERT INTO &quot;verses&quot; VALUES (540,6,10,'ca niște întristați, dar totdeauna veseli; ca niște săraci, deși îmbogățim pe mulți; ca neavând nimic, dar stăpânind toate lucrurile.');
INSERT INTO &quot;verses&quot; VALUES (540,6,11,'&lt;pb/&gt;Am vorbit deschis cu voi, corintenilor! Ni s-a lărgit inima&lt;f&gt;ⓘ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,6,12,'Nu simțămintele noastre față de voi au slăbit&lt;f&gt;ⓙ&lt;/f&gt;, ci ale voastre față de noi.');
INSERT INTO &quot;verses&quot; VALUES (540,6,13,'&lt;i&gt;Faceți și voi&lt;/i&gt; la fel ca răsplată — ca unor copii ai mei vă vorbesc&lt;f&gt;ⓚ&lt;/f&gt; — lărgiți-vă și voi!');
INSERT INTO &quot;verses&quot; VALUES (540,6,14,'&lt;pb/&gt;Nu&lt;f&gt;ⓛ&lt;/f&gt; vă înjugați la un jug nepotrivit cu cei necredincioși! Căci ce&lt;f&gt;ⓜ&lt;/f&gt; asociere poate fi între dreptate și fărădelege? Sau ce au în comun lumina și întunericul?');
INSERT INTO &quot;verses&quot; VALUES (540,6,15,'Ce înțelegere poate fi între Hristos și Beliar&lt;f&gt;[15]&lt;/f&gt;? Sau ce legătură are cel credincios cu cel necredincios?');
INSERT INTO &quot;verses&quot; VALUES (540,6,16,'Cum se împacă templul lui Dumnezeu cu idolii? Căci noi&lt;f&gt;ⓝ&lt;/f&gt; suntem templul Dumnezeului celui viu, după cum a zis Dumnezeu: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Eu voi&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;locui și voi umbla în mijlocul lor;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Eu voi fi Dumnezeul lor, și ei vor fi poporul Meu&lt;/e&gt;.&lt;f&gt;[16]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,6,17,'&lt;t&gt;De aceea&lt;f&gt;ⓟ&lt;/f&gt;: &lt;e&gt;Ieșiți din mijlocul lor&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și despărțiți-vă de ei, zice Domnul.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Nu vă atingeți de ce este necurat,&lt;/e&gt; &lt;f&gt;[17]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și vă voi primi!&lt;/e&gt; &lt;f&gt;[18]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,6,18,'&lt;t&gt;&lt;e&gt;Eu vă&lt;/e&gt; &lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;voi fi Tată,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și voi Îmi veți fi fii și fiice,&lt;/e&gt; &lt;f&gt;[19]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;zice Domnul cel Atotputernic.&lt;/e&gt; &lt;f&gt;[20]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,7,1,'&lt;pb/&gt;Deci&lt;f&gt;ⓡ&lt;/f&gt;, fiindcă avem astfel de făgăduințe, preaiubiților, să ne curățim de orice întinare a cărnii și a duhului și să ne ducem sfințirea până la capăt, cu frică de Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (540,7,2,'&lt;pb/&gt;Faceți-ne loc &lt;i&gt;în inima voastră&lt;/i&gt;! N-am nedreptățit pe&lt;f&gt;ⓢ&lt;/f&gt; nimeni, n-am păgubit pe nimeni, n-am înșelat pe nimeni.');
INSERT INTO &quot;verses&quot; VALUES (540,7,3,'Nu spun aceste lucruri ca să vă osândesc, căci&lt;f&gt;ⓣ&lt;/f&gt; am spus deja că sunteți în inimile noastre, ca să murim împreună și să trăim împreună.');
INSERT INTO &quot;verses&quot; VALUES (540,7,4,'Am o mare&lt;f&gt;ⓤ&lt;/f&gt; încredere în voi. Mă laud cu voi în multe privințe. Sunt plin&lt;f&gt;ⓥ&lt;/f&gt; de mângâiere&lt;f&gt;ⓦ&lt;/f&gt;, îmi saltă inima de bucurie în toate necazurile noastre.');
INSERT INTO &quot;verses&quot; VALUES (540,7,5,'&lt;pb/&gt;Căci și după venirea&lt;f&gt;ⓧ&lt;/f&gt; noastră în Macedonia, trupul nostru n-a avut nicio odihnă. Am&lt;f&gt;ⓨ&lt;/f&gt; avut tot felul de necazuri: din&lt;f&gt;ⓩ&lt;/f&gt; afară lupte, dinăuntru temeri.');
INSERT INTO &quot;verses&quot; VALUES (540,7,6,'Dar Dumnezeu&lt;f&gt;ⓐ&lt;/f&gt;, care îi mângâie pe cei smeriți, ne-a mângâiat prin venirea lui Tit.');
INSERT INTO &quot;verses&quot; VALUES (540,7,7,'Și nu numai prin venirea&lt;f&gt;ⓑ&lt;/f&gt; lui, ci și prin mângâierea cu care a fost el însuși mângâiat la voi. El ne-a adus vești despre dorința voastră arzătoare, despre mâhnirea voastră și despre râvna voastră pentru mine, așa că bucuria mea a fost și mai mare.');
INSERT INTO &quot;verses&quot; VALUES (540,7,8,'Cu toate&lt;f&gt;ⓒ&lt;/f&gt; că v-am întristat prin epistola mea, nu-mi pare rău, dar și dacă mi-ar fi părut rău — căci văd că epistola aceea v-a întristat (fie și pentru scurtă vreme) —');
INSERT INTO &quot;verses&quot; VALUES (540,7,9,'acum mă bucur, nu pentru că v-ați întristat, ci pentru că v-ați întristat spre pocăință. Căci v-ați întristat&lt;f&gt;ⓓ&lt;/f&gt; după &lt;i&gt;voia lui&lt;/i&gt; Dumnezeu, ca să nu aveți nicio pagubă din partea noastră.');
INSERT INTO &quot;verses&quot; VALUES (540,7,10,'Într-adevăr, când întristarea este după &lt;i&gt;voia lui&lt;/i&gt; Dumnezeu, aduce o pocăință &lt;i&gt;care duce&lt;/i&gt; la mântuire și de care nu-ți pare rău, pe când&lt;f&gt;ⓔ&lt;/f&gt; întristarea lumii aduce moarte.');
INSERT INTO &quot;verses&quot; VALUES (540,7,11,'Căci uite, tocmai întristarea aceasta &lt;i&gt;a voastră&lt;/i&gt; după &lt;i&gt;voia lui&lt;/i&gt; Dumnezeu ce însuflețire a trezit în voi! Și ce cuvinte de dezvinovățire! Ce indignare! Ce frică! Ce dorință arzătoare! Ce râvnă! Ce pedeapsă! În toate ați dovedit că sunteți nevinovați în privința aceasta.');
INSERT INTO &quot;verses&quot; VALUES (540,7,12,'Așa că, dacă v-am scris, nu v-am scris nici din pricina celui ce a făcut ocara&lt;f&gt;[21]&lt;/f&gt;, nici din pricina celui ce a suferit ocara, ci&lt;f&gt;ⓕ&lt;/f&gt; ca să se arate marea voastră însuflețire pentru noi înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,7,13,'De aceea am fost mângâiați. &lt;pb/&gt;Dar, pe lângă mângâierea &lt;i&gt;aceasta&lt;/i&gt; a noastră, ne-am bucurat și mai mult de bucuria lui Tit, al cărui duh a&lt;f&gt;ⓖ&lt;/f&gt; fost răcorit de voi toți.');
INSERT INTO &quot;verses&quot; VALUES (540,7,14,'Și dacă m-am lăudat cu voi înaintea lui în vreo privință, n-am fost dat de rușine. Ci, după cum în orice lucru v-am spus adevărul, tot așa și lauda noastră cu voi înaintea lui Tit a fost adevărată.');
INSERT INTO &quot;verses&quot; VALUES (540,7,15,'El are o și mai mare dragoste pentru voi când își aduce aminte de ascultarea&lt;f&gt;ⓗ&lt;/f&gt; voastră, a tuturor, și de primirea pe care i-ați făcut-o cu frică și cutremur.');
INSERT INTO &quot;verses&quot; VALUES (540,7,16,'Mă bucur că mă pot încrede în voi în&lt;f&gt;ⓘ&lt;/f&gt; toate privințele.');
INSERT INTO &quot;verses&quot; VALUES (540,8,1,'&lt;pb/&gt;Fraților, vă facem cunoscut harul pe care l-a dat Dumnezeu în bisericile Macedoniei.');
INSERT INTO &quot;verses&quot; VALUES (540,8,2,'În mijlocul multor încercări și necazuri, bucuria lor nespus de mare și sărăcia lor lucie&lt;f&gt;ⓙ&lt;/f&gt; au dat naștere la un belșug de dărnicie din partea lor.');
INSERT INTO &quot;verses&quot; VALUES (540,8,3,'Vă mărturisesc că au dăruit de bunăvoie, după puterea lor și chiar peste puterile lor.');
INSERT INTO &quot;verses&quot; VALUES (540,8,4,'Și ne-au cerut cu mari stăruințe favoarea de a lua parte la strângerea&lt;f&gt;ⓚ&lt;/f&gt; de ajutoare&lt;f&gt;[22]&lt;/f&gt; pentru sfinți.');
INSERT INTO &quot;verses&quot; VALUES (540,8,5,'Și, peste așteptările noastre, s-au dăruit pe ei înșiși mai întâi Domnului și &lt;i&gt;apoi&lt;/i&gt; nouă, prin voia lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,8,6,'Astfel&lt;f&gt;ⓛ&lt;/f&gt;, l-am rugat pe Tit ca, după cum a început această binefacere&lt;f&gt;[23]&lt;/f&gt;, tot astfel să o ducă la bun sfârșit.');
INSERT INTO &quot;verses&quot; VALUES (540,8,7,'După cum sporiți în&lt;f&gt;ⓜ&lt;/f&gt; toate lucrurile — în credință, în cuvânt, în cunoștință, în orice râvnă și în dragostea voastră pentru noi — căutați să&lt;f&gt;ⓝ&lt;/f&gt; sporiți și în această binefacere!');
INSERT INTO &quot;verses&quot; VALUES (540,8,8,'&lt;pb/&gt;Nu&lt;f&gt;ⓞ&lt;/f&gt; spun lucrul acesta ca pe o poruncă, ci ca să pun la încercare, prin râvna altora, temeinicia dragostei voastre.');
INSERT INTO &quot;verses&quot; VALUES (540,8,9,'Căci cunoașteți harul&lt;f&gt;[24]&lt;/f&gt; Domnului nostru Isus Hristos. El&lt;f&gt;ⓟ&lt;/f&gt;, măcar că era bogat, S-a făcut sărac pentru voi, pentru ca, prin sărăcia Lui, voi să vă îmbogățiți.');
INSERT INTO &quot;verses&quot; VALUES (540,8,10,'În această privință am&lt;f&gt;ⓠ&lt;/f&gt; un sfat&lt;f&gt;ⓡ&lt;/f&gt; folositor pentru voi, cei care, încă de acum un an, ați început nu numai să faceți, ci să și&lt;f&gt;ⓢ&lt;/f&gt; vreți aceasta:');
INSERT INTO &quot;verses&quot; VALUES (540,8,11,'duceți la bun sfârșit ce &lt;i&gt;mai rămâne&lt;/i&gt; de făcut, pentru ca pe măsura bunăvoinței să fie și înfăptuirea, potrivit cu ceea ce aveți!');
INSERT INTO &quot;verses&quot; VALUES (540,8,12,'Pentru că&lt;f&gt;ⓣ&lt;/f&gt;, dacă există bunăvoință, &lt;i&gt;darul&lt;/i&gt; este primit &lt;i&gt;avându-se în vedere&lt;/i&gt; ce are cineva, nu ce n-are.');
INSERT INTO &quot;verses&quot; VALUES (540,8,13,'Nu că alții &lt;i&gt;ar trebui&lt;/i&gt; să fie ușurați, iar voi strâmtorați, ci &lt;i&gt;este vorba de&lt;/i&gt; un echilibru:');
INSERT INTO &quot;verses&quot; VALUES (540,8,14,'în împrejurarea de acum, prisosul vostru &lt;i&gt;să acopere&lt;/i&gt; lipsurile lor, pentru ca și prisosul lor să acopere lipsurile voastre, și &lt;i&gt;astfel&lt;/i&gt; să fie un echilibru,');
INSERT INTO &quot;verses&quot; VALUES (540,8,15,'după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Cel ce &lt;i&gt;strânsese&lt;/i&gt; mult n-avea &lt;i&gt;nimic&lt;/i&gt; de prisos,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și cel ce &lt;i&gt;strânsese&lt;/i&gt; puțin nu ducea lipsă.&lt;/e&gt; &lt;f&gt;[25]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,8,16,'&lt;pb/&gt;Mulțumiri fie aduse lui Dumnezeu, care a pus în inima lui Tit aceeași râvnă pentru voi!');
INSERT INTO &quot;verses&quot; VALUES (540,8,17,'Căci el nu doar a primit îndemnul&lt;f&gt;ⓤ&lt;/f&gt; &lt;i&gt;nostru&lt;/i&gt;, ci, cuprins de o mare însuflețire, a și pornit de bunăvoie spre voi.');
INSERT INTO &quot;verses&quot; VALUES (540,8,18,'L-am trimis cu el și pe fratele&lt;f&gt;ⓥ&lt;/f&gt; a cărui laudă în Evanghelie &lt;i&gt;este răspândită&lt;/i&gt; în toate bisericile.');
INSERT INTO &quot;verses&quot; VALUES (540,8,19,'Mai mult, el a fost ales&lt;f&gt;ⓦ&lt;/f&gt; de biserici să ne fie însoțitor în această &lt;i&gt;lucrare de&lt;/i&gt; binefacere, pe care o săvârșim spre&lt;f&gt;ⓧ&lt;/f&gt; slava Domnului și ca o dovadă a bunăvoinței noastre.');
INSERT INTO &quot;verses&quot; VALUES (540,8,20,'&lt;i&gt;Vrem să&lt;/i&gt; ne ferim în felul acesta, ca să nu fim criticați cu privire la acest belșug de ajutoare de care îngrijim.');
INSERT INTO &quot;verses&quot; VALUES (540,8,21,'Căci suntem&lt;f&gt;ⓨ&lt;/f&gt; preocupați de ceea &lt;e&gt;ce este bine nu numai înaintea Domnului, ci și înaintea oamenilor&lt;/e&gt;&lt;f&gt;[26]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,8,22,'L-am trimis cu ei pe fratele nostru a cărui râvnă am pus-o adesea la încercare, în multe împrejurări, și care, de data aceasta, arată mult mai multă râvnă din pricina marii lui încrederi în voi.');
INSERT INTO &quot;verses&quot; VALUES (540,8,23,'Astfel, fie că e vorba de Tit, părtașul și tovarășul meu de lucru în mijlocul vostru, fie că e vorba de frații noștri, care sunt trimișii&lt;f&gt;[27]&lt;/f&gt; bisericilor&lt;f&gt;ⓩ&lt;/f&gt; și slava lui Hristos,');
INSERT INTO &quot;verses&quot; VALUES (540,8,24,'dovediți-vă dragostea față de ei în fața bisericilor și &lt;i&gt;arătați-le&lt;/i&gt; că avem motive să ne lăudăm&lt;f&gt;ⓐ&lt;/f&gt; cu voi!');
INSERT INTO &quot;verses&quot; VALUES (540,9,1,'&lt;pb/&gt;Cu privire la strângerea&lt;f&gt;ⓑ&lt;/f&gt; de ajutoare pentru sfinți, este de prisos să vă mai scriu.');
INSERT INTO &quot;verses&quot; VALUES (540,9,2,'Cunosc, într-adevăr, bunăvoința&lt;f&gt;ⓒ&lt;/f&gt; voastră, motiv pentru care mă laud cu&lt;f&gt;ⓓ&lt;/f&gt; voi la macedoneni și le spun că Ahaia&lt;f&gt;ⓔ&lt;/f&gt; este pregătită de acum un an. Și râvna voastră i-a îmbărbătat pe cei mai mulți dintre ei.');
INSERT INTO &quot;verses&quot; VALUES (540,9,3,'I-am&lt;f&gt;ⓕ&lt;/f&gt; trimis &lt;i&gt;totuși&lt;/i&gt; pe frați, pentru ca &lt;i&gt;vorbele&lt;/i&gt; noastre de laudă cu privire la voi să nu fie zadarnice în această privință, ci să fiți gata, cum am spus;');
INSERT INTO &quot;verses&quot; VALUES (540,9,4,'ca nu cumva, dacă vin cu mine macedonenii și vă găsesc nepregătiți, să pățim rușine noi — ca să nu zic voi — în această privință.');
INSERT INTO &quot;verses&quot; VALUES (540,9,5,'De aceea, am socotit necesar să-i rog pe frați să vină mai înainte la voi și să pregătească &lt;i&gt;strângerea&lt;/i&gt; binecuvântării&lt;f&gt;[28]&lt;/f&gt; promise de voi, ca ea să fie gata, &lt;i&gt;înțeleasă&lt;/i&gt; ca binecuvântare, nu ca ceva stors cu sila.');
INSERT INTO &quot;verses&quot; VALUES (540,9,6,'&lt;pb/&gt;Să&lt;f&gt;ⓖ&lt;/f&gt; știți: cine seamănă puțin&lt;f&gt;[29]&lt;/f&gt; puțin va secera, iar cine seamănă mult&lt;f&gt;[30]&lt;/f&gt; mult va secera.');
INSERT INTO &quot;verses&quot; VALUES (540,9,7,'Fiecare să dea după cum a hotărât în inimă, nu&lt;f&gt;ⓗ&lt;/f&gt; cu părere de rău sau din obligație, căci &lt;e&gt;pe cine dă&lt;/e&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;e&gt;cu bucurie îl iubește Dumnezeu&lt;/e&gt;&lt;f&gt;[31]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,9,8,'Și Dumnezeu poate&lt;f&gt;ⓙ&lt;/f&gt; să vă dea din belșug orice har, pentru ca, având totdeauna destul din toate lucrurile, să faceți din belșug fapte bune,');
INSERT INTO &quot;verses&quot; VALUES (540,9,9,'după cum este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;A împărțit, a dat săracilor,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;dreptatea lui rămâne în veac.&lt;/e&gt; &lt;f&gt;[32]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,9,10,'&lt;pb/&gt;Cel ce &lt;e&gt;dă sămânță semănătorului și pâine pentru hrană&lt;/e&gt;&lt;f&gt;[33]&lt;/f&gt; vă va da și vă va înmulți și vouă sămânța &lt;i&gt;de semănat&lt;/i&gt; și va face să crească roadele dreptății&lt;f&gt;ⓚ&lt;/f&gt; voastre.');
INSERT INTO &quot;verses&quot; VALUES (540,9,11,'În felul acesta veți fi îmbogățiți în toate privințele, pentru orice dărnicie, care&lt;f&gt;ⓛ&lt;/f&gt;, prin noi, va face să se aducă mulțumiri lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,9,12,'Căci îndeplinirea acestei slujbe nu numai că acoperă&lt;f&gt;ⓜ&lt;/f&gt; nevoile sfinților, ci duce și la un belșug de mulțumiri către Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (540,9,13,'Dovada &lt;i&gt;dată de voi&lt;/i&gt; sub forma acestui ajutor îi face să-L slăvească&lt;f&gt;ⓝ&lt;/f&gt; pe Dumnezeu pentru supunerea voastră &lt;i&gt;care însoțește&lt;/i&gt; mărturisirea Evangheliei lui Hristos și pentru dărnicia ajutorului vostru&lt;f&gt;ⓞ&lt;/f&gt; față de ei și față de toți.&lt;f&gt;[34]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,9,14,'În tot acest timp, ei se gândesc cu dor la voi, în rugăciune, pentru harul&lt;f&gt;ⓟ&lt;/f&gt; nespus de mare al lui Dumnezeu față de voi.');
INSERT INTO &quot;verses&quot; VALUES (540,9,15,'Mulțumiri fie aduse lui Dumnezeu pentru&lt;f&gt;ⓠ&lt;/f&gt; darul Lui nemaipomenit!');
INSERT INTO &quot;verses&quot; VALUES (540,10,1,'&lt;pb/&gt;Eu însumi&lt;f&gt;ⓡ&lt;/f&gt;, Pavel, vă rog, pentru blândețea și bunătatea lui Hristos — eu, cel&lt;f&gt;ⓢ&lt;/f&gt; smerit când sunt de față în mijlocul vostru, dar plin de îndrăzneală împotriva voastră când sunt departe —');
INSERT INTO &quot;verses&quot; VALUES (540,10,2,'vă rog să nu mă faceți ca, atunci când voi fi de față, să&lt;f&gt;ⓣ&lt;/f&gt; apelez la îndrăzneala aceea pe care am de gând s-o întrebuințez împotriva unora care consideră că noi umblăm potrivit firii pământești.');
INSERT INTO &quot;verses&quot; VALUES (540,10,3,'Deși trăim în firea pământească, nu ne luptăm potrivit firii pământești,');
INSERT INTO &quot;verses&quot; VALUES (540,10,4,'căci&lt;f&gt;ⓤ&lt;/f&gt; armele cu care luptăm&lt;f&gt;ⓥ&lt;/f&gt; noi nu sunt pământești, ci&lt;f&gt;ⓦ&lt;/f&gt; sunt puternice, &lt;i&gt;întărite&lt;/i&gt; de Dumnezeu ca să&lt;f&gt;ⓧ&lt;/f&gt; nimicească întăriturile. Noi nimicim&lt;f&gt;ⓨ&lt;/f&gt; izvodirile &lt;i&gt;minții&lt;/i&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,10,5,'și orice înălțime care se ridică împotriva cunoașterii lui Dumnezeu, și orice gând îl facem rob ascultării de Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,10,6,'Mai mult, suntem gata să pedepsim orice neascultare, de îndată&lt;f&gt;ⓩ&lt;/f&gt; ce ascultarea voastră va ajunge&lt;f&gt;ⓐ&lt;/f&gt; deplină.');
INSERT INTO &quot;verses&quot; VALUES (540,10,7,'&lt;pb/&gt;Voi vă uitați la&lt;f&gt;ⓑ&lt;/f&gt; înfățișare.&lt;f&gt;[35]&lt;/f&gt; Dacă&lt;f&gt;ⓒ&lt;/f&gt; cineva crede că „este al lui Hristos”, să aibă în vedere că, după cum el este al lui Hristos, tot așa suntem și noi&lt;f&gt;ⓓ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,10,8,'Și chiar dacă m-aș lăuda ceva mai mult cu&lt;f&gt;ⓔ&lt;/f&gt; autoritatea pe care ne-a dat-o Domnul pentru zidirea voastră, nu pentru nimicirea voastră, tot nu&lt;f&gt;ⓕ&lt;/f&gt; mi-ar fi rușine.');
INSERT INTO &quot;verses&quot; VALUES (540,10,9,'Nu vreau să par că &lt;i&gt;urmăresc&lt;/i&gt; să vă înfricoșez prin epistolele mele.');
INSERT INTO &quot;verses&quot; VALUES (540,10,10,'„De fapt”, zic ei, „epistolele lui sunt cu greutate și pline de putere, dar&lt;f&gt;ⓖ&lt;/f&gt; prezența lui trupească este moale, iar cuvântul&lt;f&gt;ⓗ&lt;/f&gt; lui este &lt;i&gt;vrednic&lt;/i&gt; de dispreț.”');
INSERT INTO &quot;verses&quot; VALUES (540,10,11,'Cine &lt;i&gt;gândește&lt;/i&gt; așa să aibă în vedere că, așa cum suntem în cuvânt, prin epistolele noastre, când nu suntem de față, tot așa vom fi și în faptă, când vom fi de față!');
INSERT INTO &quot;verses&quot; VALUES (540,10,12,'&lt;pb/&gt;Negreșit&lt;f&gt;ⓘ&lt;/f&gt;, nu avem îndrăzneala să ne comparăm ori să ne punem în rând cu unii dintre cei care se vorbesc singuri de bine. Dar ei, fiindcă se măsoară cu ei înșiși și tot cu ei înșiși se compară, sunt fără pricepere.');
INSERT INTO &quot;verses&quot; VALUES (540,10,13,'Noi&lt;f&gt;ⓙ&lt;/f&gt; însă nu ne lăudăm fără măsură, ci după măsura câmpului de lucru pe care ni l-a însemnat Dumnezeu, încât &lt;i&gt;am putut&lt;/i&gt; să ajungem până la voi.');
INSERT INTO &quot;verses&quot; VALUES (540,10,14,'Nu ne întindem prea mult, ca și când n-am fi ajuns până la voi, căci&lt;f&gt;ⓚ&lt;/f&gt; până la voi am ajuns cu Evanghelia lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,10,15,'N-am întrecut măsura, încât să ne lăudăm cu&lt;f&gt;ⓛ&lt;/f&gt; ostenelile altora, ci avem nădejdea că, pe măsură ce credința voastră crește, va spori nespus de mult între voi &lt;i&gt;și lucrarea noastră&lt;/i&gt;, potrivit câmpului nostru de lucru.');
INSERT INTO &quot;verses&quot; VALUES (540,10,16,'Așa că vom &lt;i&gt;putea&lt;/i&gt; propovădui Evanghelia și în ținuturile care sunt dincolo de voi, ca să nu ne lăudăm cu &lt;i&gt;lucrări&lt;/i&gt; gata făcute, din câmpul altuia.');
INSERT INTO &quot;verses&quot; VALUES (540,10,17,'&lt;e&gt;Cine&lt;/e&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;e&gt;se laudă să se laude în Domnul.&lt;/e&gt;&lt;f&gt;[36]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,10,18,'Pentru că nu&lt;f&gt;ⓝ&lt;/f&gt; cine se vorbește singur de bine va fi primit, ci acela pe&lt;f&gt;ⓞ&lt;/f&gt; care Domnul îl vorbește de bine.');
INSERT INTO &quot;verses&quot; VALUES (540,11,1,'&lt;pb/&gt;O, de ați putea suferi puțintică nebunie&lt;f&gt;ⓟ&lt;/f&gt; din partea mea! Așadar, răbdați-mă!');
INSERT INTO &quot;verses&quot; VALUES (540,11,2,'Căci gelozia&lt;f&gt;ⓠ&lt;/f&gt; mea pentru voi este ca gelozia lui Dumnezeu, pentru că v-am&lt;f&gt;ⓡ&lt;/f&gt; logodit cu un bărbat, ca să vă înfățișez înaintea&lt;f&gt;ⓢ&lt;/f&gt; lui Hristos ca pe o fecioară curată&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (540,11,3,'Dar mă tem că, după cum șarpele&lt;f&gt;ⓤ&lt;/f&gt; a amăgit-o pe Eva cu șiretlicul lui, tot așa și gândurile voastre s-ar &lt;i&gt;putea&lt;/i&gt; abate&lt;f&gt;ⓥ&lt;/f&gt; de la sinceritatea și curăția pentru Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,11,4,'Căci, dacă vine cineva și vă propovăduiește un alt Isus, pe care noi nu L-am propovăduit, sau dacă &lt;i&gt;este vorba să&lt;/i&gt; primiți un alt duh decât cel primit sau o altă evanghelie&lt;f&gt;ⓦ&lt;/f&gt; decât cea primită&lt;f&gt;[37]&lt;/f&gt;, multă răbdare mai aveți cu el!');
INSERT INTO &quot;verses&quot; VALUES (540,11,5,'&lt;pb/&gt;Dar consider că nici eu nu sunt cu&lt;f&gt;ⓧ&lt;/f&gt; nimic mai prejos de &lt;i&gt;acești&lt;/i&gt; minunați apostoli!');
INSERT INTO &quot;verses&quot; VALUES (540,11,6,'Chiar dacă sunt&lt;f&gt;ⓨ&lt;/f&gt; nepriceput în vorbire, nu sunt și în&lt;f&gt;ⓩ&lt;/f&gt; cunoaștere, și am arătat lucrul acesta printre voi în tot felul și în toate&lt;f&gt;ⓐ&lt;/f&gt; privințele.');
INSERT INTO &quot;verses&quot; VALUES (540,11,7,'Sau am făcut un păcat când m-am smerit pe mine însumi&lt;f&gt;ⓑ&lt;/f&gt; ca să fiți înălțați voi, fiindcă v-am vestit fără plată Evanghelia lui Dumnezeu?');
INSERT INTO &quot;verses&quot; VALUES (540,11,8,'Am jefuit alte biserici, primind de la ele o plată, ca să vă pot sluji vouă.');
INSERT INTO &quot;verses&quot; VALUES (540,11,9,'Și când eram la voi și m-am găsit în lipsuri, n-am&lt;f&gt;ⓒ&lt;/f&gt; fost povară nimănui, căci de lipsurile mele s-au îngrijit frații&lt;f&gt;ⓓ&lt;/f&gt; care veniseră din Macedonia. În toate m-am ferit și mă voi feri să&lt;f&gt;ⓔ&lt;/f&gt; vă fiu în vreun fel povară.');
INSERT INTO &quot;verses&quot; VALUES (540,11,10,'Pe adevărul lui Hristos care este în&lt;f&gt;ⓕ&lt;/f&gt; mine, această&lt;f&gt;ⓖ&lt;/f&gt; pricină de laudă pe care o am nu va fi trecută sub tăcere în ținuturile Ahaiei!');
INSERT INTO &quot;verses&quot; VALUES (540,11,11,'De ce&lt;f&gt;ⓗ&lt;/f&gt;? Pentru că nu v-aș iubi? Dumnezeu știe!');
INSERT INTO &quot;verses&quot; VALUES (540,11,12,'&lt;pb/&gt;Dar ceea ce am făcut voi face &lt;i&gt;și mai departe&lt;/i&gt;, ca&lt;f&gt;ⓘ&lt;/f&gt; să tai orice prilej celor ce caută un prilej de a fi considerați deopotrivă cu noi în lucrurile cu care se laudă.');
INSERT INTO &quot;verses&quot; VALUES (540,11,13,'Astfel &lt;i&gt;de oameni&lt;/i&gt; sunt&lt;f&gt;ⓙ&lt;/f&gt; apostoli mincinoși, lucrători&lt;f&gt;ⓚ&lt;/f&gt; înșelători, care se dau drept apostoli ai lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (540,11,14,'Și nu este de mirare, căci Satana însuși se dă drept înger&lt;f&gt;ⓛ&lt;/f&gt; de lumină.');
INSERT INTO &quot;verses&quot; VALUES (540,11,15,'Nu este mare lucru dacă și slujitorii lui se dau drept slujitori&lt;f&gt;ⓜ&lt;/f&gt; ai dreptății. Sfârșitul lor&lt;f&gt;ⓝ&lt;/f&gt; va fi după faptele lor.');
INSERT INTO &quot;verses&quot; VALUES (540,11,16,'&lt;pb/&gt;Iarăși&lt;f&gt;ⓞ&lt;/f&gt; spun: să nu mă creadă nimeni nebun! Dacă nu, suferiți-mă cel puțin ca nebun, ca să mă laud și eu un pic!');
INSERT INTO &quot;verses&quot; VALUES (540,11,17,'Ce spun în&lt;f&gt;ⓟ&lt;/f&gt; această încercare de a mă lăuda, nu&lt;f&gt;ⓠ&lt;/f&gt; &lt;i&gt;spun&lt;/i&gt; după Domnul, ci ca &lt;i&gt;și cum aș vorbi&lt;/i&gt; din nebunie.');
INSERT INTO &quot;verses&quot; VALUES (540,11,18,'De vreme ce&lt;f&gt;ⓡ&lt;/f&gt; mulți se laudă cu &lt;i&gt;lucruri care țin de&lt;/i&gt; firea pământească, mă voi lăuda și eu.');
INSERT INTO &quot;verses&quot; VALUES (540,11,19,'De altfel, voi îi răbdați cu plăcere pe nebuni, voi&lt;f&gt;ⓢ&lt;/f&gt;, care sunteți înțelepți.');
INSERT INTO &quot;verses&quot; VALUES (540,11,20,'Dacă&lt;f&gt;ⓣ&lt;/f&gt; vă înrobește cineva, dacă vă mănâncă cineva &lt;i&gt;de vii&lt;/i&gt;, dacă vă exploatează cineva, dacă vă privește cineva de sus, dacă vă bate cineva peste obraz, iată că răbdați.');
INSERT INTO &quot;verses&quot; VALUES (540,11,21,'Spre rușinea mea o spun: &lt;i&gt;în această privință&lt;/i&gt; am&lt;f&gt;ⓤ&lt;/f&gt; fost slabi! &lt;pb/&gt;Totuși, oricare&lt;f&gt;ⓥ&lt;/f&gt; ar fi lucrurile despre care îndrăznește cineva &lt;i&gt;să se laude&lt;/i&gt; — vorbesc din nebunie — îndrăznesc și eu.');
INSERT INTO &quot;verses&quot; VALUES (540,11,22,'Sunt ei evrei? Și eu sunt! Sunt ei israeliți? Și eu sunt! Sunt ei sămânța lui Avraam? Și&lt;f&gt;ⓦ&lt;/f&gt; eu sunt!');
INSERT INTO &quot;verses&quot; VALUES (540,11,23,'Sunt ei slujitori ai lui Hristos? Vorbesc ca un ieșit din minți: eu și mai mult. În osteneli&lt;f&gt;ⓧ&lt;/f&gt;, și mai mult; în temnițe, și mai mult; în bătăi&lt;f&gt;ⓨ&lt;/f&gt;, fără număr; de multe ori în primejdii de moarte&lt;f&gt;ⓩ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (540,11,24,'De cinci ori am primit de la iudei patruzeci&lt;f&gt;ⓐ&lt;/f&gt; de lovituri fără una;');
INSERT INTO &quot;verses&quot; VALUES (540,11,25,'de trei ori am fost bătut&lt;f&gt;ⓑ&lt;/f&gt; cu nuiele; o dată am fost împroșcat&lt;f&gt;ⓒ&lt;/f&gt; cu pietre; de trei ori am suferit&lt;f&gt;ⓓ&lt;/f&gt; naufragiu; o noapte și o zi am fost în largul mării.');
INSERT INTO &quot;verses&quot; VALUES (540,11,26,'Deseori am fost în călătorii, în primejdii pe râuri, în primejdii din partea tâlharilor, în&lt;f&gt;ⓔ&lt;/f&gt; primejdii din partea celor din neamul meu, în&lt;f&gt;ⓕ&lt;/f&gt; primejdii din partea păgânilor, în primejdii în cetate, în primejdii în pustiu, în primejdii pe mare, în primejdii între cei care se prefac că sunt frați,');
INSERT INTO &quot;verses&quot; VALUES (540,11,27,'în osteneli și necazuri, în nopți albe&lt;f&gt;ⓖ&lt;/f&gt; adesea, în foame&lt;f&gt;ⓗ&lt;/f&gt; și sete, în posturi adesea, în frig și lipsă de îmbrăcăminte!');
INSERT INTO &quot;verses&quot; VALUES (540,11,28,'Și, pe lângă aceste lucruri, în fiecare zi mă apasă grija&lt;f&gt;ⓘ&lt;/f&gt; pentru toate bisericile.');
INSERT INTO &quot;verses&quot; VALUES (540,11,29,'Cine&lt;f&gt;ⓙ&lt;/f&gt; este slab și să nu fiu și eu slab? Cine cade în păcat și eu să nu ard?');
INSERT INTO &quot;verses&quot; VALUES (540,11,30,'&lt;pb/&gt;Dacă trebuie să mă laud, mă&lt;f&gt;ⓚ&lt;/f&gt; voi lăuda numai cu lucrurile privitoare la slăbiciunea mea.');
INSERT INTO &quot;verses&quot; VALUES (540,11,31,'Dumnezeu&lt;f&gt;ⓛ&lt;/f&gt; și Tatăl Domnului nostru Isus [Hristos], care&lt;f&gt;ⓜ&lt;/f&gt; este binecuvântat în veci, știe că nu mint!');
INSERT INTO &quot;verses&quot; VALUES (540,11,32,'În&lt;f&gt;ⓝ&lt;/f&gt; Damasc, dregătorul regelui Areta&lt;f&gt;[38]&lt;/f&gt; păzea cetatea damascenilor ca să mă prindă,');
INSERT INTO &quot;verses&quot; VALUES (540,11,33,'însă am fost dat jos într-o coșniță, printr-o fereastră din zid, și am scăpat din mâinile lui.');
INSERT INTO &quot;verses&quot; VALUES (540,12,1,'&lt;pb/&gt;E nevoie să mă laud; nu că ar fi de vreun folos, dar voi trece totuși la vedeniile și descoperirile Domnului.');
INSERT INTO &quot;verses&quot; VALUES (540,12,2,'Cunosc un om în&lt;f&gt;ⓞ&lt;/f&gt; Hristos care, acum paisprezece ani, a fost&lt;f&gt;ⓟ&lt;/f&gt; răpit până în al treilea cer (dacă a fost cu trupul, nu știu; dacă a fost fără trup, nu știu — Dumnezeu știe).');
INSERT INTO &quot;verses&quot; VALUES (540,12,3,'Și știu că omul acesta (dacă a fost în trup sau fără trup, nu știu — Dumnezeu știe)');
INSERT INTO &quot;verses&quot; VALUES (540,12,4,'a fost răpit în rai&lt;f&gt;ⓠ&lt;/f&gt; și a auzit cuvinte care nu se pot spune și pe care nu-i este îngăduit unui om să le rostească.');
INSERT INTO &quot;verses&quot; VALUES (540,12,5,'Cu un astfel de om mă voi lăuda, dar&lt;f&gt;ⓡ&lt;/f&gt;, în ce mă privește, nu mă voi lăuda decât cu slăbiciunile.');
INSERT INTO &quot;verses&quot; VALUES (540,12,6,'Și dacă aș vrea&lt;f&gt;ⓢ&lt;/f&gt; să mă laud, n-aș &lt;i&gt;părea&lt;/i&gt; nebun, căci aș spune adevărul, dar mă feresc, ca să nu-mi pună nimeni în seamă altceva decât ce vede la mine sau ce aude de la mine,');
INSERT INTO &quot;verses&quot; VALUES (540,12,7,'dată fiind strălucirea acestor descoperiri. De aceea, ca să nu mă umflu de mândrie, mi s-a dat o țepușă&lt;f&gt;ⓣ&lt;/f&gt; în trup, un trimis&lt;f&gt;ⓤ&lt;/f&gt; al Satanei, ca să mă pălmuiască și &lt;i&gt;să mă împiedice&lt;/i&gt; să mă îngâmf.');
INSERT INTO &quot;verses&quot; VALUES (540,12,8,'De trei&lt;f&gt;ⓥ&lt;/f&gt; ori L-am rugat pe Domnul să o înlăture de la mine,');
INSERT INTO &quot;verses&quot; VALUES (540,12,9,'dar El mi-a zis: &lt;J&gt;„Harul Meu îți este de ajuns, căci puterea &lt;i&gt;Mea&lt;/i&gt; în slăbiciune este făcută desăvârșită.”&lt;/J&gt; Deci mă&lt;f&gt;ⓦ&lt;/f&gt; voi lăuda mult mai bucuros cu slăbiciunile mele, pentru ca puterea lui Hristos să locuiască în mine.');
INSERT INTO &quot;verses&quot; VALUES (540,12,10,'De aceea mă bucur&lt;f&gt;ⓧ&lt;/f&gt; în slăbiciuni, în defăimări, în nevoi, în prigoniri, în strâmtorări pentru Hristos. Când sunt slab, atunci sunt tare.');
INSERT INTO &quot;verses&quot; VALUES (540,12,11,'&lt;pb/&gt;Am ajuns nebun&lt;f&gt;ⓨ&lt;/f&gt;, voi m-ați silit. Se cuvenea să primesc aprecieri din partea voastră, căci, deși nu sunt&lt;f&gt;ⓩ&lt;/f&gt; nimic, n-am&lt;f&gt;ⓐ&lt;/f&gt; fost deloc mai prejos de acești minunați apostoli.');
INSERT INTO &quot;verses&quot; VALUES (540,12,12,'Semnele&lt;f&gt;ⓑ&lt;/f&gt; unui apostol au fost săvârșite între voi cu toată răbdarea, prin semne, puteri și minuni.');
INSERT INTO &quot;verses&quot; VALUES (540,12,13,'Într-adevăr, cu ce ați fost voi puși mai prejos decât celelalte biserici, în afara faptului că eu&lt;f&gt;ⓒ&lt;/f&gt; unul nu v-am fost povară? O, iertați-mi nedreptatea aceasta!');
INSERT INTO &quot;verses&quot; VALUES (540,12,14,'&lt;pb/&gt;Iată,&lt;f&gt;ⓓ&lt;/f&gt; sunt gata să vin la voi a treia oară, dar nu vă voi fi povară, căci nu caut bunurile voastre, ci pe voi înșivă! De altfel, nu&lt;f&gt;ⓔ&lt;/f&gt; copiii sunt datori să agonisească pentru părinți, ci părinții pentru copii.');
INSERT INTO &quot;verses&quot; VALUES (540,12,15,'Și eu voi&lt;f&gt;ⓕ&lt;/f&gt; cheltui prea bucuros &lt;i&gt;din ale mele&lt;/i&gt; și mă voi cheltui cu totul &lt;i&gt;pe mine însumi&lt;/i&gt; pentru&lt;f&gt;ⓖ&lt;/f&gt; sufletele voastre. Dacă vă iubesc mai mult, să fiu iubit mai puțin&lt;f&gt;ⓗ&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (540,12,16,'Fie și-așa! Eu, unul, nu&lt;f&gt;ⓘ&lt;/f&gt; v-am împovărat deloc. Dar, zic ei, ca un om șiret ce sunt, v-am luat prin vicleșug.');
INSERT INTO &quot;verses&quot; VALUES (540,12,17,'Dar&lt;f&gt;ⓙ&lt;/f&gt; am tras eu oare vreun folos de la voi prin vreunul din cei pe care i-am trimis la voi?');
INSERT INTO &quot;verses&quot; VALUES (540,12,18,'L-am&lt;f&gt;ⓚ&lt;/f&gt; rugat pe Tit &lt;i&gt;să meargă&lt;/i&gt;, iar împreună cu el l-am trimis pe&lt;f&gt;ⓛ&lt;/f&gt; fratele &lt;i&gt;nostru&lt;/i&gt;; a tras Tit vreun folos de la voi? N-am umblat noi în același duh și pe aceleași urme?');
INSERT INTO &quot;verses&quot; VALUES (540,12,19,'&lt;pb/&gt;De multă vreme&lt;f&gt;ⓜ&lt;/f&gt; vă închipuiți că &lt;i&gt;vrem să&lt;/i&gt; ne apărăm înaintea voastră. Noi vorbim înaintea&lt;f&gt;ⓝ&lt;/f&gt; lui Dumnezeu în Hristos și toate&lt;f&gt;ⓞ&lt;/f&gt; aceste lucruri, preaiubiților, le spunem pentru zidirea voastră!');
INSERT INTO &quot;verses&quot; VALUES (540,12,20,'Căci mă tem ca nu cumva, la venirea mea, să vă găsesc cum n-aș vrea, iar eu&lt;f&gt;ⓟ&lt;/f&gt; să fiu găsit de voi cum n-ați vrea. &lt;i&gt;Mă tem să nu găsesc&lt;/i&gt; ceartă, invidie, mânii, ambiții, defăimări, bârfeli, îngâmfări, tulburări.');
INSERT INTO &quot;verses&quot; VALUES (540,12,21,'&lt;i&gt;Mă tem ca&lt;/i&gt;, atunci când voi veni din nou, să&lt;f&gt;ⓠ&lt;/f&gt; nu mă smerească Dumnezeul meu cu privire la voi și să &lt;i&gt;nu ajung să-i&lt;/i&gt; plâng pe mulți din cei ce&lt;f&gt;ⓡ&lt;/f&gt; au păcătuit mai înainte și nu s-au pocăit de necurăția, desfrâul&lt;f&gt;ⓢ&lt;/f&gt; și dezmățul la care s-au dedat.');
INSERT INTO &quot;verses&quot; VALUES (540,13,1,'&lt;pb/&gt;Vin la voi pentru a&lt;f&gt;ⓣ&lt;/f&gt; treia oară. &lt;e&gt;Pe mărturia a doi&lt;/e&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;sau trei martori să fie întemeiată orice pricină&lt;/e&gt;.&lt;f&gt;[39]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (540,13,2,'După cum am spus&lt;f&gt;ⓥ&lt;/f&gt; &lt;i&gt;deja&lt;/i&gt; când am fost de față a doua oară, la fel și azi, când nu sunt de față, le spun dinainte&lt;f&gt;ⓦ&lt;/f&gt; celor ce au păcătuit și tuturor celorlalți că, dacă mă voi întoarce, voi fi&lt;f&gt;ⓧ&lt;/f&gt; necruțător,');
INSERT INTO &quot;verses&quot; VALUES (540,13,3,'fiindcă cereți o dovadă că prin mine vorbește&lt;f&gt;ⓨ&lt;/f&gt; Hristos, care nu este slab față de&lt;f&gt;ⓩ&lt;/f&gt; voi, ci este plin de putere între voi;');
INSERT INTO &quot;verses&quot; VALUES (540,13,4,'căci&lt;f&gt;ⓐ&lt;/f&gt; a fost răstignit în slăbiciune, dar trăiește&lt;f&gt;ⓑ&lt;/f&gt; prin puterea lui Dumnezeu. &lt;i&gt;Tot astfel&lt;/i&gt;, și noi&lt;f&gt;ⓒ&lt;/f&gt; suntem slabi în El, dar vom trăi cu El, prin puterea lui Dumnezeu față de voi.');
INSERT INTO &quot;verses&quot; VALUES (540,13,5,'&lt;pb/&gt;Pe voi înșivă încercați-vă&lt;f&gt;ⓓ&lt;/f&gt; dacă sunteți în credință! Pe voi înșivă puneți-vă la încercare! Nu recunoașteți, în ce vă privește, că&lt;f&gt;ⓔ&lt;/f&gt; Isus Hristos este în voi? Doar dacă nu cumva sunteți&lt;f&gt;ⓕ&lt;/f&gt; lepădați&lt;f&gt;[40]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (540,13,6,'Însă nădăjduiesc că veți recunoaște că noi nu suntem lepădați.');
INSERT INTO &quot;verses&quot; VALUES (540,13,7,'Ne rugăm lui Dumnezeu să nu faceți nimic rău; nu ca să ne&lt;f&gt;ⓖ&lt;/f&gt; arătăm noi înșine buni&lt;f&gt;[41]&lt;/f&gt;, ci ca să faceți binele, chiar dacă noi am ajunge să fim lepădați.');
INSERT INTO &quot;verses&quot; VALUES (540,13,8,'Căci nu putem face nimic împotriva adevărului, ci &lt;i&gt;numai&lt;/i&gt; pentru adevăr.');
INSERT INTO &quot;verses&quot; VALUES (540,13,9,'Ne bucurăm când&lt;f&gt;ⓗ&lt;/f&gt; noi suntem slabi, iar voi sunteți tari. Tocmai pentru aceasta ne rugăm — pentru desăvârșirea&lt;f&gt;ⓘ&lt;/f&gt; voastră.');
INSERT INTO &quot;verses&quot; VALUES (540,13,10,'De aceea&lt;f&gt;ⓙ&lt;/f&gt; vă scriu aceste lucruri când nu sunt de față, pentru ca, atunci când voi fi de față, să&lt;f&gt;ⓚ&lt;/f&gt; nu mă port aspru, potrivit cu&lt;f&gt;ⓛ&lt;/f&gt; autoritatea pe care mi-a dat-o Domnul pentru zidire, nu pentru nimicire.');
INSERT INTO &quot;verses&quot; VALUES (540,13,11,'&lt;pb/&gt;Încolo, fraților, bucurați-vă&lt;f&gt;ⓜ&lt;/f&gt;, desăvârșiți-vă, îmbărbătați-vă, aveți aceleași gânduri, trăiți în&lt;f&gt;ⓝ&lt;/f&gt; pace, și Dumnezeul dragostei și al păcii va fi cu voi!');
INSERT INTO &quot;verses&quot; VALUES (540,13,12,'Spuneți-vă salutări unii altora&lt;f&gt;ⓞ&lt;/f&gt; cu o sfântă sărutare!');
INSERT INTO &quot;verses&quot; VALUES (540,13,13,'&lt;pb/&gt;Toți sfinții vă trimit salutări.');
INSERT INTO &quot;verses&quot; VALUES (540,13,14,'Harul&lt;f&gt;ⓟ&lt;/f&gt; Domnului Isus Hristos, dragostea lui Dumnezeu și împărtășirea&lt;f&gt;ⓠ&lt;/f&gt; Sfântului Duh să fie cu voi cu toți! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (550,1,1,'&lt;pb/&gt;Pavel, apostol nu de la oameni, nici&lt;f&gt;ⓐ&lt;/f&gt; printr-un om, ci prin&lt;f&gt;ⓑ&lt;/f&gt; Isus Hristos și prin Dumnezeu Tatăl, care&lt;f&gt;ⓒ&lt;/f&gt; L-a înviat din morți,');
INSERT INTO &quot;verses&quot; VALUES (550,1,2,'și toți frații care&lt;f&gt;ⓓ&lt;/f&gt; sunt împreună cu mine către bisericile&lt;f&gt;ⓔ&lt;/f&gt; din Galatia:');
INSERT INTO &quot;verses&quot; VALUES (550,1,3,'Har&lt;f&gt;ⓕ&lt;/f&gt; și pace vouă de la Dumnezeu Tatăl și de la Domnul nostru Isus Hristos,');
INSERT INTO &quot;verses&quot; VALUES (550,1,4,'care&lt;f&gt;ⓖ&lt;/f&gt; S-a dat pe Sine Însuși pentru păcatele noastre ca să ne smulgă din acest&lt;f&gt;ⓗ&lt;/f&gt; veac rău, după voia Dumnezeului și Tatălui nostru!');
INSERT INTO &quot;verses&quot; VALUES (550,1,5,'A Lui să fie slava în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (550,1,6,'&lt;pb/&gt;Sunt uluit că treceți așa de repede de la Acela care v-a chemat prin harul lui Hristos la o altă evanghelie&lt;f&gt;ⓘ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (550,1,7,'Nu că&lt;f&gt;ⓙ&lt;/f&gt; ar fi o alta, dar sunt unii care vă tulbură și vor să răstoarne&lt;f&gt;[1]&lt;/f&gt; Evanghelia lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (550,1,8,'Dar chiar dacă noi&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;înșine&lt;/i&gt; sau un înger din cer v-ar propovădui o altă evanghelie decât cea pe care v-am propovăduit-o &lt;i&gt;noi&lt;/i&gt;, să fie anatema!');
INSERT INTO &quot;verses&quot; VALUES (550,1,9,'Cum am mai spus, zic și acum, din nou: dacă vă propovăduiește cineva o altă evanghelie&lt;f&gt;ⓛ&lt;/f&gt; decât cea pe care ați primit-o, să fie anatema!');
INSERT INTO &quot;verses&quot; VALUES (550,1,10,'&lt;pb/&gt;Acum, caut&lt;f&gt;ⓜ&lt;/f&gt; eu oare&lt;f&gt;ⓝ&lt;/f&gt; să câștig bunăvoința oamenilor sau, &lt;i&gt;dimpotrivă&lt;/i&gt;, a lui Dumnezeu? Caut&lt;f&gt;ⓞ&lt;/f&gt; &lt;i&gt;oare&lt;/i&gt; să le fiu pe plac oamenilor? Dacă aș căuta să le fiu pe plac oamenilor, n-aș fi robul lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (550,1,11,'&lt;pb/&gt;Vă fac cunoscut, fraților&lt;f&gt;ⓟ&lt;/f&gt;, că Evanghelia propovăduită de mine nu este de origine omenească,');
INSERT INTO &quot;verses&quot; VALUES (550,1,12,'pentru că nu de la oameni am primit-o&lt;f&gt;ⓠ&lt;/f&gt; sau am învățat-o, ci prin&lt;f&gt;ⓡ&lt;/f&gt; descoperirea lui Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (550,1,13,'Ați auzit care era purtarea mea de altădată, în iudaism, cum prigoneam peste&lt;f&gt;ⓢ&lt;/f&gt; măsură Biserica lui Dumnezeu și căutam s-o nimicesc&lt;f&gt;ⓣ&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,1,14,'și cum înaintasem în iudaism mai mult decât mulți din neamul meu de o vârstă cu mine, căci eram mult mai zelos&lt;f&gt;ⓤ&lt;/f&gt; pentru tradițiile strămoșești&lt;f&gt;ⓥ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (550,1,15,'Dar când Dumnezeu, care&lt;f&gt;ⓦ&lt;/f&gt; m-a pus deoparte din pântecele mamei mele și m-a chemat prin harul Său, a găsit cu cale');
INSERT INTO &quot;verses&quot; VALUES (550,1,16,'să-L&lt;f&gt;ⓧ&lt;/f&gt; descopere prin mine pe Fiul Său ca să-L propovăduiesc&lt;f&gt;ⓨ&lt;/f&gt; printre neamuri, nu m-am sfătuit cu vreun&lt;f&gt;ⓩ&lt;/f&gt; om,');
INSERT INTO &quot;verses&quot; VALUES (550,1,17,'nici nu m-am suit la Ierusalim, la cei ce erau apostoli înainte de mine, ci m-am dus degrabă în Arabia, după care m-am întors din nou în Damasc.');
INSERT INTO &quot;verses&quot; VALUES (550,1,18,'Apoi, după trei ani, m-am suit&lt;f&gt;ⓐ&lt;/f&gt; la Ierusalim ca să fac cunoștință cu Chifa și am rămas la el cincisprezece zile.');
INSERT INTO &quot;verses&quot; VALUES (550,1,19,'Dar n-am văzut pe&lt;f&gt;ⓑ&lt;/f&gt; vreun alt apostol, ci &lt;i&gt;numai&lt;/i&gt; pe&lt;f&gt;ⓒ&lt;/f&gt; Iacov, fratele Domnului.');
INSERT INTO &quot;verses&quot; VALUES (550,1,20,'Înaintea lui Dumnezeu vă spun: nu mint&lt;f&gt;ⓓ&lt;/f&gt; în ceea ce vă scriu!');
INSERT INTO &quot;verses&quot; VALUES (550,1,21,'După aceea&lt;f&gt;ⓔ&lt;/f&gt;, m-am dus în ținuturile Siriei și Ciliciei.');
INSERT INTO &quot;verses&quot; VALUES (550,1,22,'Și eram necunoscut la față bisericilor din Iudeea care sunt în Hristos.');
INSERT INTO &quot;verses&quot; VALUES (550,1,23,'Ele doar auzeau spunându-se: „Cel care ne prigonea odinioară acum propovăduiește credința pe care cândva căuta s-o nimicească!”');
INSERT INTO &quot;verses&quot; VALUES (550,1,24,'Și-L slăveau pe Dumnezeu pentru mine.');
INSERT INTO &quot;verses&quot; VALUES (550,2,1,'&lt;pb/&gt;Apoi, după paisprezece ani, m-am&lt;f&gt;ⓕ&lt;/f&gt; suit din nou la Ierusalim împreună cu Barnaba și l-am luat cu mine și pe Tit.');
INSERT INTO &quot;verses&quot; VALUES (550,2,2,'M-am suit în urma unei descoperiri și — deoparte, celor mai cu vază — le-am&lt;f&gt;ⓖ&lt;/f&gt; prezentat Evanghelia pe care o propovăduiesc printre neamuri, ca nu cumva să alerg&lt;f&gt;ⓗ&lt;/f&gt; sau să fi alergat în zadar.');
INSERT INTO &quot;verses&quot; VALUES (550,2,3,'Dar nici măcar Tit, care era cu mine, deși era grec, n-a fost silit să se circumcidă.');
INSERT INTO &quot;verses&quot; VALUES (550,2,4,'&lt;i&gt;Tulburarea s-a ivit&lt;/i&gt;&lt;f&gt;[2]&lt;/f&gt; din pricina fraților&lt;f&gt;ⓘ&lt;/f&gt; prefăcuți, furișați &lt;i&gt;printre noi&lt;/i&gt;, care s-au strecurat ca să pândească libertatea&lt;f&gt;ⓙ&lt;/f&gt; pe care o avem în Hristos Isus, &lt;i&gt;cu gând&lt;/i&gt; să&lt;f&gt;ⓚ&lt;/f&gt; ne ia în robie.');
INSERT INTO &quot;verses&quot; VALUES (550,2,5,'Dar n-am cedat și nu ne-am supus lor nicio clipă &lt;i&gt;măcar&lt;/i&gt;, pentru ca adevărul&lt;f&gt;ⓛ&lt;/f&gt; Evangheliei să rămână cu voi.');
INSERT INTO &quot;verses&quot; VALUES (550,2,6,'&lt;pb/&gt;Cei&lt;f&gt;ⓜ&lt;/f&gt; considerați ca fiind cineva (oricine ar fi fost ei, îmi este indiferent; Dumnezeu&lt;f&gt;ⓝ&lt;/f&gt; nu caută la fața oamenilor), cei cu vază, așadar, nu&lt;f&gt;ⓞ&lt;/f&gt; mi-au adăugat nimic.');
INSERT INTO &quot;verses&quot; VALUES (550,2,7,'Dimpotrivă, când au văzut&lt;f&gt;ⓟ&lt;/f&gt; că mie îmi fusese încredințată Evanghelia pentru cei necircumciși, după cum lui Petru &lt;i&gt;îi fusese încredințată&lt;/i&gt; pentru cei circumciși');
INSERT INTO &quot;verses&quot; VALUES (550,2,8,'(căci Cel ce făcuse din Petru apostolul celor circumciși făcuse&lt;f&gt;ⓠ&lt;/f&gt; și din mine &lt;i&gt;apostolul&lt;/i&gt; neamurilor&lt;f&gt;ⓡ&lt;/f&gt;),');
INSERT INTO &quot;verses&quot; VALUES (550,2,9,'și când au recunoscut harul&lt;f&gt;ⓢ&lt;/f&gt; care-mi fusese dat, Iacov, Chifa și Ioan, care sunt priviți ca stâlpi&lt;f&gt;ⓣ&lt;/f&gt;, ne-au întins mie și lui Barnaba mâna dreaptă în semn de colaborare, pentru ca noi să mergem la neamuri, iar ei la cei circumciși.');
INSERT INTO &quot;verses&quot; VALUES (550,2,10,'Numai să ne aducem aminte de cei săraci, ceea ce&lt;f&gt;ⓤ&lt;/f&gt; m-am și străduit să fac.');
INSERT INTO &quot;verses&quot; VALUES (550,2,11,'&lt;pb/&gt;Dar&lt;f&gt;ⓥ&lt;/f&gt; când a venit Chifa&lt;f&gt;[3]&lt;/f&gt; în Antiohia, l-am înfruntat fățiș, căci era de osândit:');
INSERT INTO &quot;verses&quot; VALUES (550,2,12,'înainte de venirea unora de la Iacov, mânca&lt;f&gt;ⓦ&lt;/f&gt; împreună cu neamurile, dar când au venit ei, s-a retras și s-a ținut deoparte, fiindcă se temea de cei circumciși.');
INSERT INTO &quot;verses&quot; VALUES (550,2,13,'Împreună cu el au început să se prefacă și ceilalți iudei, încât până și Barnaba s-a lăsat atras în fățărnicia lor.');
INSERT INTO &quot;verses&quot; VALUES (550,2,14,'Când am văzut eu că nu umblă drept după adevărul Evangheliei, i-am spus lui Chifa înaintea&lt;f&gt;ⓧ&lt;/f&gt; tuturor: „Dacă&lt;f&gt;ⓨ&lt;/f&gt; tu, care ești iudeu, trăiești ca neamurile, nu ca un iudeu, cum de îi silești pe cei dintre neamuri să trăiască precum iudeii?”');
INSERT INTO &quot;verses&quot; VALUES (550,2,15,'Noi&lt;f&gt;ⓩ&lt;/f&gt; suntem iudei din naștere, nu păcătoși&lt;f&gt;ⓐ&lt;/f&gt; dintre neamuri.');
INSERT INTO &quot;verses&quot; VALUES (550,2,16,'Dar fiindcă știm&lt;f&gt;ⓑ&lt;/f&gt; că omul nu este îndreptățit prin faptele Legii, ci&lt;f&gt;ⓒ&lt;/f&gt; prin credința în Isus Hristos, și noi am crezut în Hristos Isus, ca să fim îndreptățiți prin credința în Hristos, nu prin&lt;f&gt;ⓓ&lt;/f&gt; faptele Legii, fiindcă prin faptele Legii nimeni nu va fi îndreptățit.');
INSERT INTO &quot;verses&quot; VALUES (550,2,17,'Dar dacă, în timp ce căutăm să fim îndreptățiți în Hristos, am fi noi înșine găsiți păcătoși&lt;f&gt;ⓔ&lt;/f&gt;, atunci &lt;i&gt;devine&lt;/i&gt; oare Hristos un slujitor al păcatului? Nicidecum!');
INSERT INTO &quot;verses&quot; VALUES (550,2,18,'Căci, dacă zidesc iarăși ceea ce am dărâmat, mă dovedesc călcător de Lege.');
INSERT INTO &quot;verses&quot; VALUES (550,2,19,'Însă eu, prin&lt;f&gt;ⓕ&lt;/f&gt; Lege, am&lt;f&gt;ⓖ&lt;/f&gt; murit față de Lege ca să trăiesc&lt;f&gt;ⓗ&lt;/f&gt; pentru Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (550,2,20,'Am fost răstignit&lt;f&gt;ⓘ&lt;/f&gt; împreună cu Hristos&lt;f&gt;[4]&lt;/f&gt; și nu mai trăiesc eu, ci Hristos trăiește în mine. Și viața pe care o trăiesc acum în trup o trăiesc&lt;f&gt;ⓙ&lt;/f&gt; prin credința în Fiul lui Dumnezeu, care m-a iubit&lt;f&gt;ⓚ&lt;/f&gt; și S-a dat pe Sine pentru mine.');
INSERT INTO &quot;verses&quot; VALUES (550,2,21,'Nu &lt;i&gt;vreau să&lt;/i&gt; fac zadarnic harul lui Dumnezeu; căci, dacă&lt;f&gt;ⓛ&lt;/f&gt; prin Lege se capătă dreptatea, atunci degeaba a murit Hristos.');
INSERT INTO &quot;verses&quot; VALUES (550,3,1,'&lt;pb/&gt;O, galateni nechibzuiți! Cine&lt;f&gt;ⓜ&lt;/f&gt; v-a fermecat&lt;f&gt;ⓝ&lt;/f&gt; pe voi, sub ochii cărora a fost zugrăvit Isus Hristos ca răstignit?');
INSERT INTO &quot;verses&quot; VALUES (550,3,2,'Numai aceasta vreau să aflu de la voi: prin faptele Legii ați primit &lt;i&gt;voi&lt;/i&gt; Duhul&lt;f&gt;ⓞ&lt;/f&gt; sau prin credința&lt;f&gt;ⓟ&lt;/f&gt; în cele auzite&lt;f&gt;[5]&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (550,3,3,'Atât de nechibzuiți sunteți? După ce ați început&lt;f&gt;ⓠ&lt;/f&gt; prin Duhul, acum &lt;i&gt;vreți să&lt;/i&gt; sfârșiți prin firea pământească&lt;f&gt;ⓡ&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (550,3,4,'În zadar&lt;f&gt;ⓢ&lt;/f&gt; ați avut parte de atâtea? Eu cred că nu în zadar.&lt;f&gt;[6]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,3,5,'Cel ce&lt;f&gt;ⓣ&lt;/f&gt; vă dăruiește Duhul și face minuni printre voi &lt;i&gt;le face oare&lt;/i&gt; prin faptele Legii sau prin credința în cele auzite?');
INSERT INTO &quot;verses&quot; VALUES (550,3,6,'&lt;pb/&gt;După cum &lt;e&gt;Avraam&lt;/e&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;L-a crezut pe Dumnezeu, și credința i s-a socotit ca dreptate&lt;/e&gt;&lt;f&gt;[7]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (550,3,7,'înțelegeți deci că fii ai lui Avraam sunt cei&lt;f&gt;ⓥ&lt;/f&gt; care au credință.');
INSERT INTO &quot;verses&quot; VALUES (550,3,8,'Scriptura, fiindcă a prevăzut&lt;f&gt;ⓦ&lt;/f&gt; că Dumnezeu îndreptățește neamurile prin credință, i-a dat mai dinainte lui Avraam &lt;i&gt;această&lt;/i&gt; veste bună: &lt;e&gt;Toate neamurile vor fi binecuvântate în&lt;/e&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;tine&lt;/e&gt;.&lt;f&gt;[8]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,3,9,'Prin urmare, cei ce au credință sunt binecuvântați împreună cu Avraam cel credincios.');
INSERT INTO &quot;verses&quot; VALUES (550,3,10,'&lt;pb/&gt;Dar toți cei ce se bizuie pe faptele Legii sunt sub blestem, pentru că este scris: &lt;e&gt;Blestemat&lt;/e&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;este oricine nu stăruie în toate lucrurile scrise în cartea Legii, ca să le facă&lt;/e&gt;.&lt;f&gt;[9]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,3,11,'Și că&lt;f&gt;ⓩ&lt;/f&gt; nimeni nu e îndreptățit înaintea lui Dumnezeu prin Lege este limpede: &lt;e&gt;Cel&lt;/e&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;e&gt;drept prin credință va trăi.&lt;/e&gt;&lt;f&gt;[10]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,3,12,'Însă&lt;f&gt;ⓑ&lt;/f&gt; Legea nu se bizuie pe credință; dimpotrivă: &lt;e&gt;Cine&lt;/e&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;face aceste lucruri va trăi prin ele&lt;/e&gt;.&lt;f&gt;[11]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,3,13,'Hristos&lt;f&gt;ⓓ&lt;/f&gt; ne-a răscumpărat de sub blestemul Legii făcându-Se blestem pentru noi — fiindcă este scris: &lt;e&gt;Blestemat este oricine este atârnat pe lemn&lt;/e&gt;&lt;f&gt;[12]&lt;/f&gt; —');
INSERT INTO &quot;verses&quot; VALUES (550,3,14,'pentru ca&lt;f&gt;ⓔ&lt;/f&gt; binecuvântarea lui Avraam să vină peste neamuri în Hristos Isus și &lt;i&gt;astfel&lt;/i&gt; să primim prin credință Duhul&lt;f&gt;ⓕ&lt;/f&gt; făgăduit.');
INSERT INTO &quot;verses&quot; VALUES (550,3,15,'&lt;pb/&gt;Fraților (ca să iau o pildă din viața de zi cu zi), un testament&lt;f&gt;ⓖ&lt;/f&gt;, fie el și omenesc, odată întărit&lt;f&gt;[13]&lt;/f&gt;, nu mai poate fi anulat sau modificat.');
INSERT INTO &quot;verses&quot; VALUES (550,3,16,'Or, făgăduințele i-au fost făcute lui&lt;f&gt;ⓗ&lt;/f&gt; Avraam și seminței lui. Nu zice: „și semințelor”, ca &lt;i&gt;și cum ar fi vorba&lt;/i&gt; de mai multe, ci ca și cum ar fi vorba de una: &lt;e&gt;și seminței tale&lt;/e&gt;&lt;f&gt;[14]&lt;/f&gt;, adică Hristos.');
INSERT INTO &quot;verses&quot; VALUES (550,3,17,'Iată ce vreau să zic: testamentul&lt;f&gt;[15]&lt;/f&gt; pe care l-a întărit Dumnezeu mai înainte nu poate fi anulat&lt;f&gt;ⓘ&lt;/f&gt; de Legea dată&lt;f&gt;ⓙ&lt;/f&gt; după patru sute treizeci de ani, astfel încât făgăduința să fie nimicită.');
INSERT INTO &quot;verses&quot; VALUES (550,3,18,'Căci dacă moștenirea&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;ar veni&lt;/i&gt; pe baza Legii, n-ar&lt;f&gt;ⓛ&lt;/f&gt; mai &lt;i&gt;fi&lt;/i&gt; pe baza făgăduinței; însă lui Avraam Dumnezeu i-a dăruit-o prin făgăduință.');
INSERT INTO &quot;verses&quot; VALUES (550,3,19,'&lt;pb/&gt;Atunci care e rostul Legii? Ea&lt;f&gt;ⓜ&lt;/f&gt; a fost adăugată din pricina călcărilor &lt;i&gt;de lege&lt;/i&gt;, până când avea să vină „Sămânța” căreia I se făcuse făgăduința; ea a fost dată&lt;f&gt;ⓝ&lt;/f&gt; prin îngeri, prin mâna unui mijlocitor&lt;f&gt;ⓞ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (550,3,20,'Dar mijlocitorul nu &lt;i&gt;are de-a face cu&lt;/i&gt; o singură parte, pe când&lt;f&gt;ⓟ&lt;/f&gt; Dumnezeu este Unul &lt;i&gt;singur&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (550,3,21,'Așadar, este Legea împotriva făgăduințelor lui Dumnezeu? Nicidecum! Dacă&lt;f&gt;ⓠ&lt;/f&gt; s-ar fi dat o Lege care să poată da viața, cu siguranță dreptatea ar fi venit din Lege.');
INSERT INTO &quot;verses&quot; VALUES (550,3,22,'Dar Scriptura a închis totul&lt;f&gt;ⓡ&lt;/f&gt; sub păcat, pentru ca făgăduința să le fie dată, prin credința în Isus Hristos, celor ce cred.');
INSERT INTO &quot;verses&quot; VALUES (550,3,23,'&lt;pb/&gt;Înainte de venirea credinței, eram sub paza Legii, închiși în vederea credinței care trebuia să fie descoperită.');
INSERT INTO &quot;verses&quot; VALUES (550,3,24,'Astfel, Legea&lt;f&gt;ⓢ&lt;/f&gt; ne-a fost un îndrumător&lt;f&gt;[16]&lt;/f&gt; spre Hristos, ca&lt;f&gt;ⓣ&lt;/f&gt; să fim îndreptățiți prin credință.');
INSERT INTO &quot;verses&quot; VALUES (550,3,25,'După ce a venit credința, nu mai suntem sub &lt;i&gt;acest&lt;/i&gt; îndrumător.');
INSERT INTO &quot;verses&quot; VALUES (550,3,26,'Căci toți&lt;f&gt;ⓤ&lt;/f&gt; sunteți fii ai lui Dumnezeu prin credința în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (550,3,27,'Toți care&lt;f&gt;ⓥ&lt;/f&gt; în Hristos v-ați botezat, cu&lt;f&gt;ⓦ&lt;/f&gt; Hristos v-ați îmbrăcat.');
INSERT INTO &quot;verses&quot; VALUES (550,3,28,'Nu&lt;f&gt;ⓧ&lt;/f&gt; mai este nici iudeu, nici grec; nu mai este nici rob, nici om liber; nu mai este nici parte bărbătească, nici parte femeiască, fiindcă toți sunteți una&lt;f&gt;ⓨ&lt;/f&gt; în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (550,3,29,'Și dacă&lt;f&gt;ⓩ&lt;/f&gt; sunteți ai lui Hristos, atunci sunteți „sămânța” lui Avraam, moștenitori&lt;f&gt;ⓐ&lt;/f&gt; prin făgăduință.');
INSERT INTO &quot;verses&quot; VALUES (550,4,1,'&lt;pb/&gt;Ce spun eu este că, atâta vreme cât moștenitorul este nevârstnic, nu se deosebește cu nimic de un rob: cu toate că este stăpân peste toate,');
INSERT INTO &quot;verses&quot; VALUES (550,4,2,'stă sub autoritatea tutorilor și a îngrijitorilor până la vremea hotărâtă de tatăl său.');
INSERT INTO &quot;verses&quot; VALUES (550,4,3,'Tot astfel și noi, când eram nevârstnici, eram&lt;f&gt;ⓑ&lt;/f&gt; înrobiți de învățăturile elementare&lt;f&gt;[17]&lt;/f&gt; ale lumii.');
INSERT INTO &quot;verses&quot; VALUES (550,4,4,'Dar când&lt;f&gt;ⓒ&lt;/f&gt; s-a împlinit vremea, Dumnezeu L-a trimis pe Fiul Său, născut&lt;f&gt;ⓓ&lt;/f&gt; din femeie&lt;f&gt;ⓔ&lt;/f&gt;, născut sub Lege,');
INSERT INTO &quot;verses&quot; VALUES (550,4,5,'ca să-i&lt;f&gt;ⓕ&lt;/f&gt; răscumpere pe cei de sub Lege, și &lt;i&gt;astfel&lt;/i&gt;&lt;f&gt;ⓖ&lt;/f&gt; să căpătăm înfierea.');
INSERT INTO &quot;verses&quot; VALUES (550,4,6,'Și pentru că sunteți fii, Dumnezeu ni L-a trimis în inimi pe Duhul&lt;f&gt;ⓗ&lt;/f&gt; Fiului Său, care strigă: „&lt;e&gt;Avva&lt;/e&gt;!”, adică „Tată!”');
INSERT INTO &quot;verses&quot; VALUES (550,4,7,'Așa că nu mai ești rob, ci fiu; și dacă&lt;f&gt;ⓘ&lt;/f&gt; ești fiu, ești și moștenitor prin Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (550,4,8,'&lt;pb/&gt;Înainte, când nu-L cunoșteați&lt;f&gt;ⓙ&lt;/f&gt; pe Dumnezeu, ați slujit ca robi&lt;f&gt;ⓚ&lt;/f&gt; celor care, prin natura lor, nu sunt dumnezei.');
INSERT INTO &quot;verses&quot; VALUES (550,4,9,'Dar acum, după ce L-ați cunoscut pe Dumnezeu — mai bine zis, după ce ați fost cunoscuți de Dumnezeu — cum&lt;f&gt;ⓛ&lt;/f&gt; de vă întoarceți iarăși la acele învățături&lt;f&gt;ⓜ&lt;/f&gt; elementare, slabe și sărăcăcioase de care vreți să vă lăsați iarăși înrobiți?');
INSERT INTO &quot;verses&quot; VALUES (550,4,10,'Voi țineți zile&lt;f&gt;ⓝ&lt;/f&gt;, luni, vremuri și ani.');
INSERT INTO &quot;verses&quot; VALUES (550,4,11,'Mă tem să&lt;f&gt;ⓞ&lt;/f&gt; nu mă fi ostenit degeaba pentru voi.');
INSERT INTO &quot;verses&quot; VALUES (550,4,12,'&lt;pb/&gt;Fraților, vă rog să fiți ca mine, căci și eu sunt ca voi. Nu&lt;f&gt;ⓟ&lt;/f&gt; m-ați nedreptățit cu nimic.');
INSERT INTO &quot;verses&quot; VALUES (550,4,13,'Știți că&lt;f&gt;ⓠ&lt;/f&gt;, prima&lt;f&gt;ⓡ&lt;/f&gt; dată, v-am propovăduit Evanghelia ca urmare a unei slăbiciuni a trupului,');
INSERT INTO &quot;verses&quot; VALUES (550,4,14,'dar n-ați arătat nici dispreț, nici dezgust față de ceea ce era o ispită pentru voi în trupul meu, ci m-ați primit ca&lt;f&gt;ⓢ&lt;/f&gt; pe un înger al lui Dumnezeu, ca&lt;f&gt;ⓣ&lt;/f&gt; pe Hristos Isus &lt;i&gt;Însuși&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (550,4,15,'Ce s-a ales de fericirea voastră? Pot da mărturie despre voi că, dacă ar fi fost cu putință, v-ați fi scos &lt;i&gt;până și&lt;/i&gt; ochii și mi i-ați fi dat.');
INSERT INTO &quot;verses&quot; VALUES (550,4,16,'M-am făcut oare vrăjmașul vostru pentru că&lt;f&gt;ⓤ&lt;/f&gt; v-am spus adevărul?');
INSERT INTO &quot;verses&quot; VALUES (550,4,17,'Nu cu gând bun&lt;f&gt;ⓥ&lt;/f&gt; sunt ei plini de râvnă pentru voi, ci vor să vă despartă &lt;i&gt;de noi&lt;/i&gt;, ca să fiți plini de râvnă pentru ei.');
INSERT INTO &quot;verses&quot; VALUES (550,4,18,'Bine ar fi să arătați întotdeauna râvnă cu un scop bun, nu numai când sunt eu prezent cu voi.');
INSERT INTO &quot;verses&quot; VALUES (550,4,19,'Copilașii mei&lt;f&gt;ⓦ&lt;/f&gt;, din pricina cărora iarăși simt durerile nașterii până când va lua Hristos chip în voi!');
INSERT INTO &quot;verses&quot; VALUES (550,4,20,'&lt;i&gt;Cât de mult&lt;/i&gt; aș vrea să fiu acum prezent cu voi și să-mi schimb tonul, căci nu mai știu ce să cred despre voi!');
INSERT INTO &quot;verses&quot; VALUES (550,4,21,'&lt;pb/&gt;Spuneți-mi voi, care vreți să fiți sub Lege, n-ascultați &lt;i&gt;voi&lt;/i&gt; Legea?');
INSERT INTO &quot;verses&quot; VALUES (550,4,22,'Căci este scris că Avraam a avut doi fii: unul&lt;f&gt;ⓧ&lt;/f&gt; din &lt;i&gt;cea care era&lt;/i&gt; slujnică și unul&lt;f&gt;ⓨ&lt;/f&gt; din &lt;i&gt;cea care era&lt;/i&gt; liberă.');
INSERT INTO &quot;verses&quot; VALUES (550,4,23,'Dar cel din slujnică s-a&lt;f&gt;ⓩ&lt;/f&gt; născut în chip firesc, însă&lt;f&gt;ⓐ&lt;/f&gt; cel din femeia liberă &lt;i&gt;s-a născut&lt;/i&gt; prin făgăduință,');
INSERT INTO &quot;verses&quot; VALUES (550,4,24,'lucruri care au un înțeles alegoric, căci aceste &lt;i&gt;femei&lt;/i&gt; sunt două legăminte: unul, de pe muntele Sinai&lt;f&gt;ⓑ&lt;/f&gt;, naște pentru robie, și este Agar.');
INSERT INTO &quot;verses&quot; VALUES (550,4,25,'Agar este muntele Sinai din Arabia și corespunde Ierusalimului de acum, căci este în robie împreună cu copiii săi.');
INSERT INTO &quot;verses&quot; VALUES (550,4,26,'Dar &lt;i&gt;cetatea&lt;/i&gt; Ierusalimului&lt;f&gt;ⓒ&lt;/f&gt; de sus este liberă, și ea este mama noastră.');
INSERT INTO &quot;verses&quot; VALUES (550,4,27,'Fiindcă este scris: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Bucură-te, stearpo, care nu naști!&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Izbucnește &lt;i&gt;de bucurie&lt;/i&gt; și strigă, tu, care nu suferi durerile nașterii!&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Căci mulți vor fi copiii celei părăsite,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;mai mulți decât ai celei care are bărbat.&lt;/e&gt; &lt;f&gt;[18]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,4,28,'&lt;pb/&gt;Și voi, fraților, ca și Isaac, sunteți copii&lt;f&gt;ⓓ&lt;/f&gt; ai făgăduinței.');
INSERT INTO &quot;verses&quot; VALUES (550,4,29,'Și după cum odinioară cel&lt;f&gt;ⓔ&lt;/f&gt; ce se născuse în chip firesc îl prigonea pe cel ce se născuse în chip duhovnicesc&lt;f&gt;[19]&lt;/f&gt;, tot așa&lt;f&gt;ⓕ&lt;/f&gt; este și acum.');
INSERT INTO &quot;verses&quot; VALUES (550,4,30,'Dar ce zice Scriptura&lt;f&gt;ⓖ&lt;/f&gt;? &lt;e&gt;Izgonește-i pe slujnică și pe fiul ei, căci fiul&lt;/e&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;slujnicei nu va moșteni împreună cu fiul femeii libere.&lt;/e&gt;&lt;f&gt;[20]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,4,31,'De aceea, fraților, &lt;i&gt;noi&lt;/i&gt; nu suntem copiii slujnicei, ci&lt;f&gt;ⓘ&lt;/f&gt; ai femeii libere.');
INSERT INTO &quot;verses&quot; VALUES (550,5,1,'&lt;pb/&gt;Hristos ne-a izbăvit &lt;i&gt;ca să avem&lt;/i&gt; libertate. Rămâneți deci tari&lt;f&gt;ⓙ&lt;/f&gt; și nu vă plecați iarăși sub&lt;f&gt;ⓚ&lt;/f&gt; jugul robiei.');
INSERT INTO &quot;verses&quot; VALUES (550,5,2,'&lt;pb/&gt;Iată, eu, Pavel, vă spun că, dacă&lt;f&gt;ⓛ&lt;/f&gt; vă circumcideți, Hristos nu vă va folosi la nimic.');
INSERT INTO &quot;verses&quot; VALUES (550,5,3,'Și dau iarăși mărturie oricărui om care se circumcide că&lt;f&gt;ⓜ&lt;/f&gt; este dator să împlinească toată Legea.');
INSERT INTO &quot;verses&quot; VALUES (550,5,4,'Voi, care vreți să fiți îndreptățiți prin Lege, v-ați&lt;f&gt;ⓝ&lt;/f&gt; despărțit de Hristos; ați căzut din&lt;f&gt;ⓞ&lt;/f&gt; har.');
INSERT INTO &quot;verses&quot; VALUES (550,5,5,'Căci noi, cu ajutorul Duhului, așteptăm&lt;f&gt;ⓟ&lt;/f&gt; prin credință dreptatea la care nădăjduim.');
INSERT INTO &quot;verses&quot; VALUES (550,5,6,'Căci&lt;f&gt;ⓠ&lt;/f&gt; în Hristos Isus nici circumcizia, nici necircumcizia nu contează, ci credința&lt;f&gt;ⓡ&lt;/f&gt; care lucrează prin dragoste.');
INSERT INTO &quot;verses&quot; VALUES (550,5,7,'&lt;pb/&gt;Voi alergați&lt;f&gt;ⓢ&lt;/f&gt; bine; cine&lt;f&gt;ⓣ&lt;/f&gt; v-a tăiat calea ca să n-ascultați de adevăr?');
INSERT INTO &quot;verses&quot; VALUES (550,5,8,'&lt;i&gt;Această formă de&lt;/i&gt; înduplecare nu vine de la Cel ce&lt;f&gt;ⓤ&lt;/f&gt; vă cheamă.');
INSERT INTO &quot;verses&quot; VALUES (550,5,9,'Puțină&lt;f&gt;ⓥ&lt;/f&gt; drojdie&lt;f&gt;[21]&lt;/f&gt; face să dospească toată plămădeala.');
INSERT INTO &quot;verses&quot; VALUES (550,5,10,'Eu&lt;f&gt;ⓦ&lt;/f&gt; sunt încredințat în Domnul în ce vă privește că nu veți gândi altfel. Iar cel&lt;f&gt;ⓧ&lt;/f&gt; ce vă tulbură își va&lt;f&gt;ⓨ&lt;/f&gt; purta osânda, oricine ar fi el.');
INSERT INTO &quot;verses&quot; VALUES (550,5,11,'Dar eu&lt;f&gt;ⓩ&lt;/f&gt;, fraților, dacă mai propovăduiesc circumcizia, de ce mai sunt prigonit? Atunci, pricina de poticnire a crucii s-a dus&lt;f&gt;ⓐ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (550,5,12,'Și&lt;f&gt;ⓑ&lt;/f&gt; tăia-s-ar&lt;f&gt;[22]&lt;/f&gt; odată cei ce&lt;f&gt;ⓒ&lt;/f&gt; vă tulbură!');
INSERT INTO &quot;verses&quot; VALUES (550,5,13,'&lt;pb/&gt;Voi ați fost chemați la libertate, fraților. Numai nu faceți din libertate&lt;f&gt;ⓓ&lt;/f&gt; un prilej ca să trăiți pentru firea pământească, ci slujiți-vă unii altora în dragoste!');
INSERT INTO &quot;verses&quot; VALUES (550,5,14,'Căci toată&lt;f&gt;ⓔ&lt;/f&gt; Legea se cuprinde într-o singură poruncă: &lt;e&gt;Să-l&lt;/e&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;iubești pe aproapele tău ca pe tine însuți&lt;/e&gt;.&lt;f&gt;[23]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,5,15,'Dar dacă vă mușcați și vă mâncați unii pe alții, luați seama să nu vă nimiciți unii pe alții!');
INSERT INTO &quot;verses&quot; VALUES (550,5,16,'&lt;pb/&gt;Zic dar: umblați&lt;f&gt;ⓖ&lt;/f&gt; prin Duhul, și &lt;i&gt;astfel&lt;/i&gt; nu veți împlini poftele firii pământești!');
INSERT INTO &quot;verses&quot; VALUES (550,5,17,'Firea&lt;f&gt;ⓗ&lt;/f&gt; pământească poftește împotriva Duhului, iar Duhul — împotriva firii pământești, căci acestea se împotrivesc una alteia, ca&lt;f&gt;ⓘ&lt;/f&gt; să nu faceți tot ce vreți.');
INSERT INTO &quot;verses&quot; VALUES (550,5,18,'Dacă&lt;f&gt;ⓙ&lt;/f&gt; sunteți călăuziți de Duhul, nu sunteți sub Lege.');
INSERT INTO &quot;verses&quot; VALUES (550,5,19,'Și faptele&lt;f&gt;ⓚ&lt;/f&gt; firii pământești sunt cunoscute și sunt acestea: [adulterul,] desfrâul, necurăția, dezmățul,');
INSERT INTO &quot;verses&quot; VALUES (550,5,20,'închinarea la idoli, vrăjitoria, dușmăniile, cearta, ambiția egoistă&lt;f&gt;[24]&lt;/f&gt;, mâniile, intrigile, dezbinările, împărțirea în grupuri,');
INSERT INTO &quot;verses&quot; VALUES (550,5,21,'invidiile, [uciderile,] bețiile, chefurile și altele de felul acesta. Cu privire la ele vă spun mai dinainte, cum am mai spus: cei&lt;f&gt;ⓛ&lt;/f&gt; ce fac astfel de lucruri nu vor moșteni Împărăția lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (550,5,22,'Roada&lt;f&gt;ⓜ&lt;/f&gt; Duhului dimpotrivă este: dragostea, bucuria, pacea, îndelunga răbdare, bunătatea&lt;f&gt;ⓝ&lt;/f&gt;, facerea de bine, credincioșia&lt;f&gt;ⓞ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (550,5,23,'blândețea, înfrânarea. Legea nu este împotriva unor astfel de lucruri.');
INSERT INTO &quot;verses&quot; VALUES (550,5,24,'Cei ce sunt ai lui Hristos Isus și-au răstignit&lt;f&gt;ⓟ&lt;/f&gt; firea pământească împreună cu patimile și poftele ei.');
INSERT INTO &quot;verses&quot; VALUES (550,5,25,'Dacă&lt;f&gt;ⓠ&lt;/f&gt; trăim prin Duhul, să și umblăm prin Duhul!');
INSERT INTO &quot;verses&quot; VALUES (550,5,26,'Să&lt;f&gt;ⓡ&lt;/f&gt; nu umblăm după slavă deșartă, întărâtându-ne unii pe alții și fiind invidioși unii pe alții.');
INSERT INTO &quot;verses&quot; VALUES (550,6,1,'&lt;pb/&gt;Fraților, chiar dacă&lt;f&gt;ⓢ&lt;/f&gt; un om ar cădea într-o greșeală&lt;f&gt;[25]&lt;/f&gt;, voi, cei duhovnicești, să-l îndreptați cu&lt;f&gt;ⓣ&lt;/f&gt; duhul blândeții. Și ia seama la tine însuți, ca&lt;f&gt;ⓤ&lt;/f&gt; să nu fii ispitit și tu!');
INSERT INTO &quot;verses&quot; VALUES (550,6,2,'Purtați-vă&lt;f&gt;ⓥ&lt;/f&gt; poverile unii altora și veți împlini astfel legea&lt;f&gt;ⓦ&lt;/f&gt; lui Hristos!');
INSERT INTO &quot;verses&quot; VALUES (550,6,3,'Dacă&lt;f&gt;ⓧ&lt;/f&gt; vreunul crede că este cineva, cu toate că nu este nimic, se înșală.');
INSERT INTO &quot;verses&quot; VALUES (550,6,4,'Fiecare&lt;f&gt;ⓨ&lt;/f&gt; să-și cerceteze propria lucrare, și atunci va avea motiv de laudă numai în ce-l privește pe el, și nu&lt;f&gt;ⓩ&lt;/f&gt; măsurându-se cu altul;');
INSERT INTO &quot;verses&quot; VALUES (550,6,5,'căci fiecare își va purta propria lui sarcină.');
INSERT INTO &quot;verses&quot; VALUES (550,6,6,'&lt;pb/&gt;Cine&lt;f&gt;ⓐ&lt;/f&gt; primește învățătură din Cuvânt să facă parte din toate bunurile sale &lt;i&gt;și&lt;/i&gt; celui ce-l învață!');
INSERT INTO &quot;verses&quot; VALUES (550,6,7,'Nu vă înșelați: Dumnezeu nu Se lasă batjocorit! Ce&lt;f&gt;ⓑ&lt;/f&gt; seamănă omul, aceea va și secera.');
INSERT INTO &quot;verses&quot; VALUES (550,6,8,'Cine&lt;f&gt;ⓒ&lt;/f&gt; seamănă pentru firea lui pământească va secera din firea pământească putrezire, dar cine seamănă pentru Duhul va secera din Duhul viață veșnică.');
INSERT INTO &quot;verses&quot; VALUES (550,6,9,'Să&lt;f&gt;ⓓ&lt;/f&gt; fim neobosiți în facerea binelui, căci, la vremea cuvenită, vom secera, dacă&lt;f&gt;ⓔ&lt;/f&gt; nu vom cădea de oboseală.');
INSERT INTO &quot;verses&quot; VALUES (550,6,10,'Așadar, ori de câte ori avem prilejul, să&lt;f&gt;ⓕ&lt;/f&gt; le facem bine tuturor, și mai ales celor din familia&lt;f&gt;ⓖ&lt;/f&gt; credinței&lt;f&gt;[26]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (550,6,11,'&lt;pb/&gt;Priviți cu ce litere mari v-am scris, &lt;i&gt;chiar&lt;/i&gt; cu mâna mea!');
INSERT INTO &quot;verses&quot; VALUES (550,6,12,'Toți cei ce vor să facă o impresie bună când e vorba de trup vă&lt;f&gt;ⓗ&lt;/f&gt; silesc să vă circumcideți numai&lt;f&gt;ⓘ&lt;/f&gt; ca să nu fie prigoniți&lt;f&gt;ⓙ&lt;/f&gt; ei pentru crucea lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (550,6,13,'Căci nici ei, care sunt circumciși, nu păzesc Legea, dar doresc ca voi să vă circumcideți ca să se poată lăuda cu trupul vostru.');
INSERT INTO &quot;verses&quot; VALUES (550,6,14,'În ce mă privește&lt;f&gt;ⓚ&lt;/f&gt;, departe de mine gândul să mă laud cu altceva decât cu crucea Domnului nostru Isus Hristos, prin care lumea este răstignită&lt;f&gt;ⓛ&lt;/f&gt; față de mine, iar eu, față de lume!');
INSERT INTO &quot;verses&quot; VALUES (550,6,15,'Căci în&lt;f&gt;ⓜ&lt;/f&gt; Hristos Isus nici circumcizia, nici necircumcizia nu contează, ci &lt;i&gt;a fi&lt;/i&gt; o creație nouă&lt;f&gt;ⓝ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (550,6,16,'Cât despre toți cei care vor urma&lt;f&gt;ⓞ&lt;/f&gt; acest principiu&lt;f&gt;[27]&lt;/f&gt;, pace și îndurare să fie peste ei și peste Israelul&lt;f&gt;ⓟ&lt;/f&gt; lui Dumnezeu!&lt;f&gt;[28]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (550,6,17,'&lt;pb/&gt;De acum încolo nimeni să nu-mi mai facă necazuri, pentru că port&lt;f&gt;ⓠ&lt;/f&gt; semnele Domnului Isus în trupul meu!');
INSERT INTO &quot;verses&quot; VALUES (550,6,18,'&lt;pb/&gt;Harul&lt;f&gt;ⓡ&lt;/f&gt; Domnului nostru Isus Hristos să fie cu duhul vostru, fraților! Amin!');
INSERT INTO &quot;verses&quot; VALUES (560,1,1,'&lt;pb/&gt;Pavel, apostol al lui Hristos Isus prin voia lui Dumnezeu, către sfinții care sunt în Efes, către &lt;i&gt;cei&lt;/i&gt; credincioși în Hristos Isus:');
INSERT INTO &quot;verses&quot; VALUES (560,1,2,'Har și pace de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (560,1,3,'&lt;pb/&gt;Binecuvântat să fie Dumnezeu, Tatăl Domnului nostru Isus Hristos, care ne-a binecuvântat cu tot felul de binecuvântări duhovnicești în locurile cerești în Hristos,');
INSERT INTO &quot;verses&quot; VALUES (560,1,4,'fiindcă ne-a&lt;f&gt;ⓐ&lt;/f&gt; ales în El înainte&lt;f&gt;ⓑ&lt;/f&gt; de întemeierea lumii ca să fim&lt;f&gt;ⓒ&lt;/f&gt; sfinți și neprihăniți înaintea Lui, după ce, din dragoste,');
INSERT INTO &quot;verses&quot; VALUES (560,1,5,'ne-a rânduit mai&lt;f&gt;ⓓ&lt;/f&gt; dinainte pentru înfiere&lt;f&gt;ⓔ&lt;/f&gt; prin Isus Hristos, după buna plăcere a voii Sale,');
INSERT INTO &quot;verses&quot; VALUES (560,1,6,'spre lauda harului Său glorios, pe care ni l-a dăruit în Cel Preaiubit&lt;f&gt;ⓕ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (560,1,7,'În&lt;f&gt;ⓖ&lt;/f&gt; El avem răscumpărarea, prin sângele Lui, iertarea păcatelor&lt;f&gt;[1]&lt;/f&gt;, după bogăția&lt;f&gt;ⓗ&lt;/f&gt; harului Său,');
INSERT INTO &quot;verses&quot; VALUES (560,1,8,'pe care l-a răspândit din belșug peste noi. Prin înțelepciune și pricepere,');
INSERT INTO &quot;verses&quot; VALUES (560,1,9,'ne-a descoperit&lt;f&gt;ⓘ&lt;/f&gt; taina voii Sale, după buna lui plăcere de care&lt;f&gt;ⓙ&lt;/f&gt; a dat dovadă în &lt;i&gt;Hristos&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (560,1,10,'în vederea planului &lt;i&gt;arătat&lt;/i&gt; la împlinirea&lt;f&gt;ⓚ&lt;/f&gt; vremurilor, ca&lt;f&gt;ⓛ&lt;/f&gt; să unească&lt;f&gt;[2]&lt;/f&gt; în Hristos toate lucrurile&lt;f&gt;ⓜ&lt;/f&gt;: cele din ceruri și cele de pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (560,1,11,'În El&lt;f&gt;ⓝ&lt;/f&gt; am fost făcuți și moștenitori&lt;f&gt;[3]&lt;/f&gt;, fiind rânduiți mai dinainte după hotărârea Aceluia care lucrează totul după sfatul voii Sale,');
INSERT INTO &quot;verses&quot; VALUES (560,1,12,'ca&lt;f&gt;ⓞ&lt;/f&gt; să &lt;i&gt;slujim&lt;/i&gt; de laudă slavei Sale, noi, care ne-am pus mai dinainte nădejdea în Hristos.');
INSERT INTO &quot;verses&quot; VALUES (560,1,13,'În El și voi, după ce ați auzit cuvântul&lt;f&gt;ⓟ&lt;/f&gt; adevărului — Evanghelia mântuirii voastre — și ați crezut în El, ați fost pecetluiți cu&lt;f&gt;ⓠ&lt;/f&gt; Duhul Sfânt, care fusese făgăduit');
INSERT INTO &quot;verses&quot; VALUES (560,1,14,'și care este arvuna&lt;f&gt;[4]&lt;/f&gt; moștenirii noastre, pentru&lt;f&gt;ⓡ&lt;/f&gt; răscumpărarea celor câștigați &lt;i&gt;de Dumnezeu&lt;/i&gt;, spre lauda slavei Lui.');
INSERT INTO &quot;verses&quot; VALUES (560,1,15,'&lt;pb/&gt;De aceea și eu, de când am auzit&lt;f&gt;ⓢ&lt;/f&gt; despre credința în Domnul Isus care este în voi și despre dragostea voastră pentru toți sfinții,');
INSERT INTO &quot;verses&quot; VALUES (560,1,16,'nu&lt;f&gt;ⓣ&lt;/f&gt; încetez să aduc mulțumiri pentru voi când vă pomenesc în rugăciunile mele.');
INSERT INTO &quot;verses&quot; VALUES (560,1,17,'Și &lt;i&gt;mă rog&lt;/i&gt; ca Dumnezeul Domnului nostru Isus Hristos, Tatăl slavei, să&lt;f&gt;ⓤ&lt;/f&gt; vă dea un duh de înțelepciune și de descoperire în cunoașterea Lui');
INSERT INTO &quot;verses&quot; VALUES (560,1,18,'și să vă lumineze ochii&lt;f&gt;ⓥ&lt;/f&gt; inimii ca să înțelegeți care este nădejdea&lt;f&gt;ⓦ&lt;/f&gt; chemării Lui, care este bogăția moștenirii Lui glorioase în sfinți');
INSERT INTO &quot;verses&quot; VALUES (560,1,19,'și care este față de noi, credincioșii, nemărginita mărime a puterii Sale, după&lt;f&gt;ⓧ&lt;/f&gt; lucrarea marii Lui puteri&lt;f&gt;[5]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (560,1,20,'pe care a desfășurat-o în Hristos, când L-a înviat&lt;f&gt;ⓨ&lt;/f&gt; din morți și L-a pus&lt;f&gt;ⓩ&lt;/f&gt; să stea la dreapta Sa, în locurile cerești,');
INSERT INTO &quot;verses&quot; VALUES (560,1,21,'mai&lt;f&gt;ⓐ&lt;/f&gt; presus de orice căpetenie, stăpânire, putere, domnie și orice nume care se poate rosti nu doar în veacul acesta, ci și în cel viitor.');
INSERT INTO &quot;verses&quot; VALUES (560,1,22,'&lt;e&gt;Toate&lt;/e&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;I le-a pus sub picioare&lt;/e&gt;&lt;f&gt;[6]&lt;/f&gt; și L-a pus — deasupra tuturor lucrurilor — cap&lt;f&gt;ⓒ&lt;/f&gt; al Bisericii,');
INSERT INTO &quot;verses&quot; VALUES (560,1,23,'care este trupul Lui, plinătatea Celui ce&lt;f&gt;ⓓ&lt;/f&gt; împlinește totul în toți.');
INSERT INTO &quot;verses&quot; VALUES (560,2,1,'&lt;pb/&gt;Voi&lt;f&gt;ⓔ&lt;/f&gt; erați&lt;f&gt;ⓕ&lt;/f&gt; morți în abaterile și în păcatele voastre,');
INSERT INTO &quot;verses&quot; VALUES (560,2,2,'în&lt;f&gt;ⓖ&lt;/f&gt; care umblați odinioară după mersul&lt;f&gt;[7]&lt;/f&gt; lumii acesteia, după stăpânul puterii văzduhului, &lt;i&gt;anume&lt;/i&gt; duhul care lucrează acum în fiii neascultării&lt;f&gt;ⓗ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (560,2,3,'Între&lt;f&gt;ⓘ&lt;/f&gt; ei eram și noi toți odinioară, când trăiam în poftele&lt;f&gt;ⓙ&lt;/f&gt; firii noastre pământești, când împlineam voile firii pământești și ale gândurilor &lt;i&gt;noastre&lt;/i&gt; și eram din fire copii ai mâniei, ca și ceilalți.');
INSERT INTO &quot;verses&quot; VALUES (560,2,4,'Dar Dumnezeu, care este bogat&lt;f&gt;ⓚ&lt;/f&gt; în îndurare, pentru dragostea cea mare cu care ne-a iubit,');
INSERT INTO &quot;verses&quot; VALUES (560,2,5,'măcar&lt;f&gt;ⓛ&lt;/f&gt; că eram morți în abaterile noastre, ne-a&lt;f&gt;ⓜ&lt;/f&gt; adus la viață împreună cu Hristos (prin har sunteți mântuiți),');
INSERT INTO &quot;verses&quot; VALUES (560,2,6,'ne-a înălțat împreună &lt;i&gt;cu El&lt;/i&gt; și ne-a pus să stăm împreună în locurile cerești în Hristos Isus,');
INSERT INTO &quot;verses&quot; VALUES (560,2,7,'ca să arate în veacurile viitoare nemărginita bogăție a harului Său în bunătatea&lt;f&gt;ⓝ&lt;/f&gt; Lui față de noi în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (560,2,8,'Căci prin har sunteți mântuiți prin&lt;f&gt;ⓞ&lt;/f&gt; credință. Și acest lucru&lt;f&gt;[8]&lt;/f&gt; nu vine de la voi, ci este darul&lt;f&gt;ⓟ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (560,2,9,'Nu&lt;f&gt;ⓠ&lt;/f&gt; prin fapte, ca să nu se laude nimeni.');
INSERT INTO &quot;verses&quot; VALUES (560,2,10,'Căci noi suntem lucrarea&lt;f&gt;ⓡ&lt;/f&gt; Lui și am fost zidiți în Hristos Isus pentru faptele bune pe care le-a pregătit Dumnezeu mai dinainte ca să umblăm în ele.');
INSERT INTO &quot;verses&quot; VALUES (560,2,11,'&lt;pb/&gt;De aceea, voi, care altădată erați neamuri din naștere, numiți necircumciși de către cei care se cheamă circumciși și care sunt circumciși în trup&lt;f&gt;ⓢ&lt;/f&gt; de mâna omului, aduceți-vă aminte');
INSERT INTO &quot;verses&quot; VALUES (560,2,12,'că în vremea aceea&lt;f&gt;ⓣ&lt;/f&gt; erați fără Hristos, fără&lt;f&gt;ⓤ&lt;/f&gt; drept de cetățenie în Israel&lt;f&gt;[9]&lt;/f&gt;, străini de legămintele făgăduinței, fără nădejde și fără Dumnezeu în lume!');
INSERT INTO &quot;verses&quot; VALUES (560,2,13,'Dar acum&lt;f&gt;ⓥ&lt;/f&gt;, în Hristos Isus, voi, care odinioară erați departe, ați ajuns aproape prin sângele lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (560,2,14,'Căci El&lt;f&gt;ⓦ&lt;/f&gt; este pacea noastră, care din doi&lt;f&gt;[10]&lt;/f&gt; a&lt;f&gt;ⓧ&lt;/f&gt; făcut unul, după ce a surpat&lt;f&gt;ⓨ&lt;/f&gt;, prin trupul Lui, zidul vrăjmășiei care era la mijloc și care ne despărțea.');
INSERT INTO &quot;verses&quot; VALUES (560,2,15,'El a nimicit Legea poruncilor cu rânduielile ei, pentru ca din cei doi să creeze în El Însuși un singur om&lt;f&gt;ⓩ&lt;/f&gt; nou, făcând astfel pace,');
INSERT INTO &quot;verses&quot; VALUES (560,2,16,'și să îi împace&lt;f&gt;ⓐ&lt;/f&gt; pe amândoi cu Dumnezeu într-un singur trup, prin cruce, după ce a nimicit vrăjmășia prin ea.');
INSERT INTO &quot;verses&quot; VALUES (560,2,17,'El a venit și a vestit pace pentru voi, cei de departe, și pace pentru cei de aproape.');
INSERT INTO &quot;verses&quot; VALUES (560,2,18,'Căci prin&lt;f&gt;ⓑ&lt;/f&gt; El și unii, și alții avem intrare la Tatăl într-un Duh.');
INSERT INTO &quot;verses&quot; VALUES (560,2,19,'Așadar, voi nu mai sunteți străini sau oaspeți&lt;f&gt;[11]&lt;/f&gt;, ci sunteți cetățeni&lt;f&gt;ⓒ&lt;/f&gt; împreună cu sfinții, oameni din casa lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (560,2,20,'fiind zidiți&lt;f&gt;ⓓ&lt;/f&gt; pe temelia apostolilor și a profeților, piatra&lt;f&gt;ⓔ&lt;/f&gt; din capul unghiului fiind Însuși Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (560,2,21,'În El toată clădirea, bine închegată, crește ca să fie un templu&lt;f&gt;ⓕ&lt;/f&gt; sfânt în Domnul,');
INSERT INTO &quot;verses&quot; VALUES (560,2,22,'prin care sunteți zidiți împreună ca locaș al lui Dumnezeu prin Duhul.');
INSERT INTO &quot;verses&quot; VALUES (560,3,1,'&lt;pb/&gt;Iată de ce eu, Pavel, întemnițatul&lt;f&gt;ⓖ&lt;/f&gt; lui Hristos Isus pentru voi, neamurile…');
INSERT INTO &quot;verses&quot; VALUES (560,3,2,'(căci veți fi auzit de isprăvnicia&lt;f&gt;ⓗ&lt;/f&gt; harului lui Dumnezeu care mi-a fost dată pentru voi,');
INSERT INTO &quot;verses&quot; VALUES (560,3,3,'anume că prin&lt;f&gt;ⓘ&lt;/f&gt; revelație&lt;f&gt;ⓙ&lt;/f&gt; mi-a&lt;f&gt;ⓚ&lt;/f&gt; fost descoperită &lt;i&gt;această&lt;/i&gt; taină, după cum v-am scris în puține cuvinte.');
INSERT INTO &quot;verses&quot; VALUES (560,3,4,'Citindu-le, puteți vedea cum înțeleg eu taina lui Hristos,');
INSERT INTO &quot;verses&quot; VALUES (560,3,5,'care în celelalte generații nu le-a fost descoperită fiilor oamenilor așa cum&lt;f&gt;ⓛ&lt;/f&gt; le-a fost revelată în prezent sfinților Lui apostoli și profeți prin Duhul:');
INSERT INTO &quot;verses&quot; VALUES (560,3,6,'și anume că neamurile sunt&lt;f&gt;ⓜ&lt;/f&gt; moștenitoare împreună &lt;i&gt;cu noi&lt;/i&gt;, parte din același trup și părtașe&lt;f&gt;ⓝ&lt;/f&gt; la aceeași făgăduință în Hristos Isus prin Evanghelie,');
INSERT INTO &quot;verses&quot; VALUES (560,3,7,'al cărei&lt;f&gt;ⓞ&lt;/f&gt; slujitor am fost făcut eu după darul harului lui Dumnezeu, dat mie prin lucrarea puterii Lui.');
INSERT INTO &quot;verses&quot; VALUES (560,3,8,'&lt;pb/&gt;Mie&lt;f&gt;ⓟ&lt;/f&gt;, celui mai neînsemnat dintre toți sfinții, mi-a fost dat harul acesta: să&lt;f&gt;ⓠ&lt;/f&gt; vestesc neamurilor bogățiile nepătrunse ale lui Hristos');
INSERT INTO &quot;verses&quot; VALUES (560,3,9,'și să-i luminez pe toți cu privire la isprăvnicia tainei&lt;f&gt;ⓡ&lt;/f&gt; ascunse&lt;f&gt;ⓢ&lt;/f&gt; din veacuri în Dumnezeu, care&lt;f&gt;ⓣ&lt;/f&gt; a creat toate lucrurile,');
INSERT INTO &quot;verses&quot; VALUES (560,3,10,'pentru ca înțelepciunea nespus de felurită a lui Dumnezeu să&lt;f&gt;ⓤ&lt;/f&gt; fie cunoscută acum, prin Biserică, de către căpeteniile și stăpânirile&lt;f&gt;ⓥ&lt;/f&gt; din locurile cerești,');
INSERT INTO &quot;verses&quot; VALUES (560,3,11,'după&lt;f&gt;ⓦ&lt;/f&gt; planul de veacuri pe care l-a făcut în Hristos Isus, Domnul nostru.');
INSERT INTO &quot;verses&quot; VALUES (560,3,12,'În El avem îndrăzneală și intrare&lt;f&gt;ⓧ&lt;/f&gt; cu&lt;f&gt;ⓨ&lt;/f&gt; încredere, prin credința în El.');
INSERT INTO &quot;verses&quot; VALUES (560,3,13,'De aceea, vă rog&lt;f&gt;ⓩ&lt;/f&gt; să nu vă pierdeți cumpătul din pricina necazurilor mele pentru voi: aceasta este slava voastră.)');
INSERT INTO &quot;verses&quot; VALUES (560,3,14,'&lt;pb/&gt;… Iată, &lt;i&gt;așadar,&lt;/i&gt; de ce&lt;f&gt;[12]&lt;/f&gt; îmi plec genunchii înaintea Tatălui [Domnului nostru Isus Hristos],');
INSERT INTO &quot;verses&quot; VALUES (560,3,15,'din care își trage numele orice&lt;f&gt;ⓐ&lt;/f&gt; familie în ceruri și pe pământ,');
INSERT INTO &quot;verses&quot; VALUES (560,3,16,'&lt;i&gt;și-L rog&lt;/i&gt; ca, potrivit cu&lt;f&gt;ⓑ&lt;/f&gt; bogăția slavei Sale, să vă facă să vă întăriți în putere, prin Duhul Lui, în omul&lt;f&gt;ⓒ&lt;/f&gt; dinăuntru,');
INSERT INTO &quot;verses&quot; VALUES (560,3,17,'așa încât&lt;f&gt;ⓓ&lt;/f&gt; Hristos să locuiască în inimile voastre prin credință; pentru ca&lt;f&gt;ⓔ&lt;/f&gt;, fiind înrădăcinați și întemeiați în dragoste,');
INSERT INTO &quot;verses&quot; VALUES (560,3,18,'să&lt;f&gt;ⓕ&lt;/f&gt; puteți pricepe împreună cu toți sfinții care&lt;f&gt;ⓖ&lt;/f&gt; sunt lărgimea, lungimea, înălțimea și adâncimea');
INSERT INTO &quot;verses&quot; VALUES (560,3,19,'și să cunoașteți dragostea lui Hristos, care este mai presus de cunoaștere, ca să ajungeți plini de&lt;f&gt;ⓗ&lt;/f&gt; toată plinătatea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (560,3,20,'&lt;pb/&gt;Iar a Aceluia&lt;f&gt;ⓘ&lt;/f&gt; care poate să facă nespus&lt;f&gt;ⓙ&lt;/f&gt; mai mult decât cerem sau gândim noi, potrivit&lt;f&gt;ⓚ&lt;/f&gt; puterii care lucrează în noi,');
INSERT INTO &quot;verses&quot; VALUES (560,3,21,'a Lui&lt;f&gt;ⓛ&lt;/f&gt; să fie slava în Biserică și în Hristos Isus, din neam în neam, în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (560,4,1,'&lt;pb/&gt;Vă sfătuiesc deci eu, cel întemnițat pentru Domnul, să vă purtați&lt;f&gt;ⓜ&lt;/f&gt; într-un chip vrednic de chemarea pe care ați primit-o,');
INSERT INTO &quot;verses&quot; VALUES (560,4,2,'cu&lt;f&gt;ⓝ&lt;/f&gt; toată smerenia și blândețea, cu îndelungă răbdare; îngăduiți-vă unii pe alții în dragoste');
INSERT INTO &quot;verses&quot; VALUES (560,4,3,'și căutați să păstrați unitatea Duhului prin legătura păcii&lt;f&gt;ⓞ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (560,4,4,'&lt;pb/&gt;Este un&lt;f&gt;ⓟ&lt;/f&gt; singur trup și un singur Duh, după cum și voi ați fost chemați la o singură nădejde a chemării voastre.');
INSERT INTO &quot;verses&quot; VALUES (560,4,5,'&lt;pb/&gt;Este un&lt;f&gt;ⓠ&lt;/f&gt; singur Domn, o&lt;f&gt;ⓡ&lt;/f&gt; singură credință, un singur botez.');
INSERT INTO &quot;verses&quot; VALUES (560,4,6,'&lt;pb/&gt;Este un&lt;f&gt;ⓢ&lt;/f&gt; singur Dumnezeu și Tată al tuturor&lt;f&gt;ⓣ&lt;/f&gt;, care este mai presus de toți, prin toți și în toți.');
INSERT INTO &quot;verses&quot; VALUES (560,4,7,'&lt;pb/&gt;Dar fiecăruia&lt;f&gt;ⓤ&lt;/f&gt; dintre noi harul i-a fost dat după măsura darului lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (560,4,8,'De aceea se spune: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;S-a suit&lt;/e&gt; &lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;în înălțimi, a luat robia roabă,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;a dat daruri oamenilor.&lt;/e&gt; &lt;f&gt;[13]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (560,4,9,'&lt;pb/&gt;Și&lt;f&gt;ⓦ&lt;/f&gt; acest &lt;e&gt;S-a suit&lt;/e&gt; ce înseamnă decât că, înainte, S-a coborât în părțile mai de jos ale pământului?');
INSERT INTO &quot;verses&quot; VALUES (560,4,10,'Cel ce S-a coborât este același cu Cel ce &lt;e&gt;S-a suit&lt;/e&gt;&lt;f&gt;ⓧ&lt;/f&gt; mai presus de toate cerurile, ca să umple toate lucrurile.');
INSERT INTO &quot;verses&quot; VALUES (560,4,11,'Și El&lt;f&gt;ⓨ&lt;/f&gt; i-a dat pe unii apostoli, pe alții profeți, pe alții evangheliști, pe alții păstori&lt;f&gt;ⓩ&lt;/f&gt; și învățători,');
INSERT INTO &quot;verses&quot; VALUES (560,4,12,'pentru&lt;f&gt;ⓐ&lt;/f&gt; pregătirea sfinților, în vederea lucrării de slujire, pentru zidirea trupului lui Hristos,');
INSERT INTO &quot;verses&quot; VALUES (560,4,13,'până vom ajunge toți la unitatea credinței și a cunoașterii Fiului lui Dumnezeu, la starea de om mare&lt;f&gt;ⓑ&lt;/f&gt;, la înălțimea staturii plinătății lui Hristos,');
INSERT INTO &quot;verses&quot; VALUES (560,4,14,'ca să nu mai fim copii&lt;f&gt;ⓒ&lt;/f&gt;, plutind încoace și încolo, purtați de orice vânt de învățătură, prin viclenia oamenilor și prin șiretenia lor în&lt;f&gt;ⓓ&lt;/f&gt; mijloacele de amăgire.');
INSERT INTO &quot;verses&quot; VALUES (560,4,15,'Dimpotrivă, să spunem&lt;f&gt;ⓔ&lt;/f&gt; adevărul în dragoste și să creștem în El în toate privințele. El&lt;f&gt;ⓕ&lt;/f&gt;, Hristos, este Capul');
INSERT INTO &quot;verses&quot; VALUES (560,4,16,'din care&lt;f&gt;ⓖ&lt;/f&gt; tot trupul, bine închegat și strâns legat prin toate încheieturile care îl susțin, își primește creșterea potrivit cu lucrarea fiecărei părți în măsura ei și se zidește în dragoste.');
INSERT INTO &quot;verses&quot; VALUES (560,4,17,'&lt;pb/&gt;De aceea vă spun și vă îndemn în Domnul: nu&lt;f&gt;ⓗ&lt;/f&gt; mai trăiți cum trăiesc păgânii, în deșertăciunea gândurilor lor,');
INSERT INTO &quot;verses&quot; VALUES (560,4,18,'având mintea întunecată&lt;f&gt;ⓘ&lt;/f&gt; și fiind străini&lt;f&gt;ⓙ&lt;/f&gt; de viața cu Dumnezeu, din pricina neștiinței în care se află și din pricina împietririi inimii lor!');
INSERT INTO &quot;verses&quot; VALUES (560,4,19,'Ei&lt;f&gt;ⓚ&lt;/f&gt; și-au pierdut orice pic de simțire, s-au&lt;f&gt;ⓛ&lt;/f&gt; dedat la dezmăț și săvârșesc cu lăcomie tot soiul de necurății.');
INSERT INTO &quot;verses&quot; VALUES (560,4,20,'&lt;pb/&gt;Voi însă nu așa ați învățat despre Hristos,');
INSERT INTO &quot;verses&quot; VALUES (560,4,21,'dacă, într-adevăr, L-ați ascultat și dacă ați fost învățați de El, potrivit adevărului care este în Isus.');
INSERT INTO &quot;verses&quot; VALUES (560,4,22,'În ce privește viața voastră din trecut, să&lt;f&gt;ⓜ&lt;/f&gt; vă dezbrăcați de omul&lt;f&gt;ⓝ&lt;/f&gt; cel vechi, care se strică după&lt;f&gt;ⓞ&lt;/f&gt; poftele înșelătoare,');
INSERT INTO &quot;verses&quot; VALUES (560,4,23,'să vă înnoiți&lt;f&gt;ⓟ&lt;/f&gt; în duhul minții voastre');
INSERT INTO &quot;verses&quot; VALUES (560,4,24,'și să&lt;f&gt;ⓠ&lt;/f&gt; vă îmbrăcați în omul cel nou, creat&lt;f&gt;ⓡ&lt;/f&gt; după &lt;i&gt;chipul lui&lt;/i&gt; Dumnezeu în dreptatea și sfințenia care vin din adevăr.');
INSERT INTO &quot;verses&quot; VALUES (560,4,25,'&lt;pb/&gt;De aceea, lăsați-vă de minciună: &lt;e&gt;fiecare dintre voi să spună&lt;/e&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;aproapelui său adevărul&lt;/e&gt;&lt;f&gt;[14]&lt;/f&gt;, pentru că suntem&lt;f&gt;ⓣ&lt;/f&gt; mădulare unii altora!');
INSERT INTO &quot;verses&quot; VALUES (560,4,26,'&lt;e&gt;Mâniați-vă și nu păcătuiți!&lt;/e&gt;&lt;f&gt;[15]&lt;/f&gt; Să n-apună soarele peste mânia voastră');
INSERT INTO &quot;verses&quot; VALUES (560,4,27,'și să nu-i&lt;f&gt;ⓤ&lt;/f&gt; dați prilej Diavolului!');
INSERT INTO &quot;verses&quot; VALUES (560,4,28,'Cine fura să nu mai fure, ci mai degrabă să lucreze&lt;f&gt;ⓥ&lt;/f&gt; cu mâinile lui ce este bine, ca să aibă ce să împartă cu cel&lt;f&gt;ⓦ&lt;/f&gt; lipsit!');
INSERT INTO &quot;verses&quot; VALUES (560,4,29,'Niciun&lt;f&gt;ⓧ&lt;/f&gt; cuvânt stricat să nu vă iasă din gură, ci unul&lt;f&gt;ⓨ&lt;/f&gt; bun, pentru&lt;f&gt;ⓩ&lt;/f&gt; zidire, după cum e nevoie, ca să le dea har celor ce-l aud!');
INSERT INTO &quot;verses&quot; VALUES (560,4,30,'Să&lt;f&gt;ⓐ&lt;/f&gt; nu-L întristați pe Duhul Sfânt al lui Dumnezeu, prin&lt;f&gt;ⓑ&lt;/f&gt; care ați fost pecetluiți pentru ziua răscumpărării&lt;f&gt;ⓒ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (560,4,31,'Orice&lt;f&gt;ⓓ&lt;/f&gt; amărăciune, orice izbucnire, orice mânie, orice strigare, orice defăimare&lt;f&gt;ⓔ&lt;/f&gt; să piară din mijlocul vostru, împreună cu orice fel de răutate!');
INSERT INTO &quot;verses&quot; VALUES (560,4,32,'Dimpotrivă, fiți&lt;f&gt;ⓕ&lt;/f&gt; buni și miloși unii cu alții; iertați-vă&lt;f&gt;ⓖ&lt;/f&gt; unul pe altul, după cum v-a iertat și Dumnezeu pe voi în Hristos!');
INSERT INTO &quot;verses&quot; VALUES (560,5,1,'&lt;pb/&gt;Urmați&lt;f&gt;ⓗ&lt;/f&gt; deci pilda lui Dumnezeu ca niște copii preaiubiți!');
INSERT INTO &quot;verses&quot; VALUES (560,5,2,'Trăiți&lt;f&gt;ⓘ&lt;/f&gt; în dragoste, după cum&lt;f&gt;ⓙ&lt;/f&gt; și Hristos ne-a iubit și S-a dat pe Sine pentru noi ca ofrandă și ca jertfă de un miros plăcut lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (560,5,3,'&lt;pb/&gt;Desfrânarea&lt;f&gt;ⓚ&lt;/f&gt;, necurăția de orice fel sau lăcomia nici să nu fie pomenite între voi, așa cum se cuvine unor sfinți!');
INSERT INTO &quot;verses&quot; VALUES (560,5,4,'Să nu se audă nici&lt;f&gt;ⓛ&lt;/f&gt; vorbe de rușine, vorbe nechibzuite, glume proaste, &lt;i&gt;lucruri&lt;/i&gt; care nu se cuvin, ci mai degrabă cuvinte de mulțumire!');
INSERT INTO &quot;verses&quot; VALUES (560,5,5,'Căci știți bine&lt;f&gt;ⓜ&lt;/f&gt; că niciun &lt;i&gt;om&lt;/i&gt; desfrânat, necurat sau lacom, care este un închinător la idoli&lt;f&gt;ⓝ&lt;/f&gt;, nu are&lt;f&gt;ⓞ&lt;/f&gt; parte de moștenire în Împărăția lui Hristos și a lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (560,5,6,'&lt;pb/&gt;Nimeni&lt;f&gt;ⓟ&lt;/f&gt; să nu vă înșele cu vorbe deșarte, căci din pricina acestor lucruri vine&lt;f&gt;ⓠ&lt;/f&gt; mânia lui Dumnezeu peste fiii neascultării!');
INSERT INTO &quot;verses&quot; VALUES (560,5,7,'Așadar, să nu vă întovărășiți deloc cu ei!');
INSERT INTO &quot;verses&quot; VALUES (560,5,8,'Odinioară erați&lt;f&gt;ⓡ&lt;/f&gt; întuneric, dar acum sunteți&lt;f&gt;ⓢ&lt;/f&gt; lumină în Domnul. Umblați deci ca niște copii ai luminii,');
INSERT INTO &quot;verses&quot; VALUES (560,5,9,'căci rodul luminii stă în orice bunătate, în dreptate și în adevăr!');
INSERT INTO &quot;verses&quot; VALUES (560,5,10,'Cercetați&lt;f&gt;ⓣ&lt;/f&gt; ce este plăcut înaintea Domnului');
INSERT INTO &quot;verses&quot; VALUES (560,5,11,'și nu&lt;f&gt;ⓤ&lt;/f&gt; luați nicidecum parte la lucrările&lt;f&gt;ⓥ&lt;/f&gt; neroditoare ale întunericului, ci mai degrabă osândiți-le!');
INSERT INTO &quot;verses&quot; VALUES (560,5,12,'Căci ar fi rușinos fie și numai să spunem ce fac ei în ascuns.');
INSERT INTO &quot;verses&quot; VALUES (560,5,13,'Dar toate&lt;f&gt;ⓦ&lt;/f&gt; aceste lucruri, când sunt osândite de lumină, sunt date la iveală; pentru că ceea ce scoate totul la iveală este lumina.');
INSERT INTO &quot;verses&quot; VALUES (560,5,14,'De aceea zice &lt;i&gt;Scriptura&lt;/i&gt;: &lt;pb/&gt;&lt;t&gt;„Deșteaptă-te&lt;f&gt;ⓧ&lt;/f&gt;, tu, care dormi,&lt;/t&gt; &lt;t&gt;scoală-te&lt;f&gt;ⓨ&lt;/f&gt; din morți,&lt;/t&gt; &lt;t&gt;și Hristos te va lumina!”&lt;f&gt;[16]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (560,5,15,'&lt;pb/&gt;Luați deci bine seama la felul în care umblați, nu ca niște neînțelepți, ci ca niște înțelepți!');
INSERT INTO &quot;verses&quot; VALUES (560,5,16,'Răscumpărați&lt;f&gt;ⓩ&lt;/f&gt; vremea, căci zilele sunt rele!');
INSERT INTO &quot;verses&quot; VALUES (560,5,17,'De aceea nu fiți nepricepuți, ci înțelegeți&lt;f&gt;ⓐ&lt;/f&gt; care&lt;f&gt;ⓑ&lt;/f&gt; este voia Domnului!');
INSERT INTO &quot;verses&quot; VALUES (560,5,18,'Nu&lt;f&gt;ⓒ&lt;/f&gt; vă îmbătați de vin, care duce la destrăbălare! Dimpotrivă, fiți plini de Duh!');
INSERT INTO &quot;verses&quot; VALUES (560,5,19,'Vorbiți între voi cu&lt;f&gt;ⓓ&lt;/f&gt; psalmi, cântări de laudă și cântări duhovnicești, cântând și lăudându-L pe Domnul din inimă!');
INSERT INTO &quot;verses&quot; VALUES (560,5,20,'Mulțumiți-I&lt;f&gt;ⓔ&lt;/f&gt; întotdeauna lui Dumnezeu Tatăl pentru toate lucrurile în Numele Domnului nostru Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (560,5,21,'&lt;pb/&gt;Supuneți-vă&lt;f&gt;ⓕ&lt;/f&gt; unii altora în frica lui Hristos!');
INSERT INTO &quot;verses&quot; VALUES (560,5,22,'Soțiilor&lt;f&gt;ⓖ&lt;/f&gt;, fiți supuse soților voștri ca Domnului,');
INSERT INTO &quot;verses&quot; VALUES (560,5,23,'căci soțul&lt;f&gt;ⓗ&lt;/f&gt; este capul soției, după cum și Hristos&lt;f&gt;ⓘ&lt;/f&gt; este capul Bisericii, El, Mântuitorul trupului&lt;f&gt;[17]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (560,5,24,'După cum Biserica Îi este supusă lui Hristos, tot astfel și soțiile să le fie supuse soților lor în toate privințele!');
INSERT INTO &quot;verses&quot; VALUES (560,5,25,'&lt;pb/&gt;Soților&lt;f&gt;ⓙ&lt;/f&gt;, iubiți-vă soțiile așa cum a iubit Hristos Biserica și S-a&lt;f&gt;ⓚ&lt;/f&gt; dat pe Sine pentru ea');
INSERT INTO &quot;verses&quot; VALUES (560,5,26,'ca s-o sfințească, după ce a curățit-o prin&lt;f&gt;ⓛ&lt;/f&gt; spălarea în apă, prin cuvânt&lt;f&gt;ⓜ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (560,5,27,'ca&lt;f&gt;ⓝ&lt;/f&gt; să înfățișeze pentru Sine această Biserică slăvită, fără pată, fără zbârcitură sau altceva de felul acesta, ci sfântă și fără prihană!');
INSERT INTO &quot;verses&quot; VALUES (560,5,28,'Tot astfel trebuie să-și iubească și soții soțiile, ca pe trupurile lor. Cine își iubește soția se iubește pe sine.');
INSERT INTO &quot;verses&quot; VALUES (560,5,29,'Căci nimeni nu și-a urât vreodată trupul, ci îl hrănește și îl îngrijește cu drag, precum Hristos Biserica;');
INSERT INTO &quot;verses&quot; VALUES (560,5,30,'pentru că&lt;f&gt;ⓞ&lt;/f&gt; suntem mădulare ale trupului Lui, [&lt;e&gt;carne din carnea Lui și os din oasele Lui&lt;/e&gt;&lt;f&gt;[18]&lt;/f&gt;].');
INSERT INTO &quot;verses&quot; VALUES (560,5,31,'&lt;e&gt;De aceea&lt;/e&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;va lăsa omul pe tatăl său și pe mama sa și se va lipi de nevasta sa, și cei doi&lt;/e&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;vor fi un singur trup&lt;/e&gt;.&lt;f&gt;[19]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (560,5,32,'Taina aceasta este mare (vorbesc despre Hristos și Biserică).');
INSERT INTO &quot;verses&quot; VALUES (560,5,33,'Așadar, fiecare din&lt;f&gt;ⓡ&lt;/f&gt; voi să-și iubească soția ca pe sine; și soția să-și&lt;f&gt;ⓢ&lt;/f&gt; respecte soțul!');
INSERT INTO &quot;verses&quot; VALUES (560,6,1,'&lt;pb/&gt;Copii, ascultați&lt;f&gt;ⓣ&lt;/f&gt; în Domnul de părinții voștri, căci este drept!');
INSERT INTO &quot;verses&quot; VALUES (560,6,2,'&lt;e&gt;Cinstește-i&lt;/e&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;pe tatăl tău și pe mama ta&lt;/e&gt; — este cea dintâi poruncă însoțită de o făgăduință —');
INSERT INTO &quot;verses&quot; VALUES (560,6,3,'&lt;e&gt;ca să-ți fie bine&lt;/e&gt;&lt;f&gt;[20]&lt;/f&gt; &lt;e&gt;și să trăiești multă vreme pe pământ.&lt;/e&gt;&lt;f&gt;[21]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (560,6,4,'Și voi, părinților&lt;f&gt;ⓥ&lt;/f&gt;, nu-i întărâtați&lt;f&gt;[22]&lt;/f&gt; la mânie pe copiii voștri, ci creșteți-i&lt;f&gt;ⓦ&lt;/f&gt; în disciplina și învățătura Domnului!');
INSERT INTO &quot;verses&quot; VALUES (560,6,5,'&lt;pb/&gt;Robilor&lt;f&gt;ⓧ&lt;/f&gt;, ascultați de stăpânii voștri pământești cu&lt;f&gt;ⓨ&lt;/f&gt; frică și cutremur, cu o inimă sinceră, ca de Hristos!');
INSERT INTO &quot;verses&quot; VALUES (560,6,6,'Slujiți-le nu&lt;f&gt;ⓩ&lt;/f&gt; numai când sunteți sub ochii lor, precum cei ce caută să le fie pe plac oamenilor, ci ca niște robi ai lui Hristos, care fac din &lt;i&gt;tot&lt;/i&gt; sufletul voia lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (560,6,7,'Slujiți-le cu tragere de inimă&lt;f&gt;[23]&lt;/f&gt;, ca pentru Domnul, nu ca pentru oameni,');
INSERT INTO &quot;verses&quot; VALUES (560,6,8,'căci știți&lt;f&gt;ⓐ&lt;/f&gt; că fiecare, rob sau liber, își va primi răsplata de la Domnul după binele pe care l-a făcut!');
INSERT INTO &quot;verses&quot; VALUES (560,6,9,'Și voi, stăpânilor&lt;f&gt;ⓑ&lt;/f&gt;, purtați-vă la fel cu ei; lăsați amenințările&lt;f&gt;ⓒ&lt;/f&gt;, ca unii care știți că Stăpânul — atât al lor, cât și al vostru — este în cer și&lt;f&gt;ⓓ&lt;/f&gt; că înaintea Lui nu se are în vedere fața omului!');
INSERT INTO &quot;verses&quot; VALUES (560,6,10,'&lt;pb/&gt;Încolo, [fraților,] întăriți-vă în Domnul și în puterea tăriei Lui!');
INSERT INTO &quot;verses&quot; VALUES (560,6,11,'Îmbrăcați-vă&lt;f&gt;ⓔ&lt;/f&gt; cu toată armura lui Dumnezeu, ca să puteți rezista împotriva uneltirilor Diavolului!');
INSERT INTO &quot;verses&quot; VALUES (560,6,12,'Căci lupta noastră nu este împotriva cărnii și a sângelui, ci împotriva căpeteniilor&lt;f&gt;ⓕ&lt;/f&gt;, a stăpânirilor, a conducătorilor&lt;f&gt;ⓖ&lt;/f&gt; veacului&lt;f&gt;[24]&lt;/f&gt; întunecat de acum, a duhurilor&lt;f&gt;[25]&lt;/f&gt; răutății care sunt în locurile cerești.');
INSERT INTO &quot;verses&quot; VALUES (560,6,13,'De aceea&lt;f&gt;ⓗ&lt;/f&gt;, luați toată armura lui Dumnezeu, ca să vă puteți împotrivi în ziua cea rea și să rămâneți în picioare după ce veți fi biruit totul!');
INSERT INTO &quot;verses&quot; VALUES (560,6,14,'Stați gata având mijlocul încins&lt;f&gt;ⓘ&lt;/f&gt; cu adevărul, &lt;i&gt;fiind&lt;/i&gt; îmbrăcați&lt;f&gt;ⓙ&lt;/f&gt; cu platoșa dreptății');
INSERT INTO &quot;verses&quot; VALUES (560,6,15,'și încălțați&lt;f&gt;ⓚ&lt;/f&gt; cu râvna Evangheliei păcii!');
INSERT INTO &quot;verses&quot; VALUES (560,6,16,'Peste toate acestea, luați scutul credinței, cu care veți putea stinge toate săgețile aprinse ale Celui Rău!');
INSERT INTO &quot;verses&quot; VALUES (560,6,17,'Luați&lt;f&gt;ⓛ&lt;/f&gt; și coiful mântuirii și sabia&lt;f&gt;ⓜ&lt;/f&gt; Duhului, care este Cuvântul lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (560,6,18,'Faceți în toată vremea, prin Duhul, tot felul de rugăciuni&lt;f&gt;ⓝ&lt;/f&gt; și cereri! Vegheați la aceasta cu toată stăruința și cu rugăciune pentru toți sfinții');
INSERT INTO &quot;verses&quot; VALUES (560,6,19,'și pentru&lt;f&gt;ⓞ&lt;/f&gt; mine ca, ori de câte ori îmi deschid gura, să mi se dea cuvânt ca să fac cunoscută cu îndrăzneală taina Evangheliei,');
INSERT INTO &quot;verses&quot; VALUES (560,6,20,'al cărei sol în&lt;f&gt;ⓟ&lt;/f&gt; lanțuri sunt&lt;f&gt;ⓠ&lt;/f&gt;, și astfel să vorbesc despre ea cu&lt;f&gt;ⓡ&lt;/f&gt; îndrăzneală, cum trebuie să vorbesc!');
INSERT INTO &quot;verses&quot; VALUES (560,6,21,'&lt;pb/&gt;Ca&lt;f&gt;ⓢ&lt;/f&gt; să aflați și voi &lt;i&gt;vești&lt;/i&gt; cu privire la mine (anume ce mai fac), Tihic&lt;f&gt;ⓣ&lt;/f&gt;, frate preaiubit și slujitor credincios în Domnul, vă va face cunoscut totul.');
INSERT INTO &quot;verses&quot; VALUES (560,6,22,'L-am&lt;f&gt;ⓤ&lt;/f&gt; trimis la voi tocmai în acest scop: ca să aflați vești despre starea noastră și să vă aducă mângâiere în inimi.');
INSERT INTO &quot;verses&quot; VALUES (560,6,23,'&lt;pb/&gt;Pace&lt;f&gt;ⓥ&lt;/f&gt; &lt;i&gt;tuturor&lt;/i&gt; fraților și dragoste împreună cu credință de la Dumnezeu Tatăl și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (560,6,24,'Harul să fie cu toți cei ce-L iubesc pe Domnul nostru Isus Hristos&lt;f&gt;ⓦ&lt;/f&gt; cu o &lt;i&gt;dragoste&lt;/i&gt; nepieritoare! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (570,1,1,'&lt;pb/&gt;Pavel și Timotei, robi ai lui Hristos Isus, către toți sfinții în&lt;f&gt;ⓐ&lt;/f&gt; Hristos Isus care sunt în Filipi, împreună cu episcopii&lt;f&gt;[1]&lt;/f&gt; și diaconii:');
INSERT INTO &quot;verses&quot; VALUES (570,1,2,'Har&lt;f&gt;ⓑ&lt;/f&gt; și pace vouă de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (570,1,3,'&lt;pb/&gt;Îi mulțumesc&lt;f&gt;ⓒ&lt;/f&gt; Dumnezeului meu — ori de câte ori îmi aduc aminte de voi');
INSERT INTO &quot;verses&quot; VALUES (570,1,4,'în toate rugăciunile pe care le fac mereu cu bucurie pentru voi toți —');
INSERT INTO &quot;verses&quot; VALUES (570,1,5,'pentru&lt;f&gt;ⓓ&lt;/f&gt; participarea voastră la &lt;i&gt;vestirea&lt;/i&gt; Evangheliei din prima zi până acum.');
INSERT INTO &quot;verses&quot; VALUES (570,1,6,'Sunt încredințat că Acela care a început în voi o lucrare&lt;f&gt;ⓔ&lt;/f&gt; bună o va desăvârși până&lt;f&gt;ⓕ&lt;/f&gt; în ziua lui Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (570,1,7,'Este drept să gândesc astfel despre voi toți, căci vă port în inima mea, fiindcă atât în lanțurile&lt;f&gt;ⓖ&lt;/f&gt; mele, cât și în apărarea&lt;f&gt;ⓗ&lt;/f&gt; și întărirea Evangheliei&lt;f&gt;ⓘ&lt;/f&gt; sunteți&lt;f&gt;ⓙ&lt;/f&gt; toți părtași cu mine la &lt;i&gt;același&lt;/i&gt; har.');
INSERT INTO &quot;verses&quot; VALUES (570,1,8,'Martor îmi este Dumnezeu&lt;f&gt;ⓚ&lt;/f&gt; că&lt;f&gt;ⓛ&lt;/f&gt; am față de voi un dor plin de iubirea lui Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (570,1,9,'Și mă rog ca&lt;f&gt;ⓜ&lt;/f&gt; dragostea voastră să sporească tot mai mult, însoțită de cunoștință și orice pricepere,');
INSERT INTO &quot;verses&quot; VALUES (570,1,10,'ca să&lt;f&gt;ⓝ&lt;/f&gt; deosebiți lucrurile vrednice de laudă și &lt;i&gt;astfel&lt;/i&gt;&lt;f&gt;ⓞ&lt;/f&gt; să fiți curați și fără pată în&lt;f&gt;ⓟ&lt;/f&gt; ziua lui Hristos,');
INSERT INTO &quot;verses&quot; VALUES (570,1,11,'plini de rodul dreptății care &lt;i&gt;vine&lt;/i&gt; prin&lt;f&gt;ⓠ&lt;/f&gt; Isus Hristos, spre slava&lt;f&gt;ⓡ&lt;/f&gt; și lauda lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (570,1,12,'&lt;pb/&gt;Vreau să știți, fraților, că împrejurările în care mă găsesc au dus mai degrabă la înaintarea Evangheliei,');
INSERT INTO &quot;verses&quot; VALUES (570,1,13,'așa încât, în&lt;f&gt;ⓢ&lt;/f&gt; tot pretoriul și pentru toți ceilalți, este limpede că sunt pus în lanțuri pentru Hristos,');
INSERT INTO &quot;verses&quot; VALUES (570,1,14,'iar cei mai mulți frați, din pricina lanțurilor mele, și-au pus încrederea în Domnul și au mai multă îndrăzneală să vestească fără teamă Cuvântul &lt;i&gt;lui Dumnezeu&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (570,1,15,'În timp ce unii Îl propovăduiesc pe Hristos din invidie și din duh de ceartă&lt;f&gt;ⓣ&lt;/f&gt;, alții &lt;i&gt;Îl propovăduiesc&lt;/i&gt; din bunăvoință.');
INSERT INTO &quot;verses&quot; VALUES (570,1,16,'Unii, din dragoste, știind că sunt rânduit&lt;f&gt;ⓤ&lt;/f&gt; pentru apărarea Evangheliei;');
INSERT INTO &quot;verses&quot; VALUES (570,1,17,'ceilalți, din ambiție, Îl vestesc pe Hristos nu cu gând curat, ci cu gând să mai adauge un necaz lanțurilor mele.');
INSERT INTO &quot;verses&quot; VALUES (570,1,18,'Și ce dacă? Indiferent cum, fie de ochii lumii, fie din toată inima, Hristos este vestit, ceea ce mă bucură. Dar mă voi bucura și');
INSERT INTO &quot;verses&quot; VALUES (570,1,19,'la gândul că lucrul acesta se va întoarce spre mântuirea &lt;i&gt;mea&lt;/i&gt; prin rugăciunea voastră&lt;f&gt;ⓥ&lt;/f&gt; și cu ajutorul Duhului&lt;f&gt;ⓦ&lt;/f&gt; lui Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (570,1,20,'Aștept cu înfocare și nădăjduiesc&lt;f&gt;ⓧ&lt;/f&gt; că nu voi fi dat de rușine&lt;f&gt;ⓨ&lt;/f&gt; cu nimic, ci că&lt;f&gt;ⓩ&lt;/f&gt; Hristos va fi preamărit cu toată îndrăzneala în trupul meu, acum ca întotdeauna, fie că trăiesc, fie că mor.');
INSERT INTO &quot;verses&quot; VALUES (570,1,21,'Căci pentru mine a trăi este Hristos și a muri este un câștig.');
INSERT INTO &quot;verses&quot; VALUES (570,1,22,'Dar dacă am de trăit în trup, &lt;i&gt;am și&lt;/i&gt; rod în lucrare; și nu știu ce să aleg.');
INSERT INTO &quot;verses&quot; VALUES (570,1,23,'Sunt strâns&lt;f&gt;ⓐ&lt;/f&gt; din două părți: îmi doresc să plec&lt;f&gt;ⓑ&lt;/f&gt; și să fiu împreună cu Hristos, căci ar fi mult mai bine,');
INSERT INTO &quot;verses&quot; VALUES (570,1,24,'dar pentru voi este mai necesar să rămân în trup.');
INSERT INTO &quot;verses&quot; VALUES (570,1,25,'De altfel, sunt încredințat&lt;f&gt;ⓒ&lt;/f&gt; și știu că voi rămâne și voi trăi cu voi toți pentru înaintarea voastră și pentru bucuria pe care v-o dă credința,');
INSERT INTO &quot;verses&quot; VALUES (570,1,26,'și astfel, prin întoarcerea mea la voi, să aveți în mine un mare prilej de laudă&lt;f&gt;ⓓ&lt;/f&gt; în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (570,1,27,'&lt;pb/&gt;Numai purtați-vă&lt;f&gt;ⓔ&lt;/f&gt; într-un chip vrednic de Evanghelia lui Hristos, pentru ca, fie că ajung să vă văd, fie că rămân departe și aud &lt;i&gt;vești&lt;/i&gt; despre voi, &lt;i&gt;să știu&lt;/i&gt; că&lt;f&gt;ⓕ&lt;/f&gt; rămâneți tari într-un duh și că luptați cu un suflet&lt;f&gt;ⓖ&lt;/f&gt; pentru credința pe care o dă Evanghelia,');
INSERT INTO &quot;verses&quot; VALUES (570,1,28,'fără să vă lăsați în vreo privință înspăimântați de împotrivitori, căci acesta este semnul&lt;f&gt;ⓗ&lt;/f&gt; &lt;i&gt;care pentru ei vestește&lt;/i&gt; nimicirea, iar&lt;f&gt;ⓘ&lt;/f&gt; pentru voi mântuirea de la Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (570,1,29,'Căci cu privire la Hristos, vouă&lt;f&gt;ⓙ&lt;/f&gt; vi s-a dat harul nu&lt;f&gt;ⓚ&lt;/f&gt; numai să credeți în El, ci să și suferiți pentru El');
INSERT INTO &quot;verses&quot; VALUES (570,1,30,'și să și duceți&lt;f&gt;ⓛ&lt;/f&gt; aceeași luptă pe care&lt;f&gt;ⓜ&lt;/f&gt; ați văzut-o la mine și pe care acum auziți că o duc.');
INSERT INTO &quot;verses&quot; VALUES (570,2,1,'&lt;pb/&gt;Deci, dacă este vreun îndemn în Hristos, dacă este vreo mângâiere în dragoste, dacă este vreo părtășie în Duhul, dacă este vreo milostivire și vreo îndurare,');
INSERT INTO &quot;verses&quot; VALUES (570,2,2,'faceți-mi bucuria deplină și aveți același gând, aceeași dragoste, uniți într-un suflet și-un gând&lt;f&gt;ⓝ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (570,2,3,'Nu faceți nimic&lt;f&gt;ⓞ&lt;/f&gt; din ambiție sau din slavă deșartă, ci, în smerenie&lt;f&gt;ⓟ&lt;/f&gt;, să-i priviți pe alții mai presus de voi înșivă!');
INSERT INTO &quot;verses&quot; VALUES (570,2,4,'Fiecare din voi să nu aibă în vedere doar foloasele lui, ci&lt;f&gt;ⓠ&lt;/f&gt; și foloasele altora!');
INSERT INTO &quot;verses&quot; VALUES (570,2,5,'&lt;pb/&gt;Între voi să&lt;f&gt;ⓡ&lt;/f&gt; fie gândirea aceasta, care era și în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (570,2,6,'&lt;pb/&gt;&lt;t&gt;El, măcar că&lt;f&gt;ⓢ&lt;/f&gt; avea chipul lui Dumnezeu,&lt;/t&gt; &lt;t&gt;n-a&lt;f&gt;ⓣ&lt;/f&gt; socotit drept un lucru de apucat&lt;f&gt;[2]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;a fi deopotrivă cu Dumnezeu&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,2,7,'&lt;t&gt;ci&lt;f&gt;ⓤ&lt;/f&gt; S-a dezbrăcat&lt;f&gt;[3]&lt;/f&gt; de Sine Însuși,&lt;/t&gt; &lt;t&gt;a luat chip de rob&lt;f&gt;ⓥ&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;i&gt;și&lt;/i&gt; s-a făcut&lt;f&gt;ⓦ&lt;/f&gt; asemenea oamenilor.&lt;/t&gt; &lt;t&gt;La înfățișare a fost găsit ca un om,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,2,8,'&lt;t&gt;S-a smerit,&lt;/t&gt; &lt;t&gt;a fost&lt;f&gt;ⓧ&lt;/f&gt; ascultător până la moarte,&lt;/t&gt; &lt;t&gt;și &lt;i&gt;încă&lt;/i&gt; moarte de cruce.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,2,9,'&lt;t&gt;De aceea și Dumnezeu L-a&lt;f&gt;ⓨ&lt;/f&gt; înălțat nespus&lt;/t&gt; &lt;t&gt;și I-a&lt;f&gt;ⓩ&lt;/f&gt; dăruit Numele&lt;/t&gt; &lt;t&gt;care este mai presus de orice nume;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,2,10,'&lt;t&gt;pentru ca&lt;f&gt;ⓐ&lt;/f&gt;, în Numele lui Isus,&lt;/t&gt; &lt;t&gt;să se plece orice genunchi&lt;/t&gt; &lt;t&gt;din ceruri, de pe pământ și de sub pământ&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,2,11,'&lt;t&gt;și orice&lt;f&gt;ⓑ&lt;/f&gt; limbă să dea mărturie&lt;/t&gt; &lt;t&gt;că Isus Hristos este Domn,&lt;/t&gt; &lt;t&gt;spre slava lui Dumnezeu Tatăl.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,2,12,'&lt;pb/&gt;Astfel dar, preaiubiții mei, după cum&lt;f&gt;ⓒ&lt;/f&gt; totdeauna ați fost ascultători, nu numai când sunt eu de față, ci cu mult mai mult acum, în lipsa mea, duceți până la capăt mântuirea voastră cu frică&lt;f&gt;ⓓ&lt;/f&gt; și cutremur!');
INSERT INTO &quot;verses&quot; VALUES (570,2,13,'Căci Dumnezeu&lt;f&gt;ⓔ&lt;/f&gt; este cel care lucrează în voi și voința, și înfăptuirea, după buna Lui plăcere.');
INSERT INTO &quot;verses&quot; VALUES (570,2,14,'Faceți toate lucrurile fără&lt;f&gt;ⓕ&lt;/f&gt; murmur și fără șovăieli&lt;f&gt;ⓖ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (570,2,15,'ca să fiți fără prihană și curați, copii&lt;f&gt;ⓗ&lt;/f&gt; ai lui Dumnezeu fără cusur în&lt;f&gt;ⓘ&lt;/f&gt; mijlocul unei&lt;f&gt;ⓙ&lt;/f&gt; generații ticăloase și stricate, în care străluciți&lt;f&gt;ⓚ&lt;/f&gt; ca niște lumini în lume,');
INSERT INTO &quot;verses&quot; VALUES (570,2,16,'ținând cu tărie Cuvântul vieții; și astfel, în ziua lui Hristos, mă voi putea lăuda&lt;f&gt;ⓛ&lt;/f&gt; că nici n-am&lt;f&gt;ⓜ&lt;/f&gt; alergat și nici nu m-am ostenit în zadar.');
INSERT INTO &quot;verses&quot; VALUES (570,2,17,'Și chiar dacă voi fi&lt;f&gt;ⓝ&lt;/f&gt; turnat ca o jertfă de băutură peste jertfa și&lt;f&gt;ⓞ&lt;/f&gt; slujba credinței voastre, eu sunt&lt;f&gt;ⓟ&lt;/f&gt; bucuros și mă bucur cu voi toți.');
INSERT INTO &quot;verses&quot; VALUES (570,2,18,'Tot astfel și voi fiți bucuroși; bucurați-vă împreună cu mine!');
INSERT INTO &quot;verses&quot; VALUES (570,2,19,'&lt;pb/&gt;Nădăjduiesc în Domnul Isus să-l trimit în curând la voi pe Timotei&lt;f&gt;ⓠ&lt;/f&gt;, ca să fiu și eu încurajat când voi avea știri despre voi.');
INSERT INTO &quot;verses&quot; VALUES (570,2,20,'N-am pe nimeni&lt;f&gt;ⓡ&lt;/f&gt; &lt;i&gt;altcineva&lt;/i&gt; care să fie de același gând cu mine și care să se preocupe sincer de starea voastră,');
INSERT INTO &quot;verses&quot; VALUES (570,2,21,'căci toți umblă&lt;f&gt;ⓢ&lt;/f&gt; după interesul lor, și nu după al lui Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (570,2,22,'Știți vrednicia lui, cum, asemenea&lt;f&gt;ⓣ&lt;/f&gt; unui copil cu tatăl lui, a slujit&lt;f&gt;[4]&lt;/f&gt; împreună cu mine pentru Evanghelie.');
INSERT INTO &quot;verses&quot; VALUES (570,2,23,'Pe el, așadar, nădăjduiesc să-l trimit la voi de îndată ce voi vedea ce întorsătură iau lucrurile cu privire la mine.');
INSERT INTO &quot;verses&quot; VALUES (570,2,24,'Și am&lt;f&gt;ⓤ&lt;/f&gt; încredere în Domnul că și eu voi veni în curând.');
INSERT INTO &quot;verses&quot; VALUES (570,2,25,'&lt;pb/&gt;Am socotit necesar să-l trimit la voi pe Epafrodit&lt;f&gt;ⓥ&lt;/f&gt;, fratele și tovarășul meu de lucru&lt;f&gt;ⓦ&lt;/f&gt; și de luptă, apostolul și slujitorul&lt;f&gt;ⓧ&lt;/f&gt; vostru pentru&lt;f&gt;ⓨ&lt;/f&gt; nevoile mele.');
INSERT INTO &quot;verses&quot; VALUES (570,2,26,'Căci&lt;f&gt;ⓩ&lt;/f&gt; îi era foarte dor de voi toți și era foarte mâhnit pentru că aflaserăți că este bolnav.');
INSERT INTO &quot;verses&quot; VALUES (570,2,27,'Ce-i drept, a fost bolnav și foarte aproape de moarte, dar Dumnezeu a avut milă de el. Și nu numai de el, ci și de mine, ca să n-am întristare peste întristare.');
INSERT INTO &quot;verses&quot; VALUES (570,2,28,'De aceea l-am și trimis cât mai grabnic, ca să-l vedeți și să vă bucurați din nou, iar eu să am mai puțină întristare.');
INSERT INTO &quot;verses&quot; VALUES (570,2,29,'Primiți-l deci în Domnul, cu toată bucuria, și prețuiți-i&lt;f&gt;ⓐ&lt;/f&gt; pe astfel de &lt;i&gt;oameni&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (570,2,30,'Căci pentru lucrarea lui Hristos a fost &lt;i&gt;el&lt;/i&gt; aproape de moarte și și-a pus viața în joc, ca&lt;f&gt;ⓑ&lt;/f&gt; să împlinească ce lipsea slujbei voastre pentru mine.');
INSERT INTO &quot;verses&quot; VALUES (570,3,1,'&lt;pb/&gt;Încolo, frații mei, bucurați-vă&lt;f&gt;ⓒ&lt;/f&gt; în Domnul! Mie nu-mi este greu să vă scriu mereu aceleași lucruri, iar pentru voi este &lt;i&gt;mai&lt;/i&gt; sigur &lt;i&gt;așa&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (570,3,2,'&lt;pb/&gt;Păziți-vă&lt;f&gt;ⓓ&lt;/f&gt; de câinii &lt;i&gt;aceia&lt;/i&gt;; păziți-vă de lucrătorii&lt;f&gt;ⓔ&lt;/f&gt; &lt;i&gt;aceia&lt;/i&gt; răi; păziți-vă&lt;f&gt;ⓕ&lt;/f&gt; de cei care își mutilează trupul&lt;f&gt;[5]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (570,3,3,'Căci cei circumciși&lt;f&gt;ⓖ&lt;/f&gt; suntem noi, care&lt;f&gt;ⓗ&lt;/f&gt; aducem închinare prin Duhul lui Dumnezeu, care ne&lt;f&gt;ⓘ&lt;/f&gt; lăudăm în Hristos Isus și care nu ne bizuim pe cele trupești&lt;f&gt;[6]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,3,4,'(măcar că eu&lt;f&gt;ⓙ&lt;/f&gt; &lt;i&gt;unul&lt;/i&gt; aș avea motiv să mă bizui și pe lucrurile trupești). Dacă vreun altul crede că se poate bizui pe lucrurile trupești, eu cu atât mai mult:');
INSERT INTO &quot;verses&quot; VALUES (570,3,5,'am fost circumcis&lt;f&gt;ⓚ&lt;/f&gt; în a opta zi, sunt din&lt;f&gt;ⓛ&lt;/f&gt; neamul lui Israel, din&lt;f&gt;ⓜ&lt;/f&gt; seminția lui Beniamin, evreu din evrei; după Lege, fariseu&lt;f&gt;ⓝ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (570,3,6,'după&lt;f&gt;ⓞ&lt;/f&gt; râvnă, prigonitor&lt;f&gt;ⓟ&lt;/f&gt; al Bisericii; după dreptatea&lt;f&gt;ⓠ&lt;/f&gt; pe care o dă Legea, fără prihană.');
INSERT INTO &quot;verses&quot; VALUES (570,3,7,'Dar lucrurile&lt;f&gt;ⓡ&lt;/f&gt; care pentru mine erau câștiguri am ajuns să le socotesc o pierdere de dragul lui Hristos.');
INSERT INTO &quot;verses&quot; VALUES (570,3,8,'Ba încă socotesc totul ca o pierdere față de valoarea nespus de mare a&lt;f&gt;ⓢ&lt;/f&gt; cunoașterii lui Hristos Isus, Domnul meu. Pentru El am pierdut toate și le socotesc un gunoi, ca să-L câștig pe Hristos');
INSERT INTO &quot;verses&quot; VALUES (570,3,9,'și să fiu găsit în El, nu având o dreptate a mea&lt;f&gt;ⓣ&lt;/f&gt;, &lt;i&gt;dată&lt;/i&gt; de Lege, ci aceea&lt;f&gt;ⓤ&lt;/f&gt; care &lt;i&gt;se capătă&lt;/i&gt; prin credința în Hristos, dreptate &lt;i&gt;dată&lt;/i&gt; de Dumnezeu în baza credinței.');
INSERT INTO &quot;verses&quot; VALUES (570,3,10,'&lt;i&gt;Vreau&lt;/i&gt; să-L cunosc pe El și puterea învierii Lui și părtășia&lt;f&gt;ⓥ&lt;/f&gt; suferințelor Lui și să mă fac asemenea cu moartea Lui,');
INSERT INTO &quot;verses&quot; VALUES (570,3,11,'numai să ajung&lt;f&gt;ⓦ&lt;/f&gt; cumva la învierea din morți.');
INSERT INTO &quot;verses&quot; VALUES (570,3,12,'&lt;pb/&gt;Nu că am și dobândit &lt;i&gt;toate acestea&lt;/i&gt; sau că am și ajuns&lt;f&gt;ⓧ&lt;/f&gt; desăvârșit, dar caut să le apuc, întrucât și eu am fost apucat de Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (570,3,13,'Fraților, eu nu cred că le-am apucat încă, dar fac un singur lucru: uitând&lt;f&gt;ⓨ&lt;/f&gt; ce este în urma mea și aruncându-mă spre ce este înainte,');
INSERT INTO &quot;verses&quot; VALUES (570,3,14,'alerg&lt;f&gt;ⓩ&lt;/f&gt; spre țintă, către premiul chemării cerești a lui Dumnezeu în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (570,3,15,'Astfel &lt;i&gt;trebuie&lt;/i&gt; să gândim &lt;i&gt;noi&lt;/i&gt;, care suntem desăvârșiți&lt;f&gt;ⓐ&lt;/f&gt;; iar dacă &lt;i&gt;se întâmplă să&lt;/i&gt; gândiți altfel&lt;f&gt;ⓑ&lt;/f&gt;, și în acea privință Dumnezeu vă va lumina.');
INSERT INTO &quot;verses&quot; VALUES (570,3,16,'Dar în lucrurile în care am ajuns de aceeași&lt;f&gt;ⓒ&lt;/f&gt; părere, să umblăm la fel&lt;f&gt;ⓓ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (570,3,17,'&lt;pb/&gt;Călcați pe urmele mele&lt;f&gt;ⓔ&lt;/f&gt;, fraților, și uitați-vă &lt;i&gt;bine&lt;/i&gt; la cei care se poartă potrivit pildei pe care o aveți&lt;f&gt;ⓕ&lt;/f&gt; în noi!');
INSERT INTO &quot;verses&quot; VALUES (570,3,18,'Căci v-am spus de multe ori și vă mai spun și acum, plângând: mulți se poartă ca vrăjmași&lt;f&gt;ⓖ&lt;/f&gt; ai crucii lui Hristos, și');
INSERT INTO &quot;verses&quot; VALUES (570,3,19,'sfârșitul lor&lt;f&gt;ⓗ&lt;/f&gt; va fi pieirea. Dumnezeul lor&lt;f&gt;ⓘ&lt;/f&gt; este pântecul, slava le este în rușinea lor și se gândesc&lt;f&gt;ⓙ&lt;/f&gt; la lucrurile pământești.');
INSERT INTO &quot;verses&quot; VALUES (570,3,20,'Dar cetățenia&lt;f&gt;ⓚ&lt;/f&gt; noastră este în ceruri, de unde Îl și așteptăm&lt;f&gt;ⓛ&lt;/f&gt; ca Mântuitor pe Domnul Isus Hristos,');
INSERT INTO &quot;verses&quot; VALUES (570,3,21,'care&lt;f&gt;ⓜ&lt;/f&gt; va schimba starea umilă a trupului nostru și îl va face asemenea trupului Său de slavă prin puterea pe care o are de a-Și&lt;f&gt;ⓝ&lt;/f&gt; supune toate lucrurile.');
INSERT INTO &quot;verses&quot; VALUES (570,4,1,'&lt;pb/&gt;De aceea, preaiubiții și mult doriții mei frați, bucuria&lt;f&gt;ⓞ&lt;/f&gt; și cununa mea, rămâneți astfel tari în Domnul, preaiubiților!');
INSERT INTO &quot;verses&quot; VALUES (570,4,2,'&lt;pb/&gt;Le îndemn pe Evodia și pe Sintichia să aibă același gând&lt;f&gt;ⓟ&lt;/f&gt; în Domnul,');
INSERT INTO &quot;verses&quot; VALUES (570,4,3,'iar pe tine, adevărat tovarăș de jug, te rog să le vii în ajutor. Ele au lucrat împreună cu mine pentru Evanghelie, cu Clement și cu ceilalți tovarăși de lucru ai mei, ale căror nume sunt scrise în cartea&lt;f&gt;ⓠ&lt;/f&gt; vieții.');
INSERT INTO &quot;verses&quot; VALUES (570,4,4,'&lt;pb/&gt;Bucurați-vă&lt;f&gt;ⓡ&lt;/f&gt; totdeauna în Domnul! Iarăși zic: bucurați-vă!');
INSERT INTO &quot;verses&quot; VALUES (570,4,5,'Blândețea voastră să fie cunoscută de toți oamenii! Domnul este aproape&lt;f&gt;ⓢ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (570,4,6,'Nu&lt;f&gt;ⓣ&lt;/f&gt; vă îngrijorați de nimic, ci, în orice lucru, aduceți cu mulțumire cererile voastre la cunoștința lui Dumnezeu, prin rugăciuni și cereri!');
INSERT INTO &quot;verses&quot; VALUES (570,4,7,'Și pacea&lt;f&gt;ⓤ&lt;/f&gt; lui Dumnezeu, care întrece orice pricepere, vă va păzi inimile și gândurile în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (570,4,8,'&lt;pb/&gt;Încolo, fraților, tot ce este adevărat, tot ce este vrednic de cinste, tot ce este drept, tot ce este curat, tot ce este vrednic de iubit, tot ce are nume bun, orice virtute sau lucru vrednic de laudă, acestea să vă preocupe!');
INSERT INTO &quot;verses&quot; VALUES (570,4,9,'Ce ați învățat, ce ați primit, ce ați auzit de la mine și ce ați văzut la mine, aceea să faceți! Și Dumnezeul păcii va fi cu voi.');
INSERT INTO &quot;verses&quot; VALUES (570,4,10,'&lt;pb/&gt;M-am bucurat mult în Domnul că, în sfârșit, v-ați înnoit iarăși grija&lt;f&gt;ⓥ&lt;/f&gt; față de mine. Vă gândeați voi la așa ceva, dar vă lipsea prilejul.');
INSERT INTO &quot;verses&quot; VALUES (570,4,11,'Nu zic lucrul acesta având în vedere lipsurile mele; căci am învățat să fiu&lt;f&gt;ⓦ&lt;/f&gt; mulțumit cu ceea ce am.');
INSERT INTO &quot;verses&quot; VALUES (570,4,12,'Știu&lt;f&gt;ⓧ&lt;/f&gt; să trăiesc modest și știu să trăiesc în belșug. În orice împrejurări m-am deprins&lt;f&gt;[7]&lt;/f&gt; să fiu sătul sau flămând, în belșug sau în lipsă.');
INSERT INTO &quot;verses&quot; VALUES (570,4,13,'Pot totul&lt;f&gt;ⓨ&lt;/f&gt; prin [Hristos,] Cel ce mă întărește.');
INSERT INTO &quot;verses&quot; VALUES (570,4,14,'Dar bine ați făcut că ați&lt;f&gt;ⓩ&lt;/f&gt; fost părtași la strâmtorarea mea.');
INSERT INTO &quot;verses&quot; VALUES (570,4,15,'Voi înșivă știți, filipenilor, că, la începutul Evangheliei, când am plecat din Macedonia, nicio&lt;f&gt;ⓐ&lt;/f&gt; biserică nu mi-a fost părtașă în ce privește dăruirea și primirea în afară de voi.&lt;f&gt;[8]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (570,4,16,'Căci și în Tesalonic mi-ați trimis de câteva ori&lt;f&gt;[9]&lt;/f&gt; &lt;i&gt;ajutorul&lt;/i&gt; de care aveam nevoie.');
INSERT INTO &quot;verses&quot; VALUES (570,4,17,'Nu că aș umbla după &lt;i&gt;vreun&lt;/i&gt; dar. Dimpotrivă, umblu după rodul&lt;f&gt;ⓑ&lt;/f&gt; care sporește în folosul&lt;f&gt;[10]&lt;/f&gt; vostru.');
INSERT INTO &quot;verses&quot; VALUES (570,4,18,'Am primit toate &lt;i&gt;cele trimise&lt;/i&gt; și sunt în belșug. Am de toate de când am primit prin Epafrodit&lt;f&gt;ⓒ&lt;/f&gt; ce mi-ați trimis — un miros&lt;f&gt;ⓓ&lt;/f&gt; de mireasmă plăcută, o jertfă&lt;f&gt;ⓔ&lt;/f&gt; bine primită și plăcută lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (570,4,19,'Și Dumnezeul meu să împlinească&lt;f&gt;ⓕ&lt;/f&gt; toate trebuințele voastre, după&lt;f&gt;ⓖ&lt;/f&gt; bogăția Sa, în slavă, în Hristos Isus!');
INSERT INTO &quot;verses&quot; VALUES (570,4,20,'A lui&lt;f&gt;ⓗ&lt;/f&gt; Dumnezeu, Tatăl nostru, să fie slava în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (570,4,21,'&lt;pb/&gt;Spuneți-i salutări fiecărui sfânt în Hristos Isus! Frații care&lt;f&gt;ⓘ&lt;/f&gt; sunt cu mine vă trimit salutări.');
INSERT INTO &quot;verses&quot; VALUES (570,4,22,'Toți sfinții vă trimit salutări, mai&lt;f&gt;ⓙ&lt;/f&gt; ales cei din casa cezarului.');
INSERT INTO &quot;verses&quot; VALUES (570,4,23,'&lt;pb/&gt;Harul&lt;f&gt;ⓚ&lt;/f&gt; Domnului Isus Hristos să fie cu duhul vostru! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (580,1,1,'&lt;pb/&gt;Pavel, apostol&lt;f&gt;ⓐ&lt;/f&gt; al lui Hristos Isus prin voia lui Dumnezeu, și fratele Timotei');
INSERT INTO &quot;verses&quot; VALUES (580,1,2,'către sfinții și frații credincioși în&lt;f&gt;ⓑ&lt;/f&gt; Hristos din Colose: Har și pace de la Dumnezeu, Tatăl nostru, [și de la Domnul Isus Hristos]!');
INSERT INTO &quot;verses&quot; VALUES (580,1,3,'&lt;pb/&gt;Îi mulțumim&lt;f&gt;ⓒ&lt;/f&gt; neîncetat lui Dumnezeu, Tatăl Domnului nostru Isus Hristos, când ne rugăm pentru voi,');
INSERT INTO &quot;verses&quot; VALUES (580,1,4,'căci am auzit despre credința voastră în Hristos Isus și despre dragostea&lt;f&gt;ⓓ&lt;/f&gt; pe care o aveți față de toți sfinții');
INSERT INTO &quot;verses&quot; VALUES (580,1,5,'datorită nădejdii care vă este păstrată&lt;f&gt;ⓔ&lt;/f&gt; în ceruri; despre ea ați auzit mai înainte în Cuvântul adevărului, și anume Evanghelia');
INSERT INTO &quot;verses&quot; VALUES (580,1,6,'care a ajuns până la voi. După&lt;f&gt;ⓕ&lt;/f&gt; cum aduce&lt;f&gt;ⓖ&lt;/f&gt; rod și crește în toată lumea, tot astfel &lt;i&gt;lucrează&lt;/i&gt; și între voi, din ziua în care ați auzit și ați cunoscut harul&lt;f&gt;ⓗ&lt;/f&gt; lui Dumnezeu în adevăr,');
INSERT INTO &quot;verses&quot; VALUES (580,1,7,'cum ați învățat de la Epafras&lt;f&gt;ⓘ&lt;/f&gt;, preaiubitul nostru tovarăș de slujbă. El este un slujitor&lt;f&gt;ⓙ&lt;/f&gt; credincios al lui Hristos pentru voi');
INSERT INTO &quot;verses&quot; VALUES (580,1,8,'și ne-a făcut cunoscută dragostea&lt;f&gt;ⓚ&lt;/f&gt; voastră în Duhul.');
INSERT INTO &quot;verses&quot; VALUES (580,1,9,'&lt;pb/&gt;De aceea&lt;f&gt;ⓛ&lt;/f&gt; și noi, din ziua când am auzit &lt;i&gt;aceste vești&lt;/i&gt;, nu încetăm să ne rugăm pentru voi și să cerem să&lt;f&gt;ⓜ&lt;/f&gt; vă umpleți de cunoașterea&lt;f&gt;ⓝ&lt;/f&gt; voii Lui — în orice fel de înțelepciune&lt;f&gt;ⓞ&lt;/f&gt; și pricepere duhovnicească —');
INSERT INTO &quot;verses&quot; VALUES (580,1,10,'&lt;i&gt;încât&lt;/i&gt;&lt;f&gt;ⓟ&lt;/f&gt; să vă purtați într-un chip vrednic de Domnul și&lt;f&gt;ⓠ&lt;/f&gt; să-I fiți plăcuți în orice lucru, aducând rod&lt;f&gt;ⓡ&lt;/f&gt; în tot felul de fapte bune și crescând în cunoașterea lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (580,1,11,'întăriți&lt;f&gt;ⓢ&lt;/f&gt; cu toată puterea, potrivit cu tăria slavei Lui, pentru răbdare&lt;f&gt;ⓣ&lt;/f&gt; și îndelungă răbdare deplină. Cu bucurie&lt;f&gt;ⓤ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (580,1,12,'aduceți-I mulțumiri&lt;f&gt;ⓥ&lt;/f&gt; Tatălui, care v-a învrednicit să aveți parte de moștenirea sfinților în lumină!');
INSERT INTO &quot;verses&quot; VALUES (580,1,13,'El ne-a izbăvit de sub puterea&lt;f&gt;ⓦ&lt;/f&gt; întunericului și&lt;f&gt;ⓧ&lt;/f&gt; ne-a strămutat în Împărăția Fiului Său preaiubit&lt;f&gt;[1]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (580,1,14,'în&lt;f&gt;ⓨ&lt;/f&gt; care avem răscumpărarea [&lt;i&gt;și&lt;/i&gt;, prin sângele Lui], iertarea păcatelor.');
INSERT INTO &quot;verses&quot; VALUES (580,1,15,'&lt;pb/&gt;&lt;t&gt;El este chipul&lt;f&gt;ⓩ&lt;/f&gt; Dumnezeului nevăzut,&lt;/t&gt; &lt;t&gt;Întâiul&lt;f&gt;ⓐ&lt;/f&gt; Născut&lt;f&gt;[2]&lt;/f&gt; &lt;i&gt;pus&lt;/i&gt; peste toată creația,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (580,1,16,'&lt;t&gt;Căci prin El&lt;f&gt;ⓑ&lt;/f&gt; au fost create toate lucrurile&lt;/t&gt; &lt;t&gt;care sunt în ceruri și pe pământ,&lt;/t&gt; &lt;t&gt;cele văzute și cele nevăzute:&lt;/t&gt; &lt;t&gt;fie tronuri, fie domnii&lt;f&gt;ⓒ&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;fie căpetenii, fie stăpâniri.&lt;/t&gt; &lt;t&gt;Toate au fost făcute prin El&lt;f&gt;ⓓ&lt;/f&gt; și pentru El.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (580,1,17,'&lt;t&gt;El&lt;f&gt;ⓔ&lt;/f&gt; este mai înainte de toate&lt;/t&gt; &lt;t&gt;și toate se țin prin El.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (580,1,18,'&lt;t&gt;El&lt;f&gt;ⓕ&lt;/f&gt; este Capul trupului, al Bisericii.&lt;/t&gt; &lt;t&gt;El este începutul,&lt;/t&gt; &lt;t&gt;Întâiul Născut&lt;f&gt;ⓖ&lt;/f&gt; dintre cei morți,&lt;/t&gt; &lt;t&gt;pentru ca în toate lucrurile să aibă întâietate.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (580,1,19,'&lt;t&gt;Căci &lt;i&gt;Dumnezeu&lt;/i&gt; a găsit de cuviință&lt;/t&gt; &lt;t&gt;ca toată plinătatea să locuiască în&lt;f&gt;ⓗ&lt;/f&gt; El&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (580,1,20,'&lt;t&gt;și prin El să împace totul&lt;f&gt;ⓘ&lt;/f&gt; cu Sine —&lt;/t&gt; &lt;t&gt;făcând pace prin sângele crucii Lui,&lt;/t&gt; &lt;t&gt;prin El — atât cele de pe pământ,&lt;/t&gt; &lt;t&gt;cât și cele care sunt în ceruri.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (580,1,21,'&lt;pb/&gt;Și pe voi, care&lt;f&gt;ⓙ&lt;/f&gt; odinioară erați înstrăinați și vrăjmași în cuget prin faptele voastre rele&lt;f&gt;ⓚ&lt;/f&gt;, El v-a împăcat acum');
INSERT INTO &quot;verses&quot; VALUES (580,1,22,'prin&lt;f&gt;ⓛ&lt;/f&gt; trupul Lui de carne, prin moarte, ca&lt;f&gt;ⓜ&lt;/f&gt; să vă înfățișeze înaintea Lui sfinți, fără prihană și fără vină;');
INSERT INTO &quot;verses&quot; VALUES (580,1,23,'&lt;i&gt;aceasta,&lt;/i&gt; negreșit, dacă rămâneți în credință — întemeiați&lt;f&gt;ⓝ&lt;/f&gt; și neclintiți — fără să vă abateți&lt;f&gt;ⓞ&lt;/f&gt; de la nădejdea Evangheliei pe care ați&lt;f&gt;ⓟ&lt;/f&gt; auzit-o, care a fost propovăduită oricărei făpturi de sub cer și al cărei slujitor am fost făcut eu, Pavel.');
INSERT INTO &quot;verses&quot; VALUES (580,1,24,'&lt;pb/&gt;Acum mă bucur în suferințele mele pentru&lt;f&gt;ⓠ&lt;/f&gt; voi și, în trupul meu, împlinesc ce lipsește&lt;f&gt;ⓡ&lt;/f&gt; suferințelor lui Hristos, pentru trupul Lui, care este Biserica.');
INSERT INTO &quot;verses&quot; VALUES (580,1,25,'Slujitorul ei am fost făcut eu, după isprăvnicia&lt;f&gt;ⓢ&lt;/f&gt; pe care mi-a dat-o Dumnezeu în folosul vostru, ca să răspândesc din plin Cuvântul lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (580,1,26,'taina&lt;f&gt;ⓣ&lt;/f&gt; ținută ascunsă de veacuri și de generații, dar descoperită acum sfinților Lui,');
INSERT INTO &quot;verses&quot; VALUES (580,1,27,'cărora&lt;f&gt;ⓤ&lt;/f&gt; Dumnezeu a vrut să le descopere care este bogăția&lt;f&gt;ⓥ&lt;/f&gt; slavei acestei taine între neamuri, anume: Hristos în voi, nădejdea&lt;f&gt;ⓦ&lt;/f&gt; slavei.');
INSERT INTO &quot;verses&quot; VALUES (580,1,28,'Pe El Îl propovăduim noi și sfătuim pe orice om și învățăm pe orice om cu toată înțelepciunea ca&lt;f&gt;ⓧ&lt;/f&gt; să-i înfățișăm pe toți oamenii desăvârșiți în Hristos [Isus].');
INSERT INTO &quot;verses&quot; VALUES (580,1,29,'Pentru aceasta&lt;f&gt;ⓨ&lt;/f&gt; mă trudesc și mă lupt&lt;f&gt;ⓩ&lt;/f&gt; după lucrarea&lt;f&gt;ⓐ&lt;/f&gt; Lui, care se manifestă prin mine cu putere.');
INSERT INTO &quot;verses&quot; VALUES (580,2,1,'&lt;pb/&gt;Vreau să știți cât de mare luptă&lt;f&gt;ⓑ&lt;/f&gt; duc pentru voi, pentru cei din Laodiceea și pentru toți cei care nu m-au văzut la față,');
INSERT INTO &quot;verses&quot; VALUES (580,2,2,'pentru ca inimile lor să primească mângâiere, să fie uniți în dragoste și să capete din plin bogăția siguranței pe care o dă priceperea, ca să cunoască&lt;f&gt;ⓒ&lt;/f&gt; taina lui Dumnezeu [Tatăl], &lt;i&gt;adică&lt;/i&gt; pe Hristos,');
INSERT INTO &quot;verses&quot; VALUES (580,2,3,'în&lt;f&gt;ⓓ&lt;/f&gt; care sunt ascunse toate comorile înțelepciunii și ale cunoașterii.');
INSERT INTO &quot;verses&quot; VALUES (580,2,4,'Spun aceasta pentru ca&lt;f&gt;ⓔ&lt;/f&gt; nimeni să nu vă înșele cu vorbe iscusite.');
INSERT INTO &quot;verses&quot; VALUES (580,2,5,'Căci, deși&lt;f&gt;ⓕ&lt;/f&gt; sunt departe cu trupul, cu duhul sunt alături de voi și mă bucur să văd rânduiala &lt;i&gt;care domnește&lt;/i&gt; între voi și tăria credinței voastre în Hristos.');
INSERT INTO &quot;verses&quot; VALUES (580,2,6,'&lt;pb/&gt;Astfel, după cum&lt;f&gt;ⓖ&lt;/f&gt; L-ați primit pe Hristos Isus Domnul, în El să și umblați,');
INSERT INTO &quot;verses&quot; VALUES (580,2,7,'fiind înrădăcinați și zidiți în El, întăriți prin credință, după cum ați fost învățați, sporind în recunoștință.');
INSERT INTO &quot;verses&quot; VALUES (580,2,8,'Luați seama&lt;f&gt;ⓗ&lt;/f&gt; ca nimeni să nu vă fure cu filozofia și cu o amăgire deșartă după tradiția&lt;f&gt;ⓘ&lt;/f&gt; oamenilor, după învățăturile elementare&lt;f&gt;[3]&lt;/f&gt; ale lumii&lt;f&gt;ⓙ&lt;/f&gt;, și nu după Hristos.');
INSERT INTO &quot;verses&quot; VALUES (580,2,9,'Căci în El&lt;f&gt;ⓚ&lt;/f&gt; locuiește trupește toată plinătatea Dumnezeirii,');
INSERT INTO &quot;verses&quot; VALUES (580,2,10,'iar voi sunteți făcuți deplini&lt;f&gt;ⓛ&lt;/f&gt; în El, care&lt;f&gt;ⓜ&lt;/f&gt; este capul oricărei căpetenii&lt;f&gt;ⓝ&lt;/f&gt; și stăpâniri.');
INSERT INTO &quot;verses&quot; VALUES (580,2,11,'În El ați fost circumciși&lt;f&gt;ⓞ&lt;/f&gt; nu cu o circumcizie făcută de mâna &lt;i&gt;omului&lt;/i&gt;, ci cu circumcizia &lt;i&gt;făcută de&lt;/i&gt; Hristos, &lt;i&gt;care constă&lt;/i&gt; în dezbrăcarea&lt;f&gt;ⓟ&lt;/f&gt; de trupul supus firii pământești,');
INSERT INTO &quot;verses&quot; VALUES (580,2,12,'după ce ați fost îngropați&lt;f&gt;ⓠ&lt;/f&gt; împreună cu El prin botez, prin care ați și înviat&lt;f&gt;ⓡ&lt;/f&gt; împreună cu El prin credința în puterea lui Dumnezeu, care&lt;f&gt;ⓢ&lt;/f&gt; L-a înviat din morți.');
INSERT INTO &quot;verses&quot; VALUES (580,2,13,'Pe voi&lt;f&gt;ⓣ&lt;/f&gt;, care erați morți în abaterile voastre și în necircumcizia firii voastre pământești, Dumnezeu v-a adus la viață împreună cu El, după ce ne-a iertat toate abaterile.');
INSERT INTO &quot;verses&quot; VALUES (580,2,14,'A șters&lt;f&gt;ⓤ&lt;/f&gt; înscrisul care era împotriva noastră, cu tot cu rânduielile lui care ne condamnau, și l-a înlăturat, pironindu-l pe cruce.');
INSERT INTO &quot;verses&quot; VALUES (580,2,15,'A dezbrăcat&lt;f&gt;ⓥ&lt;/f&gt; căpeteniile și stăpânirile și le-a făcut de ocară în văzul tuturor, triumfând asupra lor prin &lt;i&gt;cruce&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (580,2,16,'&lt;pb/&gt;Așadar, nimeni să&lt;f&gt;ⓦ&lt;/f&gt; nu vă judece cu privire la mâncare sau băutură, cu&lt;f&gt;ⓧ&lt;/f&gt; privire la o zi de sărbătoare&lt;f&gt;ⓨ&lt;/f&gt;, de lună nouă sau de sabat;');
INSERT INTO &quot;verses&quot; VALUES (580,2,17,'acestea&lt;f&gt;ⓩ&lt;/f&gt; sunt umbra lucrurilor viitoare, însă trupul&lt;f&gt;[4]&lt;/f&gt; este Hristos.');
INSERT INTO &quot;verses&quot; VALUES (580,2,18,'Nimeni să nu vă răpească premiul &lt;i&gt;alergării&lt;/i&gt;, atras de asceză și închinare adusă îngerilor, complăcându-se în&lt;f&gt;ⓐ&lt;/f&gt; lucruri pe care le-ar fi văzut, umflat de o mândrie deșartă, din pricina gândurilor firii lui pământești.');
INSERT INTO &quot;verses&quot; VALUES (580,2,19,'&lt;i&gt;Un astfel de om&lt;/i&gt; nu se ține strâns de Capul&lt;f&gt;ⓑ&lt;/f&gt; din care tot trupul, hrănit și strâns legat, cu ajutorul încheieturilor și legăturilor, își primește creșterea pe care i-o dă Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (580,2,20,'&lt;pb/&gt;Dacă ați murit&lt;f&gt;ⓒ&lt;/f&gt; împreună cu Hristos față de învățăturile elementare ale lumii, de ce, ca și cum ați trăi încă în lume, vă supuneți la rânduieli &lt;i&gt;precum&lt;/i&gt;:');
INSERT INTO &quot;verses&quot; VALUES (580,2,21,'„Nu&lt;f&gt;ⓓ&lt;/f&gt; lua, nu gusta, nu&lt;f&gt;ⓔ&lt;/f&gt; atinge”?');
INSERT INTO &quot;verses&quot; VALUES (580,2,22,'&lt;i&gt;Acestea privesc&lt;/i&gt; lucruri menite să piară prin întrebuințare și sunt întemeiate pe&lt;f&gt;ⓕ&lt;/f&gt; porunci și învățături ale oamenilor;');
INSERT INTO &quot;verses&quot; VALUES (580,2,23,'ele trec drept&lt;f&gt;ⓖ&lt;/f&gt; înțelepciune prin așa-zisa închinare, prin asceză și prin asprimea față de trup, dar nu au nicio valoare împotriva răsfățării firii pământești.');
INSERT INTO &quot;verses&quot; VALUES (580,3,1,'&lt;pb/&gt;Dacă deci ați&lt;f&gt;ⓗ&lt;/f&gt; înviat împreună cu Hristos, căutați lucrurile de sus, unde Hristos stă la dreapta lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (580,3,2,'Gândiți-vă la lucrurile de sus, nu la cele de pe pământ!');
INSERT INTO &quot;verses&quot; VALUES (580,3,3,'Căci&lt;f&gt;ⓘ&lt;/f&gt; voi ați murit, și viața voastră este ascunsă cu Hristos în Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (580,3,4,'Când&lt;f&gt;ⓙ&lt;/f&gt; Se va arăta Hristos, viața voastră, atunci vă veți arăta și voi împreună cu El în&lt;f&gt;ⓚ&lt;/f&gt; slavă.');
INSERT INTO &quot;verses&quot; VALUES (580,3,5,'&lt;pb/&gt;Omorâți&lt;f&gt;ⓛ&lt;/f&gt;, așadar, mădularele [voastre] de pe pământ: desfrâul&lt;f&gt;ⓜ&lt;/f&gt;, necurăția, patima, pofta rea&lt;f&gt;ⓝ&lt;/f&gt; și lăcomia, care este închinare la idoli!');
INSERT INTO &quot;verses&quot; VALUES (580,3,6,'Din pricina&lt;f&gt;ⓞ&lt;/f&gt; acestor lucruri vine mânia lui Dumnezeu peste fiii neascultării.');
INSERT INTO &quot;verses&quot; VALUES (580,3,7,'În numărul&lt;f&gt;ⓟ&lt;/f&gt; acestor oameni ați fost și voi odată, când trăiați în aceste &lt;i&gt;păcate&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (580,3,8,'Dar&lt;f&gt;ⓠ&lt;/f&gt; acum lăsați-vă de toate acestea: mânie, furie, răutate, defăimare, vorbe rușinoase care v-ar putea ieși din gură!');
INSERT INTO &quot;verses&quot; VALUES (580,3,9,'Nu&lt;f&gt;ⓡ&lt;/f&gt; vă mințiți unii pe alții, întrucât&lt;f&gt;ⓢ&lt;/f&gt; v-ați dezbrăcat de omul cel vechi, cu faptele lui,');
INSERT INTO &quot;verses&quot; VALUES (580,3,10,'și v-ați îmbrăcat cu &lt;i&gt;omul&lt;/i&gt; cel nou, care se înnoiește în cunoaștere, după&lt;f&gt;ⓣ&lt;/f&gt; chipul Celui ce l-a&lt;f&gt;ⓤ&lt;/f&gt; creat!');
INSERT INTO &quot;verses&quot; VALUES (580,3,11,'&lt;i&gt;Drept urmare,&lt;/i&gt; nu mai este nici grec&lt;f&gt;ⓥ&lt;/f&gt;, nici iudeu, nici circumcizie, nici necircumcizie, nici barbar, scit&lt;f&gt;[5]&lt;/f&gt;, rob sau liber, ci Hristos este totul și în toți.');
INSERT INTO &quot;verses&quot; VALUES (580,3,12,'&lt;pb/&gt;Așadar&lt;f&gt;ⓦ&lt;/f&gt;, ca niște aleși ai lui Dumnezeu sfinți și preaiubiți, îmbrăcați-vă cu o inimă&lt;f&gt;ⓧ&lt;/f&gt; &lt;i&gt;plină&lt;/i&gt; de îndurare, cu bunătate, cu smerenie, cu blândețe, cu îndelungă răbdare!');
INSERT INTO &quot;verses&quot; VALUES (580,3,13,'Îngăduiți-vă&lt;f&gt;ⓨ&lt;/f&gt; unii pe alții și, dacă unul are motiv să se plângă de altul, iertați-vă unul pe altul! Cum v-a iertat Domnul, așa &lt;i&gt;iertați-vă&lt;/i&gt; și voi!');
INSERT INTO &quot;verses&quot; VALUES (580,3,14,'Dar&lt;f&gt;ⓩ&lt;/f&gt; mai presus de toate acestea, îmbrăcați-vă&lt;f&gt;ⓐ&lt;/f&gt; cu dragostea, care este legătura desăvârșirii!');
INSERT INTO &quot;verses&quot; VALUES (580,3,15,'Pacea&lt;f&gt;ⓑ&lt;/f&gt; lui Hristos, la care ați fost chemați &lt;i&gt;ca să alcătuiți&lt;/i&gt; un singur trup&lt;f&gt;ⓒ&lt;/f&gt;, să stăpânească în inimile voastre! Și fiți&lt;f&gt;ⓓ&lt;/f&gt; recunoscători!');
INSERT INTO &quot;verses&quot; VALUES (580,3,16,'Cuvântul lui Hristos să locuiască în voi din belșug! Învățați-vă și sfătuiți-vă unii pe alții cu toată înțelepciunea! Cu mulțumire în inimile voastre, cântați-I lui Dumnezeu psalmi, cântări de laudă&lt;f&gt;ⓔ&lt;/f&gt; și cântări duhovnicești!');
INSERT INTO &quot;verses&quot; VALUES (580,3,17,'Și orice&lt;f&gt;ⓕ&lt;/f&gt; faceți, prin cuvânt sau faptă, să faceți totul în Numele Domnului Isus, mulțumindu-I&lt;f&gt;ⓖ&lt;/f&gt; prin El lui Dumnezeu Tatăl.');
INSERT INTO &quot;verses&quot; VALUES (580,3,18,'&lt;pb/&gt;Soțiilor&lt;f&gt;ⓗ&lt;/f&gt;, fiți supuse soților voștri, cum&lt;f&gt;ⓘ&lt;/f&gt; se cuvine în Domnul!');
INSERT INTO &quot;verses&quot; VALUES (580,3,19,'Soților&lt;f&gt;ⓙ&lt;/f&gt;, iubiți-vă soțiile și nu țineți necaz&lt;f&gt;ⓚ&lt;/f&gt; pe ele&lt;f&gt;[6]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (580,3,20,'Copii&lt;f&gt;ⓛ&lt;/f&gt;, ascultați în Domnul de părinții voștri, în&lt;f&gt;ⓜ&lt;/f&gt; toate privințele, căci lucrul acesta este plăcut!');
INSERT INTO &quot;verses&quot; VALUES (580,3,21,'Părinților&lt;f&gt;ⓝ&lt;/f&gt;, nu-i întărâtați&lt;f&gt;[7]&lt;/f&gt; pe copiii voștri, ca să nu se descurajeze!');
INSERT INTO &quot;verses&quot; VALUES (580,3,22,'&lt;pb/&gt;Robilor&lt;f&gt;ⓞ&lt;/f&gt;, ascultați în&lt;f&gt;ⓟ&lt;/f&gt; toate privințele de stăpânii voștri pământești&lt;f&gt;ⓠ&lt;/f&gt;; nu numai când sunteți sub ochii lor, precum cei ce caută să le fie pe plac oamenilor, ci cu o inimă sinceră, ca unii care vă temeți de Domnul!');
INSERT INTO &quot;verses&quot; VALUES (580,3,23,'Orice&lt;f&gt;ⓡ&lt;/f&gt; faceți, să faceți din &lt;i&gt;tot&lt;/i&gt; sufletul, ca pentru Domnul, nu ca pentru oameni,');
INSERT INTO &quot;verses&quot; VALUES (580,3,24,'fiindcă știți că&lt;f&gt;ⓢ&lt;/f&gt; de la Domnul veți primi răsplata moștenirii! Voi Îi slujiți Domnului Hristos&lt;f&gt;ⓣ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (580,3,25,'iar cine face răul își va primi plata după răul pe care l-a făcut; și nu&lt;f&gt;ⓤ&lt;/f&gt; se are în vedere fața omului.');
INSERT INTO &quot;verses&quot; VALUES (580,4,1,'&lt;pb/&gt;Stăpânilor, dați-le&lt;f&gt;ⓥ&lt;/f&gt; robilor voștri ce este drept și ce li se cuvine, căci știți că și voi aveți un Stăpân în cer!');
INSERT INTO &quot;verses&quot; VALUES (580,4,2,'&lt;pb/&gt;Stăruiți&lt;f&gt;ⓦ&lt;/f&gt; și vegheați în rugăciune, cu&lt;f&gt;ⓧ&lt;/f&gt; mulțumiri!');
INSERT INTO &quot;verses&quot; VALUES (580,4,3,'Rugați-vă&lt;f&gt;ⓨ&lt;/f&gt; totodată și pentru noi, ca Dumnezeu să ne deschidă&lt;f&gt;ⓩ&lt;/f&gt; o ușă pentru Cuvânt, ca să vestesc taina&lt;f&gt;ⓐ&lt;/f&gt; lui Hristos, pentru&lt;f&gt;ⓑ&lt;/f&gt; care, &lt;i&gt;iată,&lt;/i&gt; mă găsesc în lanțuri:');
INSERT INTO &quot;verses&quot; VALUES (580,4,4,'ca s-o fac cunoscută așa cum trebuie!');
INSERT INTO &quot;verses&quot; VALUES (580,4,5,'Purtați-vă&lt;f&gt;ⓒ&lt;/f&gt; cu înțelepciune față de cei din afară și răscumpărați&lt;f&gt;ⓓ&lt;/f&gt; vremea!');
INSERT INTO &quot;verses&quot; VALUES (580,4,6,'Vorbirea voastră să fie totdeauna cu har&lt;f&gt;ⓔ&lt;/f&gt;, dreasă&lt;f&gt;ⓕ&lt;/f&gt; cu sare, ca să&lt;f&gt;ⓖ&lt;/f&gt; știți cum trebuie să-i răspundeți fiecăruia!');
INSERT INTO &quot;verses&quot; VALUES (580,4,7,'&lt;pb/&gt;&lt;i&gt;Veștile&lt;/i&gt; &lt;f&gt;ⓗ&lt;/f&gt; cu privire la mine le veți afla toate de la Tihic, frate preaiubit și slujitor credincios, tovarășul meu de slujbă în Domnul.');
INSERT INTO &quot;verses&quot; VALUES (580,4,8,'L-am&lt;f&gt;ⓘ&lt;/f&gt; trimis la voi tocmai în acest scop — ca să aflați vești despre noi și să vă aducă mângâiere în inimi —');
INSERT INTO &quot;verses&quot; VALUES (580,4,9,'împreună cu Onisim&lt;f&gt;ⓙ&lt;/f&gt;, frate credincios și preaiubit, care este dintre ai voștri. Ei vă vor povesti tot ce se petrece aici.');
INSERT INTO &quot;verses&quot; VALUES (580,4,10,'&lt;pb/&gt;Aristarh&lt;f&gt;ⓚ&lt;/f&gt;, &lt;i&gt;care este&lt;/i&gt; întemnițat împreună cu mine, vă trimite salutări; de asemenea, Marcu&lt;f&gt;ⓛ&lt;/f&gt;, vărul lui Barnaba (cu privire la care ați primit poruncă să-l primiți bine dacă vine la voi),');
INSERT INTO &quot;verses&quot; VALUES (580,4,11,'și Isus, zis Iust. Ei sunt dintre cei circumciși, singurii care au lucrat împreună &lt;i&gt;cu mine&lt;/i&gt; pentru Împărăția lui Dumnezeu, oameni care mi-au adus îmbărbătare.');
INSERT INTO &quot;verses&quot; VALUES (580,4,12,'Epafras&lt;f&gt;ⓜ&lt;/f&gt;, care este dintre ai voștri, vă trimite salutări. El, rob al lui Hristos Isus, totdeauna se&lt;f&gt;ⓝ&lt;/f&gt; luptă pentru voi în rugăciunile sale, ca să stați neclintiți în tot &lt;i&gt;ce ține de&lt;/i&gt; voia lui Dumnezeu, desăvârșiți&lt;f&gt;ⓞ&lt;/f&gt; și plini de convingere.');
INSERT INTO &quot;verses&quot; VALUES (580,4,13,'Mărturisesc despre el că trudește mult pentru voi, pentru cei din Laodiceea și pentru cei din Hierapolis.');
INSERT INTO &quot;verses&quot; VALUES (580,4,14,'Luca&lt;f&gt;ⓟ&lt;/f&gt;, doctorul preaiubit, și Dima&lt;f&gt;ⓠ&lt;/f&gt; vă trimit salutări.');
INSERT INTO &quot;verses&quot; VALUES (580,4,15,'&lt;pb/&gt;Spuneți-le salutări fraților din Laodiceea, Nimfei și bisericii din casa ei!');
INSERT INTO &quot;verses&quot; VALUES (580,4,16,'După ce va fi citită această&lt;f&gt;ⓡ&lt;/f&gt; epistolă la voi, faceți în așa fel încât să fie citită și în biserica laodiceenilor; iar voi, la rândul vostru, să citiți epistola&lt;f&gt;[8]&lt;/f&gt; care vă va veni din Laodiceea!');
INSERT INTO &quot;verses&quot; VALUES (580,4,17,'Și spuneți-i lui Arhip: „Ia seama la slujba pe care ai primit-o în Domnul, ca s-o împlinești!”');
INSERT INTO &quot;verses&quot; VALUES (580,4,18,'&lt;pb/&gt;Salutul l-am scris eu, Pavel, cu mâna mea. Aduceți-vă aminte de lanțurile mele! Harul să fie cu voi! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (590,1,1,'&lt;pb/&gt;Pavel, Silvan&lt;f&gt;ⓐ&lt;/f&gt; și Timotei către biserica tesalonicenilor, &lt;i&gt;care este&lt;/i&gt; în Dumnezeu Tatăl și în Domnul Isus Hristos: Har și pace [de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos]!');
INSERT INTO &quot;verses&quot; VALUES (590,1,2,'&lt;pb/&gt;Îi mulțumim&lt;f&gt;ⓑ&lt;/f&gt; totdeauna lui Dumnezeu pentru voi toți și vă amintim în rugăciunile noastre,');
INSERT INTO &quot;verses&quot; VALUES (590,1,3,'căci ne aducem aminte fără încetare&lt;f&gt;ⓒ&lt;/f&gt;, înaintea lui Dumnezeu, Tatăl nostru, de lucrarea&lt;f&gt;ⓓ&lt;/f&gt; credinței &lt;i&gt;voastre&lt;/i&gt;, de&lt;f&gt;ⓔ&lt;/f&gt; osteneala dragostei voastre și de nădejdea neclintită &lt;i&gt;pe care o aveți&lt;/i&gt; în Domnul nostru Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (590,1,4,'Știm, frați preaiubiți de Dumnezeu, că El v-a ales&lt;f&gt;ⓕ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (590,1,5,'fiindcă Evanghelia&lt;f&gt;ⓖ&lt;/f&gt; noastră v-a fost vestită nu doar cu vorbe, ci și cu putere, prin Duhul Sfânt, și cu multă convingere. De altfel, știți&lt;f&gt;ⓗ&lt;/f&gt; cum ne-am purtat între voi de dragul vostru.');
INSERT INTO &quot;verses&quot; VALUES (590,1,6,'Și voi&lt;f&gt;ⓘ&lt;/f&gt; înșivă ați călcat pe urmele mele și pe ale Domnului, întrucât ați primit Cuvântul în multe necazuri cu&lt;f&gt;ⓙ&lt;/f&gt; bucuria &lt;i&gt;care vine&lt;/i&gt; de la Duhul Sfânt,');
INSERT INTO &quot;verses&quot; VALUES (590,1,7,'așa încât ați ajuns o pildă pentru toți credincioșii din Macedonia și din Ahaia.');
INSERT INTO &quot;verses&quot; VALUES (590,1,8,'Într-adevăr, nu doar că, de la voi, Cuvântul Domnului a&lt;f&gt;ⓚ&lt;/f&gt; răsunat&lt;f&gt;ⓛ&lt;/f&gt; în Macedonia și în Ahaia, ci vestea despre credința voastră în Dumnezeu s-a răspândit pretutindeni, așa încât n-avem nevoie să mai vorbim de ea.');
INSERT INTO &quot;verses&quot; VALUES (590,1,9,'Căci se istorisește cu privire la noi ce&lt;f&gt;ⓜ&lt;/f&gt; primire am avut la voi și&lt;f&gt;ⓝ&lt;/f&gt; cum v-ați întors la Dumnezeu de la idoli, ca să slujiți Dumnezeului celui viu și adevărat');
INSERT INTO &quot;verses&quot; VALUES (590,1,10,'și ca să-L&lt;f&gt;ⓞ&lt;/f&gt; așteptați din&lt;f&gt;ⓟ&lt;/f&gt; ceruri pe Fiul Său, pe&lt;f&gt;ⓠ&lt;/f&gt; care L-a înviat din morți — pe Isus, care ne izbăvește de&lt;f&gt;ⓡ&lt;/f&gt; mânia viitoare.');
INSERT INTO &quot;verses&quot; VALUES (590,2,1,'&lt;pb/&gt;Voi&lt;f&gt;ⓢ&lt;/f&gt; înșivă știți, fraților, că venirea noastră la voi n-a fost zadarnică.');
INSERT INTO &quot;verses&quot; VALUES (590,2,2,'După ce am suferit și am fost batjocoriți în Filipi&lt;f&gt;ⓣ&lt;/f&gt;, precum știți, am îndrăznit, cu ajutorul Dumnezeului nostru, să vă vestim&lt;f&gt;ⓤ&lt;/f&gt; Evanghelia lui Dumnezeu în mijlocul multor&lt;f&gt;ⓥ&lt;/f&gt; zbateri.');
INSERT INTO &quot;verses&quot; VALUES (590,2,3,'Căci&lt;f&gt;ⓦ&lt;/f&gt; îndemnul nostru nu &lt;i&gt;izvorăște&lt;/i&gt; nici din rătăcire, nici din necurăție, nici din viclenie.');
INSERT INTO &quot;verses&quot; VALUES (590,2,4,'Ci, fiindcă&lt;f&gt;ⓧ&lt;/f&gt; Dumnezeu ne-a găsit vrednici să&lt;f&gt;ⓨ&lt;/f&gt; ne încredințeze Evanghelia, vorbim nu ca să le fim pe plac&lt;f&gt;ⓩ&lt;/f&gt; oamenilor, ci lui Dumnezeu, care ne cercetează inimile.');
INSERT INTO &quot;verses&quot; VALUES (590,2,5,'Într-adevăr, după cum știți, niciodată n-am&lt;f&gt;ⓐ&lt;/f&gt; folosit vorbe lingușitoare, n-am fost mânați de lăcomie (Dumnezeu &lt;i&gt;ne&lt;/i&gt; e martor)');
INSERT INTO &quot;verses&quot; VALUES (590,2,6,'și n-am&lt;f&gt;ⓑ&lt;/f&gt; căutat slavă de la oameni — nici de la voi, nici de la alții —');
INSERT INTO &quot;verses&quot; VALUES (590,2,7,'deși am fi putut să ne impunem&lt;f&gt;[1]&lt;/f&gt; ca apostoli&lt;f&gt;ⓒ&lt;/f&gt; ai lui Hristos; dimpotrivă, ne-am arătat ca niște copilași&lt;f&gt;[2]&lt;/f&gt; în mijlocul vostru. Ca o doică ce-și crește cu drag copiii,');
INSERT INTO &quot;verses&quot; VALUES (590,2,8,'tot astfel, în dragostea noastră fierbinte pentru voi, am găsit de cuviință să vă dăm&lt;f&gt;ⓓ&lt;/f&gt; nu numai Evanghelia lui Dumnezeu, ci și sufletele&lt;f&gt;ⓔ&lt;/f&gt; noastre; atât de scumpi ne ajunseserăți.');
INSERT INTO &quot;verses&quot; VALUES (590,2,9,'Vă aduceți aminte, fraților, de osteneala și de truda noastră: în timp ce lucram zi&lt;f&gt;ⓕ&lt;/f&gt; și noapte ca să nu vă împovărăm pe niciunul dintre voi, v-am propovăduit Evanghelia lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (590,2,10,'Voi și Dumnezeu sunteți martori că&lt;f&gt;ⓖ&lt;/f&gt; am avut o purtare sfântă, dreaptă și fără cusur față de voi, care credeți.');
INSERT INTO &quot;verses&quot; VALUES (590,2,11,'După cum știți, am fost pentru fiecare dintre voi ca un tată față de copiii lui:');
INSERT INTO &quot;verses&quot; VALUES (590,2,12,'vă sfătuiam, vă mângâiam și vă îndemnam să&lt;f&gt;ⓗ&lt;/f&gt; umblați în chip vrednic de Dumnezeu, care&lt;f&gt;ⓘ&lt;/f&gt; vă cheamă la Împărăția și la slava Sa.');
INSERT INTO &quot;verses&quot; VALUES (590,2,13,'&lt;pb/&gt;De aceea și noi Îi mulțumim fără încetare lui Dumnezeu că, atunci când ați primit Cuvântul lui Dumnezeu auzit de la noi, l-ați primit nu&lt;f&gt;ⓙ&lt;/f&gt; ca pe cuvântul oamenilor, ci, așa cum și este într-adevăr, ca pe Cuvântul lui Dumnezeu, care lucrează în voi, cei ce credeți.');
INSERT INTO &quot;verses&quot; VALUES (590,2,14,'Voi, fraților, ați călcat pe urmele bisericilor&lt;f&gt;ⓚ&lt;/f&gt; lui Dumnezeu din Iudeea, care sunt în Hristos Isus; pentru că și voi ați suferit din partea celor de un neam cu voi aceleași&lt;f&gt;ⓛ&lt;/f&gt; &lt;i&gt;lucruri&lt;/i&gt; pe care le-au suferit ele din partea&lt;f&gt;ⓜ&lt;/f&gt; iudeilor.');
INSERT INTO &quot;verses&quot; VALUES (590,2,15,'Aceștia&lt;f&gt;ⓝ&lt;/f&gt; i-au omorât pe Domnul Isus și pe profeți&lt;f&gt;ⓞ&lt;/f&gt;, pe noi ne-au prigonit, nu-I sunt plăcuți lui Dumnezeu și sunt vrăjmași tuturor oamenilor,');
INSERT INTO &quot;verses&quot; VALUES (590,2,16,'căci&lt;f&gt;ⓟ&lt;/f&gt; ne opresc să le vorbim neamurilor, ca să nu fie mântuite, și &lt;i&gt;astfel&lt;/i&gt; pun totdeauna vârf păcatelor lor. Dar i-a&lt;f&gt;ⓠ&lt;/f&gt; ajuns mânia lui Dumnezeu în cele din urmă!');
INSERT INTO &quot;verses&quot; VALUES (590,2,17,'&lt;pb/&gt;Noi, fraților, după ce am fost despărțiți de voi pentru o scurtă vreme, cu fața&lt;f&gt;ⓡ&lt;/f&gt;, dar nu &lt;i&gt;și&lt;/i&gt; cu inima, am căutat și mai mult să vă vedem chipul&lt;f&gt;ⓢ&lt;/f&gt;, cu mare dor.');
INSERT INTO &quot;verses&quot; VALUES (590,2,18,'Astfel, ne-am dorit să venim la voi (eu, Pavel, o dată, &lt;i&gt;ba chiar&lt;/i&gt; de două ori), dar ne-a împiedicat Satana&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (590,2,19,'Căci cine&lt;f&gt;ⓤ&lt;/f&gt; este nădejdea sau bucuria sau cununa noastră de laudă înaintea Domnului nostru Isus la&lt;f&gt;ⓥ&lt;/f&gt; venirea Lui? Oare nu voi?');
INSERT INTO &quot;verses&quot; VALUES (590,2,20,'Da, voi sunteți slava și bucuria noastră!');
INSERT INTO &quot;verses&quot; VALUES (590,3,1,'&lt;pb/&gt;De aceea, când n-am mai putut răbda, am&lt;f&gt;ⓦ&lt;/f&gt; găsit de cuviință să fim lăsați singuri în Atena');
INSERT INTO &quot;verses&quot; VALUES (590,3,2,'și vi l-am trimis pe Timotei, fratele nostru și lucrător împreună cu Dumnezeu în Evanghelia lui Hristos, ca să vă întărească și să vă îmbărbăteze în credința voastră,');
INSERT INTO &quot;verses&quot; VALUES (590,3,3,'pentru ca nimeni &lt;i&gt;dintre voi&lt;/i&gt; să nu se clatine în aceste necazuri, căci voi înșivă știți că la&lt;f&gt;ⓧ&lt;/f&gt; aceasta suntem rânduiți.');
INSERT INTO &quot;verses&quot; VALUES (590,3,4,'Și&lt;f&gt;ⓨ&lt;/f&gt; când eram la voi, vă spuneam dinainte că vom suferi necazuri, ceea ce s-a și întâmplat, după cum știți.');
INSERT INTO &quot;verses&quot; VALUES (590,3,5,'De aceea și eu, când n-am mai putut răbda, am trimis ca să aflu despre credința voastră, ca&lt;f&gt;ⓩ&lt;/f&gt; nu cumva să vă fi ispitit Ispititorul, iar osteneala&lt;f&gt;ⓐ&lt;/f&gt; noastră să fi ajuns zadarnică.');
INSERT INTO &quot;verses&quot; VALUES (590,3,6,'&lt;pb/&gt;Dar&lt;f&gt;ⓑ&lt;/f&gt; acum a venit Timotei de la voi la noi și ne-a adus vești bune despre credința și dragostea voastră, că păstrați mereu o plăcută aducere aminte despre noi și că doriți să ne vedeți, după cum și noi dorim să vă vedem pe voi.');
INSERT INTO &quot;verses&quot; VALUES (590,3,7,'De aceea, fraților, în toate strâmtorările și necazurile noastre, am&lt;f&gt;ⓒ&lt;/f&gt; fost mângâiați cu privire la voi prin credința voastră;');
INSERT INTO &quot;verses&quot; VALUES (590,3,8,'căci acum, dacă voi rămâneți&lt;f&gt;ⓓ&lt;/f&gt; tari în Domnul, noi trăim.');
INSERT INTO &quot;verses&quot; VALUES (590,3,9,'Și oare cum&lt;f&gt;ⓔ&lt;/f&gt; am putea să-I mulțumim îndeajuns lui Dumnezeu cu privire la voi pentru toată bucuria pe care o avem înaintea Dumnezeului nostru, datorită vouă?');
INSERT INTO &quot;verses&quot; VALUES (590,3,10,'Zi și noapte&lt;f&gt;ⓕ&lt;/f&gt; Îl rugăm&lt;f&gt;ⓖ&lt;/f&gt; nespus să&lt;f&gt;ⓗ&lt;/f&gt; vă putem vedea chipul și&lt;f&gt;ⓘ&lt;/f&gt; să putem împlini lipsurile credinței voastre.');
INSERT INTO &quot;verses&quot; VALUES (590,3,11,'&lt;pb/&gt;Însuși Dumnezeu, Tatăl nostru, și Domnul nostru Isus [Hristos] să netezească drumul nostru spre voi!');
INSERT INTO &quot;verses&quot; VALUES (590,3,12,'Iar pe voi Domnul să&lt;f&gt;ⓙ&lt;/f&gt; vă facă să creșteți și să sporiți în dragoste unii față de alții&lt;f&gt;ⓚ&lt;/f&gt; și față de toți &lt;i&gt;oamenii&lt;/i&gt;, după cum și noi &lt;i&gt;sporim în dragoste&lt;/i&gt; față de voi,');
INSERT INTO &quot;verses&quot; VALUES (590,3,13,'pentru ca&lt;f&gt;ⓛ&lt;/f&gt; inimile voastre să se întărească și să fie fără prihană în sfințenie înaintea lui Dumnezeu, Tatăl nostru, la venirea Domnului nostru Isus Hristos împreună cu&lt;f&gt;ⓜ&lt;/f&gt; toți sfinții Săi! Amin!');
INSERT INTO &quot;verses&quot; VALUES (590,4,1,'&lt;pb/&gt;Încolo, fraților, vă rugăm și vă îndemnăm, în &lt;i&gt;numele&lt;/i&gt; Domnului Isus, să trăiți potrivit &lt;i&gt;învățăturilor&lt;/i&gt;&lt;f&gt;ⓝ&lt;/f&gt; primite de la noi cu privire la felul în care trebuie să umblați și să-I fiți plăcuți lui Dumnezeu, ca să sporiți și mai mult&lt;f&gt;ⓞ&lt;/f&gt; &lt;i&gt;în toate&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (590,4,2,'Știți, într-adevăr, ce porunci v-am dat prin Domnul Isus.');
INSERT INTO &quot;verses&quot; VALUES (590,4,3,'Voia&lt;f&gt;ⓟ&lt;/f&gt; lui Dumnezeu este sfințirea voastră: să vă feriți de desfrâu&lt;f&gt;ⓠ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (590,4,4,'fiecare din voi să știe&lt;f&gt;ⓡ&lt;/f&gt; să-și stăpânească vasul&lt;f&gt;[3]&lt;/f&gt; în sfințenie și cinste,');
INSERT INTO &quot;verses&quot; VALUES (590,4,5,'nu&lt;f&gt;ⓢ&lt;/f&gt; în patimi și pofte ca&lt;f&gt;ⓣ&lt;/f&gt; neamurile, care&lt;f&gt;ⓤ&lt;/f&gt; nu-L cunosc pe Dumnezeu;');
INSERT INTO &quot;verses&quot; VALUES (590,4,6,'astfel, &lt;i&gt;nimeni&lt;/i&gt;&lt;f&gt;ⓥ&lt;/f&gt; să nu depășească&lt;f&gt;[4]&lt;/f&gt; &lt;i&gt;limitele&lt;/i&gt; și să-l înșele pe aproapele său în &lt;i&gt;această&lt;/i&gt; privință, căci Domnul pedepsește&lt;f&gt;ⓦ&lt;/f&gt; toate aceste lucruri, după cum v-am prevenit și v-am spus apăsat.');
INSERT INTO &quot;verses&quot; VALUES (590,4,7,'Căci Dumnezeu nu ne-a chemat la necurăție, ci&lt;f&gt;ⓧ&lt;/f&gt; la sfințire.');
INSERT INTO &quot;verses&quot; VALUES (590,4,8,'De aceea&lt;f&gt;ⓨ&lt;/f&gt;, cine nesocotește &lt;i&gt;aceste învățături&lt;/i&gt; nu pe un om îl nesocotește, ci pe Dumnezeu, care vă dă și Duhul Său cel Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (590,4,9,'&lt;pb/&gt;Cu privire la dragostea frățească nu aveți&lt;f&gt;ⓩ&lt;/f&gt; nevoie să vă scriem, căci voi înșivă&lt;f&gt;ⓐ&lt;/f&gt; sunteți învățați de Dumnezeu să&lt;f&gt;ⓑ&lt;/f&gt; vă iubiți unii pe alții,');
INSERT INTO &quot;verses&quot; VALUES (590,4,10,'ceea ce și&lt;f&gt;ⓒ&lt;/f&gt; faceți pentru toți frații din întreaga Macedonie. Dar vă îndemnăm, fraților, să sporiți tot mai mult în ea;');
INSERT INTO &quot;verses&quot; VALUES (590,4,11,'căutați să trăiți în liniște, să&lt;f&gt;ⓓ&lt;/f&gt; vă vedeți de ale voastre și să&lt;f&gt;ⓔ&lt;/f&gt; lucrați cu mâinile voastre, după cum v-am poruncit,');
INSERT INTO &quot;verses&quot; VALUES (590,4,12,'pentru ca&lt;f&gt;ⓕ&lt;/f&gt;, astfel, să vă purtați frumos cu cei din afară și să nu duceți lipsă de nimic&lt;f&gt;[5]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (590,4,13,'&lt;pb/&gt;Fraților, nu vrem să fiți în necunoștință despre cei ce au adormit, ca să nu vă întristați ca ceilalți, care&lt;f&gt;ⓖ&lt;/f&gt; n-au nădejde.');
INSERT INTO &quot;verses&quot; VALUES (590,4,14,'Căci, de vreme ce&lt;f&gt;ⓗ&lt;/f&gt; credem că Isus a murit și a înviat, &lt;i&gt;credem&lt;/i&gt; și că Dumnezeu, prin Isus, îi va aduce&lt;f&gt;ⓘ&lt;/f&gt; cu El&lt;f&gt;[6]&lt;/f&gt; pe cei ce au adormit.');
INSERT INTO &quot;verses&quot; VALUES (590,4,15,'Acest lucru vi-l spunem prin cuvântul Domnului: noi, cei&lt;f&gt;ⓙ&lt;/f&gt; vii, care vom fi rămas până la venirea Domnului, nu o vom lua nicidecum înaintea celor adormiți.');
INSERT INTO &quot;verses&quot; VALUES (590,4,16,'Căci Însuși Domnul&lt;f&gt;ⓚ&lt;/f&gt;, cu un glas poruncitor, ca de arhanghel, și cu trâmbița&lt;f&gt;ⓛ&lt;/f&gt; lui Dumnezeu, Se va coborî din cer, și întâi vor învia cei morți&lt;f&gt;ⓜ&lt;/f&gt; în Hristos.');
INSERT INTO &quot;verses&quot; VALUES (590,4,17,'Apoi&lt;f&gt;ⓝ&lt;/f&gt;, noi, cei vii, care vom fi rămas, vom fi răpiți împreună cu ei în&lt;f&gt;ⓞ&lt;/f&gt; nori ca să-L întâmpinăm pe Domnul în văzduh; și astfel vom&lt;f&gt;ⓟ&lt;/f&gt; fi totdeauna cu Domnul.');
INSERT INTO &quot;verses&quot; VALUES (590,4,18,'Mângâiați-vă deci&lt;f&gt;ⓠ&lt;/f&gt; unii pe alții cu aceste cuvinte!');
INSERT INTO &quot;verses&quot; VALUES (590,5,1,'&lt;pb/&gt;Cât despre vremuri&lt;f&gt;ⓡ&lt;/f&gt; și soroace, nu e nevoie să vi se scrie, fraților.');
INSERT INTO &quot;verses&quot; VALUES (590,5,2,'Voi înșivă știți foarte bine că ziua&lt;f&gt;ⓢ&lt;/f&gt; Domnului va veni ca un hoț noaptea.');
INSERT INTO &quot;verses&quot; VALUES (590,5,3,'Când vor zice: „Pace și siguranță!”, atunci un prăpăd&lt;f&gt;ⓣ&lt;/f&gt; neașteptat se va abate peste ei, ca durerile nașterii peste femeia însărcinată, și cu niciun chip nu vor scăpa.');
INSERT INTO &quot;verses&quot; VALUES (590,5,4,'Dar&lt;f&gt;ⓤ&lt;/f&gt; voi, fraților, nu sunteți în întuneric, astfel încât ziua &lt;i&gt;aceea&lt;/i&gt; să vină peste voi ca un hoț.');
INSERT INTO &quot;verses&quot; VALUES (590,5,5,'Voi toți sunteți fii&lt;f&gt;ⓥ&lt;/f&gt; ai luminii și fii ai zilei. Noi nu suntem ai nopții, nici ai întunericului.');
INSERT INTO &quot;verses&quot; VALUES (590,5,6,'De aceea&lt;f&gt;ⓦ&lt;/f&gt; să nu dormim ca ceilalți, ci să&lt;f&gt;ⓧ&lt;/f&gt; veghem și să fim treji.');
INSERT INTO &quot;verses&quot; VALUES (590,5,7,'Căci cei ce&lt;f&gt;ⓨ&lt;/f&gt; dorm dorm noaptea&lt;f&gt;ⓩ&lt;/f&gt;, și cei ce se îmbată se îmbată noaptea.');
INSERT INTO &quot;verses&quot; VALUES (590,5,8,'Dar noi, care suntem &lt;i&gt;fii&lt;/i&gt; ai zilei, să fim treji, să ne îmbrăcăm&lt;f&gt;ⓐ&lt;/f&gt; cu platoșa credinței și a dragostei și să avem drept coif nădejdea mântuirii.');
INSERT INTO &quot;verses&quot; VALUES (590,5,9,'Fiindcă Dumnezeu&lt;f&gt;ⓑ&lt;/f&gt; nu ne-a rânduit la mânie, ci&lt;f&gt;ⓒ&lt;/f&gt; ca să căpătăm mântuirea prin Domnul nostru Isus Hristos,');
INSERT INTO &quot;verses&quot; VALUES (590,5,10,'care&lt;f&gt;ⓓ&lt;/f&gt; a murit pentru noi, pentru ca, fie că veghem, fie că dormim, să trăim împreună cu El.');
INSERT INTO &quot;verses&quot; VALUES (590,5,11,'De aceea, mângâiați-vă și zidiți-vă unii pe alții, după cum și faceți!');
INSERT INTO &quot;verses&quot; VALUES (590,5,12,'&lt;pb/&gt;Vă rugăm, fraților, să-i&lt;f&gt;ⓔ&lt;/f&gt; onorați pe cei ce se ostenesc între voi, care vă cârmuiesc în Domnul și vă sfătuiesc.');
INSERT INTO &quot;verses&quot; VALUES (590,5,13,'Să-i prețuiți foarte mult, în dragoste, din pricina lucrării lor. Trăiți în pace&lt;f&gt;ⓕ&lt;/f&gt; unii cu alții!');
INSERT INTO &quot;verses&quot; VALUES (590,5,14,'Vă îndemnăm de asemenea, fraților, să-i mustrați&lt;f&gt;ⓖ&lt;/f&gt; pe cei ce trăiesc în neorânduială, să-i îmbărbătați pe cei deznădăjduiți, să-i sprijiniți&lt;f&gt;ⓗ&lt;/f&gt; pe cei slabi, să fiți&lt;f&gt;ⓘ&lt;/f&gt; îndelung răbdători cu toți.');
INSERT INTO &quot;verses&quot; VALUES (590,5,15,'Luați seama&lt;f&gt;ⓙ&lt;/f&gt; ca nimeni să nu-i întoarcă altuia rău pentru rău, ci urmăriți&lt;f&gt;ⓚ&lt;/f&gt; întotdeauna binele, atât între voi, cât și față de toți!');
INSERT INTO &quot;verses&quot; VALUES (590,5,16,'Bucurați-vă&lt;f&gt;ⓛ&lt;/f&gt; întotdeauna!');
INSERT INTO &quot;verses&quot; VALUES (590,5,17,'Rugați-vă&lt;f&gt;ⓜ&lt;/f&gt; neîncetat!');
INSERT INTO &quot;verses&quot; VALUES (590,5,18,'Fiți recunoscători în toate&lt;f&gt;ⓝ&lt;/f&gt;, căci aceasta este voia lui Dumnezeu, în Hristos Isus, cu privire la voi!');
INSERT INTO &quot;verses&quot; VALUES (590,5,19,'Nu&lt;f&gt;ⓞ&lt;/f&gt; stingeți Duhul!');
INSERT INTO &quot;verses&quot; VALUES (590,5,20,'Nu&lt;f&gt;ⓟ&lt;/f&gt; disprețuiți profețiile,');
INSERT INTO &quot;verses&quot; VALUES (590,5,21,'ci cercetați&lt;f&gt;ⓠ&lt;/f&gt; totul și păstrați ce este bun!');
INSERT INTO &quot;verses&quot; VALUES (590,5,22,'Feriți-vă de orice formă de rău!');
INSERT INTO &quot;verses&quot; VALUES (590,5,23,'&lt;pb/&gt;Dumnezeul păcii să&lt;f&gt;ⓡ&lt;/f&gt; vă sfințească El Însuși pe deplin, iar duhul vostru, sufletul vostru și trupul vostru să fie păzite întregi, fără cusur, la venirea Domnului nostru Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (590,5,24,'Cel ce v-a chemat este credincios&lt;f&gt;ⓢ&lt;/f&gt; și va face lucrul acesta.');
INSERT INTO &quot;verses&quot; VALUES (590,5,25,'Fraților, rugați-vă&lt;f&gt;ⓣ&lt;/f&gt; și pentru noi!');
INSERT INTO &quot;verses&quot; VALUES (590,5,26,'Spuneți-le salutări&lt;f&gt;ⓤ&lt;/f&gt; tuturor fraților cu o sărutare sfântă!');
INSERT INTO &quot;verses&quot; VALUES (590,5,27,'Vă rog fierbinte, înaintea Domnului, ca&lt;f&gt;ⓥ&lt;/f&gt; epistola &lt;i&gt;aceasta&lt;/i&gt; să le fie citită tuturor fraților.');
INSERT INTO &quot;verses&quot; VALUES (590,5,28,'Harul&lt;f&gt;ⓦ&lt;/f&gt; Domnului nostru Isus Hristos &lt;i&gt;să fie&lt;/i&gt; cu voi! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (600,1,1,'&lt;pb/&gt;Pavel, Silvan și&lt;f&gt;ⓐ&lt;/f&gt; Timotei către biserica tesalonicenilor, &lt;i&gt;care este&lt;/i&gt; în&lt;f&gt;ⓑ&lt;/f&gt; Dumnezeu, Tatăl nostru, și în Domnul Isus Hristos:');
INSERT INTO &quot;verses&quot; VALUES (600,1,2,'Har&lt;f&gt;ⓒ&lt;/f&gt; și pace de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (600,1,3,'&lt;pb/&gt;Trebuie&lt;f&gt;ⓓ&lt;/f&gt; să-I mulțumim lui Dumnezeu totdeauna pentru voi, fraților, după cum se cuvine, deoarece credința voastră crește din belșug, iar dragostea fiecăruia din voi față de ceilalți sporește tot mai mult.');
INSERT INTO &quot;verses&quot; VALUES (600,1,4,'De aceea noi ne&lt;f&gt;ⓔ&lt;/f&gt; lăudăm cu voi în bisericile lui Dumnezeu pentru&lt;f&gt;ⓕ&lt;/f&gt; statornicia și credincioșia voastră în&lt;f&gt;ⓖ&lt;/f&gt; toate prigonirile și necazurile pe care le suferiți.');
INSERT INTO &quot;verses&quot; VALUES (600,1,5,'&lt;i&gt;Aceasta este&lt;/i&gt; o&lt;f&gt;ⓗ&lt;/f&gt; dovadă clară despre dreapta judecată a lui Dumnezeu, ca să fiți găsiți vrednici de Împărăția lui Dumnezeu, pentru&lt;f&gt;ⓘ&lt;/f&gt; care și suferiți.');
INSERT INTO &quot;verses&quot; VALUES (600,1,6,'Fiindcă&lt;f&gt;ⓙ&lt;/f&gt; este drept din partea lui Dumnezeu să aducă necaz peste cei care vă pricinuiesc necaz,');
INSERT INTO &quot;verses&quot; VALUES (600,1,7,'iar vouă, celor în necaz, &lt;i&gt;să vă dea&lt;/i&gt; odihnă&lt;f&gt;ⓚ&lt;/f&gt; împreună cu noi, la descoperirea Domnului&lt;f&gt;ⓛ&lt;/f&gt; Isus din cer, cu îngerii puterii Lui&lt;f&gt;[1]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (600,1,8,'într-un&lt;f&gt;ⓜ&lt;/f&gt; foc învăpăiat, ca să-i pedepsească pe cei ce&lt;f&gt;ⓝ&lt;/f&gt; nu-L cunosc pe Dumnezeu și pe cei ce nu&lt;f&gt;ⓞ&lt;/f&gt; ascultă de Evanghelia Domnului nostru Isus [Hristos].');
INSERT INTO &quot;verses&quot; VALUES (600,1,9,'Ei&lt;f&gt;ⓟ&lt;/f&gt; vor avea ca pedeapsă o nimicire veșnică de la fața Domnului și de la&lt;f&gt;ⓠ&lt;/f&gt; slava puterii Lui,');
INSERT INTO &quot;verses&quot; VALUES (600,1,10,'când&lt;f&gt;ⓡ&lt;/f&gt; va veni în ziua aceea ca să fie proslăvit între sfinții Săi și&lt;f&gt;ⓢ&lt;/f&gt; privit cu uimire de toți cei ce au crezut; căci mărturia noastră între voi a fost crezută.');
INSERT INTO &quot;verses&quot; VALUES (600,1,11,'&lt;pb/&gt;De aceea ne și rugăm totdeauna pentru voi ca Dumnezeul nostru să&lt;f&gt;ⓣ&lt;/f&gt; vă facă vrednici de chemarea &lt;i&gt;Lui&lt;/i&gt; și, prin puterea &lt;i&gt;Lui&lt;/i&gt;, să împlinească &lt;i&gt;în voi&lt;/i&gt; orice dorință de a face binele&lt;f&gt;ⓤ&lt;/f&gt; și orice lucrare izvorâtă din credință,');
INSERT INTO &quot;verses&quot; VALUES (600,1,12,'pentru ca&lt;f&gt;ⓥ&lt;/f&gt; Numele Domnului nostru Isus [Hristos] să fie proslăvit prin voi, &lt;i&gt;precum&lt;/i&gt; și voi în El, potrivit cu harul Dumnezeului nostru și al Domnului Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (600,2,1,'&lt;pb/&gt;În ce&lt;f&gt;ⓦ&lt;/f&gt; privește venirea Domnului nostru Isus Hristos și&lt;f&gt;ⓧ&lt;/f&gt; strângerea noastră laolaltă cu El, vă rugăm, fraților,');
INSERT INTO &quot;verses&quot; VALUES (600,2,2,'să&lt;f&gt;ⓨ&lt;/f&gt; nu vă clătinați așa de repede în mintea voastră și, de asemenea, să nu vă lăsați tulburați de vreun duh, un cuvânt sau o epistolă &lt;i&gt;care ar fi&lt;/i&gt; de la noi — cum că ziua Domnului ar fi venit.');
INSERT INTO &quot;verses&quot; VALUES (600,2,3,'&lt;pb/&gt;Nimeni să&lt;f&gt;ⓩ&lt;/f&gt; nu vă amăgească în vreun fel, căci &lt;i&gt;ziua aceea&lt;/i&gt; nu&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;va sosi&lt;/i&gt; înainte să fi venit lepădarea de credință&lt;f&gt;[2]&lt;/f&gt; și să se fi descoperit omul&lt;f&gt;ⓑ&lt;/f&gt; fărădelegii&lt;f&gt;[3]&lt;/f&gt;, fiul pierzării,');
INSERT INTO &quot;verses&quot; VALUES (600,2,4,'cel care se împotrivește și se&lt;f&gt;ⓒ&lt;/f&gt; înalță mai&lt;f&gt;ⓓ&lt;/f&gt; presus de orice așa-zis dumnezeu sau obiect care primește închinare, până acolo încât să se așeze în Templul lui Dumnezeu, dându-se drept Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (600,2,5,'Nu vă aduceți aminte că, pe când eram încă la voi, vă spuneam aceste lucruri?');
INSERT INTO &quot;verses&quot; VALUES (600,2,6,'Și acum știți ce-l oprește să nu se descopere &lt;i&gt;decât&lt;/i&gt; la vremea lui.');
INSERT INTO &quot;verses&quot; VALUES (600,2,7,'Taina&lt;f&gt;ⓔ&lt;/f&gt; fărădelegii este deja la lucru; trebuie doar ca cel care &lt;i&gt;îl&lt;/i&gt; oprește acum să fie luat din drum.');
INSERT INTO &quot;verses&quot; VALUES (600,2,8,'Și atunci se va descoperi cel nelegiuit, pe care Domnul Isus îl va nimici cu&lt;f&gt;ⓕ&lt;/f&gt; suflarea gurii Sale și-l va prăpădi cu arătarea venirii Sale.');
INSERT INTO &quot;verses&quot; VALUES (600,2,9,'Arătarea &lt;i&gt;celui nelegiuit&lt;/i&gt; se va face prin lucrarea Satanei, cu tot felul de minuni, semne&lt;f&gt;ⓖ&lt;/f&gt; și puteri mincinoase');
INSERT INTO &quot;verses&quot; VALUES (600,2,10,'și cu toate amăgirile nelegiuirii pentru cei ce sunt pe calea pierzării pentru că n-au primit dragostea de adevăr ca să fie mântuiți.');
INSERT INTO &quot;verses&quot; VALUES (600,2,11,'De aceea&lt;f&gt;ⓗ&lt;/f&gt; Dumnezeu le trimite o lucrare de rătăcire, ca să&lt;f&gt;ⓘ&lt;/f&gt; creadă o minciună');
INSERT INTO &quot;verses&quot; VALUES (600,2,12,'și &lt;i&gt;astfel&lt;/i&gt; să fie osândiți toți cei ce n-au crezut adevărul, ci au&lt;f&gt;ⓙ&lt;/f&gt; găsit plăcere în nelegiuire.');
INSERT INTO &quot;verses&quot; VALUES (600,2,13,'&lt;pb/&gt;Noi&lt;f&gt;ⓚ&lt;/f&gt; însă, frați preaiubiți de Domnul, trebuie să-I mulțumim totdeauna lui Dumnezeu pentru voi, căci Dumnezeu&lt;f&gt;ⓛ&lt;/f&gt; v-a ales&lt;f&gt;ⓜ&lt;/f&gt; ca prim rod&lt;f&gt;[4]&lt;/f&gt; pentru mântuire, în&lt;f&gt;ⓝ&lt;/f&gt; sfințirea făcută de Duhul și credința în adevăr.');
INSERT INTO &quot;verses&quot; VALUES (600,2,14,'La aceasta v-a chemat El, prin Evanghelia noastră, urmând să dobândiți&lt;f&gt;ⓞ&lt;/f&gt; slava Domnului nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (600,2,15,'&lt;pb/&gt;Așadar, fraților, rămâneți&lt;f&gt;ⓟ&lt;/f&gt; tari și țineți învățăturile&lt;f&gt;[5]&lt;/f&gt; pe care le-ați primit&lt;f&gt;ⓠ&lt;/f&gt; fie prin viu grai, fie prin epistola noastră!');
INSERT INTO &quot;verses&quot; VALUES (600,2,16,'Și Însuși&lt;f&gt;ⓡ&lt;/f&gt; Domnul nostru Isus Hristos și Dumnezeu, Tatăl nostru, care&lt;f&gt;ⓢ&lt;/f&gt; ne-a iubit și ne-a dat prin harul &lt;i&gt;Său&lt;/i&gt; o mângâiere veșnică și o bună nădejde&lt;f&gt;ⓣ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (600,2,17,'să vă mângâie inimile și să&lt;f&gt;ⓤ&lt;/f&gt; vă întărească în orice lucru și cuvânt bun!');
INSERT INTO &quot;verses&quot; VALUES (600,3,1,'&lt;pb/&gt;Încolo, fraților, rugați-vă&lt;f&gt;ⓥ&lt;/f&gt; pentru noi, ca să se răspândească repede Cuvântul Domnului și să fie proslăvit, cum este la voi,');
INSERT INTO &quot;verses&quot; VALUES (600,3,2,'și ca să&lt;f&gt;ⓦ&lt;/f&gt; fim izbăviți de oameni rătăciți și răi, căci nu toți au credința!');
INSERT INTO &quot;verses&quot; VALUES (600,3,3,'Credincios&lt;f&gt;ⓧ&lt;/f&gt; este Domnul: El vă va întări și vă va&lt;f&gt;ⓨ&lt;/f&gt; păzi de Cel Rău.');
INSERT INTO &quot;verses&quot; VALUES (600,3,4,'Cu privire la voi, suntem convinși&lt;f&gt;ⓩ&lt;/f&gt; în Domnul că faceți și veți face cum vă poruncim.');
INSERT INTO &quot;verses&quot; VALUES (600,3,5,'Domnul&lt;f&gt;ⓐ&lt;/f&gt; să vă îndrepte inimile spre dragostea lui Dumnezeu și răbdarea lui Hristos!');
INSERT INTO &quot;verses&quot; VALUES (600,3,6,'&lt;pb/&gt;În Numele Domnului nostru Isus Hristos, vă poruncim, fraților, să&lt;f&gt;ⓑ&lt;/f&gt; vă țineți departe de&lt;f&gt;ⓒ&lt;/f&gt; orice frate care trăiește în&lt;f&gt;ⓓ&lt;/f&gt; neorânduială, și nu după învățăturile&lt;f&gt;ⓔ&lt;/f&gt; pe care le-ați primit de la noi.');
INSERT INTO &quot;verses&quot; VALUES (600,3,7,'Voi înșivă știți cum&lt;f&gt;ⓕ&lt;/f&gt; trebuie să călcați pe urmele noastre; căci noi&lt;f&gt;ⓖ&lt;/f&gt; n-am trăit în neorânduială între voi,');
INSERT INTO &quot;verses&quot; VALUES (600,3,8,'nici n-am mâncat fără plată pâinea nimănui, ci cu osteneală și trudă&lt;f&gt;ⓗ&lt;/f&gt;, muncind zi și noapte, ca să nu împovărăm pe niciunul dintre voi.');
INSERT INTO &quot;verses&quot; VALUES (600,3,9,'Nu&lt;f&gt;ⓘ&lt;/f&gt; că n-am avea dreptul &lt;i&gt;acesta&lt;/i&gt;, ci fiindcă &lt;i&gt;am vrut&lt;/i&gt; să vă dăm în&lt;f&gt;ⓙ&lt;/f&gt; noi înșine o pildă, ca să călcați pe urmele noastre.');
INSERT INTO &quot;verses&quot; VALUES (600,3,10,'De altfel, când eram la voi, vă porunceam astfel: „Dacă cineva&lt;f&gt;ⓚ&lt;/f&gt; nu vrea să lucreze, nici să nu mănânce!”');
INSERT INTO &quot;verses&quot; VALUES (600,3,11,'&lt;pb/&gt;Auzim însă că unii dintre voi trăiesc&lt;f&gt;ⓛ&lt;/f&gt; în neorânduială, nu lucrează nimic&lt;f&gt;ⓜ&lt;/f&gt;, ci se țin de nimicuri&lt;f&gt;[6]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (600,3,12,'Unor astfel de oameni le poruncim&lt;f&gt;ⓝ&lt;/f&gt; și le spunem în Domnul &lt;i&gt;nostru&lt;/i&gt; Isus Hristos să-și&lt;f&gt;ⓞ&lt;/f&gt; mănânce pâinea lucrând în liniște.');
INSERT INTO &quot;verses&quot; VALUES (600,3,13,'Voi însă, fraților, fiți&lt;f&gt;ⓟ&lt;/f&gt; neobosiți în facerea binelui!');
INSERT INTO &quot;verses&quot; VALUES (600,3,14,'Iar dacă cineva nu ascultă de cuvântul nostru din &lt;i&gt;această&lt;/i&gt; epistolă, însemnați-vi-l și să&lt;f&gt;ⓠ&lt;/f&gt; nu aveți legături cu el, ca să-i fie rușine!');
INSERT INTO &quot;verses&quot; VALUES (600,3,15,'Să&lt;f&gt;ⓡ&lt;/f&gt; nu-l socotiți ca pe un vrăjmaș, ci&lt;f&gt;ⓢ&lt;/f&gt; să-l sfătuiți ca pe un frate!');
INSERT INTO &quot;verses&quot; VALUES (600,3,16,'&lt;pb/&gt;Domnul&lt;f&gt;ⓣ&lt;/f&gt; păcii să vă dea El Însuși întotdeauna pacea în orice fel! Domnul să fie cu voi toți!');
INSERT INTO &quot;verses&quot; VALUES (600,3,17,'&lt;pb/&gt;Salutul&lt;f&gt;ⓤ&lt;/f&gt; este &lt;i&gt;scris&lt;/i&gt; cu mâna mea: de la Pavel. Acesta este semnul din fiecare epistolă; așa scriu &lt;i&gt;eu&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (600,3,18,'&lt;pb/&gt;Harul Domnului nostru Isus Hristos să fie cu voi toți! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (610,1,1,'&lt;pb/&gt;Pavel, apostol al lui Hristos Isus prin&lt;f&gt;ⓐ&lt;/f&gt; porunca lui&lt;f&gt;ⓑ&lt;/f&gt; Dumnezeu, Mântuitorul nostru, și a lui Hristos Isus, nădejdea&lt;f&gt;ⓒ&lt;/f&gt; noastră,');
INSERT INTO &quot;verses&quot; VALUES (610,1,2,'către Timotei&lt;f&gt;ⓓ&lt;/f&gt;, adevăratul&lt;f&gt;ⓔ&lt;/f&gt; meu copil în credință: Har&lt;f&gt;ⓕ&lt;/f&gt;, îndurare și pace de la Dumnezeu, Tatăl [nostru], și de la Hristos Isus, Domnul nostru!');
INSERT INTO &quot;verses&quot; VALUES (610,1,3,'&lt;pb/&gt;După cum te-am rugat la plecarea&lt;f&gt;ⓖ&lt;/f&gt; mea în Macedonia, rămâi în Efes, ca&lt;f&gt;ⓗ&lt;/f&gt; să le poruncești unora să nu dea o altă învățătură');
INSERT INTO &quot;verses&quot; VALUES (610,1,4,'și nici să nu&lt;f&gt;ⓘ&lt;/f&gt; ia seama la mituri și genealogii fără sfârșit, care mai degrabă dau&lt;f&gt;ⓙ&lt;/f&gt; naștere la dispute decât fac să înainteze planul&lt;f&gt;[1]&lt;/f&gt; lui Dumnezeu, &lt;i&gt;cunoscut&lt;/i&gt; prin credință!');
INSERT INTO &quot;verses&quot; VALUES (610,1,5,'Ținta&lt;f&gt;ⓚ&lt;/f&gt; poruncii este dragostea, care&lt;f&gt;ⓛ&lt;/f&gt; vine dintr-o inimă curată, dintr-o conștiință bună și dintr-o credință neprefăcută.');
INSERT INTO &quot;verses&quot; VALUES (610,1,6,'Unii, fiindcă s-au îndepărtat de aceste lucruri, au rătăcit &lt;i&gt;și s-au dedat&lt;/i&gt; la flecării&lt;f&gt;ⓜ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (610,1,7,'doresc să fie învățători ai Legii, dar nu înțeleg nici lucrurile despre care vorbesc&lt;f&gt;ⓝ&lt;/f&gt;, nici cele asupra cărora insistă.');
INSERT INTO &quot;verses&quot; VALUES (610,1,8,'&lt;pb/&gt;Noi știm că Legea&lt;f&gt;ⓞ&lt;/f&gt; este bună dacă cineva o folosește legitim,');
INSERT INTO &quot;verses&quot; VALUES (610,1,9,'înțelegând&lt;f&gt;ⓟ&lt;/f&gt; că nu pentru cel drept este făcută Legea, ci pentru cei nelegiuiți și răzvrătiți, pentru cei fără evlavie și păcătoși, pentru cei nesfinți și lumești&lt;f&gt;[2]&lt;/f&gt;, pentru ucigașii de tată și de mamă, pentru ucigașii de oameni,');
INSERT INTO &quot;verses&quot; VALUES (610,1,10,'pentru desfrânați, pentru homosexuali&lt;f&gt;[3]&lt;/f&gt;, pentru traficanții de sclavi&lt;f&gt;[4]&lt;/f&gt;, pentru mincinoși, pentru cei ce jură strâmb și pentru orice altă &lt;i&gt;faptă&lt;/i&gt; contrară învățăturii&lt;f&gt;ⓠ&lt;/f&gt; sănătoase —');
INSERT INTO &quot;verses&quot; VALUES (610,1,11,'&lt;i&gt;învățătură&lt;/i&gt; potrivită cu Evanghelia slavei fericitului&lt;f&gt;ⓡ&lt;/f&gt; Dumnezeu care&lt;f&gt;ⓢ&lt;/f&gt; mi-a fost încredințată.');
INSERT INTO &quot;verses&quot; VALUES (610,1,12,'&lt;pb/&gt;Îi mulțumesc lui Hristos Isus, Domnul nostru, care&lt;f&gt;ⓣ&lt;/f&gt; m-a întărit, fiindcă&lt;f&gt;ⓤ&lt;/f&gt; m-a socotit vrednic de încredere și m-a pus&lt;f&gt;ⓥ&lt;/f&gt; în slujba &lt;i&gt;Lui&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (610,1,13,'măcar că&lt;f&gt;ⓦ&lt;/f&gt; mai înainte eram un hulitor, un prigonitor și un batjocoritor. Dar am căpătat îndurare, pentru că lucram din neștiință&lt;f&gt;ⓧ&lt;/f&gt;, în necredință!');
INSERT INTO &quot;verses&quot; VALUES (610,1,14,'Și harul&lt;f&gt;ⓨ&lt;/f&gt; Domnului nostru s-a înmulțit peste măsură de mult împreună cu&lt;f&gt;ⓩ&lt;/f&gt; credința și&lt;f&gt;ⓐ&lt;/f&gt; cu dragostea care sunt în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (610,1,15,'O, adevărat&lt;f&gt;ⓑ&lt;/f&gt; și cu totul vrednic de primit este cuvântul &lt;i&gt;acesta&lt;/i&gt;: Hristos&lt;f&gt;ⓒ&lt;/f&gt; Isus a venit în lume ca să-i mântuiască pe păcătoși, dintre care cel dintâi sunt eu.');
INSERT INTO &quot;verses&quot; VALUES (610,1,16,'Tocmai de aceea am căpătat&lt;f&gt;ⓓ&lt;/f&gt; îndurare, pentru ca Hristos Isus să-Și arate în mine, cel dintâi, toată îndelunga Lui răbdare, ca&lt;f&gt;ⓔ&lt;/f&gt; o pildă pentru cei ce vor crede în El ca să aibă viață veșnică.');
INSERT INTO &quot;verses&quot; VALUES (610,1,17,'Ale Împăratului&lt;f&gt;ⓕ&lt;/f&gt; veșniciilor, ale nepieritorului&lt;f&gt;ⓖ&lt;/f&gt;, nevăzutului&lt;f&gt;ⓗ&lt;/f&gt; și singurului Dumnezeu să fie cinstea și slava în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (610,1,18,'&lt;pb/&gt;Timotei, fiule, îți încredințez această poruncă&lt;f&gt;ⓘ&lt;/f&gt; potrivit&lt;f&gt;ⓙ&lt;/f&gt; profețiilor făcute mai înainte despre tine, ca prin ele să te&lt;f&gt;ⓚ&lt;/f&gt; lupți lupta cea bună');
INSERT INTO &quot;verses&quot; VALUES (610,1,19,'având&lt;f&gt;ⓛ&lt;/f&gt; credință și o conștiință curată, pe care unii au părăsit-o și au naufragiat în ce privește credința.');
INSERT INTO &quot;verses&quot; VALUES (610,1,20,'Din numărul lor sunt Imeneu&lt;f&gt;ⓜ&lt;/f&gt; și Alexandru&lt;f&gt;ⓝ&lt;/f&gt;, pe care i-am&lt;f&gt;ⓞ&lt;/f&gt; dat pe mâna Satanei, ca să se învețe să nu &lt;i&gt;mai&lt;/i&gt; hulească.');
INSERT INTO &quot;verses&quot; VALUES (610,2,1,'&lt;pb/&gt;Vă îndemn deci, înainte de toate, să faceți cereri, rugăciuni, mijlociri, mulțumiri pentru toți oamenii,');
INSERT INTO &quot;verses&quot; VALUES (610,2,2,'pentru&lt;f&gt;ⓟ&lt;/f&gt; împărați și pentru&lt;f&gt;ⓠ&lt;/f&gt; toți cei ce sunt înălțați în dregătorii, ca să putem duce astfel o viață pașnică și liniștită, cu toată evlavia și &lt;i&gt;cu toată&lt;/i&gt; demnitatea.');
INSERT INTO &quot;verses&quot; VALUES (610,2,3,'Lucrul acesta este bun și bine primit înaintea lui Dumnezeu, Mântuitorul nostru,');
INSERT INTO &quot;verses&quot; VALUES (610,2,4,'care&lt;f&gt;ⓡ&lt;/f&gt; vrea ca toți oamenii să fie mântuiți și&lt;f&gt;ⓢ&lt;/f&gt; să vină la cunoștința deplină a adevărului.');
INSERT INTO &quot;verses&quot; VALUES (610,2,5,'&lt;pb/&gt;&lt;t&gt;Căci&lt;f&gt;ⓣ&lt;/f&gt; este un singur Dumnezeu&lt;/t&gt; &lt;t&gt;și Unul&lt;f&gt;ⓤ&lt;/f&gt; singur este mijlocitor între Dumnezeu și oameni:&lt;/t&gt; &lt;t&gt;omul Hristos Isus,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,2,6,'&lt;t&gt;care&lt;f&gt;ⓥ&lt;/f&gt; S-a dat pe Sine răscumpărare pentru toți,&lt;/t&gt; &lt;t&gt;ca mărturie&lt;f&gt;ⓦ&lt;/f&gt; la vremea cuvenită&lt;f&gt;ⓧ&lt;/f&gt;.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,2,7,'&lt;pb/&gt;Pentru aceasta am fost pus eu propovăduitor&lt;f&gt;ⓨ&lt;/f&gt;, apostol — spun adevărul [în Hristos], nu mint — și învățător&lt;f&gt;ⓩ&lt;/f&gt; al neamurilor în credință și adevăr.');
INSERT INTO &quot;verses&quot; VALUES (610,2,8,'&lt;pb/&gt;Vreau ca bărbații să se roage în&lt;f&gt;ⓐ&lt;/f&gt; orice loc și să ridice&lt;f&gt;ⓑ&lt;/f&gt; &lt;i&gt;spre cer&lt;/i&gt; mâini curate, fără mânie și fără ceartă&lt;f&gt;[5]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (610,2,9,'Tot astfel, femeile&lt;f&gt;ⓒ&lt;/f&gt; să se împodobească în chip cuviincios, cu modestie și decență&lt;f&gt;[6]&lt;/f&gt;, nu cu împletituri &lt;i&gt;de păr&lt;/i&gt;, nici cu aur, cu mărgăritare sau cu haine scumpe,');
INSERT INTO &quot;verses&quot; VALUES (610,2,10,'ci&lt;f&gt;ⓓ&lt;/f&gt; cu fapte bune, cum se cuvine femeilor care spun că sunt evlavioase&lt;f&gt;[7]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (610,2,11,'Femeia să învețe în tăcere, cu toată supunerea.');
INSERT INTO &quot;verses&quot; VALUES (610,2,12,'Femeii&lt;f&gt;ⓔ&lt;/f&gt; nu-i dau voie să dea învățătură, nici&lt;f&gt;ⓕ&lt;/f&gt; să aibă stăpânire asupra bărbatului, ci să stea în tăcere.');
INSERT INTO &quot;verses&quot; VALUES (610,2,13,'Căci întâi a fost plămădit Adam&lt;f&gt;ⓖ&lt;/f&gt; și apoi Eva.');
INSERT INTO &quot;verses&quot; VALUES (610,2,14,'Și nu Adam&lt;f&gt;ⓗ&lt;/f&gt; a fost amăgit, ci femeia, fiind amăgită, a ajuns să calce porunca.');
INSERT INTO &quot;verses&quot; VALUES (610,2,15,'Totuși ea va fi mântuită prin nașterea-de-copii&lt;f&gt;[8]&lt;/f&gt;; numai să stăruie&lt;f&gt;[9]&lt;/f&gt; în credință, în dragoste și în sfințenie, cu &lt;i&gt;toată&lt;/i&gt; chibzuința.');
INSERT INTO &quot;verses&quot; VALUES (610,3,1,'&lt;pb/&gt;Adevărat&lt;f&gt;ⓘ&lt;/f&gt; este cuvântul&lt;f&gt;[10]&lt;/f&gt; &lt;i&gt;acesta&lt;/i&gt;: Dacă râvnește cineva să fie&lt;f&gt;ⓙ&lt;/f&gt; episcop&lt;f&gt;[11]&lt;/f&gt;, dorește un lucru&lt;f&gt;ⓚ&lt;/f&gt; bun.');
INSERT INTO &quot;verses&quot; VALUES (610,3,2,'Dar trebuie ca episcopul&lt;f&gt;ⓛ&lt;/f&gt; să fie mai presus de orice reproș, bărbatul&lt;f&gt;ⓜ&lt;/f&gt; unei singure soții, cu mintea trează, chibzuit, prezentabil, ospitalier, în stare&lt;f&gt;ⓝ&lt;/f&gt; să-i învețe pe alții.');
INSERT INTO &quot;verses&quot; VALUES (610,3,3,'Să nu fie nici dedat la vin&lt;f&gt;ⓞ&lt;/f&gt;, nici bătăuș&lt;f&gt;ⓟ&lt;/f&gt;, [nici&lt;f&gt;ⓠ&lt;/f&gt; dornic de câștig mârșav,] ci să fie blând&lt;f&gt;ⓡ&lt;/f&gt;, nu certăreț, nu iubitor de bani,');
INSERT INTO &quot;verses&quot; VALUES (610,3,4,'să-și conducă bine casa și să-și țină&lt;f&gt;ⓢ&lt;/f&gt; copiii în supunere cu toată demnitatea.');
INSERT INTO &quot;verses&quot; VALUES (610,3,5,'Căci dacă cineva nu știe să-și conducă bine propria casă, cum va avea grijă de biserica lui Dumnezeu?');
INSERT INTO &quot;verses&quot; VALUES (610,3,6,'Să nu fie întors &lt;i&gt;la Dumnezeu&lt;/i&gt; de curând, ca nu cumva să se îngâmfe și să cadă în osânda Diavolului&lt;f&gt;[12]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (610,3,7,'Trebuie să aibă și o bună mărturie din&lt;f&gt;ⓣ&lt;/f&gt; partea celor din afară, ca să nu ajungă de ocară și&lt;f&gt;ⓤ&lt;/f&gt; să cadă în cursa Diavolului.');
INSERT INTO &quot;verses&quot; VALUES (610,3,8,'&lt;pb/&gt;Diaconii&lt;f&gt;ⓥ&lt;/f&gt;, de asemenea, trebuie să fie demni, nu cu două fețe&lt;f&gt;[13]&lt;/f&gt;, nu&lt;f&gt;ⓦ&lt;/f&gt; băutori de mult vin, nu dornici de câștig mârșav');
INSERT INTO &quot;verses&quot; VALUES (610,3,9,'și să păstreze&lt;f&gt;ⓧ&lt;/f&gt; taina credinței cu un cuget curat.');
INSERT INTO &quot;verses&quot; VALUES (610,3,10,'Trebuie mai întâi cercetați și apoi să fie diaconi, dacă sunt fără vină.');
INSERT INTO &quot;verses&quot; VALUES (610,3,11,'Femeile&lt;f&gt;[14]&lt;/f&gt;, de asemenea&lt;f&gt;ⓨ&lt;/f&gt;, trebuie să fie demne, nebârfitoare, cu mintea trează, credincioase în toate lucrurile.');
INSERT INTO &quot;verses&quot; VALUES (610,3,12,'Diaconii să fie bărbați ai unei singure soții și să-și conducă bine copiii și casele lor.');
INSERT INTO &quot;verses&quot; VALUES (610,3,13,'Pentru că cei ce&lt;f&gt;ⓩ&lt;/f&gt; slujesc bine ca diaconi își dobândesc o poziție&lt;f&gt;[15]&lt;/f&gt; de cinste și o mare îndrăzneală în credința care este în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (610,3,14,'&lt;pb/&gt;Îți scriu aceste lucruri cu nădejdea că voi veni la tine în curând');
INSERT INTO &quot;verses&quot; VALUES (610,3,15,'și ca să știi — în caz că zăbovesc — cum trebuie să te porți&lt;f&gt;[16]&lt;/f&gt; în&lt;f&gt;ⓐ&lt;/f&gt; casa lui Dumnezeu, care este biserica Dumnezeului celui viu, stâlpul și temelia adevărului.');
INSERT INTO &quot;verses&quot; VALUES (610,3,16,'Și, fără îndoială, mare este taina evlaviei&lt;f&gt;[17]&lt;/f&gt;: &lt;pb/&gt;&lt;t&gt;„Cel ce&lt;f&gt;ⓑ&lt;/f&gt; s-a arătat în trup&lt;/t&gt; &lt;t&gt;a fost dovedit&lt;f&gt;ⓒ&lt;/f&gt; drept prin Duhul,&lt;/t&gt; &lt;t&gt;a fost văzut&lt;f&gt;ⓓ&lt;/f&gt; de îngeri,&lt;/t&gt; &lt;t&gt;a fost propovăduit&lt;f&gt;ⓔ&lt;/f&gt; printre neamuri,&lt;/t&gt; &lt;t&gt;a fost crezut&lt;f&gt;ⓕ&lt;/f&gt; în lume,&lt;/t&gt; &lt;t&gt;a fost înălțat&lt;f&gt;ⓖ&lt;/f&gt; în slavă.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,4,1,'&lt;pb/&gt;Dar Duhul spune lămurit&lt;f&gt;ⓗ&lt;/f&gt; că, în vremurile din urmă&lt;f&gt;[18]&lt;/f&gt;, unii se vor lepăda de credință și vor lua seama la duhuri înșelătoare și la învățăturile demonilor,');
INSERT INTO &quot;verses&quot; VALUES (610,4,2,'&lt;i&gt;abătuți&lt;/i&gt;&lt;f&gt;ⓘ&lt;/f&gt; de fățărnicia unor oameni care vorbesc minciuni &lt;i&gt;și care sunt&lt;/i&gt; însemnați cu fierul&lt;f&gt;ⓙ&lt;/f&gt; roșu în cugetul lor.');
INSERT INTO &quot;verses&quot; VALUES (610,4,3,'Ei opresc&lt;f&gt;ⓚ&lt;/f&gt; căsătoria și folosirea mâncărurilor&lt;f&gt;ⓛ&lt;/f&gt; pe care Dumnezeu le-a făcut ca să fie luate cu mulțumiri&lt;f&gt;ⓜ&lt;/f&gt; de către cei ce cred și cunosc adevărul.');
INSERT INTO &quot;verses&quot; VALUES (610,4,4,'Căci tot ce a creat Dumnezeu este bun și nimic nu trebuie lepădat dacă se ia cu mulțumiri;');
INSERT INTO &quot;verses&quot; VALUES (610,4,5,'pentru că este sfințit prin Cuvântul lui Dumnezeu și prin rugăciune.');
INSERT INTO &quot;verses&quot; VALUES (610,4,6,'&lt;pb/&gt;Dacă le vei pune în &lt;i&gt;vedere&lt;/i&gt; fraților aceste lucruri, vei fi un bun slujitor al lui Hristos Isus, fiindcă te hrănești&lt;f&gt;ⓝ&lt;/f&gt; cu cuvintele credinței și ale bunei învățături pe care ai urmat-o &lt;i&gt;până acum&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (610,4,7,'Ferește-te de poveștile&lt;f&gt;[19]&lt;/f&gt; lumești&lt;f&gt;ⓞ&lt;/f&gt; și băbești! Deprinde-te să trăiești cu evlavie!');
INSERT INTO &quot;verses&quot; VALUES (610,4,8,'Căci deprinderea&lt;f&gt;[20]&lt;/f&gt; trupească este de puțin folos&lt;f&gt;ⓟ&lt;/f&gt;, pe când&lt;f&gt;ⓠ&lt;/f&gt; evlavia este folositoare în toate privințele, întrucât ea are făgăduința vieții de acum și a celei viitoare.');
INSERT INTO &quot;verses&quot; VALUES (610,4,9,'Adevărat&lt;f&gt;ⓡ&lt;/f&gt; și cu totul vrednic de primit este cuvântul &lt;i&gt;acesta&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (610,4,10,'Pentru aceasta ne ostenim&lt;f&gt;ⓢ&lt;/f&gt; și ne luptăm, fiindcă ne-am&lt;f&gt;ⓣ&lt;/f&gt; pus nădejdea în Dumnezeul cel viu, care este Mântuitorul tuturor oamenilor, mai ales al celor credincioși.');
INSERT INTO &quot;verses&quot; VALUES (610,4,11,'&lt;pb/&gt;Poruncește&lt;f&gt;ⓤ&lt;/f&gt; și învață aceste lucruri!');
INSERT INTO &quot;verses&quot; VALUES (610,4,12,'Nimeni&lt;f&gt;ⓥ&lt;/f&gt; să nu-ți disprețuiască tinerețea, ci fii&lt;f&gt;ⓦ&lt;/f&gt; o pildă pentru credincioși: în vorbire, în purtare, în dragoste, în credință, în curăție&lt;f&gt;[21]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (610,4,13,'Până voi veni, ocupă-te cu citirea &lt;i&gt;publică&lt;/i&gt;&lt;f&gt;[22]&lt;/f&gt; și cu îndemnurile și învățătura &lt;i&gt;pe care le dai altora&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (610,4,14,'Nu&lt;f&gt;ⓧ&lt;/f&gt; fi nepăsător față de darul care este în tine, care ți-a fost dat prin&lt;f&gt;ⓨ&lt;/f&gt; profeție, cu&lt;f&gt;ⓩ&lt;/f&gt; punerea mâinilor de către ceata prezbiterilor!');
INSERT INTO &quot;verses&quot; VALUES (610,4,15,'Pune-ți pe inimă aceste lucruri, îndeletnicește-te cu ele, pentru ca înaintarea ta să fie văzută de toți!');
INSERT INTO &quot;verses&quot; VALUES (610,4,16,'Fii cu luare aminte&lt;f&gt;ⓐ&lt;/f&gt; asupra ta însuți și asupra învățăturii &lt;i&gt;pe care o dai altora&lt;/i&gt;; stăruie în aceste lucruri, căci, dacă vei face așa, aceasta va fi spre mântuirea ta și a celor&lt;f&gt;ⓑ&lt;/f&gt; ce te ascultă!');
INSERT INTO &quot;verses&quot; VALUES (610,5,1,'&lt;pb/&gt;Nu-l&lt;f&gt;ⓒ&lt;/f&gt; mustra pe bătrân, ci sfătuiește-l ca pe un tată; pe tineri &lt;i&gt;sfătuiește-i&lt;/i&gt; ca pe niște frați,');
INSERT INTO &quot;verses&quot; VALUES (610,5,2,'pe femeile bătrâne — ca pe niște mame, pe cele tinere — ca pe niște surori, cu toată curăția!');
INSERT INTO &quot;verses&quot; VALUES (610,5,3,'&lt;pb/&gt;Cinstește-le&lt;f&gt;[23]&lt;/f&gt; pe văduvele care sunt&lt;f&gt;ⓓ&lt;/f&gt; cu adevărat văduve!');
INSERT INTO &quot;verses&quot; VALUES (610,5,4,'Dacă o văduvă are copii sau nepoți&lt;f&gt;[24]&lt;/f&gt;, aceștia să se deprindă să fie devotați mai întâi față de cei din casa lor și să&lt;f&gt;ⓔ&lt;/f&gt; răsplătească &lt;i&gt;ostenelile&lt;/i&gt; părinților, căci&lt;f&gt;ⓕ&lt;/f&gt; lucrul acesta este plăcut înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (610,5,5,'Cea&lt;f&gt;ⓖ&lt;/f&gt; cu adevărat văduvă, care a rămas singură, și-a pus nădejdea în Dumnezeu și stăruie&lt;f&gt;ⓗ&lt;/f&gt; zi și noapte în&lt;f&gt;ⓘ&lt;/f&gt; cereri și rugăciuni.');
INSERT INTO &quot;verses&quot; VALUES (610,5,6,'Dar&lt;f&gt;ⓙ&lt;/f&gt; cea dedată la plăceri, deși trăiește, este moartă.');
INSERT INTO &quot;verses&quot; VALUES (610,5,7,'Poruncește și aceste&lt;f&gt;ⓚ&lt;/f&gt; lucruri, pentru ca &lt;i&gt;văduvele&lt;/i&gt; să fie mai presus de orice reproș!');
INSERT INTO &quot;verses&quot; VALUES (610,5,8,'Dacă cineva nu se îngrijește de ai lui, și mai ales de cei din casa lui, s-a&lt;f&gt;ⓛ&lt;/f&gt; lepădat de credință și&lt;f&gt;ⓜ&lt;/f&gt; este mai rău decât un necredincios.');
INSERT INTO &quot;verses&quot; VALUES (610,5,9,'&lt;pb/&gt;O văduvă, ca să fie înscrisă &lt;i&gt;în lista văduvelor&lt;/i&gt;, trebuie să nu aibă mai puțin de șaizeci de ani, să fi fost&lt;f&gt;ⓝ&lt;/f&gt; soția unui singur bărbat,');
INSERT INTO &quot;verses&quot; VALUES (610,5,10,'să fie cunoscută pentru faptele ei bune, să fi crescut copii, să fi fost primitoare&lt;f&gt;ⓞ&lt;/f&gt; de oaspeți, să fi spălat&lt;f&gt;ⓟ&lt;/f&gt; picioarele sfinților, să-i fi ajutat pe cei greu încercați, să fi făcut tot felul de fapte bune.');
INSERT INTO &quot;verses&quot; VALUES (610,5,11,'Dar pe văduvele mai tinere să nu le primești, pentru că, atunci când pofta le desparte de Hristos, vor să se mărite din nou');
INSERT INTO &quot;verses&quot; VALUES (610,5,12,'și își atrag osândă fiindcă își încalcă făgăduința&lt;f&gt;[25]&lt;/f&gt; de la început.');
INSERT INTO &quot;verses&quot; VALUES (610,5,13,'Totodată&lt;f&gt;ⓠ&lt;/f&gt;, se deprind să fie fără nicio îndeletnicire&lt;f&gt;[26]&lt;/f&gt; și să umble din casă în casă; și nu numai că sunt fără nicio îndeletnicire, ci sunt și guralive și iscoditoare&lt;f&gt;[27]&lt;/f&gt;, vorbind ce nu trebuie.');
INSERT INTO &quot;verses&quot; VALUES (610,5,14,'Vreau&lt;f&gt;ⓡ&lt;/f&gt; deci ca &lt;i&gt;văduvele&lt;/i&gt; mai tinere să se mărite, să aibă copii și să-și administreze &lt;i&gt;bine&lt;/i&gt; casa, pentru a&lt;f&gt;ⓢ&lt;/f&gt; nu le da potrivnicilor&lt;f&gt;[28]&lt;/f&gt; niciun prilej de batjocură,');
INSERT INTO &quot;verses&quot; VALUES (610,5,15,'căci unele s-au și întors să-l urmeze pe Satana.');
INSERT INTO &quot;verses&quot; VALUES (610,5,16,'Dacă vreo credincioasă [sau vreun credincios] are văduve în familie, să le ajute. Biserica nu trebuie să fie împovărată &lt;i&gt;cu ele&lt;/i&gt;, ca să le poată ajuta pe cele&lt;f&gt;ⓣ&lt;/f&gt; care sunt cu adevărat văduve.');
INSERT INTO &quot;verses&quot; VALUES (610,5,17,'&lt;pb/&gt;Prezbiterii care cârmuiesc&lt;f&gt;ⓤ&lt;/f&gt; bine să&lt;f&gt;ⓥ&lt;/f&gt; fie vrednici de îndoită cinste&lt;f&gt;[29]&lt;/f&gt;, mai ales cei ce se ostenesc cu propovăduirea și cu învățătura &lt;i&gt;pe care o dau altora&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (610,5,18,'Căci Scriptura zice: &lt;e&gt;Să nu legi gura boului&lt;/e&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;când treieră&lt;/e&gt;&lt;f&gt;[30]&lt;/f&gt; și: &lt;e&gt;&lt;J&gt;Vrednic&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;&lt;J&gt;este lucrătorul de plata lui.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[31]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,5,19,'Împotriva unui prezbiter să nu primești învinuire decât &lt;e&gt;pe temeiul a doi sau trei martori&lt;/e&gt;&lt;f&gt;[32]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (610,5,20,'Pe&lt;f&gt;ⓨ&lt;/f&gt; cei ce păcătuiesc mustră-i&lt;f&gt;[33]&lt;/f&gt; înaintea tuturor, ca și ceilalți să aibă frică!');
INSERT INTO &quot;verses&quot; VALUES (610,5,21,'Te&lt;f&gt;ⓩ&lt;/f&gt; rog fierbinte, înaintea lui Dumnezeu, înaintea lui Hristos Isus și a îngerilor aleși, să păzești aceste lucruri fără prejudecăți și fără a fi părtinitor.');
INSERT INTO &quot;verses&quot; VALUES (610,5,22,'Să nu-ți pui mâinile&lt;f&gt;ⓐ&lt;/f&gt; peste nimeni cu grabă&lt;f&gt;[34]&lt;/f&gt; și să nu&lt;f&gt;ⓑ&lt;/f&gt; te faci părtaș la păcatele altora; pe tine păzește-te curat! (');
INSERT INTO &quot;verses&quot; VALUES (610,5,23,'Să nu mai bei numai apă, ci să întrebuințezi și puțin vin, din pricina stomacului și a deselor tale îmbolnăviri.)');
INSERT INTO &quot;verses&quot; VALUES (610,5,24,'Păcatele unor oameni sunt evidente și merg înainte la judecată, iar ale altora vin în urma lor.');
INSERT INTO &quot;verses&quot; VALUES (610,5,25,'Tot astfel, și faptele bune sunt evidente; iar cele ce nu sunt &lt;i&gt;cunoscute&lt;/i&gt; nu pot să rămână ascunse.');
INSERT INTO &quot;verses&quot; VALUES (610,6,1,'&lt;pb/&gt;Toți cei ce sunt sub jugul robiei&lt;f&gt;ⓒ&lt;/f&gt; să-i considere pe stăpânii lor vrednici de toată cinstea, pentru ca&lt;f&gt;ⓓ&lt;/f&gt; Numele lui Dumnezeu și învățătura să nu fie vorbite de rău.');
INSERT INTO &quot;verses&quot; VALUES (610,6,2,'Iar cei ce au stăpâni credincioși să nu-i disprețuiască pe motiv&lt;f&gt;ⓔ&lt;/f&gt; că sunt frați, ci să le slujească și mai bine, tocmai fiindcă cei ce se bucură de binefacerile slujbei lor sunt credincioși și preaiubiți. Învață-i pe oameni aceste&lt;f&gt;ⓕ&lt;/f&gt; lucruri și spune-le apăsat!');
INSERT INTO &quot;verses&quot; VALUES (610,6,3,'&lt;pb/&gt;Dacă cineva&lt;f&gt;ⓖ&lt;/f&gt; le dă oamenilor o altă învățătură și nu&lt;f&gt;ⓗ&lt;/f&gt; se ține de cuvintele sănătoase ale Domnului nostru Isus Hristos și de învățătura care duce la evlavie,');
INSERT INTO &quot;verses&quot; VALUES (610,6,4,'este plin de mândrie și nu pricepe nimic&lt;f&gt;ⓘ&lt;/f&gt;: ba încă are boala disputelor&lt;f&gt;ⓙ&lt;/f&gt; și a controverselor, din care se nasc invidia, cearta, defăimările, bănuielile rele,');
INSERT INTO &quot;verses&quot; VALUES (610,6,5,'nesfârșitele ciocniri de vorbe&lt;f&gt;ⓚ&lt;/f&gt; ale oamenilor&lt;f&gt;ⓛ&lt;/f&gt; stricați la minte, lipsiți de adevăr și care&lt;f&gt;ⓜ&lt;/f&gt; consideră că evlavia este un izvor de câștig. [Depărtează-te&lt;f&gt;ⓝ&lt;/f&gt; de astfel de oameni!]');
INSERT INTO &quot;verses&quot; VALUES (610,6,6,'Într-adevăr, evlavia este un mare câștig atunci când ești mulțumit&lt;f&gt;ⓞ&lt;/f&gt; cu ce ai.');
INSERT INTO &quot;verses&quot; VALUES (610,6,7,'&lt;pb/&gt;&lt;t&gt;Căci noi n-am adus nimic&lt;f&gt;ⓟ&lt;/f&gt; în lume&lt;/t&gt; &lt;t&gt;și nu putem lua nimic din ea.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,6,8,'&lt;t&gt;Dacă avem hrană și acoperământ,&lt;/t&gt; &lt;t&gt;să fim mulțumiți cu acestea.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,6,9,'&lt;pb/&gt;Cei ce vor să se îmbogățească, dimpotrivă, cad în ispită, în laț&lt;f&gt;ⓠ&lt;/f&gt; și în multe pofte nesăbuite și vătămătoare, care&lt;f&gt;ⓡ&lt;/f&gt; îi cufundă pe oameni în prăpăd și pierzare.');
INSERT INTO &quot;verses&quot; VALUES (610,6,10,'Căci&lt;f&gt;ⓢ&lt;/f&gt; iubirea de bani este rădăcina tuturor relelor, și unii, care au râvnit după &lt;i&gt;bani&lt;/i&gt;, s-au rătăcit de la credință și s-au străpuns singuri cu o mulțime de chinuri.');
INSERT INTO &quot;verses&quot; VALUES (610,6,11,'&lt;pb/&gt;&lt;t&gt;Iar tu, om&lt;f&gt;ⓣ&lt;/f&gt; al lui Dumnezeu, fugi de aceste lucruri&lt;/t&gt; &lt;t&gt;și urmărește dreptatea, evlavia, credința,&lt;/t&gt; &lt;t&gt;dragostea, răbdarea, blândețea!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,6,12,'&lt;t&gt;Luptă-te lupta cea bună a credinței;&lt;/t&gt; &lt;t&gt;apucă&lt;f&gt;ⓤ&lt;/f&gt; viața veșnică la care ai fost chemat&lt;/t&gt; &lt;t&gt;și pentru care ai&lt;f&gt;ⓥ&lt;/f&gt; făcut &lt;i&gt;acea&lt;/i&gt; frumoasă mărturisire&lt;/t&gt; &lt;t&gt;înaintea multor martori!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,6,13,'&lt;pb/&gt;Te îndemn, înaintea lui Dumnezeu, care&lt;f&gt;ⓦ&lt;/f&gt; dă viață tuturor lucrurilor, și înaintea lui Hristos Isus, care&lt;f&gt;ⓧ&lt;/f&gt; a făcut &lt;i&gt;acea&lt;/i&gt; frumoasă mărturisire înaintea lui Pilat din Pont,');
INSERT INTO &quot;verses&quot; VALUES (610,6,14,'să păzești porunca fără prihană, fără cusur, până&lt;f&gt;ⓨ&lt;/f&gt; la arătarea Domnului nostru Isus Hristos,');
INSERT INTO &quot;verses&quot; VALUES (610,6,15,'pe care o va dezvălui, la vremea ei, &lt;pb/&gt;&lt;t&gt;fericitul&lt;f&gt;ⓩ&lt;/f&gt; și singurul Stăpânitor,&lt;/t&gt; &lt;t&gt;Împăratul&lt;f&gt;ⓐ&lt;/f&gt; împăraților&lt;/t&gt; &lt;t&gt;și Domnul domnilor,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,6,16,'&lt;t&gt;singurul care&lt;f&gt;ⓑ&lt;/f&gt; are nemurirea,&lt;/t&gt; &lt;t&gt;care locuiește într-o lumină de care&lt;f&gt;ⓒ&lt;/f&gt; nu poți să te apropii,&lt;/t&gt; &lt;t&gt;pe care niciun om nu L-a văzut, nici nu-L poate vedea;&lt;/t&gt; &lt;t&gt;ale Lui să fie cinstea și puterea veșnică! Amin!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (610,6,17,'&lt;pb/&gt;Pe cei bogați în felul veacului acestuia îndeamnă-i să nu se îngâmfe și să nu-și&lt;f&gt;ⓓ&lt;/f&gt; pună nădejdea în bogății&lt;f&gt;ⓔ&lt;/f&gt; nesigure, ci în&lt;f&gt;ⓕ&lt;/f&gt; Dumnezeu, care&lt;f&gt;ⓖ&lt;/f&gt; ne dă toate lucrurile din belșug ca să ne bucurăm de ele!');
INSERT INTO &quot;verses&quot; VALUES (610,6,18,'&lt;i&gt;Îndeamnă-i&lt;/i&gt; să facă bine, să fie bogați&lt;f&gt;ⓗ&lt;/f&gt; în fapte bune, să fie darnici&lt;f&gt;ⓘ&lt;/f&gt;, gata să împartă&lt;f&gt;ⓙ&lt;/f&gt; cu alții');
INSERT INTO &quot;verses&quot; VALUES (610,6,19,'și să-și strângă&lt;f&gt;ⓚ&lt;/f&gt; astfel drept comoară pentru viitor o bună temelie, ca să apuce&lt;f&gt;ⓛ&lt;/f&gt; adevărata viață!');
INSERT INTO &quot;verses&quot; VALUES (610,6,20,'&lt;pb/&gt;Timoteie, păzește&lt;f&gt;ⓜ&lt;/f&gt; ce ți s-a încredințat&lt;f&gt;[35]&lt;/f&gt;; ferește-te&lt;f&gt;ⓝ&lt;/f&gt; de vorbăriile lumești și de contrazicerile așa-zisei „cunoașteri”,');
INSERT INTO &quot;verses&quot; VALUES (610,6,21,'pe care unii au mărturisit-o și s-au abătut&lt;f&gt;ⓞ&lt;/f&gt; de la credință! &lt;pb/&gt;Harul să fie cu voi! Amin!');
INSERT INTO &quot;verses&quot; VALUES (620,1,1,'&lt;pb/&gt;Pavel, apostol&lt;f&gt;ⓐ&lt;/f&gt; al lui Hristos Isus prin voia lui Dumnezeu, după făgăduința vieții&lt;f&gt;ⓑ&lt;/f&gt; care este în Hristos Isus,');
INSERT INTO &quot;verses&quot; VALUES (620,1,2,'către&lt;f&gt;ⓒ&lt;/f&gt; Timotei, copilul &lt;i&gt;meu&lt;/i&gt; preaiubit: Har, îndurare și pace de la Dumnezeu Tatăl și de la Hristos Isus, Domnul nostru!');
INSERT INTO &quot;verses&quot; VALUES (620,1,3,'&lt;pb/&gt;Îi mulțumesc&lt;f&gt;ⓓ&lt;/f&gt; lui Dumnezeu, căruia&lt;f&gt;ⓔ&lt;/f&gt; Îi slujesc cu o conștiință curată, din moși-strămoși, când te pomenesc neîntrerupt&lt;f&gt;ⓕ&lt;/f&gt; în cererile mele, zi și noapte.');
INSERT INTO &quot;verses&quot; VALUES (620,1,4,'Căci îmi amintesc&lt;f&gt;ⓖ&lt;/f&gt; de lacrimile tale și doresc să te văd, ca să mă umplu de bucurie.');
INSERT INTO &quot;verses&quot; VALUES (620,1,5,'Îmi aduc aminte de credința&lt;f&gt;ⓗ&lt;/f&gt; ta neprefăcută, care s-a înfiripat&lt;f&gt;[1]&lt;/f&gt; întâi în bunica ta Lois și în mama&lt;f&gt;ⓘ&lt;/f&gt; ta, Eunice, și sunt încredințat că și în tine.');
INSERT INTO &quot;verses&quot; VALUES (620,1,6,'&lt;pb/&gt;De aceea îți aduc aminte să înflăcărezi&lt;f&gt;ⓙ&lt;/f&gt; darul lui Dumnezeu care este în tine prin punerea mâinilor mele.');
INSERT INTO &quot;verses&quot; VALUES (620,1,7,'Căci Dumnezeu&lt;f&gt;ⓚ&lt;/f&gt; nu ne-a dat un duh de frică, ci&lt;f&gt;ⓛ&lt;/f&gt; de putere, de dragoste și de chibzuință.');
INSERT INTO &quot;verses&quot; VALUES (620,1,8,'Să nu-ți fie rușine deci&lt;f&gt;ⓜ&lt;/f&gt; de mărturia&lt;f&gt;ⓝ&lt;/f&gt; cu privire la Domnul nostru, nici de mine, care am fost întemnițat&lt;f&gt;ⓞ&lt;/f&gt; pentru El. Ci&lt;f&gt;ⓟ&lt;/f&gt; suferă împreună cu Evanghelia, prin puterea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (620,1,9,'&lt;pb/&gt;&lt;t&gt;El&lt;f&gt;ⓠ&lt;/f&gt; ne-a mântuit&lt;/t&gt; &lt;t&gt;și ne-a&lt;f&gt;ⓡ&lt;/f&gt; făcut o chemare sfântă,&lt;/t&gt; &lt;t&gt;nu&lt;f&gt;ⓢ&lt;/f&gt; după faptele noastre,&lt;/t&gt; &lt;t&gt;ci&lt;f&gt;ⓣ&lt;/f&gt; după hotărârea Lui și după harul&lt;/t&gt; &lt;t&gt;care ne-a fost dat în Hristos Isus&lt;/t&gt; &lt;t&gt;înainte&lt;f&gt;ⓤ&lt;/f&gt; de veșnicii,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (620,1,10,'&lt;t&gt;dar care a fost&lt;f&gt;ⓥ&lt;/f&gt; descoperit acum&lt;/t&gt; &lt;t&gt;prin arătarea Mântuitorului nostru&lt;/t&gt; &lt;t&gt;Hristos Isus,&lt;/t&gt; &lt;t&gt;care&lt;f&gt;ⓦ&lt;/f&gt; a nimicit moartea&lt;/t&gt; &lt;t&gt;și a adus la lumină viața și neputrezirea&lt;/t&gt; &lt;t&gt;prin Evanghelie.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (620,1,11,'&lt;pb/&gt;Pentru aceasta am fost pus eu propovăduitor, apostol&lt;f&gt;ⓧ&lt;/f&gt; și învățător [al neamurilor].');
INSERT INTO &quot;verses&quot; VALUES (620,1,12,'Din pricina aceasta&lt;f&gt;ⓨ&lt;/f&gt; sufăr aceste lucruri, dar nu mi-e rușine, căci&lt;f&gt;ⓩ&lt;/f&gt; știu în cine am crezut și sunt încredințat că El are putere să păzească&lt;f&gt;ⓐ&lt;/f&gt; ce I-am încredințat&lt;f&gt;[2]&lt;/f&gt; până în ziua aceea&lt;f&gt;[3]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (620,1,13,'Modelul de învățătură&lt;f&gt;ⓑ&lt;/f&gt; sănătoasă&lt;f&gt;[4]&lt;/f&gt; pe care le-ai auzit de la mine ține-l&lt;f&gt;ⓒ&lt;/f&gt; cu credința&lt;f&gt;ⓓ&lt;/f&gt; și dragostea care sunt în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (620,1,14,'Lucrul bun&lt;f&gt;ⓔ&lt;/f&gt; care ți s-a încredințat păzește-l prin Duhul Sfânt, care&lt;f&gt;ⓕ&lt;/f&gt; locuiește în noi.');
INSERT INTO &quot;verses&quot; VALUES (620,1,15,'&lt;pb/&gt;Știi că toți&lt;f&gt;ⓖ&lt;/f&gt; cei din Asia&lt;f&gt;[5]&lt;/f&gt; m-au părăsit&lt;f&gt;ⓗ&lt;/f&gt;, printre care și Fighel și Ermoghen.');
INSERT INTO &quot;verses&quot; VALUES (620,1,16,'Domnul să-Și verse îndurarea peste casa&lt;f&gt;ⓘ&lt;/f&gt; lui Onisifor, căci de multe ori mi-a înviorat sufletul și nu i-a fost rușine de lanțurile&lt;f&gt;ⓙ&lt;/f&gt; mele!');
INSERT INTO &quot;verses&quot; VALUES (620,1,17,'Nu numai atât, dar când a fost în Roma, cu multă stăruință m-a căutat și m-a găsit.');
INSERT INTO &quot;verses&quot; VALUES (620,1,18,'Domnul să-i&lt;f&gt;ⓚ&lt;/f&gt; facă parte de îndurarea Lui în ziua aceea! Tu știi foarte bine cât ajutor&lt;f&gt;ⓛ&lt;/f&gt; mi-a dat el în Efes.');
INSERT INTO &quot;verses&quot; VALUES (620,2,1,'&lt;pb/&gt;Deci tu, copilul&lt;f&gt;ⓜ&lt;/f&gt; meu, întărește-te&lt;f&gt;ⓝ&lt;/f&gt; în harul care este în Hristos Isus!');
INSERT INTO &quot;verses&quot; VALUES (620,2,2,'Și&lt;f&gt;ⓞ&lt;/f&gt; ce ai auzit de la mine înaintea multor martori încredințează&lt;f&gt;ⓟ&lt;/f&gt; unor oameni de încredere, care să fie în stare&lt;f&gt;ⓠ&lt;/f&gt; să-i învețe și pe alții!');
INSERT INTO &quot;verses&quot; VALUES (620,2,3,'Suferă&lt;f&gt;ⓡ&lt;/f&gt; împreună cu mine, ca&lt;f&gt;ⓢ&lt;/f&gt; un bun soldat al lui Hristos Isus!');
INSERT INTO &quot;verses&quot; VALUES (620,2,4,'Niciun soldat nu se lasă prins cu treburile vieții, dacă vrea să-i fie pe plac celui ce l-a înscris la oaste.');
INSERT INTO &quot;verses&quot; VALUES (620,2,5,'Și&lt;f&gt;ⓣ&lt;/f&gt; cine luptă&lt;f&gt;[6]&lt;/f&gt; la jocuri nu este încununat dacă nu a luptat după reguli.');
INSERT INTO &quot;verses&quot; VALUES (620,2,6,'Plugarul&lt;f&gt;ⓤ&lt;/f&gt; care muncește trebuie să fie primul care să-și primească partea din rod.');
INSERT INTO &quot;verses&quot; VALUES (620,2,7,'Cugetă la ce-ți spun, și Domnul îți va da pricepere în toate lucrurile!');
INSERT INTO &quot;verses&quot; VALUES (620,2,8,'&lt;pb/&gt;Adu-ți aminte de Isus Hristos cel înviat&lt;f&gt;ⓥ&lt;/f&gt; din morți, &lt;i&gt;născut&lt;/i&gt; din&lt;f&gt;ⓦ&lt;/f&gt; sămânța lui David, cum spune&lt;f&gt;ⓧ&lt;/f&gt; Evanghelia mea,');
INSERT INTO &quot;verses&quot; VALUES (620,2,9,'pentru&lt;f&gt;ⓨ&lt;/f&gt; care sufăr până&lt;f&gt;ⓩ&lt;/f&gt; acolo că sunt legat ca un răufăcător! Dar&lt;f&gt;ⓐ&lt;/f&gt; Cuvântul lui Dumnezeu nu este legat.');
INSERT INTO &quot;verses&quot; VALUES (620,2,10,'De aceea rabd totul&lt;f&gt;ⓑ&lt;/f&gt; pentru cei aleși, pentru ca&lt;f&gt;ⓒ&lt;/f&gt; și ei să capete mântuirea care este în Hristos Isus, împreună cu slava veșnică.');
INSERT INTO &quot;verses&quot; VALUES (620,2,11,'Adevărat este cuvântul &lt;i&gt;acesta&lt;/i&gt;: &lt;pb/&gt;&lt;t&gt;„Dacă&lt;f&gt;ⓓ&lt;/f&gt; am murit împreună cu El, vom și trăi împreună cu El.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (620,2,12,'&lt;t&gt;Dacă&lt;f&gt;ⓔ&lt;/f&gt; răbdăm, vom și împărăți împreună cu El.&lt;/t&gt; &lt;t&gt;Dacă&lt;f&gt;ⓕ&lt;/f&gt; ne lepădăm de El, și El Se va lepăda de noi.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (620,2,13,'&lt;t&gt;Dacă&lt;f&gt;ⓖ&lt;/f&gt; suntem necredincioși, El rămâne &lt;i&gt;totuși&lt;/i&gt; credincios,&lt;/t&gt; &lt;t&gt;căci nu Se&lt;f&gt;ⓗ&lt;/f&gt; poate tăgădui pe Sine.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (620,2,14,'&lt;pb/&gt;Adu&lt;i&gt;-le&lt;/i&gt; aminte de aceste lucruri și roagă-i fierbinte&lt;f&gt;ⓘ&lt;/f&gt; înaintea lui Dumnezeu să&lt;f&gt;ⓙ&lt;/f&gt; se ferească de certurile de cuvinte, care nu aduc alt folos decât pieirea celor ce le ascultă!');
INSERT INTO &quot;verses&quot; VALUES (620,2,15,'Străduiește-te să te înfățișezi înaintea lui Dumnezeu &lt;i&gt;ca un om&lt;/i&gt; încercat, ca un lucrător care n-are de ce să-i fie rușine și care împarte drept&lt;f&gt;[7]&lt;/f&gt; Cuvântul adevărului!');
INSERT INTO &quot;verses&quot; VALUES (620,2,16,'Ferește-te de&lt;f&gt;ⓚ&lt;/f&gt; vorbăriile lumești; căci &lt;i&gt;cei ce se complac în ele&lt;/i&gt; vor înainta și mai mult în necinstirea &lt;i&gt;lui Dumnezeu&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (620,2,17,'și cuvântul lor se va întinde ca o cangrenă. Din numărul acestora sunt Imeneu&lt;f&gt;ⓛ&lt;/f&gt; și Filet,');
INSERT INTO &quot;verses&quot; VALUES (620,2,18,'care s-au abătut de la&lt;f&gt;ⓜ&lt;/f&gt; adevăr. Ei zic că învierea&lt;f&gt;ⓝ&lt;/f&gt; a și avut loc și nimicesc credința unora.');
INSERT INTO &quot;verses&quot; VALUES (620,2,19,'Totuși, temelia tare a lui Dumnezeu stă neclintită, având pecetea aceasta: &lt;e&gt;Domnul îi cunoaște&lt;/e&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;pe cei ce sunt ai Lui&lt;/e&gt;&lt;f&gt;[8]&lt;/f&gt; și: &lt;e&gt;Oricine rostește Numele Domnului să se depărteze de fărădelege!&lt;/e&gt;&lt;f&gt;[9]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (620,2,20,'Într-o&lt;f&gt;ⓟ&lt;/f&gt; casă mare nu sunt numai vase de aur și de argint, ci și de lemn și de lut. Unele&lt;f&gt;ⓠ&lt;/f&gt; sunt de cinste, iar altele, de ocară.');
INSERT INTO &quot;verses&quot; VALUES (620,2,21,'Deci, dacă cineva se curățește de &lt;i&gt;toate&lt;/i&gt; acestea&lt;f&gt;[10]&lt;/f&gt;, va fi un vas de cinste, sfințit, folositor stăpânului său, pregătit&lt;f&gt;ⓡ&lt;/f&gt; pentru orice lucrare bună.');
INSERT INTO &quot;verses&quot; VALUES (620,2,22,'&lt;pb/&gt;Fugi de poftele tinereții și urmărește&lt;f&gt;ⓢ&lt;/f&gt; dreptatea, credința, dragostea și pacea împreună cu cei ce-L cheamă pe Domnul dintr-o&lt;f&gt;ⓣ&lt;/f&gt; inimă curată!');
INSERT INTO &quot;verses&quot; VALUES (620,2,23,'Ferește-te de întrebările&lt;f&gt;ⓤ&lt;/f&gt; prostești și izvorâte din neștiință, căci știi că dau naștere la controverse!');
INSERT INTO &quot;verses&quot; VALUES (620,2,24,'Iar robul&lt;f&gt;ⓥ&lt;/f&gt; Domnului nu trebuie să se țină de controverse, ci să fie blând cu toți&lt;f&gt;ⓦ&lt;/f&gt;, în stare să-i învețe &lt;i&gt;pe alții&lt;/i&gt;, plin de îngăduință răbdătoare,');
INSERT INTO &quot;verses&quot; VALUES (620,2,25,'să-i îndrepte cu&lt;f&gt;ⓧ&lt;/f&gt; blândețe pe împotrivitori, în nădejdea că Dumnezeu le va da pocăință, &lt;i&gt;ca să ajungă&lt;/i&gt;&lt;f&gt;ⓨ&lt;/f&gt; la cunoștința deplină a adevărului');
INSERT INTO &quot;verses&quot; VALUES (620,2,26,'și să-și vină în fire, &lt;i&gt;ieșind&lt;/i&gt; din&lt;f&gt;ⓩ&lt;/f&gt; cursa Diavolului, de care sunt ținuți captivi, ca să-i facă voia.');
INSERT INTO &quot;verses&quot; VALUES (620,3,1,'&lt;pb/&gt;Să știi că în&lt;f&gt;ⓐ&lt;/f&gt; zilele din urmă vor fi vremuri grele.');
INSERT INTO &quot;verses&quot; VALUES (620,3,2,'Căci oamenii vor fi iubitori de sine&lt;f&gt;ⓑ&lt;/f&gt;, iubitori de bani, lăudăroși&lt;f&gt;ⓒ&lt;/f&gt;, trufași&lt;f&gt;ⓓ&lt;/f&gt;, hulitori, neascultători de părinți&lt;f&gt;ⓔ&lt;/f&gt;, nerecunoscători, fără evlavie,');
INSERT INTO &quot;verses&quot; VALUES (620,3,3,'lipsiți&lt;f&gt;ⓕ&lt;/f&gt; de afecțiune, neînduplecați, bârfitori, neînfrânați&lt;f&gt;ⓖ&lt;/f&gt;, neîmblânziți, neiubitori de bine,');
INSERT INTO &quot;verses&quot; VALUES (620,3,4,'trădători&lt;f&gt;ⓗ&lt;/f&gt;, nesăbuiți, îngâmfați, iubitori&lt;f&gt;ⓘ&lt;/f&gt; mai mult de plăceri decât iubitori de Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (620,3,5,'având o formă de evlavie, dar tăgăduindu-i puterea&lt;f&gt;ⓙ&lt;/f&gt;. Depărtează-te de asemenea oameni&lt;f&gt;ⓚ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (620,3,6,'Sunt&lt;f&gt;ⓛ&lt;/f&gt; printre ei unii care se strecoară prin case și le ademenesc pe femeile ușuratice&lt;f&gt;[11]&lt;/f&gt;, îngreunate de păcate și frământate de tot felul de pofte.');
INSERT INTO &quot;verses&quot; VALUES (620,3,7,'Acestea, deși învață mereu, nu pot ajunge&lt;f&gt;ⓜ&lt;/f&gt; niciodată la cunoștința deplină a adevărului.');
INSERT INTO &quot;verses&quot; VALUES (620,3,8,'După cum Iane și Iambre&lt;f&gt;[12]&lt;/f&gt; i s-au împotrivit lui Moise, tot așa și &lt;i&gt;oamenii&lt;/i&gt;&lt;f&gt;ⓝ&lt;/f&gt; aceștia&lt;f&gt;[13]&lt;/f&gt; se împotrivesc adevărului, &lt;i&gt;ca unii care sunt&lt;/i&gt; stricați la minte și au o credință&lt;f&gt;ⓞ&lt;/f&gt; de osândit.');
INSERT INTO &quot;verses&quot; VALUES (620,3,9,'Dar nu vor înainta &lt;i&gt;prea mult&lt;/i&gt;; căci nebunia lor va fi văzută de toți, cum a fost și a celor doi.');
INSERT INTO &quot;verses&quot; VALUES (620,3,10,'&lt;pb/&gt;Tu însă&lt;f&gt;ⓟ&lt;/f&gt; ai urmat cu credincioșie învățătura mea, purtarea mea, hotărârea mea, credința mea, îndelunga mea răbdare, dragostea mea, stăruința mea,');
INSERT INTO &quot;verses&quot; VALUES (620,3,11,'prigonirile și suferințele care au venit peste mine în&lt;f&gt;ⓠ&lt;/f&gt; Antiohia, în&lt;f&gt;ⓡ&lt;/f&gt; Iconium și în&lt;f&gt;ⓢ&lt;/f&gt; Listra. Știi ce prigoniri am îndurat, dar din&lt;f&gt;ⓣ&lt;/f&gt; toate m-a izbăvit Domnul.');
INSERT INTO &quot;verses&quot; VALUES (620,3,12,'De altfel, toți&lt;f&gt;ⓤ&lt;/f&gt; cei ce vor să trăiască evlavios în Hristos Isus vor fi prigoniți.');
INSERT INTO &quot;verses&quot; VALUES (620,3,13,'Dar&lt;f&gt;ⓥ&lt;/f&gt; oamenii răi și înșelători vor înainta și mai mult în rău, îi vor amăgi &lt;i&gt;pe alții&lt;/i&gt; și se vor amăgi &lt;i&gt;și pe ei înșiși&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (620,3,14,'&lt;pb/&gt;Tu&lt;f&gt;ⓦ&lt;/f&gt; să rămâi în lucrurile pe care le-ai învățat și de care ești &lt;i&gt;deplin&lt;/i&gt; încredințat, căci știi cine sunt cei de la care le-ai învățat;');
INSERT INTO &quot;verses&quot; VALUES (620,3,15,'din copilărie cunoști Sfintele&lt;f&gt;ⓧ&lt;/f&gt; Scripturi, care pot să-ți dea înțelepciunea care duce la mântuire prin credința în Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (620,3,16,'Toată&lt;f&gt;ⓨ&lt;/f&gt; Scriptura este insuflată de Dumnezeu și de folos&lt;f&gt;ⓩ&lt;/f&gt; ca să dea învățătură, să mustre, să îndrepte, să educe în vederea dreptății,');
INSERT INTO &quot;verses&quot; VALUES (620,3,17,'pentru ca&lt;f&gt;ⓐ&lt;/f&gt; omul lui Dumnezeu să fie pregătit și destoinic&lt;f&gt;ⓑ&lt;/f&gt; pentru orice lucrare bună.');
INSERT INTO &quot;verses&quot; VALUES (620,4,1,'&lt;pb/&gt;Te rog fierbinte&lt;f&gt;ⓒ&lt;/f&gt; înaintea lui Dumnezeu și înaintea lui Hristos Isus, care&lt;f&gt;ⓓ&lt;/f&gt; va judeca viii și morții, și pentru arătarea și Împărăția Sa:');
INSERT INTO &quot;verses&quot; VALUES (620,4,2,'propovăduiește Cuvântul, fii pregătit — fie că timpul &lt;i&gt;îți&lt;/i&gt; este prielnic sau nu — mustră&lt;f&gt;[14]&lt;/f&gt;, ceartă, îndeamnă&lt;f&gt;ⓔ&lt;/f&gt; cu toată blândețea și învățătura!');
INSERT INTO &quot;verses&quot; VALUES (620,4,3,'Căci&lt;f&gt;ⓕ&lt;/f&gt; va veni vremea când oamenii nu vor suferi învățătura&lt;f&gt;ⓖ&lt;/f&gt; sănătoasă, ci, dornici să-și gâdile urechile, își vor lua învățători peste învățători, după&lt;f&gt;ⓗ&lt;/f&gt; poftele lor.');
INSERT INTO &quot;verses&quot; VALUES (620,4,4,'Își vor abate urechea de la adevăr și se vor îndrepta&lt;f&gt;ⓘ&lt;/f&gt; spre mituri.');
INSERT INTO &quot;verses&quot; VALUES (620,4,5,'Dar tu fii treaz în toate lucrurile, rabdă&lt;f&gt;ⓙ&lt;/f&gt; suferința, fă-ți lucrarea de evanghelist&lt;f&gt;ⓚ&lt;/f&gt; și împlinește-ți slujba!');
INSERT INTO &quot;verses&quot; VALUES (620,4,6,'&lt;pb/&gt;Căci eu&lt;f&gt;ⓛ&lt;/f&gt; urmează să fiu turnat ca o jertfă de băutură, și vremea plecării&lt;f&gt;ⓜ&lt;/f&gt; mele este aproape.');
INSERT INTO &quot;verses&quot; VALUES (620,4,7,'M-am luptat&lt;f&gt;ⓝ&lt;/f&gt; lupta cea bună, mi-am sfârșit alergarea, am păzit credința.');
INSERT INTO &quot;verses&quot; VALUES (620,4,8,'De acum mă așteaptă cununa&lt;f&gt;ⓞ&lt;/f&gt; dreptății, pe care mi-o va da în&lt;f&gt;ⓟ&lt;/f&gt; ziua aceea Domnul, Judecătorul cel drept. Și nu numai mie, ci și tuturor celor ce vor fi iubit venirea Lui.');
INSERT INTO &quot;verses&quot; VALUES (620,4,9,'&lt;pb/&gt;Grăbește-te să vii la mine &lt;i&gt;cât mai&lt;/i&gt; curând,');
INSERT INTO &quot;verses&quot; VALUES (620,4,10,'căci&lt;f&gt;ⓠ&lt;/f&gt; Dima, din dragoste&lt;f&gt;ⓡ&lt;/f&gt; pentru veacul de acum, m-a părăsit și a plecat la Tesalonic. Crescens s-a dus în Galatia, Tit — în Dalmația,');
INSERT INTO &quot;verses&quot; VALUES (620,4,11,'&lt;i&gt;și&lt;/i&gt; numai&lt;f&gt;ⓢ&lt;/f&gt; Luca&lt;f&gt;ⓣ&lt;/f&gt; este cu mine! Ia-l pe Marcu&lt;f&gt;ⓤ&lt;/f&gt; și adu-l cu tine, căci el îmi este de folos în slujire!');
INSERT INTO &quot;verses&quot; VALUES (620,4,12,'Pe Tihic&lt;f&gt;ⓥ&lt;/f&gt; l-am trimis la Efes.');
INSERT INTO &quot;verses&quot; VALUES (620,4,13,'Când vei veni, adu-mi mantaua, pe care am lăsat-o în Troas, la Carp, și cărțile, mai ales pe cele de piele&lt;f&gt;[15]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (620,4,14,'Alexandru&lt;f&gt;ⓦ&lt;/f&gt;, fierarul&lt;f&gt;[16]&lt;/f&gt;, mi-a făcut mult rău. Domnul îi va răsplăti&lt;f&gt;ⓧ&lt;/f&gt; după faptele lui.');
INSERT INTO &quot;verses&quot; VALUES (620,4,15,'Păzește-te și tu de el, pentru că este cu totul împotriva cuvintelor noastre!');
INSERT INTO &quot;verses&quot; VALUES (620,4,16,'La primul meu răspuns de apărare, n-a fost nimeni cu mine, ci&lt;f&gt;ⓨ&lt;/f&gt; toți m-au părăsit. Să nu li se țină în socoteală &lt;i&gt;lucrul acesta&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (620,4,17,'Însă&lt;f&gt;ⓩ&lt;/f&gt; Domnul a fost alături de mine și m-a întărit — pentru ca&lt;f&gt;ⓐ&lt;/f&gt; prin mine propovăduirea să ajungă la plinătate și s-o audă toate neamurile — și &lt;i&gt;astfel&lt;/i&gt; am fost izbăvit din&lt;f&gt;ⓑ&lt;/f&gt; gura leului.');
INSERT INTO &quot;verses&quot; VALUES (620,4,18,'Domnul&lt;f&gt;ⓒ&lt;/f&gt; mă va izbăvi de orice lucru rău și mă va mântui, &lt;i&gt;ca să ajung&lt;/i&gt; în Împărăția Lui&lt;f&gt;ⓓ&lt;/f&gt; cerească. A Lui să fie slava în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (620,4,19,'&lt;pb/&gt;Spune-le salutări Priscăi&lt;f&gt;[17]&lt;/f&gt; și lui Aquila&lt;f&gt;ⓔ&lt;/f&gt; și casei&lt;f&gt;ⓕ&lt;/f&gt; lui Onisifor!');
INSERT INTO &quot;verses&quot; VALUES (620,4,20,'Erast&lt;f&gt;ⓖ&lt;/f&gt; a rămas în Corint, iar pe Trofim&lt;f&gt;ⓗ&lt;/f&gt; l-am lăsat bolnav în Milet.');
INSERT INTO &quot;verses&quot; VALUES (620,4,21,'Grăbește-te să vii înainte de iarnă! Eubul și Pudens, Linus, Claudia și toți frații îți trimit salutări.');
INSERT INTO &quot;verses&quot; VALUES (620,4,22,'&lt;pb/&gt;Domnul [Isus] Hristos să fie cu duhul tău! Harul să fie cu voi! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (630,1,1,'&lt;pb/&gt;Pavel, rob al lui Dumnezeu și apostol al lui Isus Hristos pentru credința aleșilor lui Dumnezeu și pentru cunoașterea adevărului, care duce la evlavie&lt;f&gt;ⓐ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (630,1,2,'în nădejdea&lt;f&gt;ⓑ&lt;/f&gt; vieții veșnice, promise mai înainte de veci&lt;f&gt;ⓒ&lt;/f&gt; de Dumnezeu, care nu minte&lt;f&gt;ⓓ&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (630,1,3,'și care&lt;f&gt;ⓔ&lt;/f&gt; Și-a descoperit Cuvântul la vremea cuvenită prin propovăduirea care&lt;f&gt;ⓕ&lt;/f&gt; mi-a fost încredințată după porunca lui Dumnezeu, Mântuitorul nostru,');
INSERT INTO &quot;verses&quot; VALUES (630,1,4,'către Tit&lt;f&gt;ⓖ&lt;/f&gt;, adevăratul&lt;f&gt;ⓗ&lt;/f&gt; &lt;i&gt;meu&lt;/i&gt; copil în credința &lt;i&gt;noastră&lt;/i&gt; comună: Har și pace de la Dumnezeu Tatăl și de la Hristos Isus, Mântuitorul nostru!');
INSERT INTO &quot;verses&quot; VALUES (630,1,5,'&lt;pb/&gt;Te-am lăsat în Creta ca să pui în rânduială&lt;f&gt;ⓘ&lt;/f&gt; celelalte lucruri și să numești&lt;f&gt;ⓙ&lt;/f&gt; prezbiteri&lt;f&gt;[1]&lt;/f&gt; în fiecare cetate, după cum ți-am poruncit:');
INSERT INTO &quot;verses&quot; VALUES (630,1,6,'dacă&lt;f&gt;ⓚ&lt;/f&gt; este cineva fără vină, bărbat&lt;f&gt;ⓛ&lt;/f&gt; al unei singure soții, având copii credincioși, care să nu fie învinuiți de destrăbălare sau nesupunere.');
INSERT INTO &quot;verses&quot; VALUES (630,1,7,'Căci episcopul, ca&lt;f&gt;ⓜ&lt;/f&gt; ispravnic&lt;f&gt;[2]&lt;/f&gt; al lui Dumnezeu, trebuie să fie fără vină, nu arogant, nici mânios, nici dedat&lt;f&gt;ⓝ&lt;/f&gt; la vin, nici bătăuș, nici &lt;i&gt;dornic&lt;/i&gt; de câștig mârșav&lt;f&gt;ⓞ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (630,1,8,'ci&lt;f&gt;ⓟ&lt;/f&gt; ospitalier, iubitor de bine, chibzuit, drept, devotat, înfrânat,');
INSERT INTO &quot;verses&quot; VALUES (630,1,9,'să se țină&lt;f&gt;ⓠ&lt;/f&gt; de Cuvântul&lt;f&gt;ⓡ&lt;/f&gt; adevărat, după învățătura primită, ca să fie în stare să sfătuiască în învățătura sănătoasă&lt;f&gt;ⓢ&lt;/f&gt; și să-i înfrunte&lt;f&gt;[3]&lt;/f&gt; pe potrivnici.');
INSERT INTO &quot;verses&quot; VALUES (630,1,10,'&lt;pb/&gt;Într-adevăr, mai ales printre cei circumciși, sunt&lt;f&gt;ⓣ&lt;/f&gt; mulți nesupuși, flecari și amăgitori,');
INSERT INTO &quot;verses&quot; VALUES (630,1,11,'cărora trebuie să li se astupe gura. Ei răvășesc&lt;f&gt;ⓤ&lt;/f&gt; familii întregi, învățându-i pe oameni ce nu trebuie, pentru&lt;f&gt;ⓥ&lt;/f&gt; un câștig mârșav.');
INSERT INTO &quot;verses&quot; VALUES (630,1,12,'Unul&lt;f&gt;ⓦ&lt;/f&gt; dintre ei&lt;f&gt;[4]&lt;/f&gt;, chiar profet&lt;f&gt;[5]&lt;/f&gt; al lor, a zis: „Cretanii sunt totdeauna mincinoși, fiare rele, pântece leneșe.”&lt;f&gt;[6]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (630,1,13,'Mărturia aceasta este adevărată. De aceea&lt;f&gt;ⓧ&lt;/f&gt; mustră-i cu asprime, ca să fie sănătoși în credință&lt;f&gt;ⓨ&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (630,1,14,'și să nu&lt;f&gt;ⓩ&lt;/f&gt; se țină de povești iudaice și de porunci&lt;f&gt;ⓐ&lt;/f&gt; date de oameni care resping adevărul!');
INSERT INTO &quot;verses&quot; VALUES (630,1,15,'Totul&lt;f&gt;ⓑ&lt;/f&gt; este curat pentru cei curați, dar pentru cei întinați și necredincioși nimic nu este curat: atât mintea, cât și conștiința lor sunt întinate.');
INSERT INTO &quot;verses&quot; VALUES (630,1,16,'Ei mărturisesc că-L cunosc pe Dumnezeu, dar cu&lt;f&gt;ⓒ&lt;/f&gt; faptele Îl tăgăduiesc; sunt josnici, răzvrătiți și nepotriviți pentru vreo faptă bună.');
INSERT INTO &quot;verses&quot; VALUES (630,2,1,'&lt;pb/&gt;Tu însă vorbește lucruri care se potrivesc cu învățătura&lt;f&gt;ⓓ&lt;/f&gt; sănătoasă!');
INSERT INTO &quot;verses&quot; VALUES (630,2,2,'Cei bătrâni să fie cu mintea trează, demni, chibzuiți, sănătoși în credință&lt;f&gt;ⓔ&lt;/f&gt;, în dragoste, în răbdare!');
INSERT INTO &quot;verses&quot; VALUES (630,2,3,'De asemenea, femeile&lt;f&gt;ⓕ&lt;/f&gt; în vârstă să aibă o purtare cuviincioasă, să nu fie bârfitoare sau înrobite de vin, să-i învețe pe alții ce este bine,');
INSERT INTO &quot;verses&quot; VALUES (630,2,4,'ca să le îndrume pe femeile tinere să-și&lt;f&gt;ⓖ&lt;/f&gt; iubească bărbații și copiii,');
INSERT INTO &quot;verses&quot; VALUES (630,2,5,'să fie chibzuite, cu viața curată, să îngrijească de treburile casei, să fie bune, supuse&lt;f&gt;ⓗ&lt;/f&gt; bărbaților lor, ca să nu fie vorbit de rău Cuvântul lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (630,2,6,'&lt;pb/&gt;De asemenea, sfătuiește-i pe tineri să fie chibzuiți');
INSERT INTO &quot;verses&quot; VALUES (630,2,7,'în toate privințele și fii tu însuți un model de fapte bune&lt;f&gt;ⓘ&lt;/f&gt;! În învățătură, dă dovadă de integritate&lt;f&gt;[7]&lt;/f&gt;, demnitate,');
INSERT INTO &quot;verses&quot; VALUES (630,2,8,'vorbire&lt;f&gt;ⓙ&lt;/f&gt; sănătoasă și ireproșabilă, pentru ca&lt;f&gt;ⓚ&lt;/f&gt; potrivnicii să&lt;f&gt;ⓛ&lt;/f&gt; rămână de rușine și să nu poată spune nimic rău de noi!');
INSERT INTO &quot;verses&quot; VALUES (630,2,9,'&lt;pb/&gt;Robilor&lt;f&gt;ⓜ&lt;/f&gt; spune-le să fie supuși stăpânilor lor, să le fie pe plac în toate lucrurile, să nu le întoarcă vorba,');
INSERT INTO &quot;verses&quot; VALUES (630,2,10,'să nu fure nimic&lt;f&gt;[8]&lt;/f&gt;, ci să dea dovadă de o credincioșie desăvârșită, ca&lt;f&gt;ⓝ&lt;/f&gt; în toate privințele să facă cinste învățăturii lui Dumnezeu, Mântuitorul nostru!');
INSERT INTO &quot;verses&quot; VALUES (630,2,11,'&lt;pb/&gt;Căci harul&lt;f&gt;ⓞ&lt;/f&gt; lui Dumnezeu, care aduce mântuire pentru toți oamenii, a fost&lt;f&gt;ⓟ&lt;/f&gt; arătat');
INSERT INTO &quot;verses&quot; VALUES (630,2,12,'și&lt;f&gt;ⓠ&lt;/f&gt; ne învață s-o rupem cu păgânătatea și cu poftele lumești și să trăim în veacul de acum cu chibzuință, dreptate și evlavie,');
INSERT INTO &quot;verses&quot; VALUES (630,2,13,'așteptând&lt;f&gt;ⓡ&lt;/f&gt; fericita noastră nădejde — arătarea&lt;f&gt;ⓢ&lt;/f&gt; slavei marelui nostru Dumnezeu și Mântuitor Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (630,2,14,'El S-a&lt;f&gt;ⓣ&lt;/f&gt; dat pe Sine Însuși pentru noi, ca să ne răscumpere&lt;f&gt;ⓤ&lt;/f&gt; din orice fărădelege și să-Și curățească un popor ales&lt;f&gt;[9]&lt;/f&gt;, plin de râvnă pentru fapte bune.');
INSERT INTO &quot;verses&quot; VALUES (630,2,15,'Spune lucrurile acestea, sfătuiește și mustră&lt;f&gt;ⓥ&lt;/f&gt; cu toată autoritatea! Nimeni&lt;f&gt;ⓦ&lt;/f&gt; să nu te disprețuiască!');
INSERT INTO &quot;verses&quot; VALUES (630,3,1,'&lt;pb/&gt;Adu-le aminte să&lt;f&gt;ⓧ&lt;/f&gt; fie supuși stăpânirilor și autorităților, să le dea ascultare, să&lt;f&gt;ⓨ&lt;/f&gt; fie gata pentru orice faptă bună,');
INSERT INTO &quot;verses&quot; VALUES (630,3,2,'să nu defăimeze&lt;f&gt;ⓩ&lt;/f&gt; pe nimeni, să nu fie certăreți&lt;f&gt;ⓐ&lt;/f&gt;, ci pașnici, arătând blândețe&lt;f&gt;ⓑ&lt;/f&gt; față de toți oamenii,');
INSERT INTO &quot;verses&quot; VALUES (630,3,3,'căci și noi eram&lt;f&gt;ⓒ&lt;/f&gt; cândva fără minte, răzvrătiți, rătăciți, înrobiți de tot felul de pofte și plăceri, trăind în răutate și în invidie, vrednici de dispreț și urându-ne unii pe alții!');
INSERT INTO &quot;verses&quot; VALUES (630,3,4,'&lt;pb/&gt;&lt;t&gt;Dar când s-au arătat bunătatea&lt;f&gt;ⓓ&lt;/f&gt; și dragostea de oameni&lt;/t&gt; &lt;t&gt;a lui Dumnezeu&lt;f&gt;ⓔ&lt;/f&gt;, Mântuitorul nostru,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (630,3,5,'&lt;t&gt;El ne-a mântuit nu&lt;f&gt;ⓕ&lt;/f&gt; pentru faptele făcute de noi în dreptate,&lt;/t&gt; &lt;t&gt;ci pentru îndurarea Lui, prin spălarea nașterii din nou&lt;f&gt;ⓖ&lt;/f&gt;&lt;/t&gt; &lt;t&gt;și prin înnoirea făcută de Duhul Sfânt,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (630,3,6,'&lt;t&gt;pe care L-a&lt;f&gt;ⓗ&lt;/f&gt; revărsat peste noi din belșug&lt;/t&gt; &lt;t&gt;prin Isus Hristos, Mântuitorul nostru,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (630,3,7,'&lt;t&gt;pentru ca&lt;f&gt;ⓘ&lt;/f&gt;, odată îndreptățiți prin harul Lui,&lt;/t&gt; &lt;t&gt;să ajungem moștenitori&lt;f&gt;ⓙ&lt;/f&gt; ai vieții veșnice, după care nădăjduim&lt;f&gt;ⓚ&lt;/f&gt;.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (630,3,8,'&lt;pb/&gt;Adevărat&lt;f&gt;ⓛ&lt;/f&gt; este cuvântul &lt;i&gt;acesta&lt;/i&gt;, și vreau să spui apăsat aceste lucruri, pentru ca cei care au crezut în Dumnezeu să caute să fie cei dintâi în&lt;f&gt;ⓜ&lt;/f&gt; fapte bune. Acestea sunt bune și de folos pentru oameni!');
INSERT INTO &quot;verses&quot; VALUES (630,3,9,'Dar de&lt;f&gt;ⓝ&lt;/f&gt; întrebările prostești, de genealogii, de certuri și controverse despre Lege să te ferești, căci sunt nefolositoare și zadarnice!');
INSERT INTO &quot;verses&quot; VALUES (630,3,10,'După prima și a doua mustrare, depărtează-te&lt;f&gt;ⓞ&lt;/f&gt; de cel ce aduce dezbinări&lt;f&gt;[10]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (630,3,11,'căci știi că un astfel de om este sucit și păcătuiește, scriindu-și el însuși condamnarea!');
INSERT INTO &quot;verses&quot; VALUES (630,3,12,'&lt;pb/&gt;Când îi voi trimite la tine pe Artemas sau pe Tihic&lt;f&gt;ⓟ&lt;/f&gt;, grăbește-te să vii la mine în Nicopole&lt;f&gt;[11]&lt;/f&gt;, căci m-am hotărât să petrec iarna acolo!');
INSERT INTO &quot;verses&quot; VALUES (630,3,13,'Asigură-te degrabă că Zenas, juristul&lt;f&gt;[12]&lt;/f&gt;, și Apolo&lt;f&gt;ⓠ&lt;/f&gt; își pot continua călătoria, ca să nu ducă lipsă de nimic!');
INSERT INTO &quot;verses&quot; VALUES (630,3,14,'Trebuie ca și ai noștri să se deprindă să&lt;f&gt;ⓡ&lt;/f&gt; fie cei dintâi în fapte bune pentru nevoile arzătoare, ca să nu&lt;f&gt;ⓢ&lt;/f&gt; fie neroditori.');
INSERT INTO &quot;verses&quot; VALUES (630,3,15,'&lt;pb/&gt;Toți cei ce sunt cu mine îți trimit salutări. Spune-le salutări celor ce ne iubesc în credință! &lt;pb/&gt;Harul să fie cu voi toți! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (640,1,1,'&lt;pb/&gt;Pavel, întemnițat&lt;f&gt;ⓐ&lt;/f&gt; al lui Hristos Isus, și fratele Timotei către preaiubitul Filimon, tovarășul&lt;f&gt;ⓑ&lt;/f&gt; nostru de lucru');
INSERT INTO &quot;verses&quot; VALUES (640,1,2,'(și, de asemenea, către sora Apfia și Arhip&lt;f&gt;ⓒ&lt;/f&gt;, tovarășul nostru&lt;f&gt;ⓓ&lt;/f&gt; de luptă), și către biserica&lt;f&gt;ⓔ&lt;/f&gt; din casa ta:');
INSERT INTO &quot;verses&quot; VALUES (640,1,3,'Har&lt;f&gt;ⓕ&lt;/f&gt; și pace de la Dumnezeu, Tatăl nostru, și de la Domnul Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (640,1,4,'&lt;pb/&gt;Îi mulțumesc&lt;f&gt;ⓖ&lt;/f&gt; totdeauna Dumnezeului meu ori de câte ori îmi aduc aminte de tine în rugăciunile mele,');
INSERT INTO &quot;verses&quot; VALUES (640,1,5,'pentru că am auzit&lt;f&gt;ⓗ&lt;/f&gt; despre dragostea și credința pe care le ai față de Domnul Isus și față de toți sfinții.');
INSERT INTO &quot;verses&quot; VALUES (640,1,6,'Îl rog ca participarea ta la credință să se arate prin fapte, în vederea cunoașterii&lt;f&gt;ⓘ&lt;/f&gt; oricărui bine care ne stă în putere pentru Hristos.');
INSERT INTO &quot;verses&quot; VALUES (640,1,7,'Într-adevăr, am avut o mare bucurie și mângâiere datorită dragostei tale, fiindcă, frate, inimile&lt;f&gt;[1]&lt;/f&gt; sfinților au fost înviorate&lt;f&gt;ⓙ&lt;/f&gt; prin tine.');
INSERT INTO &quot;verses&quot; VALUES (640,1,8,'&lt;pb/&gt;De aceea, măcar că&lt;f&gt;ⓚ&lt;/f&gt; în Hristos am toată îndrăzneala să-ți poruncesc ce trebuie să faci,');
INSERT INTO &quot;verses&quot; VALUES (640,1,9,'vreau mai degrabă să-ți fac o rugăminte în numele dragostei, eu, de acum bătrânul&lt;f&gt;[2]&lt;/f&gt; Pavel și&lt;f&gt;ⓛ&lt;/f&gt; întemnițat al lui Hristos Isus.');
INSERT INTO &quot;verses&quot; VALUES (640,1,10,'Te rog pentru copilul meu pe care l-am născut&lt;f&gt;ⓜ&lt;/f&gt; în lanțurile mele, pentru&lt;f&gt;ⓝ&lt;/f&gt; Onisim&lt;f&gt;[3]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (640,1,11,'care altădată ți-a fost nefolositor, dar care acum ne va fi folositor și ție, și mie.');
INSERT INTO &quot;verses&quot; VALUES (640,1,12,'Ți-l trimit înapoi pe el, inima mea.');
INSERT INTO &quot;verses&quot; VALUES (640,1,13,'Aș fi dorit să-l țin la mine, ca&lt;f&gt;ⓞ&lt;/f&gt; să-mi slujească în locul tău cât sunt în lanțuri pentru Evanghelie,');
INSERT INTO &quot;verses&quot; VALUES (640,1,14,'dar fără învoirea ta n-am vrut să fac nimic, pentru ca&lt;f&gt;ⓟ&lt;/f&gt; binele pe care mi-l faci să nu fie silit, ci de bunăvoie.');
INSERT INTO &quot;verses&quot; VALUES (640,1,15,'&lt;pb/&gt;Poate&lt;f&gt;ⓠ&lt;/f&gt; că el a ajuns despărțit de tine pentru o vreme tocmai ca să-l primești &lt;i&gt;înapoi&lt;/i&gt; pentru veșnicie,');
INSERT INTO &quot;verses&quot; VALUES (640,1,16,'dar nu ca pe un rob, ca mai înainte, ci mult mai mult, ca pe un frate&lt;f&gt;ⓡ&lt;/f&gt; preaiubit mai ales de mine și&lt;f&gt;ⓢ&lt;/f&gt; cu atât mai mult de tine, atât omenește, cât și în Domnul!');
INSERT INTO &quot;verses&quot; VALUES (640,1,17,'Dacă mă socotești tovarășul &lt;i&gt;tău&lt;/i&gt;&lt;f&gt;ⓣ&lt;/f&gt;, primește-l ca pe mine însumi!');
INSERT INTO &quot;verses&quot; VALUES (640,1,18,'Iar dacă ți-a pricinuit vreo pagubă sau îți este dator cu ceva, pune &lt;i&gt;totul&lt;/i&gt; în socoteala mea.');
INSERT INTO &quot;verses&quot; VALUES (640,1,19,'Eu, Pavel — scriu cu mâna mea — eu voi plăti &lt;i&gt;paguba&lt;/i&gt;; ca să nu-ți mai zic că tu însuți îmi ești dator mie.');
INSERT INTO &quot;verses&quot; VALUES (640,1,20,'Da, frate, fă-mi binele &lt;i&gt;acesta&lt;/i&gt; în Domnul: înviorează-mi&lt;f&gt;ⓤ&lt;/f&gt; inima în Hristos!');
INSERT INTO &quot;verses&quot; VALUES (640,1,21,'&lt;pb/&gt;Ți-am scris fiindcă sunt încredințat&lt;f&gt;ⓥ&lt;/f&gt; de ascultarea ta și știu că vei face chiar mai mult decât îți zic.');
INSERT INTO &quot;verses&quot; VALUES (640,1,22,'Totodată, pregătește-mi un loc de găzduire, căci nădăjduiesc&lt;f&gt;ⓦ&lt;/f&gt; să vă fiu dăruit datorită&lt;f&gt;ⓧ&lt;/f&gt; rugăciunilor voastre!');
INSERT INTO &quot;verses&quot; VALUES (640,1,23,'&lt;pb/&gt;Epafras&lt;f&gt;ⓨ&lt;/f&gt;, tovarășul meu de temniță în Hristos Isus, îți trimite salutări;');
INSERT INTO &quot;verses&quot; VALUES (640,1,24,'tot așa și Marcu&lt;f&gt;ⓩ&lt;/f&gt;, Aristarh&lt;f&gt;ⓐ&lt;/f&gt;, Dima&lt;f&gt;ⓑ&lt;/f&gt; &lt;i&gt;și&lt;/i&gt; Luca&lt;f&gt;ⓒ&lt;/f&gt;, tovarășii mei de lucru.');
INSERT INTO &quot;verses&quot; VALUES (640,1,25,'&lt;pb/&gt;Harul&lt;f&gt;ⓓ&lt;/f&gt; Domnului [nostru] Isus Hristos să fie cu duhul vostru! Amin!');
INSERT INTO &quot;verses&quot; VALUES (650,1,1,'&lt;pb/&gt;După ce în vechime le-a vorbit părinților noștri prin profeți în multe rânduri&lt;f&gt;ⓐ&lt;/f&gt; și în multe feluri,');
INSERT INTO &quot;verses&quot; VALUES (650,1,2,'la sfârșitul&lt;f&gt;ⓑ&lt;/f&gt; acestor zile Dumnezeu ne-a vorbit&lt;f&gt;ⓒ&lt;/f&gt; prin Fiul, pe&lt;f&gt;ⓓ&lt;/f&gt; care L-a pus moștenitor a toate și prin&lt;f&gt;ⓔ&lt;/f&gt; care a făcut lumea&lt;f&gt;[1]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,1,3,'&lt;pb/&gt;El, care&lt;f&gt;ⓕ&lt;/f&gt; este oglindirea slavei Lui și întipărirea Ființei Lui, care le ține&lt;f&gt;ⓖ&lt;/f&gt; pe toate prin cuvântul Său puternic, după ce a făcut&lt;f&gt;ⓗ&lt;/f&gt; curățirea păcatelor, S-a așezat&lt;f&gt;ⓘ&lt;/f&gt; la dreapta Măririi, în locurile preaînalte,');
INSERT INTO &quot;verses&quot; VALUES (650,1,4,'ajungând cu atât mai presus de îngeri, cu cât a&lt;f&gt;ⓙ&lt;/f&gt; moștenit un Nume mai minunat decât al lor.');
INSERT INTO &quot;verses&quot; VALUES (650,1,5,'&lt;pb/&gt;Căci căruia dintre îngeri i-a spus &lt;i&gt;Dumnezeu&lt;/i&gt; vreodată: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Tu ești Fiul&lt;/e&gt; &lt;f&gt;ⓚ&lt;/f&gt; &lt;e&gt;Meu,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;astăzi Te-am născut&lt;/e&gt; &lt;f&gt;[2]&lt;/f&gt;?&lt;/t&gt; &lt;pb/&gt;Și iarăși: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Eu&lt;/e&gt; &lt;f&gt;ⓛ&lt;/f&gt; &lt;e&gt;Îi voi fi Tată,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și El Îmi va fi Fiu&lt;/e&gt; &lt;f&gt;[3]&lt;/f&gt;?&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,6,'&lt;pb/&gt;Și iarăși, când aduce în lume pe Cel Întâi Născut&lt;f&gt;ⓜ&lt;/f&gt;, zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Să I se închine toți&lt;/e&gt; &lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;îngerii lui Dumnezeu!&lt;/e&gt;&lt;f&gt;[4]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,7,'&lt;pb/&gt;Și dacă despre îngeri zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;El face pe îngerii Săi vânturi,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și pe slujitorii Săi, flacără de foc&lt;/e&gt; &lt;f&gt;[5]&lt;/f&gt;,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,8,'&lt;pb/&gt;despre Fiul &lt;i&gt;zice&lt;/i&gt;: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Tronul Tău, Dumnezeule, este în veci de veci;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;toiag de dreptate este toiagul domniei Tale.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,9,'&lt;t&gt;&lt;e&gt;Ai iubit dreptatea și ai urât nelegiuirea.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;De aceea, Dumnezeule, Dumnezeul Tău Te-a&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;uns&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;cu untdelemn de bucurie mai presus de tovarășii Tăi.&lt;/e&gt; &lt;f&gt;[6]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,10,'&lt;pb/&gt;Și: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;La început, Tu, Doamne, ai întemeiat pământul&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și cerurile sunt lucrarea mâinilor Tale.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,11,'&lt;t&gt;&lt;e&gt;Ele&lt;/e&gt; &lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;vor pieri, dar Tu rămâi;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;toate ca o haină se vor învechi;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,12,'&lt;t&gt;&lt;e&gt;Ca pe o manta le vei înfășura,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;ca pe o haină, &lt;e&gt;și se vor schimba,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;dar Tu ești același, și anii Tăi nu se vor sfârși.&lt;/e&gt; &lt;f&gt;[7]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,13,'&lt;pb/&gt;Și căruia dintre îngeri i-a spus El vreodată: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Șezi&lt;/e&gt; &lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;la dreapta Mea&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;până-i voi pune pe vrăjmașii Tăi reazem picioarelor Tale&lt;/e&gt; &lt;f&gt;[8]&lt;/f&gt;?&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,1,14,'&lt;pb/&gt;Nu sunt&lt;f&gt;ⓡ&lt;/f&gt; toți duhuri slujitoare, trimise în slujba celor care vor moșteni&lt;f&gt;ⓢ&lt;/f&gt; mântuirea?');
INSERT INTO &quot;verses&quot; VALUES (650,2,1,'&lt;pb/&gt;De aceea, trebuie cu atât mai mult să luăm aminte la cele auzite, ca să nu fim depărtați de ele&lt;f&gt;[9]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,2,2,'Căci, dacă Cuvântul rostit&lt;f&gt;ⓣ&lt;/f&gt; prin îngeri s-a dovedit neclintit și &lt;i&gt;dacă&lt;/i&gt; orice&lt;f&gt;ⓤ&lt;/f&gt; abatere și orice neascultare și-a primit pedeapsa cuvenită,');
INSERT INTO &quot;verses&quot; VALUES (650,2,3,'cum&lt;f&gt;ⓥ&lt;/f&gt; vom scăpa noi dacă stăm nepăsători față de o mântuire așa de mare, care&lt;f&gt;ⓦ&lt;/f&gt; și-a avut începutul în cuvintele Domnului și care a&lt;f&gt;ⓧ&lt;/f&gt; fost confirmată de cei care au auzit-o,');
INSERT INTO &quot;verses&quot; VALUES (650,2,4,'fiind adeverită de Dumnezeu&lt;f&gt;ⓨ&lt;/f&gt; cu&lt;f&gt;ⓩ&lt;/f&gt; semne, minuni și felurite puteri și cu &lt;i&gt;darurile&lt;/i&gt; Duhului Sfânt&lt;f&gt;ⓐ&lt;/f&gt;, împărțite&lt;f&gt;[10]&lt;/f&gt; după&lt;f&gt;ⓑ&lt;/f&gt; voia Sa?');
INSERT INTO &quot;verses&quot; VALUES (650,2,5,'&lt;pb/&gt;Căci nu unor îngeri le-a supus El lumea&lt;f&gt;ⓒ&lt;/f&gt; viitoare despre care vorbim.');
INSERT INTO &quot;verses&quot; VALUES (650,2,6,'Ba încă, cineva a făcut undeva următoarea mărturisire: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Ce&lt;/e&gt; &lt;f&gt;ⓓ&lt;/f&gt; &lt;e&gt;este omul ca să-Ți aduci aminte de el&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;sau fiul omului ca să-l cercetezi&lt;/e&gt; &lt;f&gt;[11]&lt;/f&gt; &lt;e&gt;?&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,2,7,'&lt;t&gt;&lt;e&gt;L-ai făcut cu puțin mai prejos decât îngerii,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;cu slavă și cinste l-ai încununat,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;[l-ai pus peste lucrările mâinilor Tale,]&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,2,8,'&lt;t&gt;&lt;e&gt;toate&lt;/e&gt; &lt;f&gt;ⓔ&lt;/f&gt; &lt;e&gt;le-ai supus sub picioarele lui.&lt;/e&gt;&lt;f&gt;[12]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;Dacă i-a supus totul, nu a lăsat nimic care să nu-i fie supus. Acum însă, nu&lt;f&gt;ⓕ&lt;/f&gt; vedem că i-ar fi supuse toate.');
INSERT INTO &quot;verses&quot; VALUES (650,2,9,'Dar Îl vedem pe Acela&lt;f&gt;ⓖ&lt;/f&gt; care a fost &lt;e&gt;făcut cu puțin mai prejos de îngeri&lt;/e&gt;, adică pe Isus, &lt;e&gt;încununat&lt;/e&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;cu slavă și cinste&lt;/e&gt;&lt;f&gt;[13]&lt;/f&gt; datorită morții pe care a suferit-o. Astfel, prin harul lui Dumnezeu, El a gustat moartea pentru&lt;f&gt;ⓘ&lt;/f&gt; toți.');
INSERT INTO &quot;verses&quot; VALUES (650,2,10,'&lt;pb/&gt;Se cuvenea&lt;f&gt;ⓙ&lt;/f&gt; ca, aducând pe mulți fii la slavă, &lt;i&gt;Dumnezeu&lt;/i&gt; — pentru&lt;f&gt;ⓚ&lt;/f&gt; care și prin care sunt toate — să-L desăvârșească&lt;f&gt;ⓛ&lt;/f&gt; prin suferințe pe întemeietorul mântuirii&lt;f&gt;ⓜ&lt;/f&gt; lor.');
INSERT INTO &quot;verses&quot; VALUES (650,2,11,'Căci Cel&lt;f&gt;ⓝ&lt;/f&gt; care sfințește și cei sfințiți sunt toți dintr-Unul &lt;i&gt;singur&lt;/i&gt;; de aceea, nu-I&lt;f&gt;ⓞ&lt;/f&gt; este rușine să-i numească frați,');
INSERT INTO &quot;verses&quot; VALUES (650,2,12,'ci zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Voi vesti Numele Tău fraților Mei;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;în mijlocul adunării Îți voi cânta laude.&lt;/e&gt; &lt;f&gt;[14]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,2,13,'&lt;pb/&gt;Și iarăși: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Îmi voi pune încrederea în El.&lt;/e&gt; &lt;f&gt;[15]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;Și iarăși: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată, eu și copiii pe care&lt;/e&gt; &lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;mi i-a dat Dumnezeu!&lt;/e&gt;&lt;f&gt;[16]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,2,14,'&lt;pb/&gt;Astfel, deoarece copiii sunt părtași sângelui și cărnii, și&lt;f&gt;ⓠ&lt;/f&gt; El a fost părtaș la ele, pentru ca&lt;f&gt;ⓡ&lt;/f&gt;, prin moarte, să-l nimicească pe cel ce are puterea morții, adică pe Diavolul,');
INSERT INTO &quot;verses&quot; VALUES (650,2,15,'și să-i elibereze pe cei care, prin frica morții&lt;f&gt;ⓢ&lt;/f&gt;, erau ținuți în robie toată viața lor.');
INSERT INTO &quot;verses&quot; VALUES (650,2,16,'Căci, negreșit, nu îngerilor le vine El în ajutor, ci seminței lui Avraam.&lt;f&gt;[17]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,2,17,'Prin urmare, trebuia să Se asemene&lt;f&gt;ⓣ&lt;/f&gt; fraților Săi în toate &lt;i&gt;privințele&lt;/i&gt;, ca să devină un mare-preot îndurător&lt;f&gt;ⓤ&lt;/f&gt; și credincios&lt;f&gt;[18]&lt;/f&gt; înaintea lui Dumnezeu, făcând &lt;i&gt;astfel&lt;/i&gt; ispășire pentru păcatele poporului.');
INSERT INTO &quot;verses&quot; VALUES (650,2,18,'Căci&lt;f&gt;ⓥ&lt;/f&gt;, întrucât El Însuși a suferit, când a fost ispitit, poate să vină în ajutor celor care sunt ispitiți.');
INSERT INTO &quot;verses&quot; VALUES (650,3,1,'&lt;pb/&gt;De aceea, frați sfinți, care aveți parte de chemarea&lt;f&gt;ⓦ&lt;/f&gt; cerească, luați aminte la Apostolul&lt;f&gt;ⓧ&lt;/f&gt; și Marele-Preot pe care Îl mărturisim, &lt;i&gt;adică&lt;/i&gt; Isus,');
INSERT INTO &quot;verses&quot; VALUES (650,3,2,'care a fost credincios Celui care L-a rânduit, cum &lt;i&gt;a fost&lt;/i&gt; și &lt;e&gt;Moise în toată casa Lui&lt;/e&gt;&lt;f&gt;[19]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,3,3,'Căci &lt;i&gt;Isus&lt;/i&gt; a fost găsit vrednic &lt;i&gt;să aibă&lt;/i&gt; o slavă cu atât mai mare decât Moise, cu cât cel ce&lt;f&gt;ⓨ&lt;/f&gt; a zidit o casă are mai multă cinste decât &lt;i&gt;casa&lt;/i&gt; însăși.');
INSERT INTO &quot;verses&quot; VALUES (650,3,4,'(Orice casă este zidită de cineva, dar Cel ce&lt;f&gt;ⓩ&lt;/f&gt; a zidit toate &lt;i&gt;lucrurile&lt;/i&gt; este Dumnezeu.)');
INSERT INTO &quot;verses&quot; VALUES (650,3,5,'Cât despre Moise&lt;f&gt;ⓐ&lt;/f&gt;, el a fost &lt;e&gt;credincios în toată casa lui Dumnezeu ca&lt;/e&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;slujitor&lt;/e&gt;&lt;f&gt;[20]&lt;/f&gt;, ca să dea mărturie&lt;f&gt;ⓒ&lt;/f&gt; despre &lt;i&gt;lucrurile&lt;/i&gt; care urmau să fie vestite.');
INSERT INTO &quot;verses&quot; VALUES (650,3,6,'Dar Hristos &lt;i&gt;este credincios&lt;/i&gt; ca Fiu&lt;f&gt;ⓓ&lt;/f&gt; peste&lt;f&gt;ⓔ&lt;/f&gt; casa &lt;i&gt;lui Dumnezeu&lt;/i&gt;, iar casa Lui suntem noi, dacă&lt;f&gt;ⓕ&lt;/f&gt; ținem [neclintite, până la sfârșit,] încrederea și nădejdea cu care ne lăudăm.');
INSERT INTO &quot;verses&quot; VALUES (650,3,7,'&lt;pb/&gt;De aceea, cum zice Duhul&lt;f&gt;ⓖ&lt;/f&gt; Sfânt: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Astăzi&lt;/e&gt; &lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;, dacă auziți glasul Lui,&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,3,8,'&lt;t&gt;&lt;e&gt;nu vă împietriți inimile ca în &lt;i&gt;ziua&lt;/i&gt; răzvrătirii,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca în ziua ispitirii în pustiu,&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,3,9,'&lt;t&gt;&lt;e&gt;când M-au ispitit părinții voștri,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;M-au pus la încercare&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și au văzut lucrările Mele &lt;i&gt;vreme de&lt;/i&gt; patruzeci de ani!&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,3,10,'&lt;t&gt;&lt;e&gt;De aceea M-am scârbit de neamul acesta&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și am zis: Ei veșnic se rătăcesc cu inima;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;n-au cunoscut căile Mele!&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,3,11,'&lt;t&gt;&lt;e&gt;Așa că am jurat în mânia Mea:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu vor intra în odihna Mea!&lt;/e&gt; &lt;f&gt;[21]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,3,12,'&lt;pb/&gt;Luați seama, fraților, ca niciunul dintre voi să nu aibă o inimă rea și necredincioasă, care să îl despartă de Dumnezeul cel viu,');
INSERT INTO &quot;verses&quot; VALUES (650,3,13,'ci îndemnați-vă unii pe alții în fiecare zi, cât încă se spune „astăzi”, ca niciunul din voi să nu se împietrească prin înșelăciunea păcatului!');
INSERT INTO &quot;verses&quot; VALUES (650,3,14,'Căci ne-am făcut părtași cu Hristos, dacă&lt;f&gt;ⓘ&lt;/f&gt; ținem până la sfârșit învoiala de la început.');
INSERT INTO &quot;verses&quot; VALUES (650,3,15,'&lt;pb/&gt;Când se zice: &lt;e&gt;Astăzi&lt;/e&gt;&lt;f&gt;ⓙ&lt;/f&gt;&lt;e&gt;, dacă auziți glasul Lui, nu vă împietriți inimile, ca în &lt;i&gt;ziua&lt;/i&gt; răzvrătirii&lt;/e&gt;&lt;f&gt;[22]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (650,3,16,'cine&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;sunt cei ce&lt;/i&gt; s-au răzvrătit după ce au auzit? Oare nu toți cei care ieșiseră din Egipt sub Moise?');
INSERT INTO &quot;verses&quot; VALUES (650,3,17,'Și de cine S-a scârbit El vreme de patruzeci de ani? Oare nu de cei care păcătuiseră și ale&lt;f&gt;ⓛ&lt;/f&gt; căror trupuri moarte au căzut în pustiu?');
INSERT INTO &quot;verses&quot; VALUES (650,3,18,'Și cui&lt;f&gt;ⓜ&lt;/f&gt; S-a jurat El că nu vor intra în odihna Lui? Oare nu celor ce s-au răzvrătit?');
INSERT INTO &quot;verses&quot; VALUES (650,3,19,'Așadar, vedem&lt;f&gt;ⓝ&lt;/f&gt; că n-au putut să intre din pricina necredinței.');
INSERT INTO &quot;verses&quot; VALUES (650,4,1,'&lt;pb/&gt;Să&lt;f&gt;ⓞ&lt;/f&gt; luăm deci bine seama ca nu cumva, câtă vreme rămâne &lt;i&gt;în picioare&lt;/i&gt; făgăduința intrării în odihna Lui, să se pomenească vreunul din voi că a sosit prea târziu.');
INSERT INTO &quot;verses&quot; VALUES (650,4,2,'Căci și noi am primit vestea bună, asemenea lor. Dar acelora nu le-a ajutat la nimic cuvântul auzit, pentru că nu s-au unit în credință cu cei care au ascultat,');
INSERT INTO &quot;verses&quot; VALUES (650,4,3,'pe când noi&lt;f&gt;ⓟ&lt;/f&gt;, care am crezut, intrăm în odihnă. După cum a spus: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Am&lt;/e&gt; &lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;jurat în mânia Mea:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu vor intra în odihna Mea!&lt;/e&gt; &lt;f&gt;[23]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;Totuși, lucrările &lt;i&gt;Lui&lt;/i&gt; se încheiaseră &lt;i&gt;încă&lt;/i&gt; de la întemeierea lumii.');
INSERT INTO &quot;verses&quot; VALUES (650,4,4,'Căci spune undeva despre ziua a șaptea astfel: &lt;e&gt;Și Dumnezeu S-a odihnit&lt;/e&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;e&gt;în ziua a șaptea de toate lucrările Lui&lt;/e&gt;.&lt;f&gt;[24]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,4,5,'Și iarăși, pe aceeași &lt;i&gt;temă&lt;/i&gt;: &lt;e&gt;Nu vor intra în odihna Mea!&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,4,6,'Fiindcă rămâne deci ca unii să intre în &lt;i&gt;odihnă&lt;/i&gt; și fiindcă cei&lt;f&gt;ⓢ&lt;/f&gt; cărora li s-a adus întâi vestea bună n-au intrat în ea din pricina neascultării,');
INSERT INTO &quot;verses&quot; VALUES (650,4,7,'&lt;i&gt;Dumnezeu&lt;/i&gt; hotărăște iarăși o zi — „astăzi” — zicând prin David, după atâta vreme, după cum s-a spus: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Astăzi, dacă auziți glasul Lui,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu vă împietriți inimile!&lt;/e&gt; &lt;f&gt;[25]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,4,8,'&lt;pb/&gt;Căci, dacă le-ar fi dat Iosua odihnă, nu mai vorbea &lt;i&gt;Dumnezeu&lt;/i&gt; după aceea despre o altă zi.');
INSERT INTO &quot;verses&quot; VALUES (650,4,9,'Rămâne deci o odihnă-de-sabat pentru poporul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (650,4,10,'Căci cine a intrat în odihna Lui s-a odihnit și el de lucrările lui, după cum Dumnezeu &lt;i&gt;S-a odihnit&lt;/i&gt; de &lt;i&gt;lucrările&lt;/i&gt; Sale.');
INSERT INTO &quot;verses&quot; VALUES (650,4,11,'Să ne grăbim deci să intrăm în acea odihnă, ca să nu cadă nimeni în aceeași&lt;f&gt;ⓣ&lt;/f&gt; pildă de neascultare.');
INSERT INTO &quot;verses&quot; VALUES (650,4,12,'Căci Cuvântul lui Dumnezeu este viu&lt;f&gt;ⓤ&lt;/f&gt; și plin de putere&lt;f&gt;[26]&lt;/f&gt;, mai tăios decât orice sabie&lt;f&gt;ⓥ&lt;/f&gt; cu două tăișuri; pătrunde&lt;f&gt;ⓦ&lt;/f&gt; și desparte sufletul de duh și încheieturile de măduvă, judecând simțirile și gândurile inimii.');
INSERT INTO &quot;verses&quot; VALUES (650,4,13,'Nicio&lt;f&gt;ⓧ&lt;/f&gt; făptură nu este ascunsă înaintea Lui, ci totul este gol și descoperit&lt;f&gt;ⓨ&lt;/f&gt; în ochii Celui căruia Îi vom da socoteală.');
INSERT INTO &quot;verses&quot; VALUES (650,4,14,'&lt;pb/&gt;Astfel, fiindcă avem un Mare-Preot&lt;f&gt;ⓩ&lt;/f&gt; însemnat, care&lt;f&gt;ⓐ&lt;/f&gt; a străbătut cerurile, pe Isus, Fiul lui Dumnezeu, să&lt;f&gt;ⓑ&lt;/f&gt; ținem tare mărturisirea &lt;i&gt;noastră&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (650,4,15,'Căci nu avem&lt;f&gt;ⓒ&lt;/f&gt; un Mare-Preot care să nu simtă cu noi în slăbiciunile noastre, ci unul care a fost ispitit&lt;f&gt;ⓓ&lt;/f&gt; ca noi în toate privințele, dar&lt;f&gt;ⓔ&lt;/f&gt; &lt;i&gt;a rămas&lt;/i&gt; fără păcat.');
INSERT INTO &quot;verses&quot; VALUES (650,4,16,'Să&lt;f&gt;ⓕ&lt;/f&gt; ne apropiem deci cu îndrăzneală&lt;f&gt;[27]&lt;/f&gt; de tronul harului, ca să primim îndurare și să găsim har și ajutor la vreme de nevoie!');
INSERT INTO &quot;verses&quot; VALUES (650,5,1,'&lt;pb/&gt;Orice mare-preot, &lt;i&gt;fiind&lt;/i&gt; luat dintre oameni, este rânduit&lt;f&gt;ⓖ&lt;/f&gt; în relația&lt;f&gt;ⓗ&lt;/f&gt; cu Dumnezeu, &lt;i&gt;anume&lt;/i&gt; să&lt;f&gt;ⓘ&lt;/f&gt; aducă daruri și jertfe pentru păcate.');
INSERT INTO &quot;verses&quot; VALUES (650,5,2,'El&lt;f&gt;ⓙ&lt;/f&gt; se poate arăta îngăduitor cu cei neștiutori și rătăciți fiindcă și&lt;f&gt;ⓚ&lt;/f&gt; el este supus slăbiciunii,');
INSERT INTO &quot;verses&quot; VALUES (650,5,3,'din pricina căreia&lt;f&gt;ⓛ&lt;/f&gt; trebuie să aducă &lt;i&gt;jertfe&lt;/i&gt; pentru păcate atât pentru popor, cât și pentru sine.');
INSERT INTO &quot;verses&quot; VALUES (650,5,4,'Dar nimeni&lt;f&gt;ⓜ&lt;/f&gt; nu-și ia singur cinstea &lt;i&gt;aceasta&lt;/i&gt;, ci &lt;i&gt;trebuie să fie&lt;/i&gt; chemat de Dumnezeu, cum a fost Aaron&lt;f&gt;ⓝ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,5,5,'&lt;pb/&gt;Tot astfel&lt;f&gt;ⓞ&lt;/f&gt;, nici Hristos nu Și-a luat singur slava de a fi Mare-Preot, ci &lt;i&gt;o are&lt;/i&gt; de la Cel ce I-a zis: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Tu ești Fiul&lt;/e&gt; &lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;Meu, astăzi Te-am născut.&lt;/e&gt;&lt;f&gt;[28]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,5,6,'&lt;pb/&gt;După cum și în alt loc zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Tu&lt;/e&gt; &lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;ești preot în veac, după rânduiala lui Melhisedec.&lt;/e&gt;&lt;f&gt;[29]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,5,7,'&lt;pb/&gt;El, care în timpul vieții Sale pământești a făcut&lt;f&gt;ⓡ&lt;/f&gt; rugăciuni și cereri cu&lt;f&gt;ⓢ&lt;/f&gt; strigăte mari și cu lacrimi către Cel ce&lt;f&gt;ⓣ&lt;/f&gt; putea să-L izbăvească de moarte (fiind ascultat datorită evlaviei&lt;f&gt;[30]&lt;/f&gt; &lt;i&gt;Sale&lt;/i&gt;),');
INSERT INTO &quot;verses&quot; VALUES (650,5,8,'a învățat ascultarea&lt;f&gt;ⓤ&lt;/f&gt; prin ceea ce a suferit, deși&lt;f&gt;ⓥ&lt;/f&gt; era Fiu.');
INSERT INTO &quot;verses&quot; VALUES (650,5,9,'Și, după ce a fost făcut desăvârșit&lt;f&gt;ⓦ&lt;/f&gt;, S-a făcut pentru toți cei ce ascultă de El sursa unei mântuiri veșnice,');
INSERT INTO &quot;verses&quot; VALUES (650,5,10,'căci a fost numit de Dumnezeu Mare-Preot &lt;e&gt;după&lt;/e&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;rânduiala lui Melhisedec&lt;/e&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,5,11,'&lt;pb/&gt;Asupra celor de mai sus avem multe&lt;f&gt;ⓨ&lt;/f&gt; de zis, &lt;i&gt;lucruri&lt;/i&gt; greu de explicat, căci v-ați făcut greoi&lt;f&gt;ⓩ&lt;/f&gt; la pricepere.');
INSERT INTO &quot;verses&quot; VALUES (650,5,12,'Deși se cuvenea să fiți învățători de multă vreme, aveți nevoie să fiți învățați iarăși&lt;f&gt;ⓐ&lt;/f&gt; adevărurile elementare ale cuvintelor lui Dumnezeu; ați ajuns să aveți nevoie de lapte&lt;f&gt;ⓑ&lt;/f&gt;, nu de hrană tare.');
INSERT INTO &quot;verses&quot; VALUES (650,5,13,'Cine se hrănește cu lapte încă nu s-a obișnuit cu învățătura despre dreptate, ci este un copil&lt;f&gt;ⓒ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,5,14,'Hrana tare este însă pentru cei maturi, pentru cei a căror judecată s-a deprins, prin întrebuințare, să deosebească&lt;f&gt;ⓓ&lt;/f&gt; binele de rău.');
INSERT INTO &quot;verses&quot; VALUES (650,6,1,'&lt;pb/&gt;De aceea, lăsând&lt;f&gt;ⓔ&lt;/f&gt; în urmă învățăturile începătoare despre Hristos, să mergem către maturitate&lt;f&gt;[31]&lt;/f&gt;, fără să mai punem iarăși temelia pocăinței de faptele moarte&lt;f&gt;ⓕ&lt;/f&gt; și a credinței în Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (650,6,2,'a învățăturii despre&lt;f&gt;ⓖ&lt;/f&gt; botezuri, punerea&lt;f&gt;ⓗ&lt;/f&gt; mâinilor, învierea morților&lt;f&gt;ⓘ&lt;/f&gt; și judecata&lt;f&gt;ⓙ&lt;/f&gt; veșnică.');
INSERT INTO &quot;verses&quot; VALUES (650,6,3,'Și vom face lucrul acesta, dacă&lt;f&gt;ⓚ&lt;/f&gt; va îngădui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (650,6,4,'&lt;pb/&gt;Căci cei care au fost luminați&lt;f&gt;ⓛ&lt;/f&gt; odată, care au gustat din darul&lt;f&gt;ⓜ&lt;/f&gt; ceresc și s-au făcut părtași&lt;f&gt;ⓝ&lt;/f&gt; Duhului Sfânt,');
INSERT INTO &quot;verses&quot; VALUES (650,6,5,'care au gustat Cuvântul cel bun al lui Dumnezeu și puterile veacului viitor&lt;f&gt;ⓞ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (650,6,6,'dar care au căzut &lt;i&gt;în apostazie&lt;/i&gt; este cu neputință&lt;f&gt;[32]&lt;/f&gt; să fie înnoiți iarăși&lt;f&gt;ⓟ&lt;/f&gt; spre pocăință, fiindcă&lt;f&gt;ⓠ&lt;/f&gt; Îl răstignesc din nou, pentru ei, pe Fiul lui Dumnezeu și-L dau să fie batjocorit.');
INSERT INTO &quot;verses&quot; VALUES (650,6,7,'Când pământul se adapă cu ploaia care coboară adesea pe el și rodește o vegetație folositoare celor pentru care este lucrat, primește binecuvântare&lt;f&gt;ⓡ&lt;/f&gt; de la Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (650,6,8,'Dar&lt;f&gt;ⓢ&lt;/f&gt; dacă rodește spini și mărăcini, este lepădat și aproape de blestem, iar sfârșitul lui este focul.');
INSERT INTO &quot;verses&quot; VALUES (650,6,9,'&lt;pb/&gt;Deși vorbim astfel, preaiubiților, în ce vă privește, suntem convinși că aveți fapte mai bune, care însoțesc mântuirea.');
INSERT INTO &quot;verses&quot; VALUES (650,6,10,'Căci&lt;f&gt;ⓣ&lt;/f&gt; Dumnezeu&lt;f&gt;ⓤ&lt;/f&gt; nu este nedrept ca să uite lucrarea&lt;f&gt;ⓥ&lt;/f&gt; voastră și dragostea pe care ați arătat-o pentru Numele Lui voi, care i-ați ajutat&lt;f&gt;ⓦ&lt;/f&gt; și încă îi ajutați pe sfinți.');
INSERT INTO &quot;verses&quot; VALUES (650,6,11,'Dorim însă ca fiecare&lt;f&gt;ⓧ&lt;/f&gt; din voi să arate aceeași râvnă, ca să-și păstreze nădejdea neclintită până la sfârșit&lt;f&gt;ⓨ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (650,6,12,'așa încât să nu fiți leneși&lt;f&gt;[33]&lt;/f&gt;, ci să călcați pe urmele celor ce, prin credință și răbdare, moștenesc&lt;f&gt;ⓩ&lt;/f&gt; făgăduințele.');
INSERT INTO &quot;verses&quot; VALUES (650,6,13,'&lt;pb/&gt;Dumnezeu, când i-a făcut lui Avraam făgăduința, fiindcă nu putea să jure pe cineva mai mare, a&lt;f&gt;ⓐ&lt;/f&gt; jurat pe Sine');
INSERT INTO &quot;verses&quot; VALUES (650,6,14,'și a zis: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Te voi binecuvânta negreșit și te voi înmulți foarte mult.&lt;/e&gt; &lt;f&gt;[34]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,6,15,'&lt;pb/&gt;Și astfel, fiindcă a așteptat cu răbdare, &lt;i&gt;Avraam&lt;/i&gt; a dobândit ce-i fusese făgăduit.');
INSERT INTO &quot;verses&quot; VALUES (650,6,16,'Oamenii obișnuiesc să jure pe cineva mai mare, iar jurământul&lt;f&gt;ⓑ&lt;/f&gt; este o întărire ce pune capăt oricărei neînțelegeri dintre ei.');
INSERT INTO &quot;verses&quot; VALUES (650,6,17,'De aceea și Dumnezeu, fiindcă voia să demonstreze cu prisosință moștenitorilor&lt;f&gt;ⓒ&lt;/f&gt; făgăduinței caracterul nestrămutat&lt;f&gt;ⓓ&lt;/f&gt; al hotărârii Lui, a intervenit cu un jurământ,');
INSERT INTO &quot;verses&quot; VALUES (650,6,18,'pentru ca, prin două lucruri nestrămutate, în care este cu neputință ca Dumnezeu să mintă, să găsim o puternică îmbărbătare noi, a căror scăpare a fost să apucăm nădejdea care ne stă&lt;f&gt;ⓔ&lt;/f&gt; înainte,');
INSERT INTO &quot;verses&quot; VALUES (650,6,19,'pe care o avem ca ancoră a sufletului, neclintită și fermă, care&lt;f&gt;ⓕ&lt;/f&gt; pătrunde dincolo de catapeteasmă,');
INSERT INTO &quot;verses&quot; VALUES (650,6,20,'unde&lt;f&gt;ⓖ&lt;/f&gt; a intrat Isus pentru noi, ca înainte-mergător, după ce a devenit&lt;f&gt;ⓗ&lt;/f&gt; Mare-Preot în veac, &lt;e&gt;după rânduiala lui Melhisedec&lt;/e&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,7,1,'&lt;pb/&gt;Acest &lt;e&gt;Melhisedec&lt;/e&gt;&lt;f&gt;ⓘ&lt;/f&gt;, &lt;e&gt;regele Salemului, preot al Dumnezeului Preaînalt, care l-a întâmpinat pe Avraam când &lt;i&gt;acesta&lt;/i&gt; se întorcea de la măcelul regilor și l-a binecuvântat,&lt;/e&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,7,2,'&lt;e&gt;căruia Avraam i-a dat zeciuială din tot&lt;/e&gt;&lt;f&gt;[35]&lt;/f&gt; — al cărui nume înseamnă mai întâi „rege al dreptății”, apoi și „rege al Salemului”, adică „rege al păcii” —');
INSERT INTO &quot;verses&quot; VALUES (650,7,3,'fără tată, fără mamă, fără spiță de neam, neavând nici început al zilelor, nici sfârșit al vieții, dar asemănător&lt;f&gt;[36]&lt;/f&gt; Fiului lui Dumnezeu, rămâne preot de-a pururi.');
INSERT INTO &quot;verses&quot; VALUES (650,7,4,'&lt;pb/&gt;Vedeți cât de mare a fost acesta, căruia &lt;e&gt;patriarhul Avraam i-a dat zeciuială&lt;/e&gt;&lt;f&gt;[37]&lt;/f&gt; din prada de război!');
INSERT INTO &quot;verses&quot; VALUES (650,7,5,'Și aceia&lt;f&gt;ⓙ&lt;/f&gt; dintre fiii lui Levi care intră în preoție au poruncă să ia zeciuială de la popor, potrivit Legii, adică de la frații lor, cu toate că și aceștia sunt ieșiți din coapsele lui Avraam.');
INSERT INTO &quot;verses&quot; VALUES (650,7,6,'Dar &lt;i&gt;Melhisedec&lt;/i&gt;, care nu se trage din ei, a primit zeciuială de la Avraam și&lt;f&gt;ⓚ&lt;/f&gt; l-a binecuvântat pe&lt;f&gt;ⓛ&lt;/f&gt; cel ce avea făgăduințele.');
INSERT INTO &quot;verses&quot; VALUES (650,7,7,'Fără îndoială, cel mai mic este binecuvântat de cel mai mare.');
INSERT INTO &quot;verses&quot; VALUES (650,7,8,'Dar într-un caz primesc zeciuială niște oameni muritori; în celălalt, cineva despre&lt;f&gt;ⓜ&lt;/f&gt; care se mărturisește că trăiește.');
INSERT INTO &quot;verses&quot; VALUES (650,7,9,'Și, ca să zicem așa, prin Avraam a dat zeciuială și Levi, care primește zeciuiala,');
INSERT INTO &quot;verses&quot; VALUES (650,7,10,'căci era încă în coapsele strămoșului &lt;i&gt;său&lt;/i&gt; când l-a întâmpinat Melhisedec &lt;i&gt;pe Avraam&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,7,11,'&lt;pb/&gt;Așadar, dacă&lt;f&gt;ⓝ&lt;/f&gt; desăvârșirea ar fi fost &lt;i&gt;cu putință&lt;/i&gt; prin preoția leviților, în timpul căreia a primit poporul Legea, ce nevoie era să se ridice un alt preot &lt;e&gt;după rânduiala lui Melhisedec&lt;/e&gt;&lt;f&gt;[38]&lt;/f&gt;, și nu &lt;i&gt;unul&lt;/i&gt; chemat după rânduiala lui Aaron?');
INSERT INTO &quot;verses&quot; VALUES (650,7,12,'Pentru că, atunci când se schimbă preoția, se cere numaidecât și o schimbare a Legii.');
INSERT INTO &quot;verses&quot; VALUES (650,7,13,'Dar Cel despre care se spun aceste lucruri face parte dintr-o altă seminție, din care nimeni n-a slujit altarului,');
INSERT INTO &quot;verses&quot; VALUES (650,7,14,'căci este limpede că Domnul nostru provine din&lt;f&gt;ⓞ&lt;/f&gt; Iuda, seminție despre care Moise n-a zis nimic cu privire la preoție.');
INSERT INTO &quot;verses&quot; VALUES (650,7,15,'Lucrul acesta se face și mai limpede dacă, după asemănarea lui Melhisedec, apare un alt preot,');
INSERT INTO &quot;verses&quot; VALUES (650,7,16,'pus &lt;i&gt;în slujbă&lt;/i&gt; nu printr-o lege privind descendența trupească, ci prin puterea unei vieți care nu poate fi nimicită.');
INSERT INTO &quot;verses&quot; VALUES (650,7,17,'Fiindcă se mărturisește &lt;i&gt;despre El&lt;/i&gt;: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Tu&lt;/e&gt; &lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;ești preot în veac după rânduiala lui Melhisedec.&lt;/e&gt;&lt;f&gt;[39]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,7,18,'&lt;pb/&gt;Astfel, pe de o parte, se desființează o poruncă anterioară, din cauza neputinței&lt;f&gt;ⓠ&lt;/f&gt; și inutilității ei');
INSERT INTO &quot;verses&quot; VALUES (650,7,19,'(căci Legea&lt;f&gt;ⓡ&lt;/f&gt; n-a desăvârșit nimic), pe de alta, este introdusă o nădejde&lt;f&gt;ⓢ&lt;/f&gt; mai bună, prin care ne apropiem de Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (650,7,20,'&lt;pb/&gt;Și în măsura în care lucrul acesta nu &lt;i&gt;s-a făcut&lt;/i&gt; fără jurământ —');
INSERT INTO &quot;verses&quot; VALUES (650,7,21,'căci, în vreme ce &lt;i&gt;leviții&lt;/i&gt; se făceau preoți fără jurământ, &lt;i&gt;Isus a ajuns preot&lt;/i&gt; prin jurământ, datorită Celui care I-a zis: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Domnul a jurat și nu Se va căi:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;„Tu ești preot în veac după rânduiala lui Melhisedec”&lt;/e&gt; &lt;f&gt;[40]&lt;/f&gt; —&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,7,22,'&lt;pb/&gt;în aceeași măsură Isus S-a făcut&lt;f&gt;ⓣ&lt;/f&gt; garantul unui legământ mai bun.');
INSERT INTO &quot;verses&quot; VALUES (650,7,23,'&lt;i&gt;Ceilalți&lt;/i&gt; erau preoți în mare număr, fiindcă moartea îi împiedica să rămână &lt;i&gt;în slujbă&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,7,24,'Dar El, fiindcă rămâne &lt;e&gt;în veac&lt;/e&gt;, are o preoție netrecătoare&lt;f&gt;[41]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,7,25,'și de aceea poate să-i mântuiască în chip desăvârșit pe cei care se apropie de Dumnezeu prin El — pentru că trăiește pururi ca să mijlocească&lt;f&gt;ⓤ&lt;/f&gt; pentru ei.');
INSERT INTO &quot;verses&quot; VALUES (650,7,26,'&lt;pb/&gt;Și tocmai un astfel de Mare-Preot ne trebuia: sfânt&lt;f&gt;ⓥ&lt;/f&gt;, neatins de rău, neîntinat, separat de păcătoși și&lt;f&gt;ⓦ&lt;/f&gt; înălțat mai presus de ceruri,');
INSERT INTO &quot;verses&quot; VALUES (650,7,27,'care n-are nevoie, zi de zi, asemenea &lt;i&gt;celorlalți&lt;/i&gt; mari-preoți, să aducă jertfe mai întâi&lt;f&gt;ⓧ&lt;/f&gt; pentru păcatele proprii și&lt;f&gt;ⓨ&lt;/f&gt; apoi pentru cele ale poporului, căci lucrul acesta&lt;f&gt;ⓩ&lt;/f&gt; l-a făcut o dată &lt;i&gt;pentru totdeauna&lt;/i&gt; când S-a adus pe Sine Însuși &lt;i&gt;ca jertfă&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,7,28,'Într-adevăr, Legea pune mari-preoți pe niște oameni&lt;f&gt;ⓐ&lt;/f&gt; cu slăbiciuni, dar cuvântul jurământului făcut după &lt;i&gt;darea&lt;/i&gt; Legii Îl pune &lt;i&gt;preot&lt;/i&gt; pe Fiul, care&lt;f&gt;ⓑ&lt;/f&gt; a fost făcut desăvârșit pe vecie.');
INSERT INTO &quot;verses&quot; VALUES (650,8,1,'&lt;pb/&gt;Punctul principal al celor spuse &lt;i&gt;mai sus&lt;/i&gt; este că avem un astfel de Mare-Preot care&lt;f&gt;ⓒ&lt;/f&gt; S-a așezat la dreapta tronului Măririi, în ceruri,');
INSERT INTO &quot;verses&quot; VALUES (650,8,2,'ca slujitor al sanctuarului&lt;f&gt;ⓓ&lt;/f&gt;, al adevăratului cort&lt;f&gt;ⓔ&lt;/f&gt;, pe care l-a ridicat Domnul, nu un om.');
INSERT INTO &quot;verses&quot; VALUES (650,8,3,'Orice mare-preot&lt;f&gt;ⓕ&lt;/f&gt; este rânduit să aducă daruri și jertfe. De aceea,&lt;f&gt;ⓖ&lt;/f&gt; era nevoie ca și El să aducă ceva.');
INSERT INTO &quot;verses&quot; VALUES (650,8,4,'Dacă ar fi pe pământ, nici n-ar fi preot, fiindcă sunt cei ce aduc darurile după Lege.');
INSERT INTO &quot;verses&quot; VALUES (650,8,5,'Ei slujesc o reprezentare și o umbră&lt;f&gt;ⓗ&lt;/f&gt; a &lt;i&gt;sanctuarului&lt;/i&gt; ceresc, după cum a fost înștiințat Moise de Dumnezeu, când urma să ridice cortul: &lt;e&gt;Ia seama&lt;/e&gt;&lt;f&gt;ⓘ&lt;/f&gt;, i s-a zis, &lt;e&gt;să faci totul după modelul care ți-a fost arătat pe munte!&lt;/e&gt;&lt;f&gt;[42]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,8,6,'Dimpotrivă, Hristos a dobândit&lt;f&gt;ⓙ&lt;/f&gt; o slujbă cu atât mai înaltă, cu cât legământul al cărui Mijlocitor este El e mai bun, fiindcă a fost statornicit pe baza unor făgăduințe mai bune.');
INSERT INTO &quot;verses&quot; VALUES (650,8,7,'&lt;pb/&gt;De&lt;f&gt;ⓚ&lt;/f&gt; altfel, dacă primul legământ ar fi fost fără cusur, n-ar mai fi fost nevoie de un al doilea.');
INSERT INTO &quot;verses&quot; VALUES (650,8,8,'Dar când îi mustră, &lt;i&gt;Dumnezeu&lt;/i&gt; zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată, vin zile, zice Domnul,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;când voi încheia cu casa lui Israel&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și cu casa lui Iuda un legământ nou,&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,8,9,'&lt;t&gt;&lt;e&gt;nu ca legământul pe care l-am făcut cu părinții lor&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;în ziua când i-am luat de mână&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ca să-i scot din țara Egiptului.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Pentru că n-au rămas în legământul Meu,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nici Eu nu M-am mai îngrijit de ei, zice Domnul.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,8,10,'&lt;t&gt;&lt;e&gt;Ci iată&lt;/e&gt; &lt;f&gt;ⓛ&lt;/f&gt; &lt;e&gt;legământul pe care-l voi încheia cu casa lui Israel&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;după acele zile, zice Domnul:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;voi pune legile Mele în cugetul lor&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și le voi scrie în inima lor;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Eu voi&lt;/e&gt; &lt;f&gt;ⓜ&lt;/f&gt; &lt;e&gt;fi Dumnezeul lor,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și ei vor fi poporul Meu.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,8,11,'&lt;t&gt;&lt;e&gt;Și nu&lt;/e&gt; &lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;va mai învăța nimeni pe semenul său&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ori pe fratele său zicând: „Cunoaște pe Domnul!”,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;căci toți Mă vor cunoaște,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;de la cel mai mic până la cel mai mare dintre ei.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,8,12,'&lt;t&gt;&lt;e&gt;Pentru că voi fi îndurător cu nelegiuirile lor&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și de păcatele&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;[și fărădelegile] lor nu-Mi voi mai aduce aminte.&lt;/e&gt;&lt;f&gt;[43]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,8,13,'&lt;pb/&gt;Când zice&lt;f&gt;ⓟ&lt;/f&gt; &lt;i&gt;&lt;e&gt;legământ&lt;/i&gt; nou&lt;/e&gt;, arată că cel dintâi este învechit. Iar ce este învechit și îmbătrânit este aproape de pieire.');
INSERT INTO &quot;verses&quot; VALUES (650,9,1,'&lt;pb/&gt;Primul &lt;i&gt;legământ&lt;/i&gt; avea și el rânduieli pentru slujbă și un sanctuar&lt;f&gt;ⓠ&lt;/f&gt; pământesc.');
INSERT INTO &quot;verses&quot; VALUES (650,9,2,'A fost ridicat&lt;f&gt;ⓡ&lt;/f&gt; un cort&lt;f&gt;[44]&lt;/f&gt;: în&lt;f&gt;ⓢ&lt;/f&gt; prima &lt;i&gt;încăpere&lt;/i&gt; erau sfeșnicul&lt;f&gt;ⓣ&lt;/f&gt;, masa&lt;f&gt;ⓤ&lt;/f&gt; și pâinile pentru înfățișare&lt;f&gt;[45]&lt;/f&gt;. Acesta se chema Locul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (650,9,3,'După&lt;f&gt;ⓥ&lt;/f&gt; a doua catapeteasmă&lt;f&gt;[46]&lt;/f&gt; &lt;i&gt;se afla&lt;/i&gt; o încăpere&lt;f&gt;[47]&lt;/f&gt; numită Locul Preasfânt&lt;f&gt;[48]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (650,9,4,'care avea altarul tămâierii&lt;f&gt;[49]&lt;/f&gt;, de aur, și&lt;f&gt;ⓦ&lt;/f&gt; chivotul legământului, ferecat cu aur. În &lt;i&gt;chivot&lt;/i&gt; erau un vas&lt;f&gt;ⓧ&lt;/f&gt; de aur cu mană, toiagul lui Aaron&lt;f&gt;ⓨ&lt;/f&gt; care înmugurise și tablele&lt;f&gt;ⓩ&lt;/f&gt; legământului.');
INSERT INTO &quot;verses&quot; VALUES (650,9,5,'Deasupra&lt;f&gt;ⓐ&lt;/f&gt; &lt;i&gt;chivotului&lt;/i&gt; se aflau heruvimii slavei, care umbreau capacul ispășirii&lt;f&gt;[50]&lt;/f&gt;, lucruri despre care nu avem acum &lt;i&gt;vreme&lt;/i&gt; să vorbim în amănunt.');
INSERT INTO &quot;verses&quot; VALUES (650,9,6,'&lt;pb/&gt;Odată pregătite lucrurile acestea astfel, preoții&lt;f&gt;ⓑ&lt;/f&gt; intrau mereu în prima încăpere ca să împlinească slujbele.');
INSERT INTO &quot;verses&quot; VALUES (650,9,7,'Dar în a doua &lt;i&gt;încăpere&lt;/i&gt; intra numai marele-preot, o dată&lt;f&gt;ⓒ&lt;/f&gt; pe an, și nu fără sânge, pe&lt;f&gt;ⓓ&lt;/f&gt; care îl aducea pentru sine și pentru păcatele din neștiință ale poporului.');
INSERT INTO &quot;verses&quot; VALUES (650,9,8,'Prin aceasta, Duhul Sfânt&lt;f&gt;ⓔ&lt;/f&gt; arăta că&lt;f&gt;ⓕ&lt;/f&gt; încă nu se arătase calea spre sanctuar, câtă vreme încă stătea în picioare primul cort&lt;f&gt;[51]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,9,9,'Acestea erau un simbol pentru vremea de acum, când se aduc daruri și jertfe care&lt;f&gt;ⓖ&lt;/f&gt; nu pot duce la desăvârșire conștiința închinătorului.');
INSERT INTO &quot;verses&quot; VALUES (650,9,10,'&lt;i&gt;Aceste&lt;/i&gt; rânduieli exterioare &lt;i&gt;au de-a face&lt;/i&gt; doar cu mâncăruri&lt;f&gt;ⓗ&lt;/f&gt;, băuturi și felurite&lt;f&gt;ⓘ&lt;/f&gt; îmbăieri, fiind rânduite până&lt;f&gt;ⓙ&lt;/f&gt; la vremea îndreptării.');
INSERT INTO &quot;verses&quot; VALUES (650,9,11,'&lt;pb/&gt;Dar Hristos, devenit Mare-Preot al bunurilor&lt;f&gt;ⓚ&lt;/f&gt; viitoare, prin&lt;f&gt;ⓛ&lt;/f&gt; cortul&lt;f&gt;[52]&lt;/f&gt; mai mare și mai desăvârșit, care nu este făcut de mână &lt;i&gt;omenească&lt;/i&gt; (adică nu face parte din această creație),');
INSERT INTO &quot;verses&quot; VALUES (650,9,12,'nu cu&lt;f&gt;ⓜ&lt;/f&gt; sânge de țapi și de viței, ci&lt;f&gt;ⓝ&lt;/f&gt; cu &lt;i&gt;însuși&lt;/i&gt; sângele Său a intrat în sanctuar, o dată pentru totdeauna, după ce a dobândit o răscumpărare veșnică.');
INSERT INTO &quot;verses&quot; VALUES (650,9,13,'Căci, dacă sângele&lt;f&gt;ⓞ&lt;/f&gt; taurilor și al țapilor și&lt;f&gt;ⓟ&lt;/f&gt; cenușa unei junci, stropită peste cei întinați, aduc sfințire, spre curățirea trupului,');
INSERT INTO &quot;verses&quot; VALUES (650,9,14,'cu cât mai mult sângele&lt;f&gt;ⓠ&lt;/f&gt; lui Hristos, care&lt;f&gt;ⓡ&lt;/f&gt;, prin Duhul cel veșnic, S-a adus pe Sine&lt;f&gt;ⓢ&lt;/f&gt; &lt;i&gt;jertfă&lt;/i&gt; fără pată lui Dumnezeu, va&lt;f&gt;ⓣ&lt;/f&gt; curăți conștiința noastră de faptele&lt;f&gt;ⓤ&lt;/f&gt; moarte, ca să-I slujim&lt;f&gt;ⓥ&lt;/f&gt; Dumnezeului celui viu!');
INSERT INTO &quot;verses&quot; VALUES (650,9,15,'&lt;pb/&gt;&lt;i&gt;Tocmai&lt;/i&gt; &lt;f&gt;ⓦ&lt;/f&gt; de aceea &lt;i&gt;Isus&lt;/i&gt; este Mijlocitorul&lt;f&gt;ⓧ&lt;/f&gt; unui legământ nou, pentru ca&lt;f&gt;ⓨ&lt;/f&gt;, prin moartea suferită pentru răscumpărarea din abaterile &lt;i&gt;făptuite&lt;/i&gt; sub primul legământ, cei chemați să capete făgăduința unei moșteniri veșnice.');
INSERT INTO &quot;verses&quot; VALUES (650,9,16,'Căci oriunde este un testament&lt;f&gt;[53]&lt;/f&gt;, trebuie să aibă loc moartea &lt;i&gt;celui ce l-a făcut&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,9,17,'Pentru că un testament&lt;f&gt;ⓩ&lt;/f&gt; intră în vigoare după moartea &lt;i&gt;lui&lt;/i&gt;, căci n-are nicio putere câtă vreme trăiește cel ce l-a făcut.');
INSERT INTO &quot;verses&quot; VALUES (650,9,18,'De aceea nici primul &lt;i&gt;legământ&lt;/i&gt; n-a fost încheiat fără sânge.');
INSERT INTO &quot;verses&quot; VALUES (650,9,19,'După ce Moise a rostit înaintea întregului popor toate poruncile, potrivit Legii, a luat&lt;f&gt;ⓐ&lt;/f&gt; sânge de viței și de țapi cu apă, lână stacojie și isop, a stropit cartea și tot norodul,');
INSERT INTO &quot;verses&quot; VALUES (650,9,20,'zicând: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Acesta&lt;/e&gt; &lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;este sângele legământului pe care l-a poruncit Dumnezeu pentru voi.&lt;/e&gt;&lt;f&gt;[54]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,9,21,'&lt;pb/&gt;De asemenea, a stropit cu sânge cortul&lt;f&gt;ⓒ&lt;/f&gt; și toate vasele de slujbă.');
INSERT INTO &quot;verses&quot; VALUES (650,9,22,'După Lege, aproape toate lucrurile sunt curățite cu sânge, iar&lt;f&gt;ⓓ&lt;/f&gt; fără vărsare de sânge nu este iertare.');
INSERT INTO &quot;verses&quot; VALUES (650,9,23,'&lt;pb/&gt;Era nevoie deci ca reprezentările&lt;f&gt;ⓔ&lt;/f&gt; lucrurilor din ceruri să fie curățite în felul acesta, iar lucrurile cerești însele &lt;i&gt;să fie curățite&lt;/i&gt; cu jertfe mai bune decât acestea.');
INSERT INTO &quot;verses&quot; VALUES (650,9,24,'Căci&lt;f&gt;ⓕ&lt;/f&gt; Hristos n-a intrat într-un sanctuar făcut de mâna &lt;i&gt;omului&lt;/i&gt;, reprezentare&lt;f&gt;[55]&lt;/f&gt; a&lt;f&gt;ⓖ&lt;/f&gt; celui adevărat, ci &lt;i&gt;a intrat&lt;/i&gt; chiar în cer, ca să Se&lt;f&gt;ⓗ&lt;/f&gt; înfățișeze acum, pentru noi, înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (650,9,25,'Dar nu ca să Se aducă pe Sine &lt;i&gt;jertfă&lt;/i&gt; de mai multe ori, ca marele-preot, care intră în sanctuar an de an cu sânge care nu este al lui,');
INSERT INTO &quot;verses&quot; VALUES (650,9,26,'fiindcă atunci ar fi trebuit să pătimească de mai multe ori de la întemeierea lumii. Dimpotrivă, El S-a arătat o singură dată&lt;f&gt;ⓘ&lt;/f&gt;, la sfârșitul&lt;f&gt;ⓙ&lt;/f&gt; veacurilor, ca să înlăture păcatul prin jertfa Sa.');
INSERT INTO &quot;verses&quot; VALUES (650,9,27,'Și&lt;f&gt;ⓚ&lt;/f&gt;, după cum oamenilor le este rânduit să moară o singură dată, iar&lt;f&gt;ⓛ&lt;/f&gt; după aceea vine judecata,');
INSERT INTO &quot;verses&quot; VALUES (650,9,28,'tot astfel și Hristos&lt;f&gt;ⓜ&lt;/f&gt;, după ce S-a adus&lt;f&gt;ⓝ&lt;/f&gt; pe Sine &lt;i&gt;jertfă&lt;/i&gt; o singură dată, ca să poarte păcatele multora&lt;f&gt;ⓞ&lt;/f&gt;, Se va arăta a doua oară, dar nu în vederea păcatului, ci pentru cei ce-L&lt;f&gt;ⓟ&lt;/f&gt; așteaptă, în vederea mântuirii.');
INSERT INTO &quot;verses&quot; VALUES (650,10,1,'&lt;pb/&gt;Legea, care are umbra&lt;f&gt;ⓠ&lt;/f&gt; bunurilor&lt;f&gt;ⓡ&lt;/f&gt; viitoare, nu înfățișarea propriu-zisă a lucrurilor, nu&lt;f&gt;ⓢ&lt;/f&gt; poate niciodată, prin aceleași jertfe care se aduc neîncetat, an de an, să-i facă desăvârșiți&lt;f&gt;ⓣ&lt;/f&gt; pe cei ce se apropie &lt;i&gt;de Dumnezeu&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,10,2,'Altfel, n-ar fi încetat oare aducerea lor dacă închinătorii, odată curățiți, n-ar mai fi avut conștiința păcatelor?');
INSERT INTO &quot;verses&quot; VALUES (650,10,3,'Dar&lt;f&gt;ⓤ&lt;/f&gt; &lt;i&gt;tocmai&lt;/i&gt; prin ele se face amintirea păcatelor an de an;');
INSERT INTO &quot;verses&quot; VALUES (650,10,4,'căci este cu neputință&lt;f&gt;ⓥ&lt;/f&gt; ca sângele taurilor și al țapilor să îndepărteze păcatele.');
INSERT INTO &quot;verses&quot; VALUES (650,10,5,'&lt;pb/&gt;De aceea, când vine în lume, &lt;i&gt;Hristos&lt;/i&gt; zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Jertfă și ofrandă n-ai voit,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ci Mi-ai pregătit un trup;&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,6,'&lt;t&gt;&lt;e&gt;arderi-de-tot&lt;/e&gt; &lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;și &lt;i&gt;jertfe&lt;/i&gt; pentru păcat&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu Ți-au fost pe plac.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,7,'&lt;t&gt;&lt;e&gt;Atunci am zis: „Iată-Mă,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;în sulul cărții este scris despre Mine,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;vin să fac voia Ta, Dumnezeule!”&lt;/e&gt; &lt;f&gt;[56]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,8,'&lt;pb/&gt;După ce a zis mai întâi: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Jertfe și ofrande,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;arderi-de-tot și jertfe pentru păcat&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;n-ai voit, nici nu Ți-au fost pe plac&lt;/e&gt; &lt;f&gt;[57]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;(jertfe aduse după Lege),');
INSERT INTO &quot;verses&quot; VALUES (650,10,9,'a zis apoi: &lt;e&gt;Iată-Mă, vin să fac voia Ta!&lt;/e&gt; &lt;i&gt;Astfel&lt;/i&gt; desființează &lt;i&gt;rânduiala&lt;/i&gt; dintâi, ca să o pună în loc pe a doua.');
INSERT INTO &quot;verses&quot; VALUES (650,10,10,'Prin&lt;f&gt;ⓧ&lt;/f&gt; această „voie” suntem sfințiți noi, prin&lt;f&gt;ⓨ&lt;/f&gt; aducerea ca jertfă a trupului lui Isus Hristos, o dată pentru totdeauna.');
INSERT INTO &quot;verses&quot; VALUES (650,10,11,'&lt;pb/&gt;Și, pe când orice preot slujește în fiecare&lt;f&gt;ⓩ&lt;/f&gt; zi și aduce în mod repetat aceleași jertfe, care&lt;f&gt;ⓐ&lt;/f&gt; niciodată nu pot șterge păcatele,');
INSERT INTO &quot;verses&quot; VALUES (650,10,12,'El&lt;f&gt;ⓑ&lt;/f&gt;, &lt;i&gt;dimpotrivă&lt;/i&gt;, după ce a adus pentru totdeauna o singură jertfă pentru păcate, S-a așezat la dreapta lui Dumnezeu');
INSERT INTO &quot;verses&quot; VALUES (650,10,13,'și așteaptă de acum &lt;i&gt;înainte&lt;/i&gt; ca vrăjmașii&lt;f&gt;ⓒ&lt;/f&gt; Lui să-I fie făcuți reazem picioarelor Lui.');
INSERT INTO &quot;verses&quot; VALUES (650,10,14,'Căci printr-o &lt;i&gt;singură&lt;/i&gt; ofrandă El&lt;f&gt;ⓓ&lt;/f&gt; i-a desăvârșit pentru totdeauna pe cei ce sunt sfințiți.');
INSERT INTO &quot;verses&quot; VALUES (650,10,15,'Despre acest lucru ne dă mărturie și Duhul Sfânt, căci, după ce zice:');
INSERT INTO &quot;verses&quot; VALUES (650,10,16,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată legământul pe care-l voi încheia&lt;/e&gt; cu ei&lt;/t&gt; &lt;t&gt;&lt;e&gt;după acele zile, zice Domnul:&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Voi pune legile Mele în inimile lor&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și le voi scrie în cugetul lor,&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,17,'&lt;t&gt;&lt;i&gt;adaugă:&lt;/i&gt; &lt;e&gt;Iar de păcatele&lt;/e&gt; și de fărădelegile lor&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu-Mi voi mai aduce aminte.&lt;/e&gt; &lt;f&gt;[58]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,18,'&lt;pb/&gt;Dar, acolo unde este iertarea acestora, nu mai este nevoie de o ofrandă pentru păcat.');
INSERT INTO &quot;verses&quot; VALUES (650,10,19,'&lt;pb/&gt;Așadar, fraților, fiindcă avem îndrăzneală să intrăm&lt;f&gt;ⓔ&lt;/f&gt; în sanctuar prin sângele&lt;f&gt;ⓕ&lt;/f&gt; lui Isus,');
INSERT INTO &quot;verses&quot; VALUES (650,10,20,'pe calea&lt;f&gt;ⓖ&lt;/f&gt; cea nouă și vie pe care ne-a deschis-o prin&lt;f&gt;ⓗ&lt;/f&gt; catapeteasmă, adică prin trupul Său,');
INSERT INTO &quot;verses&quot; VALUES (650,10,21,'și &lt;i&gt;fiindcă avem&lt;/i&gt; un Preot&lt;f&gt;ⓘ&lt;/f&gt; însemnat &lt;i&gt;pus&lt;/i&gt; peste casa&lt;f&gt;ⓙ&lt;/f&gt; lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (650,10,22,'să&lt;f&gt;ⓚ&lt;/f&gt; ne apropiem cu o inimă sinceră, cu credință deplină&lt;f&gt;ⓛ&lt;/f&gt;, cu inimile curățite de&lt;f&gt;ⓜ&lt;/f&gt; o conștiință vinovată și cu trupurile spălate&lt;f&gt;ⓝ&lt;/f&gt; cu apă curată!');
INSERT INTO &quot;verses&quot; VALUES (650,10,23,'Să&lt;f&gt;ⓞ&lt;/f&gt; ținem neclintită mărturisirea nădejdii &lt;i&gt;noastre&lt;/i&gt;, căci credincios&lt;f&gt;ⓟ&lt;/f&gt; este Cel ce a făcut făgăduința!');
INSERT INTO &quot;verses&quot; VALUES (650,10,24,'Să veghem unii asupra altora, ca să ne îndemnăm la dragoste și la fapte bune,');
INSERT INTO &quot;verses&quot; VALUES (650,10,25,'fără a neglija&lt;f&gt;ⓠ&lt;/f&gt; adunarea noastră, cum au unii obiceiul! Dimpotrivă, să ne îndemnăm &lt;i&gt;unii pe alții&lt;/i&gt;, și aceasta cu atât mai mult, cu cât vedeți că se apropie&lt;f&gt;ⓡ&lt;/f&gt; Ziua!');
INSERT INTO &quot;verses&quot; VALUES (650,10,26,'&lt;pb/&gt;Căci, dacă&lt;f&gt;ⓢ&lt;/f&gt; păcătuim în mod voit după&lt;f&gt;ⓣ&lt;/f&gt; ce am primit cunoștința adevărului, nu mai rămâne nicio jertfă pentru păcate,');
INSERT INTO &quot;verses&quot; VALUES (650,10,27,'ci o așteptare înfricoșată a judecății și văpaia&lt;f&gt;ⓤ&lt;/f&gt; unui foc care-i va mistui pe împotrivitori.');
INSERT INTO &quot;verses&quot; VALUES (650,10,28,'Cine&lt;f&gt;ⓥ&lt;/f&gt; a încălcat Legea lui Moise trebuia &lt;e&gt;să fie omorât&lt;/e&gt; fără milă &lt;e&gt;pe baza mărturiei&lt;/e&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;a doi sau trei martori&lt;/e&gt;&lt;f&gt;[59]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,10,29,'Cu cât&lt;f&gt;ⓧ&lt;/f&gt; mai aspră credeți că va fi pedeapsa de care va avea parte acela care Îl calcă în picioare pe Fiul lui Dumnezeu, nesocotește sângele legământului cu care a fost sfințit și Îl batjocorește&lt;f&gt;ⓨ&lt;/f&gt; pe Duhul harului?');
INSERT INTO &quot;verses&quot; VALUES (650,10,30,'Căci știm cine este Cel ce a zis: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;A&lt;/e&gt; &lt;f&gt;ⓩ&lt;/f&gt; &lt;e&gt;Mea este răzbunarea, Eu voi răsplăti!&lt;/e&gt;&lt;f&gt;[60]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;Și iarăși: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Domnul va judeca pe poporul Său.&lt;/e&gt; &lt;f&gt;[61]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,31,'&lt;pb/&gt;Cumplit&lt;f&gt;ⓐ&lt;/f&gt; lucru este să cazi în mâinile Dumnezeului celui viu!');
INSERT INTO &quot;verses&quot; VALUES (650,10,32,'&lt;pb/&gt;Aduceți-vă&lt;f&gt;ⓑ&lt;/f&gt; aminte de zilele de la început, când, după ce ați fost luminați&lt;f&gt;ⓒ&lt;/f&gt;, ați dus o mare&lt;f&gt;ⓓ&lt;/f&gt; luptă, &lt;i&gt;însoțită&lt;/i&gt; de suferințe:');
INSERT INTO &quot;verses&quot; VALUES (650,10,33,'uneori ați fost ținta publică&lt;f&gt;ⓔ&lt;/f&gt; a ocărilor și necazurilor, iar alteori le-ați fost alături&lt;f&gt;ⓕ&lt;/f&gt; celor care au avut parte de ele.');
INSERT INTO &quot;verses&quot; VALUES (650,10,34,'Ați suferit alături de cei întemnițați&lt;f&gt;ⓖ&lt;/f&gt; și ați acceptat cu bucurie să vă fie luate&lt;f&gt;ⓗ&lt;/f&gt; averile, știind că aveți&lt;f&gt;ⓘ&lt;/f&gt; în ceruri o avuție mai bună, care dăinuie.');
INSERT INTO &quot;verses&quot; VALUES (650,10,35,'Nu renunțați la îndrăzneala voastră, care va avea o mare răsplătire&lt;f&gt;ⓙ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (650,10,36,'Căci&lt;f&gt;ⓚ&lt;/f&gt; aveți nevoie de statornicie pentru ca, după ce ați împlinit voia lui Dumnezeu, să dobândiți&lt;f&gt;ⓛ&lt;/f&gt; ce v-a fost făgăduit.');
INSERT INTO &quot;verses&quot; VALUES (650,10,37,'&lt;pb/&gt;&lt;t&gt;Încă&lt;f&gt;ⓜ&lt;/f&gt; &lt;e&gt;puțin, foarte puțin;&lt;/e&gt;&lt;f&gt;[62]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Cel ce vine va veni și nu va zăbovi.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,38,'&lt;t&gt;&lt;e&gt;Dar cel drept&lt;/e&gt; &lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;prin credință va trăi.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Însă, dacă dă înapoi, sufletul Meu nu-Și găsește plăcerea în el.&lt;/e&gt; &lt;f&gt;[63]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,10,39,'&lt;pb/&gt;Noi însă nu suntem dintre aceia care&lt;f&gt;ⓞ&lt;/f&gt; dau înapoi, ca să piară, ci dintre aceia care au credință&lt;f&gt;ⓟ&lt;/f&gt; pentru mântuirea sufletului.');
INSERT INTO &quot;verses&quot; VALUES (650,11,1,'&lt;pb/&gt;Și credința este realitatea&lt;f&gt;[64]&lt;/f&gt; lucrurilor&lt;f&gt;ⓠ&lt;/f&gt; nădăjduite, dovada lucrurilor care nu se văd.');
INSERT INTO &quot;verses&quot; VALUES (650,11,2,'Datorită ei&lt;f&gt;ⓡ&lt;/f&gt; au primit cei din vechime o &lt;i&gt;bună&lt;/i&gt; mărturie.');
INSERT INTO &quot;verses&quot; VALUES (650,11,3,'&lt;pb/&gt;Prin credință înțelegem că lumea a&lt;f&gt;ⓢ&lt;/f&gt; fost creată prin Cuvântul lui Dumnezeu, așa că ce se vede n-a fost făcut din lucruri care se văd.');
INSERT INTO &quot;verses&quot; VALUES (650,11,4,'&lt;pb/&gt;Prin credință I-a adus Abel&lt;f&gt;ⓣ&lt;/f&gt; lui Dumnezeu o jertfă mai bună decât &lt;i&gt;cea a lui&lt;/i&gt; Cain. Prin ea a primit el mărturie că este drept, căci Dumnezeu a primit&lt;f&gt;[65]&lt;/f&gt; darurile lui; prin ea el încă&lt;f&gt;ⓤ&lt;/f&gt; vorbește, chiar dacă este mort.');
INSERT INTO &quot;verses&quot; VALUES (650,11,5,'&lt;pb/&gt;Prin credință a fost strămutat Enoh&lt;f&gt;ⓥ&lt;/f&gt;, ca să nu vadă moartea, și n-a mai fost de găsit, pentru că l-a strămutat Dumnezeu, dar înainte de strămutarea lui a primit mărturia că Îi este plăcut lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (650,11,6,'Fără credință este cu neputință să-I fim plăcuți. Căci cine se apropie de Dumnezeu trebuie să creadă că El există și că îi răsplătește pe cei ce-L caută.');
INSERT INTO &quot;verses&quot; VALUES (650,11,7,'&lt;pb/&gt;Prin credință Noe&lt;f&gt;ⓦ&lt;/f&gt;, înștiințat fiind despre lucruri care încă nu se vedeau și cuprins de o teamă sfântă, a&lt;f&gt;ⓧ&lt;/f&gt; pregătit o arcă pentru mântuirea casei sale; prin &lt;i&gt;credință&lt;/i&gt; a osândit lumea și a ajuns moștenitor al dreptății&lt;f&gt;ⓨ&lt;/f&gt; care &lt;i&gt;se capătă&lt;/i&gt; prin credință.');
INSERT INTO &quot;verses&quot; VALUES (650,11,8,'&lt;pb/&gt;Prin credință Avraam&lt;f&gt;ⓩ&lt;/f&gt;, când a fost chemat să plece într-un loc pe care urma să-l ia drept moștenire, a ascultat și a plecat fără să știe încotro merge.');
INSERT INTO &quot;verses&quot; VALUES (650,11,9,'Prin credință a peregrinat el prin țara făgăduinței ca printr-un ținut străin și a locuit în&lt;f&gt;ⓐ&lt;/f&gt; corturi ca Isaac și Iacov, care erau moștenitori&lt;f&gt;ⓑ&lt;/f&gt; împreună cu el ai aceleiași făgăduințe.');
INSERT INTO &quot;verses&quot; VALUES (650,11,10,'Căci el aștepta cetatea&lt;f&gt;ⓒ&lt;/f&gt; care are temelii &lt;i&gt;tari&lt;/i&gt;, al&lt;f&gt;ⓓ&lt;/f&gt; cărei meșter și ziditor este Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (650,11,11,'Prin credință &lt;i&gt;Avraam&lt;/i&gt;, deși Sara&lt;f&gt;ⓔ&lt;/f&gt; era stearpă, a&lt;f&gt;ⓕ&lt;/f&gt; primit putere să zămislească în ciuda vârstei trecute, fiindcă L-a socotit vrednic de încredere pe&lt;f&gt;ⓖ&lt;/f&gt; Cel ce făcuse făgăduința.');
INSERT INTO &quot;verses&quot; VALUES (650,11,12,'De aceea, dintr-un singur &lt;i&gt;om&lt;/i&gt;&lt;f&gt;ⓗ&lt;/f&gt;, și acela aproape mort, s-au născut &lt;i&gt;urmași&lt;/i&gt; în mare număr, ca&lt;f&gt;ⓘ&lt;/f&gt; stelele cerului, ca nisipul de pe malul mării, care nu se poate număra.');
INSERT INTO &quot;verses&quot; VALUES (650,11,13,'&lt;pb/&gt;În credință au murit toți aceștia, fără să fi primit&lt;f&gt;ⓙ&lt;/f&gt; lucrurile făgăduite; &lt;i&gt;doar&lt;/i&gt; le-au&lt;f&gt;ⓚ&lt;/f&gt; văzut de departe, le-au salutat și au mărturisit&lt;f&gt;ⓛ&lt;/f&gt; că sunt străini și călători pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (650,11,14,'Cei ce vorbesc astfel dau de&lt;f&gt;ⓜ&lt;/f&gt; înțeles că sunt în căutarea unei patrii.');
INSERT INTO &quot;verses&quot; VALUES (650,11,15,'Dacă le-ar fi stat gândul la cea din care ieșiseră, ar fi avut vreme să se întoarcă.');
INSERT INTO &quot;verses&quot; VALUES (650,11,16,'Dar ei își doresc o patrie mai bună, adică &lt;i&gt;una&lt;/i&gt; cerească. De aceea lui Dumnezeu nu-I este rușine să&lt;f&gt;ⓝ&lt;/f&gt; Se numească Dumnezeul lor, căci le-a&lt;f&gt;ⓞ&lt;/f&gt; pregătit o cetate.');
INSERT INTO &quot;verses&quot; VALUES (650,11,17,'&lt;pb/&gt;Prin credință l-a adus Avraam&lt;f&gt;ⓟ&lt;/f&gt; pe Isaac &lt;i&gt;jertfă&lt;/i&gt;, când a fost pus la încercare; el, care primise făgăduințele&lt;f&gt;ⓠ&lt;/f&gt;, se pregătea să-l aducă &lt;i&gt;jertfă&lt;/i&gt; pe singurul său &lt;i&gt;fiu&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (650,11,18,'despre care se spusese: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;În&lt;/e&gt; &lt;f&gt;ⓡ&lt;/f&gt; &lt;e&gt;Isaac sămânța ta va avea un nume.&lt;/e&gt;&lt;f&gt;[66]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,11,19,'&lt;pb/&gt;Socotea că Dumnezeu poate&lt;f&gt;ⓢ&lt;/f&gt; să-l învie și din morți, drept care l-a primit înapoi ca înviat din morți.');
INSERT INTO &quot;verses&quot; VALUES (650,11,20,'&lt;pb/&gt;Prin credință i-a binecuvântat Isaac&lt;f&gt;ⓣ&lt;/f&gt; pe Iacov și pe Esau cu privire la lucrurile viitoare.');
INSERT INTO &quot;verses&quot; VALUES (650,11,21,'&lt;pb/&gt;Prin credință Iacov, când trăgea să moară, i-a binecuvântat&lt;f&gt;ⓤ&lt;/f&gt; pe fiecare din fiii lui Iosif și &lt;e&gt;s-a închinat, rezemat pe vârful toiagului său&lt;/e&gt;&lt;f&gt;[67]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,11,22,'&lt;pb/&gt;Prin credință Iosif, când i s-a apropiat sfârșitul,&lt;f&gt;ⓥ&lt;/f&gt; a pomenit de ieșirea fiilor lui Israel &lt;i&gt;din Egipt&lt;/i&gt; și a dat porunci cu privire la oasele sale.');
INSERT INTO &quot;verses&quot; VALUES (650,11,23,'&lt;pb/&gt;Prin credință Moise&lt;f&gt;ⓦ&lt;/f&gt; a fost ascuns trei luni de părinții lui după naștere, pentru că vedeau că era frumos copilul și nu s-au lăsat înspăimântați de porunca&lt;f&gt;ⓧ&lt;/f&gt; regelui.');
INSERT INTO &quot;verses&quot; VALUES (650,11,24,'&lt;pb/&gt;Prin credință Moise&lt;f&gt;ⓨ&lt;/f&gt;, când s-a făcut mare, n-a vrut să fie numit fiul fiicei lui Faraon,');
INSERT INTO &quot;verses&quot; VALUES (650,11,25,'ci a ales mai degrabă să sufere cu poporul lui Dumnezeu decât să se bucure de plăcerile de o clipă ale păcatului.');
INSERT INTO &quot;verses&quot; VALUES (650,11,26,'El socotea că ocara&lt;f&gt;ⓩ&lt;/f&gt; lui Hristos este o bogăție mai mare decât comorile Egiptului, pentru că avea ochii ațintiți spre răsplătire&lt;f&gt;ⓐ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,11,27,'&lt;pb/&gt;Prin credință a părăsit Egiptul&lt;f&gt;ⓑ&lt;/f&gt;, fără să se teamă de mânia împăratului, pentru că era neabătut, ca și când L-ar fi avut înaintea ochilor pe Cel Nevăzut.');
INSERT INTO &quot;verses&quot; VALUES (650,11,28,'Prin credință a&lt;f&gt;ⓒ&lt;/f&gt; sărbătorit Paștele și a făcut stropirea cu sânge, pentru ca Nimicitorul să nu se atingă de cei întâi născuți ai lor.');
INSERT INTO &quot;verses&quot; VALUES (650,11,29,'Prin credință au&lt;f&gt;ⓓ&lt;/f&gt; străbătut ei Marea Roșie ca pe uscat, în timp ce egiptenii, când au încercat &lt;i&gt;s-o treacă&lt;/i&gt;, au fost înghițiți.');
INSERT INTO &quot;verses&quot; VALUES (650,11,30,'Prin credință au căzut zidurile&lt;f&gt;ⓔ&lt;/f&gt; Ierihonului după ce au fost înconjurate șapte zile.');
INSERT INTO &quot;verses&quot; VALUES (650,11,31,'Prin credință n-a pierit desfrânata Rahav&lt;f&gt;ⓕ&lt;/f&gt; împreună cu cei care n-au crezut, căci ea le-a primit&lt;f&gt;ⓖ&lt;/f&gt; pe iscoade cu bunăvoință&lt;f&gt;[68]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,11,32,'&lt;pb/&gt;Și câte aș mai putea zice, însă nu mi-ar ajunge vremea dacă aș vrea să povestesc despre Ghedeon&lt;f&gt;ⓗ&lt;/f&gt;, Barac&lt;f&gt;ⓘ&lt;/f&gt;, Samson&lt;f&gt;ⓙ&lt;/f&gt;, Iefta&lt;f&gt;ⓚ&lt;/f&gt;, David&lt;f&gt;ⓛ&lt;/f&gt;, Samuel&lt;f&gt;ⓜ&lt;/f&gt; și profeți!');
INSERT INTO &quot;verses&quot; VALUES (650,11,33,'Prin credință au biruit împărății, au făcut dreptate, au dobândit făgăduințe, au&lt;f&gt;ⓝ&lt;/f&gt; închis gurile leilor,');
INSERT INTO &quot;verses&quot; VALUES (650,11,34,'au&lt;f&gt;ⓞ&lt;/f&gt; stins puterea focului, au&lt;f&gt;ⓟ&lt;/f&gt; scăpat de ascuțișul sabiei, s-au vindecat de boală&lt;f&gt;ⓠ&lt;/f&gt;, au fost viteji în luptă, au pus&lt;f&gt;ⓡ&lt;/f&gt; pe fugă oștirile străine.');
INSERT INTO &quot;verses&quot; VALUES (650,11,35,'Femeile&lt;f&gt;ⓢ&lt;/f&gt; i-au primit înapoi pe cei care muriseră și care au înviat; unii, ca să dobândească o înviere mai bună, n-au vrut să primească izbăvirea și au fost torturați&lt;f&gt;[69]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,11,36,'Alții au suferit batjocuri și biciuiri, ori chiar lanțuri&lt;f&gt;ⓣ&lt;/f&gt; și temniță,');
INSERT INTO &quot;verses&quot; VALUES (650,11,37,'au&lt;f&gt;ⓤ&lt;/f&gt; fost uciși cu pietre, tăiați în două cu fierăstrăul&lt;f&gt;[70]&lt;/f&gt;, au murit uciși de sabie, au pribegit&lt;f&gt;ⓥ&lt;/f&gt; îmbrăcați cu cojoace și în piei de capră, lipsiți, prigoniți, chinuiți —');
INSERT INTO &quot;verses&quot; VALUES (650,11,38,'ei, de care lumea nu era vrednică — rătăcind prin pustietăți, în munți, prin&lt;f&gt;ⓦ&lt;/f&gt; peșteri și prin crăpăturile pământului&lt;f&gt;[71]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,11,39,'Toți aceștia, deși au fost apreciați&lt;f&gt;[72]&lt;/f&gt; pentru&lt;f&gt;ⓧ&lt;/f&gt; credința &lt;i&gt;lor&lt;/i&gt;, n-au primit ce le fusese făgăduit,');
INSERT INTO &quot;verses&quot; VALUES (650,11,40,'căci Dumnezeu avea în vedere ceva&lt;f&gt;ⓨ&lt;/f&gt; mai bun pentru noi, ca să nu ajungă ei la&lt;f&gt;ⓩ&lt;/f&gt; desăvârșire fără noi.');
INSERT INTO &quot;verses&quot; VALUES (650,12,1,'&lt;pb/&gt;De aceea și noi, fiindcă suntem înconjurați de un nor așa de mare de martori, să&lt;f&gt;ⓐ&lt;/f&gt; dăm la o parte orice piedică și păcatul care ne înfășoară așa de lesne și să&lt;f&gt;ⓑ&lt;/f&gt; alergăm stăruitor&lt;f&gt;ⓒ&lt;/f&gt; în cursa&lt;f&gt;[73]&lt;/f&gt; care ne stă înainte.');
INSERT INTO &quot;verses&quot; VALUES (650,12,2,'Să privim țintă la Întemeietorul și Desăvârșitorul credinței &lt;i&gt;noastre&lt;/i&gt;, adică la Isus, care&lt;f&gt;ⓓ&lt;/f&gt;, pentru bucuria ce Îi era pusă înainte, a răbdat crucea, disprețuind rușinea, și S-a așezat&lt;f&gt;ⓔ&lt;/f&gt; la dreapta tronului lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (650,12,3,'Luați&lt;f&gt;ⓕ&lt;/f&gt; aminte la Cel care a răbdat din partea păcătoșilor o împotrivire așa de mare, ca&lt;f&gt;ⓖ&lt;/f&gt; să nu obosiți și să vă descurajați în sufletul vostru!');
INSERT INTO &quot;verses&quot; VALUES (650,12,4,'&lt;pb/&gt;Voi încă nu v-ați împotrivit&lt;f&gt;ⓗ&lt;/f&gt; până la sânge în lupta cu păcatul');
INSERT INTO &quot;verses&quot; VALUES (650,12,5,'și ați uitat îndemnul pe care vi-l dă ca unor fii: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Fiule, nu&lt;/e&gt; &lt;f&gt;ⓘ&lt;/f&gt; &lt;e&gt;disprețui pedeapsa Domnului&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și nu te descuraja când ești mustrat de El!&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,12,6,'&lt;t&gt;&lt;e&gt;Căci Domnul îl pedepsește pe&lt;/e&gt; &lt;f&gt;ⓙ&lt;/f&gt; &lt;e&gt;cel pe care-l iubește&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și îl ceartă pe orice fiu pe care-l primește.&lt;/e&gt; &lt;f&gt;[74]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,12,7,'&lt;pb/&gt;Îndurați&lt;f&gt;ⓚ&lt;/f&gt; pedeapsa: Dumnezeu Se poartă &lt;i&gt;astfel&lt;/i&gt; cu voi &lt;i&gt;fiindcă sunteți&lt;/i&gt; fiii Lui! Într-adevăr, care este fiul pe care nu-l pedepsește tatăl &lt;i&gt;său&lt;/i&gt;?');
INSERT INTO &quot;verses&quot; VALUES (650,12,8,'Dacă ați fi scutiți de pedeapsa de care&lt;f&gt;ⓛ&lt;/f&gt; toți au parte, ați fi &lt;i&gt;copii&lt;/i&gt; nelegitimi, nu fii.');
INSERT INTO &quot;verses&quot; VALUES (650,12,9,'Și apoi, dacă părinții noștri trupești ne-au pedepsit și le-am dat cinstea cuvenită, nu ne vom supune cu atât mai mult Tatălui&lt;f&gt;ⓜ&lt;/f&gt; duhurilor, ca să trăim?');
INSERT INTO &quot;verses&quot; VALUES (650,12,10,'Ei ne pedepseau pentru puține zile, după cum găseau de cuviință, dar &lt;i&gt;Dumnezeu ne pedepsește&lt;/i&gt; cu folos, ca&lt;f&gt;ⓝ&lt;/f&gt; să ne facă părtași sfințeniei Sale.');
INSERT INTO &quot;verses&quot; VALUES (650,12,11,'La început, nicio pedeapsă nu pare &lt;i&gt;motiv&lt;/i&gt; de bucurie, ci de întristare; dar mai pe urmă aduce rodul pașnic al dreptății în cei care au trecut prin &lt;i&gt;școala&lt;/i&gt; ei&lt;f&gt;ⓞ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (650,12,12,'&lt;pb/&gt;De aceea, întăriți&lt;f&gt;ⓟ&lt;/f&gt; mâinile obosite și genunchii slăbănogiți;');
INSERT INTO &quot;verses&quot; VALUES (650,12,13,'croiți&lt;f&gt;ⓠ&lt;/f&gt; cărări drepte cu picioarele voastre, pentru ca &lt;i&gt;piciorul&lt;/i&gt; care șchiopătează să nu se scrântească, ci&lt;f&gt;ⓡ&lt;/f&gt; mai degrabă să fie vindecat!');
INSERT INTO &quot;verses&quot; VALUES (650,12,14,'Urmăriți&lt;f&gt;ⓢ&lt;/f&gt; pacea cu toți &lt;i&gt;oamenii&lt;/i&gt;, precum și sfințirea, fără&lt;f&gt;ⓣ&lt;/f&gt; de care nimeni nu-L va vedea pe Domnul!');
INSERT INTO &quot;verses&quot; VALUES (650,12,15,'Vegheați ca&lt;f&gt;ⓤ&lt;/f&gt; nimeni să nu ajungă lipsit de harul lui Dumnezeu; &lt;e&gt;nu&lt;/e&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;cumva să dea lăstari vreo rădăcină de amărăciune&lt;/e&gt;&lt;f&gt;[75]&lt;/f&gt;, să aducă tulburare și mulți să fie întinați de ea!');
INSERT INTO &quot;verses&quot; VALUES (650,12,16,'Nimeni să&lt;f&gt;ⓦ&lt;/f&gt; nu fie desfrânat sau lumesc ca Esau, care&lt;f&gt;ⓧ&lt;/f&gt; pentru o mâncare și-a vândut dreptul de întâi născut!');
INSERT INTO &quot;verses&quot; VALUES (650,12,17,'Știți că mai pe urmă, când a vrut&lt;f&gt;ⓨ&lt;/f&gt; să moștenească binecuvântarea, a&lt;f&gt;ⓩ&lt;/f&gt; fost respins. N-a mai fost loc pentru o schimbare, cu toate că o ceruse cu lacrimi.');
INSERT INTO &quot;verses&quot; VALUES (650,12,18,'&lt;pb/&gt;Voi nu v-ați apropiat de un &lt;i&gt;munte&lt;/i&gt;&lt;f&gt;ⓐ&lt;/f&gt; care se poate atinge și care era cuprins de foc, nici de întuneric, neguri și furtună,');
INSERT INTO &quot;verses&quot; VALUES (650,12,19,'nici de sunetul trâmbiței, nici de un glas care vorbea în așa fel, încât cei ce l-au auzit au cerut&lt;f&gt;ⓑ&lt;/f&gt; să nu li se mai vorbească');
INSERT INTO &quot;verses&quot; VALUES (650,12,20,'(pentru că nu puteau suporta porunca &lt;i&gt;aceasta&lt;/i&gt;: „Și&lt;f&gt;ⓒ&lt;/f&gt; o fiară, dacă se va atinge de munte, să fie ucisă cu pietre.”&lt;f&gt;[76]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,12,21,'Și&lt;f&gt;ⓓ&lt;/f&gt; atât de înfricoșătoare era priveliștea, încât Moise a zis: „&lt;e&gt;Sunt îngrozit&lt;/e&gt;&lt;f&gt;[77]&lt;/f&gt; și tremur”).');
INSERT INTO &quot;verses&quot; VALUES (650,12,22,'Voi însă v-ați apropiat de&lt;f&gt;ⓔ&lt;/f&gt; muntele Sionului și de Ierusalimul ceresc, cetatea Dumnezeului celui viu, de&lt;f&gt;ⓕ&lt;/f&gt; zecile de mii de îngeri în sărbătoare');
INSERT INTO &quot;verses&quot; VALUES (650,12,23,'și de Biserica celor întâi născuți&lt;f&gt;ⓖ&lt;/f&gt;, care&lt;f&gt;ⓗ&lt;/f&gt; sunt scriși în ceruri, de Dumnezeu, Judecătorul&lt;f&gt;ⓘ&lt;/f&gt; tuturor, de duhurile celor drepți, făcuți&lt;f&gt;ⓙ&lt;/f&gt; desăvârșiți,');
INSERT INTO &quot;verses&quot; VALUES (650,12,24,'de Isus, Mijlocitorul&lt;f&gt;ⓚ&lt;/f&gt; noului legământ, și de sângele&lt;f&gt;ⓛ&lt;/f&gt; stropirii, care vorbește mai bine decât&lt;f&gt;ⓜ&lt;/f&gt; &lt;i&gt;sângele lui&lt;/i&gt; Abel.');
INSERT INTO &quot;verses&quot; VALUES (650,12,25,'&lt;pb/&gt;Luați seama ca nu cumva să-L respingeți pe Cel ce vă vorbește! Căci dacă&lt;f&gt;ⓝ&lt;/f&gt; n-au scăpat aceia care s-au ferit de Cel ce vorbea pe pământ, cu atât mai puțin &lt;i&gt;vom scăpa&lt;/i&gt; noi dacă ne întoarcem de la Cel &lt;i&gt;ce vorbește&lt;/i&gt; din ceruri,');
INSERT INTO &quot;verses&quot; VALUES (650,12,26,'al&lt;f&gt;ⓞ&lt;/f&gt; cărui glas a clătinat pământul odinioară și care a făgăduit astfel: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Voi zgudui încă o dată nu doar pământul,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;ci și cerul.&lt;/e&gt; &lt;f&gt;[78]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,12,27,'&lt;pb/&gt;&lt;i&gt;Cuvintele&lt;/i&gt; &lt;e&gt;încă o dată&lt;/e&gt; arată schimbarea&lt;f&gt;ⓟ&lt;/f&gt; lucrurilor care pot fi clătinate, adică a lucrurilor create, ca să rămână cele de neclătinat.');
INSERT INTO &quot;verses&quot; VALUES (650,12,28,'De aceea, fiindcă primim o împărăție care nu se clatină, să-I fim recunoscători lui Dumnezeu și să-I aducem &lt;i&gt;astfel&lt;/i&gt; o închinare plăcută, cu evlavie și cu frică,');
INSERT INTO &quot;verses&quot; VALUES (650,12,29,'fiindcă Dumnezeul&lt;f&gt;ⓠ&lt;/f&gt; nostru este &lt;e&gt;un foc mistuitor&lt;/e&gt;&lt;f&gt;[79]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (650,13,1,'&lt;pb/&gt;Dragostea&lt;f&gt;ⓡ&lt;/f&gt; frățească să dăinuie!');
INSERT INTO &quot;verses&quot; VALUES (650,13,2,'Să nu dați uitării&lt;f&gt;ⓢ&lt;/f&gt; primirea de oaspeți, căci prin ea unii&lt;f&gt;ⓣ&lt;/f&gt; i-au găzduit, fără să știe, pe îngeri!');
INSERT INTO &quot;verses&quot; VALUES (650,13,3,'Aduceți-vă&lt;f&gt;ⓤ&lt;/f&gt; aminte de cei întemnițați ca și cum &lt;i&gt;voi înșivă&lt;/i&gt; ați fi întemnițați împreună cu ei; de cei chinuiți, ca unii care trăiți voi înșivă în trup!');
INSERT INTO &quot;verses&quot; VALUES (650,13,4,'Căsătoria să fie ținută în &lt;i&gt;toată&lt;/i&gt; cinstea și patul să fie nespurcat, căci&lt;f&gt;ⓥ&lt;/f&gt; pe desfrânați și pe adulteri îi va judeca Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (650,13,5,'Să nu fiți iubitori de bani! Mulțumiți-vă&lt;f&gt;ⓦ&lt;/f&gt; cu ce aveți, căci El Însuși a zis: &lt;e&gt;Nicidecum nu&lt;/e&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;te voi lăsa, cu niciun chip nu te voi părăsi&lt;/e&gt;&lt;f&gt;[80]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (650,13,6,'așa că îndrăznim să zicem: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Domnul&lt;/e&gt; &lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;este ajutorul meu și nu mă tem.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Ce mi-ar putea face omul?&lt;/e&gt; &lt;f&gt;[81]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (650,13,7,'&lt;pb/&gt;Aduceți-vă&lt;f&gt;ⓩ&lt;/f&gt; aminte de mai-marii voștri care v-au vestit Cuvântul lui Dumnezeu; cântăriți &lt;i&gt;atent&lt;/i&gt; sfârșitul felului lor de viețuire și urmați-le&lt;f&gt;ⓐ&lt;/f&gt; credința!');
INSERT INTO &quot;verses&quot; VALUES (650,13,8,'Isus Hristos este același&lt;f&gt;ⓑ&lt;/f&gt; ieri și azi și în veci.');
INSERT INTO &quot;verses&quot; VALUES (650,13,9,'Nu&lt;f&gt;ⓒ&lt;/f&gt; vă lăsați seduși de tot soiul de învățături străine, căci este bine ca inima să fie întărită prin har, nu&lt;f&gt;ⓓ&lt;/f&gt; prin mâncăruri care n-au fost de niciun folos celor ce le-au întrebuințat!');
INSERT INTO &quot;verses&quot; VALUES (650,13,10,'Noi avem&lt;f&gt;ⓔ&lt;/f&gt; un altar din care n-au dreptul să mănânce cei ce fac slujbă în cort.');
INSERT INTO &quot;verses&quot; VALUES (650,13,11,'Într-adevăr, trupurile animalelor al&lt;f&gt;ⓕ&lt;/f&gt; căror sânge este adus de marele-preot în sanctuar, pentru păcat, sunt arse în afara taberei.');
INSERT INTO &quot;verses&quot; VALUES (650,13,12,'De aceea și Isus, ca să sfințească poporul cu &lt;i&gt;însuși&lt;/i&gt; sângele Său, a&lt;f&gt;ⓖ&lt;/f&gt; pătimit dincolo de poartă.');
INSERT INTO &quot;verses&quot; VALUES (650,13,13,'Așadar, să ieșim la El, în afara taberei, și să purtăm ocara&lt;f&gt;ⓗ&lt;/f&gt; Lui!');
INSERT INTO &quot;verses&quot; VALUES (650,13,14,'Căci&lt;f&gt;ⓘ&lt;/f&gt; noi nu avem aici o cetate care dăinuie, ci suntem în căutarea celei viitoare.');
INSERT INTO &quot;verses&quot; VALUES (650,13,15,'Prin&lt;f&gt;ⓙ&lt;/f&gt; El, să-I aducem mereu lui Dumnezeu o jertfă&lt;f&gt;ⓚ&lt;/f&gt; de laudă, adică rodul&lt;f&gt;ⓛ&lt;/f&gt; buzelor care dau mărturie despre Numele Lui!');
INSERT INTO &quot;verses&quot; VALUES (650,13,16,'Și să&lt;f&gt;ⓜ&lt;/f&gt; nu dați uitării binefacerea și dărnicia, căci astfel de jertfe&lt;f&gt;ⓝ&lt;/f&gt; Îi plac lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (650,13,17,'Ascultați&lt;f&gt;ⓞ&lt;/f&gt; de mai-marii voștri și supuneți-vă &lt;i&gt;lor&lt;/i&gt;, căci ei&lt;f&gt;ⓟ&lt;/f&gt; veghează asupra sufletelor voastre, ca unii care vor da socoteală &lt;i&gt;de ele&lt;/i&gt;, ca să poată face lucrul acesta cu bucurie, nu suspinând; așa ceva nu v-ar fi de niciun folos.');
INSERT INTO &quot;verses&quot; VALUES (650,13,18,'&lt;pb/&gt;Rugați-vă&lt;f&gt;ⓠ&lt;/f&gt; pentru noi, căci suntem încredințați că avem o conștiință curată&lt;f&gt;ⓡ&lt;/f&gt; și vrem să ne purtăm frumos în toate &lt;i&gt;privințele&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (650,13,19,'Vă rog în mod deosebit&lt;f&gt;ⓢ&lt;/f&gt; să faceți lucrul acesta ca să vă fiu redat mai curând.');
INSERT INTO &quot;verses&quot; VALUES (650,13,20,'&lt;pb/&gt;Dumnezeul&lt;f&gt;ⓣ&lt;/f&gt; păcii, care&lt;f&gt;ⓤ&lt;/f&gt; — prin sângele&lt;f&gt;ⓥ&lt;/f&gt; legământului veșnic — L-a adus &lt;i&gt;înapoi&lt;/i&gt; din morți pe Marele Păstor al oilor&lt;f&gt;ⓦ&lt;/f&gt;, pe Domnul nostru Isus,');
INSERT INTO &quot;verses&quot; VALUES (650,13,21,'să&lt;f&gt;ⓧ&lt;/f&gt; vă desăvârșească în orice lucru bun, ca să faceți voia Lui, și să lucreze&lt;f&gt;ⓨ&lt;/f&gt; în noi ce este plăcut înaintea Lui, prin Isus Hristos! A Lui să fie slava în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (650,13,22,'&lt;pb/&gt;Vă rog, fraților, îngăduiți acest cuvânt de îndemn, căci&lt;f&gt;ⓩ&lt;/f&gt; v-am scris în puține cuvinte!');
INSERT INTO &quot;verses&quot; VALUES (650,13,23,'Să știți că fratelui Timotei i s-a dat drumul; dacă vine curând, vă voi vedea împreună cu el.');
INSERT INTO &quot;verses&quot; VALUES (650,13,24,'&lt;pb/&gt;Salutați-i pe toți mai-marii&lt;f&gt;ⓐ&lt;/f&gt; voștri și pe toți sfinții! Cei din Italia vă trimit salutări.');
INSERT INTO &quot;verses&quot; VALUES (650,13,25,'&lt;pb/&gt;Harul&lt;f&gt;ⓑ&lt;/f&gt; să fie cu voi toți! Amin!');
INSERT INTO &quot;verses&quot; VALUES (660,1,1,'&lt;pb/&gt;Iacov&lt;f&gt;ⓐ&lt;/f&gt;, rob&lt;f&gt;ⓑ&lt;/f&gt; al lui Dumnezeu și al Domnului Isus Hristos, către cele&lt;f&gt;ⓒ&lt;/f&gt; douăsprezece seminții care&lt;f&gt;ⓓ&lt;/f&gt; sunt răspândite &lt;i&gt;în lume&lt;/i&gt;: Salutări!');
INSERT INTO &quot;verses&quot; VALUES (660,1,2,'&lt;pb/&gt;Frații mei, când treceți prin felurite&lt;f&gt;ⓔ&lt;/f&gt; încercări, să&lt;f&gt;ⓕ&lt;/f&gt; le priviți cu toată bucuria,');
INSERT INTO &quot;verses&quot; VALUES (660,1,3,'fiindcă știți&lt;f&gt;ⓖ&lt;/f&gt; că încercarea&lt;f&gt;[1]&lt;/f&gt; credinței voastre aduce răbdare.');
INSERT INTO &quot;verses&quot; VALUES (660,1,4,'Dar răbdarea &lt;i&gt;trebuie&lt;/i&gt; să-și facă desăvârșit lucrarea, ca să fiți desăvârșiți, întregi și să nu vă lipsească nimic.');
INSERT INTO &quot;verses&quot; VALUES (660,1,5,'&lt;pb/&gt;Dacă&lt;f&gt;ⓗ&lt;/f&gt; vreunul dintre voi are nevoie de înțelepciune, să o&lt;f&gt;ⓘ&lt;/f&gt; ceară de la Dumnezeu, care dă tuturor cu mână largă și fără reproșuri, și &lt;i&gt;ea&lt;/i&gt; îi va fi dată.');
INSERT INTO &quot;verses&quot; VALUES (660,1,6,'Dar&lt;f&gt;ⓙ&lt;/f&gt; să o ceară cu credință, fără să se îndoiască deloc, pentru că cine se îndoiește seamănă cu valul mării, clătinat și dus de vânt &lt;i&gt;încoace și încolo&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,1,7,'Un astfel de om să nu se aștepte să primească ceva de la Domnul,');
INSERT INTO &quot;verses&quot; VALUES (660,1,8,'căci este un om&lt;f&gt;ⓚ&lt;/f&gt; cu inima împărțită și nestatornic în toate căile sale.');
INSERT INTO &quot;verses&quot; VALUES (660,1,9,'&lt;pb/&gt;Cel &lt;i&gt;aflat&lt;/i&gt; într-o stare smerită să se laude cu înălțarea lui.');
INSERT INTO &quot;verses&quot; VALUES (660,1,10,'Bogatul, dimpotrivă, să se laude cu smerirea lui, căci&lt;f&gt;ⓛ&lt;/f&gt; va trece ca floarea ierbii.');
INSERT INTO &quot;verses&quot; VALUES (660,1,11,'Răsare soarele cu arșița lui și usucă iarba: floarea ei cade jos, și frumusețea înfățișării ei piere — tot așa se va veșteji bogatul în umblările lui.');
INSERT INTO &quot;verses&quot; VALUES (660,1,12,'&lt;pb/&gt;Ferice de omul care rabdă ispita&lt;f&gt;[2]&lt;/f&gt;! Căci, după ce a fost găsit bun&lt;f&gt;[3]&lt;/f&gt;, va primi cununa&lt;f&gt;ⓜ&lt;/f&gt; vieții, pe care&lt;f&gt;ⓝ&lt;/f&gt; le-a făgăduit-o [Dumnezeu] celor ce-L iubesc.');
INSERT INTO &quot;verses&quot; VALUES (660,1,13,'Nimeni, când este ispitit&lt;f&gt;[4]&lt;/f&gt;, să nu zică: „Sunt ispitit de Dumnezeu”! Căci Dumnezeu nu poate fi ispitit ca să facă rău, și El &lt;i&gt;Însuși&lt;/i&gt; nu ispitește pe nimeni.');
INSERT INTO &quot;verses&quot; VALUES (660,1,14,'Ci fiecare este ispitit când este atras de propria lui poftă și momit.');
INSERT INTO &quot;verses&quot; VALUES (660,1,15,'Apoi pofta&lt;f&gt;ⓞ&lt;/f&gt;, când a zămislit, dă naștere păcatului; și păcatul, odată înfăptuit, aduce&lt;f&gt;[5]&lt;/f&gt; moarte&lt;f&gt;ⓟ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,1,16,'&lt;pb/&gt;Nu vă înșelați, preaiubiții mei frați:');
INSERT INTO &quot;verses&quot; VALUES (660,1,17,'orice&lt;f&gt;ⓠ&lt;/f&gt; &lt;i&gt;lucru&lt;/i&gt; bun pe care-l primim și orice dar desăvârșit sunt de sus, coborându-se de la Tatăl luminilor, în&lt;f&gt;ⓡ&lt;/f&gt; care nu este nici schimbare, nici umbră de mutare&lt;f&gt;[6]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,1,18,'El&lt;f&gt;ⓢ&lt;/f&gt;, potrivit voii Sale, ne-a născut prin Cuvântul adevărului ca&lt;f&gt;ⓣ&lt;/f&gt; să fim un prim rod&lt;f&gt;ⓤ&lt;/f&gt; al făpturilor Lui.');
INSERT INTO &quot;verses&quot; VALUES (660,1,19,'&lt;pb/&gt;Să știți, preaiubiții mei frați: orice om să&lt;f&gt;ⓥ&lt;/f&gt; fie grabnic la ascultare, încet&lt;f&gt;ⓦ&lt;/f&gt; la vorbire &lt;i&gt;și&lt;/i&gt; zăbavnic la mânie&lt;f&gt;ⓧ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (660,1,20,'căci mânia omului nu ajută la îndeplinirea dreptății lui Dumnezeu&lt;f&gt;[7]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,1,21,'De aceea, lepădați&lt;f&gt;ⓨ&lt;/f&gt; orice necurăție și orice revărsare de răutate și primiți cu blândețe Cuvântul sădit &lt;i&gt;în voi&lt;/i&gt;, care&lt;f&gt;ⓩ&lt;/f&gt; vă poate mântui sufletele!');
INSERT INTO &quot;verses&quot; VALUES (660,1,22,'&lt;pb/&gt;Fiți împlinitori&lt;f&gt;ⓐ&lt;/f&gt; ai Cuvântului, nu numai ascultători care se înșală singuri.');
INSERT INTO &quot;verses&quot; VALUES (660,1,23,'Căci, cine&lt;f&gt;ⓑ&lt;/f&gt; aude Cuvântul și nu-l împlinește seamănă cu un om care își privește chipul într-o oglindă');
INSERT INTO &quot;verses&quot; VALUES (660,1,24,'și, după ce s-a privit, pleacă și uită îndată cum era.');
INSERT INTO &quot;verses&quot; VALUES (660,1,25,'Dar cine&lt;f&gt;ⓒ&lt;/f&gt; își va adânci privirile în legea&lt;f&gt;ⓓ&lt;/f&gt; desăvârșită, &lt;i&gt;care este legea&lt;/i&gt; libertății, și va stărui în ea nu ca un ascultător uituc, ci ca un împlinitor cu fapta, va fi&lt;f&gt;ⓔ&lt;/f&gt; fericit în ceea ce face.');
INSERT INTO &quot;verses&quot; VALUES (660,1,26,'&lt;pb/&gt;Dacă cineva crede că este religios, dar nu-și&lt;f&gt;ⓕ&lt;/f&gt; ține în frâu limba, ci își înșală inima, religiozitatea unui astfel de om este zadarnică.');
INSERT INTO &quot;verses&quot; VALUES (660,1,27,'Religiozitatea curată și neîntinată înaintea lui Dumnezeu, Tatăl nostru, este aceasta: să-i&lt;f&gt;ⓖ&lt;/f&gt; cercetăm pe orfani și pe văduve în necazurile lor și&lt;f&gt;ⓗ&lt;/f&gt; să ne păzim neîntinați de lume.');
INSERT INTO &quot;verses&quot; VALUES (660,2,1,'&lt;pb/&gt;Frații mei, să nu țineți credința Domnului nostru Isus Hristos, &lt;i&gt;Domnul&lt;/i&gt; slavei, căutând&lt;f&gt;ⓘ&lt;/f&gt; la fața omului&lt;f&gt;ⓙ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (660,2,2,'Căci, dacă intră în adunarea voastră un om purtând un inel de aur și haine scumpe și intră și un sărac în haine ponosite,');
INSERT INTO &quot;verses&quot; VALUES (660,2,3,'iar voi îi dați atenție celui ce poartă haine scumpe și-i ziceți: „Tu stai în locul acesta bun!”, și apoi îi ziceți săracului: „Tu stai acolo în picioare!” sau: „Stai jos, la picioarele mele!”,');
INSERT INTO &quot;verses&quot; VALUES (660,2,4,'nu faceți voi oare deosebiri între voi și nu vă faceți voi judecători cu gânduri rele?');
INSERT INTO &quot;verses&quot; VALUES (660,2,5,'Ascultați, preaiubiții mei frați: nu i-a&lt;f&gt;ⓚ&lt;/f&gt; ales Dumnezeu pe săracii &lt;i&gt;acestei&lt;/i&gt; lumi &lt;i&gt;ca să-i facă&lt;/i&gt; bogați&lt;f&gt;ⓛ&lt;/f&gt; în credință și moștenitori ai Împărăției, pe care&lt;f&gt;ⓜ&lt;/f&gt; le-a făgăduit-o celor ce-L iubesc?');
INSERT INTO &quot;verses&quot; VALUES (660,2,6,'Iar voi&lt;f&gt;ⓝ&lt;/f&gt; îl înjosiți pe cel sărac! Oare nu bogații vă asupresc și nu ei vă&lt;f&gt;ⓞ&lt;/f&gt; târăsc la judecată?');
INSERT INTO &quot;verses&quot; VALUES (660,2,7,'Nu batjocoresc ei frumosul Nume care a fost invocat asupra voastră?');
INSERT INTO &quot;verses&quot; VALUES (660,2,8,'&lt;pb/&gt;Dacă, în schimb, împliniți cu adevărat Legea împărătească, potrivit Scripturii — &lt;e&gt;să-l&lt;/e&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;e&gt;iubești pe aproapele tău ca pe tine însuți&lt;/e&gt;&lt;f&gt;[8]&lt;/f&gt; — bine faceți.');
INSERT INTO &quot;verses&quot; VALUES (660,2,9,'Dar dacă aveți în vedere fața omului, faceți un păcat și sunteți osândiți de Lege ca niște călcători &lt;i&gt;de Lege&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,2,10,'Căci cine păzește toată Legea, dar greșește&lt;f&gt;[9]&lt;/f&gt; într-o singură &lt;i&gt;poruncă&lt;/i&gt; se face&lt;f&gt;ⓠ&lt;/f&gt; vinovat de toate,');
INSERT INTO &quot;verses&quot; VALUES (660,2,11,'fiindcă Cel ce a zis: &lt;e&gt;Să nu săvârșești adulter&lt;/e&gt;&lt;f&gt;[10]&lt;/f&gt; a zis și: &lt;e&gt;Să nu ucizi&lt;/e&gt;&lt;f&gt;[11]&lt;/f&gt;. Acum, dacă nu săvârșești adulter, dar ucizi, te faci călcător al Legii.');
INSERT INTO &quot;verses&quot; VALUES (660,2,12,'&lt;pb/&gt;Să vorbiți și să lucrați ca niște oameni care vor fi judecați de Legea&lt;f&gt;ⓡ&lt;/f&gt; libertății:');
INSERT INTO &quot;verses&quot; VALUES (660,2,13,'căci judecata&lt;f&gt;ⓢ&lt;/f&gt; este fără milă pentru cel ce n-a avut milă, dar mila&lt;f&gt;ⓣ&lt;/f&gt; triumfă asupra judecății.');
INSERT INTO &quot;verses&quot; VALUES (660,2,14,'&lt;pb/&gt;Frații mei, la ce bun&lt;f&gt;ⓤ&lt;/f&gt; să spună cineva că are credință, dacă nu &lt;i&gt;are&lt;/i&gt; și fapte? Poate oare credința &lt;i&gt;aceasta&lt;/i&gt; să-l mântuiască?');
INSERT INTO &quot;verses&quot; VALUES (660,2,15,'Dacă&lt;f&gt;ⓥ&lt;/f&gt; un frate sau o soră sunt goi și lipsiți de hrana de toate zilele,');
INSERT INTO &quot;verses&quot; VALUES (660,2,16,'la ce bun să le zică&lt;f&gt;ⓦ&lt;/f&gt; unul dintre voi: „Mergeți în pace, încălziți-vă și săturați-vă!” fără să le dea cele necesare trupului?');
INSERT INTO &quot;verses&quot; VALUES (660,2,17,'Tot astfel și credința: dacă n-are fapte, este moartă în ea însăși.');
INSERT INTO &quot;verses&quot; VALUES (660,2,18,'&lt;pb/&gt;Cineva însă ar putea zice: „Tu ai credință, iar eu am fapte.” Arată-mi credința ta fără fapte, iar&lt;f&gt;ⓧ&lt;/f&gt; eu îți voi arăta credința &lt;i&gt;mea&lt;/i&gt; din faptele &lt;i&gt;mele&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (660,2,19,'Tu crezi că Dumnezeu este unul, și bine faci; &lt;i&gt;dar&lt;/i&gt; și&lt;f&gt;ⓨ&lt;/f&gt; demonii cred, și se înfioară!');
INSERT INTO &quot;verses&quot; VALUES (660,2,20,'&lt;pb/&gt;Vrei deci să înțelegi, om nesocotit, că, fără fapte, credința este zadarnică&lt;f&gt;[12]&lt;/f&gt;?');
INSERT INTO &quot;verses&quot; VALUES (660,2,21,'Avraam, părintele nostru, n-a fost &lt;i&gt;el&lt;/i&gt; îndreptățit prin fapte când&lt;f&gt;ⓩ&lt;/f&gt; l-a adus pe Isaac, fiul său, jertfă pe altar?');
INSERT INTO &quot;verses&quot; VALUES (660,2,22,'&lt;i&gt;După cum&lt;/i&gt; vezi&lt;f&gt;ⓐ&lt;/f&gt;, credința lucra împreună cu faptele lui și, prin fapte, credința a ajuns desăvârșită.');
INSERT INTO &quot;verses&quot; VALUES (660,2,23,'Astfel s-a împlinit Scriptura, care zice: &lt;e&gt;Avraam L-a&lt;/e&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;crezut pe Dumnezeu, și aceasta i s-a socotit ca dreptate&lt;/e&gt;&lt;f&gt;[13]&lt;/f&gt; și el a fost numit „prietenul lui Dumnezeu”&lt;f&gt;[14]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,2,24,'Vedeți, așadar, că prin fapte este îndreptățit omul, și nu numai prin credință!');
INSERT INTO &quot;verses&quot; VALUES (660,2,25,'Tot așa, desfrânata Rahav&lt;f&gt;ⓒ&lt;/f&gt; n-a fost &lt;i&gt;ea&lt;/i&gt; îndreptățită prin fapte când i-a găzduit pe soli și i-a scos afară pe alt drum?');
INSERT INTO &quot;verses&quot; VALUES (660,2,26,'După cum trupul fără duh este mort, tot astfel și credința fără fapte este moartă.');
INSERT INTO &quot;verses&quot; VALUES (660,3,1,'&lt;pb/&gt;Frații mei, să nu&lt;f&gt;ⓓ&lt;/f&gt; fiți mulți învățători, căci știți&lt;f&gt;ⓔ&lt;/f&gt; că vom primi o judecată mai aspră!');
INSERT INTO &quot;verses&quot; VALUES (660,3,2,'Toți greșim în&lt;f&gt;ⓕ&lt;/f&gt; multe feluri. Dacă&lt;f&gt;ⓖ&lt;/f&gt; cineva nu greșește în vorbire, acela este&lt;f&gt;ⓗ&lt;/f&gt; un om desăvârșit, în stare să-și țină în frâu tot trupul.');
INSERT INTO &quot;verses&quot; VALUES (660,3,3,'&lt;i&gt;De pildă&lt;/i&gt;, dacă le punem cailor&lt;f&gt;ⓘ&lt;/f&gt; frâul în gură ca să ne asculte, le conducem tot trupul.');
INSERT INTO &quot;verses&quot; VALUES (660,3,4,'Iată și corăbiile: deși sunt atât de mari și mânate de vânturi puternice, &lt;i&gt;totuși&lt;/i&gt; sunt conduse cu ajutorul unei cârme foarte mici, după &lt;i&gt;bunul&lt;/i&gt; plac al cârmaciului.');
INSERT INTO &quot;verses&quot; VALUES (660,3,5,'Tot astfel și limba&lt;f&gt;ⓙ&lt;/f&gt; este un mădular mic, dar se fălește cu lucruri mari&lt;f&gt;ⓚ&lt;/f&gt;. Iată un foc mic ce pădure mare aprinde!');
INSERT INTO &quot;verses&quot; VALUES (660,3,6,'Limba&lt;f&gt;ⓛ&lt;/f&gt; este și ea un foc. Este lumea nelegiuirii în rândul mădularelor noastre: ea întinează&lt;f&gt;ⓜ&lt;/f&gt; tot trupul și aprinde roata vieții, fiind &lt;i&gt;ea însăși&lt;/i&gt; aprinsă de focul gheenei.');
INSERT INTO &quot;verses&quot; VALUES (660,3,7,'Tot soiul de fiare, de păsări, de târâtoare, de viețuitoare marine au fost îmblânzite și sunt îmblânzite de neamul omenesc,');
INSERT INTO &quot;verses&quot; VALUES (660,3,8,'dar limba niciun om n-o poate îmblânzi. Ea este un rău de nestăvilit, este plină&lt;f&gt;ⓝ&lt;/f&gt; de otravă de moarte.');
INSERT INTO &quot;verses&quot; VALUES (660,3,9,'Cu ea Îl binecuvântăm pe Domnul și Tatăl &lt;i&gt;nostru&lt;/i&gt; și tot cu ea îi blestemăm pe oameni, care&lt;f&gt;ⓞ&lt;/f&gt; sunt făcuți după asemănarea lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (660,3,10,'Din aceeași gură ies și binecuvântarea, și blestemul. Nu trebuie să fie așa, frații mei!');
INSERT INTO &quot;verses&quot; VALUES (660,3,11,'Oare din aceeași vână a izvorului țâșnește și apă dulce, și apă amară?');
INSERT INTO &quot;verses&quot; VALUES (660,3,12,'Frații mei, poate oare un smochin să facă măsline sau o viță să facă smochine? Nici izvorul sărat nu face apă dulce.');
INSERT INTO &quot;verses&quot; VALUES (660,3,13,'&lt;pb/&gt;Este cineva&lt;f&gt;ⓟ&lt;/f&gt; înțelept și priceput între voi? Să-și arate, prin purtarea &lt;i&gt;lui&lt;/i&gt; bună, faptele&lt;f&gt;ⓠ&lt;/f&gt; făcute cu blândețea&lt;f&gt;ⓡ&lt;/f&gt; pe care o dă înțelepciunea!');
INSERT INTO &quot;verses&quot; VALUES (660,3,14,'Dar dacă aveți în inima voastră invidie&lt;f&gt;ⓢ&lt;/f&gt; amară și &lt;i&gt;duh de&lt;/i&gt; ceartă, să nu&lt;f&gt;ⓣ&lt;/f&gt; vă lăudați și să nu mințiți împotriva adevărului!');
INSERT INTO &quot;verses&quot; VALUES (660,3,15,'Înțelepciunea aceasta&lt;f&gt;ⓤ&lt;/f&gt; nu vine de sus, ci este pământească, firească&lt;f&gt;[15]&lt;/f&gt;, demonică.');
INSERT INTO &quot;verses&quot; VALUES (660,3,16,'Căci unde&lt;f&gt;ⓥ&lt;/f&gt; este invidie și duh de ceartă, acolo sunt și tulburări și tot felul de fapte rele.');
INSERT INTO &quot;verses&quot; VALUES (660,3,17,'Într-adevăr, înțelepciunea&lt;f&gt;ⓦ&lt;/f&gt; care vine de sus este în primul rând curată, apoi împăciuitoare, blândă, ușor de înduplecat, plină de îndurare și de roade bune, nepărtinitoare, neprefăcută&lt;f&gt;ⓧ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,3,18,'Și&lt;f&gt;ⓨ&lt;/f&gt; rodul dreptății este semănat în pace pentru cei ce fac pace.');
INSERT INTO &quot;verses&quot; VALUES (660,4,1,'&lt;pb/&gt;De unde vin luptele și certurile între voi? Nu vin oare din poftele voastre, care&lt;f&gt;ⓩ&lt;/f&gt; se luptă în mădularele voastre?');
INSERT INTO &quot;verses&quot; VALUES (660,4,2,'Voi poftiți, și nu aveți; ucideți, sunteți plini de invidie, și nu izbutiți să căpătați; vă certați și vă luptați, dar nu aveți, pentru că nu cereți.');
INSERT INTO &quot;verses&quot; VALUES (660,4,3,'Sau cereți&lt;f&gt;ⓐ&lt;/f&gt;, și nu primiți pentru că&lt;f&gt;ⓑ&lt;/f&gt; cereți rău, &lt;i&gt;cu gând&lt;/i&gt; să risipiți în plăcerile voastre.');
INSERT INTO &quot;verses&quot; VALUES (660,4,4,'Suflete adultere&lt;f&gt;ⓒ&lt;/f&gt;! Nu știți că prietenia&lt;f&gt;ⓓ&lt;/f&gt; cu lumea este vrăjmășie cu Dumnezeu? Așa că cine&lt;f&gt;ⓔ&lt;/f&gt; vrea să fie prieten cu lumea se face vrăjmaș cu Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (660,4,5,'Sau credeți că degeaba vorbește Scriptura? Duhul&lt;f&gt;ⓕ&lt;/f&gt;, pe care L-a pus &lt;i&gt;Dumnezeu&lt;/i&gt; să locuiască în noi, &lt;i&gt;ne&lt;/i&gt; dorește cu &lt;i&gt;mare&lt;/i&gt; gelozie.&lt;f&gt;[16]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (660,4,6,'Însă ne dă un har și mai mare, căci zice: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Dumnezeu&lt;/e&gt; &lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;le stă împotrivă celor mândri,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;dar celor smeriți le dă har.&lt;/e&gt; &lt;f&gt;[17]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (660,4,7,'&lt;pb/&gt;Supuneți-vă deci lui Dumnezeu, împotriviți-vă&lt;f&gt;ⓗ&lt;/f&gt; Diavolului, și el va fugi de la voi!');
INSERT INTO &quot;verses&quot; VALUES (660,4,8,'Apropiați-vă&lt;f&gt;ⓘ&lt;/f&gt; de Dumnezeu, și El Se va apropia de voi! Curățiți-vă&lt;f&gt;ⓙ&lt;/f&gt; mâinile, păcătoșilor; curățiți-vă&lt;f&gt;ⓚ&lt;/f&gt; inima, oameni&lt;f&gt;ⓛ&lt;/f&gt; cu inima împărțită!');
INSERT INTO &quot;verses&quot; VALUES (660,4,9,'Simțiți-vă&lt;f&gt;ⓜ&lt;/f&gt; ticăloșia; plângeți și tânguiți-vă! Râsul vostru să se prefacă în plâns, și bucuria voastră, în întristare!');
INSERT INTO &quot;verses&quot; VALUES (660,4,10,'Smeriți-vă&lt;f&gt;ⓝ&lt;/f&gt; înaintea Domnului, și El vă va înălța!');
INSERT INTO &quot;verses&quot; VALUES (660,4,11,'&lt;pb/&gt;Nu&lt;f&gt;ⓞ&lt;/f&gt; vă vorbiți de rău unii pe alții, fraților! Cine îl vorbește de rău pe un frate sau&lt;f&gt;ⓟ&lt;/f&gt; îl judecă pe fratele său vorbește de rău Legea sau judecă Legea. Dar dacă judeci Legea, nu ești împlinitor al Legii, ci judecător.');
INSERT INTO &quot;verses&quot; VALUES (660,4,12,'Unul singur este dătătorul Legii și judecătorul: Acela care&lt;f&gt;ⓠ&lt;/f&gt; poate să mântuiască și să nimicească. Dar tu&lt;f&gt;ⓡ&lt;/f&gt; cine ești să-l judeci pe aproapele &lt;i&gt;tău&lt;/i&gt;?');
INSERT INTO &quot;verses&quot; VALUES (660,4,13,'&lt;pb/&gt;Ascultați&lt;f&gt;ⓢ&lt;/f&gt; acum, voi, care ziceți: „Astăzi sau mâine vom merge în cutare cetate, vom sta acolo un an, vom face negustorie și vom avea câștig”,');
INSERT INTO &quot;verses&quot; VALUES (660,4,14,'și nu știți ce va aduce ziua de mâine și ce este viața voastră! Sunteți un fum&lt;f&gt;ⓣ&lt;/f&gt; care se arată puțin și apoi piere.');
INSERT INTO &quot;verses&quot; VALUES (660,4,15,'Voi, dimpotrivă, ar trebui să ziceți: „Dacă Domnul va&lt;f&gt;ⓤ&lt;/f&gt; vrea, vom trăi și vom face cutare sau cutare lucru.”');
INSERT INTO &quot;verses&quot; VALUES (660,4,16,'Pe când acum vă făliți cu lăudăroșiile voastre! Orice&lt;f&gt;ⓥ&lt;/f&gt; laudă de felul acesta este rea.');
INSERT INTO &quot;verses&quot; VALUES (660,4,17,'Așadar, cine&lt;f&gt;ⓦ&lt;/f&gt; știe să facă un bine, și nu-l face este vinovat de păcat!');
INSERT INTO &quot;verses&quot; VALUES (660,5,1,'&lt;pb/&gt;Ascultați acum&lt;f&gt;ⓧ&lt;/f&gt;, voi, bogaților! Plângeți și tânguiți-vă din pricina nenorocirilor care se vor abate peste voi!');
INSERT INTO &quot;verses&quot; VALUES (660,5,2,'Bogățiile voastre au putrezit și hainele&lt;f&gt;ⓨ&lt;/f&gt; voastre sunt roase de molii.');
INSERT INTO &quot;verses&quot; VALUES (660,5,3,'Aurul și argintul vostru au ruginit&lt;f&gt;[18]&lt;/f&gt;, iar rugina lor va fi mărturie împotriva voastră: ca focul va mistui trupurile voastre! V-ați&lt;f&gt;ⓩ&lt;/f&gt; strâns comori în zilele din urmă!');
INSERT INTO &quot;verses&quot; VALUES (660,5,4,'Iată, plata&lt;f&gt;ⓐ&lt;/f&gt; lucrătorilor care v-au secerat ogoarele și pe care le-ați oprit-o prin înșelăciune strigă, iar strigătele&lt;f&gt;ⓑ&lt;/f&gt; secerătorilor au ajuns &lt;e&gt;la urechile Domnului Savaot&lt;/e&gt;&lt;f&gt;[19]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (660,5,5,'Ați&lt;f&gt;ⓒ&lt;/f&gt; trăit pe pământ în plăceri și în desfătări; v-ați săturat inimile &lt;e&gt;în ziua măcelului&lt;/e&gt;&lt;f&gt;[20]&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (660,5,6,'l-ați&lt;f&gt;ⓓ&lt;/f&gt; osândit, l-ați omorât pe cel drept, &lt;i&gt;care&lt;/i&gt; nu vi se împotrivea!');
INSERT INTO &quot;verses&quot; VALUES (660,5,7,'&lt;pb/&gt;Fiți deci îndelung răbdători, fraților, până la venirea Domnului! Iată, plugarul așteaptă rodul scump al pământului și este îndelung răbdător până primește &lt;i&gt;ploaie&lt;/i&gt;&lt;f&gt;ⓔ&lt;/f&gt; timpurie și târzie.');
INSERT INTO &quot;verses&quot; VALUES (660,5,8,'Fiți și voi îndelung răbdători, întăriți-vă inimile, căci&lt;f&gt;ⓕ&lt;/f&gt; venirea Domnului este aproape!');
INSERT INTO &quot;verses&quot; VALUES (660,5,9,'Nu&lt;f&gt;ⓖ&lt;/f&gt; vă plângeți unii împotriva altora, fraților, ca să nu fiți judecați; iată că Judecătorul este&lt;f&gt;ⓗ&lt;/f&gt; chiar la ușă.');
INSERT INTO &quot;verses&quot; VALUES (660,5,10,'Frații &lt;i&gt;mei&lt;/i&gt;, luați-i&lt;f&gt;ⓘ&lt;/f&gt; ca pildă de suferință și de îndelungă răbdare pe profeții care au vorbit în Numele Domnului!');
INSERT INTO &quot;verses&quot; VALUES (660,5,11,'Iată, &lt;i&gt;noi&lt;/i&gt; îi numim fericiți&lt;f&gt;ⓙ&lt;/f&gt; pe cei ce au răbdat. Despre răbdarea&lt;f&gt;ⓚ&lt;/f&gt; lui Iov ați auzit și ați văzut ce sfârșit i-a dat Domnul&lt;f&gt;ⓛ&lt;/f&gt; și cum Domnul&lt;f&gt;ⓜ&lt;/f&gt; este plin de milă și de îndurare.');
INSERT INTO &quot;verses&quot; VALUES (660,5,12,'&lt;pb/&gt;Mai presus de toate, frații mei, să&lt;f&gt;ⓝ&lt;/f&gt; nu vă jurați nici pe cer, nici pe pământ, nici cu vreun alt jurământ! „Da”-ul vostru să fie „da” și „nu”-ul să fie „nu”, ca să nu cădeți sub judecată!');
INSERT INTO &quot;verses&quot; VALUES (660,5,13,'&lt;pb/&gt;Este vreunul printre voi în suferință? Să se roage! Este vreunul cu inimă bună? Să cânte cântări de laudă&lt;f&gt;ⓞ&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (660,5,14,'Este vreunul printre voi bolnav? Să-i cheme pe prezbiterii bisericii ca să se roage pentru el, după ce-l vor unge&lt;f&gt;ⓟ&lt;/f&gt; cu untdelemn în Numele Domnului!');
INSERT INTO &quot;verses&quot; VALUES (660,5,15,'Rugăciunea cu credință îl va mântui pe cel bolnav, și Domnul îl va&lt;f&gt;ⓠ&lt;/f&gt; însănătoși&lt;f&gt;[21]&lt;/f&gt;; iar dacă a făcut păcate, îi vor fi iertate.');
INSERT INTO &quot;verses&quot; VALUES (660,5,16,'Mărturisiți-vă deci unii altora păcatele și rugați-vă unii pentru alții, ca să fiți vindecați! Mare&lt;f&gt;ⓡ&lt;/f&gt; putere are rugăciunea fierbinte a celui drept!&lt;f&gt;[22]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (660,5,17,'Ilie era un om supus acelorași&lt;f&gt;ⓢ&lt;/f&gt; slăbiciuni ca și noi și&lt;f&gt;ⓣ&lt;/f&gt; s-a rugat stăruitor să nu plouă și&lt;f&gt;ⓤ&lt;/f&gt; n-a plouat &lt;i&gt;deloc&lt;/i&gt; în țară trei ani și șase luni.');
INSERT INTO &quot;verses&quot; VALUES (660,5,18,'Apoi s-a rugat din nou&lt;f&gt;ⓥ&lt;/f&gt; și cerul a dat ploaie, iar pământul a rodit.');
INSERT INTO &quot;verses&quot; VALUES (660,5,19,'&lt;pb/&gt;Frații mei, să știți că, dacă&lt;f&gt;ⓦ&lt;/f&gt; vreunul dintre voi s-a rătăcit de la adevăr și este întors de altul,');
INSERT INTO &quot;verses&quot; VALUES (660,5,20,'acela care îl întoarce pe un păcătos de la calea lui rătăcită îi va&lt;f&gt;ⓧ&lt;/f&gt; scăpa sufletul de la moarte și va&lt;f&gt;ⓨ&lt;/f&gt; acoperi o mulțime de păcate.');
INSERT INTO &quot;verses&quot; VALUES (670,1,1,'&lt;pb/&gt;Petru, apostol al lui Isus Hristos, către cei care trăiesc ca străini, răspândiți prin&lt;f&gt;ⓐ&lt;/f&gt; Pont, Galatia, Capadocia, Asia&lt;f&gt;[1]&lt;/f&gt; și Bitinia, aleși');
INSERT INTO &quot;verses&quot; VALUES (670,1,2,'după preștiința lui Dumnezeu Tatăl, prin&lt;f&gt;ⓑ&lt;/f&gt; sfințirea &lt;i&gt;lucrată de&lt;/i&gt; Duhul, în vederea ascultării de Isus Hristos, cu al cărui&lt;f&gt;ⓒ&lt;/f&gt; sânge ați fost stropiți&lt;f&gt;[2]&lt;/f&gt;: Harul&lt;f&gt;ⓓ&lt;/f&gt; și pacea să vă fie înmulțite!');
INSERT INTO &quot;verses&quot; VALUES (670,1,3,'&lt;pb/&gt;Binecuvântat&lt;f&gt;ⓔ&lt;/f&gt; să fie Dumnezeu, Tatăl Domnului nostru Isus Hristos, care, după&lt;f&gt;ⓕ&lt;/f&gt; îndurarea Sa cea mare, ne-a&lt;f&gt;ⓖ&lt;/f&gt; născut din nou, prin&lt;f&gt;ⓗ&lt;/f&gt; învierea lui Isus Hristos din morți, la o nădejde vie');
INSERT INTO &quot;verses&quot; VALUES (670,1,4,'și la o moștenire nepieritoare, care e neîntinată și nici nu se poate veșteji, păstrată&lt;f&gt;ⓘ&lt;/f&gt; în ceruri pentru voi.');
INSERT INTO &quot;verses&quot; VALUES (670,1,5,'Voi&lt;f&gt;ⓙ&lt;/f&gt; sunteți păziți de puterea lui Dumnezeu prin credință pentru mântuirea gata să fie descoperită în vremea de pe urmă.');
INSERT INTO &quot;verses&quot; VALUES (670,1,6,'În tot acest timp&lt;f&gt;ⓚ&lt;/f&gt; vă bucurați, deși acum, dacă trebuie, sunteți întristați pentru&lt;f&gt;ⓛ&lt;/f&gt; puțină vreme prin felurite încercări,');
INSERT INTO &quot;verses&quot; VALUES (670,1,7,'pentru ca încercarea&lt;f&gt;[3]&lt;/f&gt; credinței&lt;f&gt;ⓜ&lt;/f&gt; voastre, cu mult mai scumpă decât aurul care piere, dar care totuși este încercat&lt;f&gt;ⓝ&lt;/f&gt; prin foc&lt;f&gt;ⓞ&lt;/f&gt;, să aibă ca urmare lauda&lt;f&gt;ⓟ&lt;/f&gt;, slava și cinstea când Se va arăta Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (670,1,8,'Deși nu L-ați văzut, voi Îl iubiți, credeți&lt;f&gt;ⓠ&lt;/f&gt; în El fără să-L vedeți și vă bucurați cu o bucurie negrăită și strălucită');
INSERT INTO &quot;verses&quot; VALUES (670,1,9,'pentru că veți dobândi, ca sfârșit&lt;f&gt;ⓡ&lt;/f&gt; al credinței voastre, mântuirea sufletelor voastre.');
INSERT INTO &quot;verses&quot; VALUES (670,1,10,'&lt;pb/&gt;Cu privire la această mântuire, cei care au profețit despre harul &lt;i&gt;dăruit&lt;/i&gt; vouă&lt;f&gt;ⓢ&lt;/f&gt; au căutat și au cercetat îndelung');
INSERT INTO &quot;verses&quot; VALUES (670,1,11,'ca să afle pe cine și ce împrejurări avea în vedere Duhul&lt;f&gt;ⓣ&lt;/f&gt; lui Hristos, care era în ei, când vestea&lt;f&gt;[4]&lt;/f&gt; mai dinainte patimile&lt;f&gt;ⓤ&lt;/f&gt; lui Hristos și slava de care aveau să fie urmate.');
INSERT INTO &quot;verses&quot; VALUES (670,1,12,'Lor le-a fost descoperit că nu pentru ei înșiși, ci pentru voi au rostit lucrurile care v-au fost propovăduite acum de cei care v-au adus Evanghelia prin Duhul Sfânt trimis din cer, lucruri pe care și îngerii doresc să le privească.');
INSERT INTO &quot;verses&quot; VALUES (670,1,13,'&lt;pb/&gt;De aceea, încingeți-vă&lt;f&gt;ⓥ&lt;/f&gt; coapsele minții&lt;f&gt;[5]&lt;/f&gt;, fiți&lt;f&gt;ⓦ&lt;/f&gt; treji și puneți-vă toată nădejdea în harul care vă va fi adus când Se va arăta Isus Hristos!');
INSERT INTO &quot;verses&quot; VALUES (670,1,14,'Ca niște copii ascultători, nu&lt;f&gt;ⓧ&lt;/f&gt; vă potriviți poftelor pe care le aveați altădată, când erați în neștiință,');
INSERT INTO &quot;verses&quot; VALUES (670,1,15,'ci&lt;f&gt;ⓨ&lt;/f&gt;, după cum Cel ce v-a chemat este sfânt, fiți și voi sfinți în toată purtarea voastră,');
INSERT INTO &quot;verses&quot; VALUES (670,1,16,'căci este scris: &lt;e&gt;Fiți&lt;/e&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;e&gt;sfinți, căci Eu sunt sfânt!&lt;/e&gt;&lt;f&gt;[6]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,1,17,'&lt;pb/&gt;Și dacă-L chemați ca Tată pe Cel ce&lt;f&gt;ⓐ&lt;/f&gt; judecă fără părtinire pe fiecare după faptele lui, purtați-vă&lt;f&gt;ⓑ&lt;/f&gt; cu frică în timpul pribegiei voastre;');
INSERT INTO &quot;verses&quot; VALUES (670,1,18,'căci știți că&lt;f&gt;ⓒ&lt;/f&gt; nu cu lucruri pieritoare — cu argint sau cu aur — ați fost răscumpărați din felul deșert de viețuire pe care-l moșteniserăți&lt;f&gt;ⓓ&lt;/f&gt; de la părinții voștri,');
INSERT INTO &quot;verses&quot; VALUES (670,1,19,'ci cu&lt;f&gt;ⓔ&lt;/f&gt; sângele scump al lui Hristos, &lt;i&gt;sânge&lt;/i&gt; ca al unui miel&lt;f&gt;ⓕ&lt;/f&gt; fără cusur și fără prihană.');
INSERT INTO &quot;verses&quot; VALUES (670,1,20,'El&lt;f&gt;ⓖ&lt;/f&gt; a fost cunoscut mai înainte de întemeierea lumii, dar a fost arătat la&lt;f&gt;ⓗ&lt;/f&gt; sfârșitul vremurilor pentru voi.');
INSERT INTO &quot;verses&quot; VALUES (670,1,21,'Prin El, voi Îi sunteți credincioși lui Dumnezeu, care L-a&lt;f&gt;ⓘ&lt;/f&gt; înviat din morți și I-a&lt;f&gt;ⓙ&lt;/f&gt; dat slavă, astfel încât credința și nădejdea voastră să fie în Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (670,1,22,'&lt;pb/&gt;Așadar, deoarece prin ascultarea de adevăr v-ați curățit sufletele [prin Duhul] ca să aveți o dragoste&lt;f&gt;ⓚ&lt;/f&gt; de frați neprefăcută, iubiți-vă cu căldură unii pe alții dintr-o inimă curată,');
INSERT INTO &quot;verses&quot; VALUES (670,1,23,'fiindcă&lt;f&gt;ⓛ&lt;/f&gt; ați fost născuți din nou nu dintr-o sămânță pieritoare, ci dintr-una nepieritoare, prin&lt;f&gt;ⓜ&lt;/f&gt; Cuvântul lui Dumnezeu, care este viu și care rămâne în veac!');
INSERT INTO &quot;verses&quot; VALUES (670,1,24,'Căci&lt;f&gt;ⓝ&lt;/f&gt; &lt;pb/&gt;&lt;t&gt;&lt;e&gt;orice făptură este ca iarba,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și toată slava ei, ca floarea ierbii.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Iarba se usucă și floarea cade jos,&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,1,25,'&lt;t&gt;&lt;e&gt;dar&lt;/e&gt; &lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;Cuvântul Domnului rămâne în veac&lt;/e&gt;&lt;f&gt;[7]&lt;/f&gt;.&lt;/t&gt; &lt;pb/&gt;Și acesta este Cuvântul care v-a fost propovăduit prin Evanghelie.');
INSERT INTO &quot;verses&quot; VALUES (670,2,1,'&lt;pb/&gt;Lepădați&lt;f&gt;ⓟ&lt;/f&gt; deci orice răutate, vicleșug, prefăcătorie, invidie și defăimare;');
INSERT INTO &quot;verses&quot; VALUES (670,2,2,'ca&lt;f&gt;ⓠ&lt;/f&gt; niște prunci născuți de curând, să doriți laptele&lt;f&gt;ⓡ&lt;/f&gt; duhovnicesc&lt;f&gt;[8]&lt;/f&gt; și curat, pentru ca prin el să creșteți spre mântuire,');
INSERT INTO &quot;verses&quot; VALUES (670,2,3,'dacă într-adevăr &lt;e&gt;ați gustat&lt;/e&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;că bun este Domnul&lt;/e&gt;&lt;f&gt;[9]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (670,2,4,'Apropiați-vă de El, Piatra vie, lepădată de oameni, dar aleasă și scumpă înaintea lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (670,2,5,'Și voi, ca niște pietre vii, sunteți zidiți ca să fiți o casă duhovnicească, o preoție&lt;f&gt;ⓣ&lt;/f&gt; sfântă, și să aduceți jertfe&lt;f&gt;ⓤ&lt;/f&gt; duhovnicești plăcute&lt;f&gt;ⓥ&lt;/f&gt; lui Dumnezeu, prin Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (670,2,6,'Căci este scris în Scriptură: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată&lt;/e&gt; &lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;, pun în Sion o piatră din capul unghiului&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;aleasă și scumpă;&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;cine se încrede în El nu va fi dat de rușine.&lt;/e&gt; &lt;f&gt;[10]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,2,7,'&lt;pb/&gt;Pentru voi, care ați crezut, este scumpă&lt;f&gt;[11]&lt;/f&gt;, dar pentru cei necredincioși, &lt;pb/&gt;&lt;t&gt;&lt;e&gt;piatra&lt;/e&gt; &lt;f&gt;ⓧ&lt;/f&gt; &lt;e&gt;pe care au lepădat-o zidarii&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;a ajuns în capul unghiului&lt;/e&gt; &lt;f&gt;[12]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,2,8,'&lt;pb/&gt;și &lt;pb/&gt;&lt;t&gt;&lt;e&gt;o piatră&lt;/e&gt; &lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;de poticnire&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și o stâncă de cădere&lt;/e&gt; &lt;f&gt;[13]&lt;/f&gt;.&lt;/t&gt; &lt;pb/&gt;Ei&lt;f&gt;ⓩ&lt;/f&gt; se poticnesc de ea pentru că nu dau crezare Cuvântului, după cum&lt;f&gt;ⓐ&lt;/f&gt; au și fost rânduiți.');
INSERT INTO &quot;verses&quot; VALUES (670,2,9,'&lt;pb/&gt;Voi însă sunteți o &lt;e&gt;seminție&lt;/e&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;e&gt;aleasă&lt;/e&gt;&lt;f&gt;[14]&lt;/f&gt;, o preoție&lt;f&gt;ⓒ&lt;/f&gt; împărătească, un neam&lt;f&gt;ⓓ&lt;/f&gt; sfânt, &lt;e&gt;un popor&lt;/e&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;e&gt;întocmit ca să vestească puterile minunate&lt;/e&gt;&lt;f&gt;[15]&lt;/f&gt; ale Celui ce v-a chemat din întuneric&lt;f&gt;ⓕ&lt;/f&gt; la lumina Sa minunată;');
INSERT INTO &quot;verses&quot; VALUES (670,2,10,'&lt;pb/&gt;&lt;t&gt;&lt;i&gt;pe voi&lt;/i&gt;, care&lt;f&gt;ⓖ&lt;/f&gt; odinioară &lt;e&gt;nu erați un popor&lt;/e&gt;&lt;f&gt;[16]&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;dar care acum sunteți poporul lui Dumnezeu;&lt;/t&gt; &lt;t&gt;&lt;i&gt;pe voi&lt;/i&gt;, care &lt;e&gt;nu aveați parte de îndurare&lt;/e&gt;&lt;f&gt;[17]&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;dar care acum ați căpătat îndurare.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,2,11,'&lt;pb/&gt;Preaiubiților, vă sfătuiesc, ca pe niște străini și călători, să&lt;f&gt;ⓗ&lt;/f&gt; vă feriți de poftele firii pământești, care&lt;f&gt;ⓘ&lt;/f&gt; se războiesc cu sufletul.');
INSERT INTO &quot;verses&quot; VALUES (670,2,12,'Să aveți&lt;f&gt;ⓙ&lt;/f&gt; o purtare bună în mijlocul păgânilor, astfel încât, chiar dacă vă vorbesc de rău ca pe niște făcători de rele, să vadă&lt;f&gt;ⓚ&lt;/f&gt; faptele voastre bune și să-L slăvească pe Dumnezeu&lt;f&gt;ⓛ&lt;/f&gt; &lt;e&gt;în ziua cercetării&lt;/e&gt;&lt;f&gt;[18]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (670,2,13,'&lt;pb/&gt;Fiți supuși&lt;f&gt;ⓜ&lt;/f&gt; oricărei autorități&lt;f&gt;[19]&lt;/f&gt; omenești, pentru Domnul: fie împăratului, ca înalt stăpânitor,');
INSERT INTO &quot;verses&quot; VALUES (670,2,14,'fie dregătorilor, ca unii care sunt trimiși de el să-i&lt;f&gt;ⓝ&lt;/f&gt; pedepsească pe cei ce fac răul și să-i laude pe cei ce fac binele!');
INSERT INTO &quot;verses&quot; VALUES (670,2,15,'Căci voia lui Dumnezeu este ca&lt;f&gt;ⓞ&lt;/f&gt;, făcând binele, să astupați gura oamenilor neștiutori și fără minte.');
INSERT INTO &quot;verses&quot; VALUES (670,2,16,'&lt;i&gt;Purtați-vă&lt;/i&gt; ca&lt;f&gt;ⓟ&lt;/f&gt; niște oameni liberi, dar nu faceți din libertate o haină a răutății, ci &lt;i&gt;purtați-vă&lt;/i&gt; ca niște robi&lt;f&gt;ⓠ&lt;/f&gt; ai lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (670,2,17,'Dați cinste&lt;f&gt;ⓡ&lt;/f&gt; tuturor oamenilor; iubiți-i pe frați; temeți-vă&lt;f&gt;ⓢ&lt;/f&gt; de Dumnezeu; dați cinste împăratului!');
INSERT INTO &quot;verses&quot; VALUES (670,2,18,'&lt;pb/&gt;Robilor&lt;f&gt;ⓣ&lt;/f&gt;, fiți supuși stăpânilor voștri cu toată frica, nu numai celor ce sunt buni și blânzi, ci și celor greu de mulțumit&lt;f&gt;[20]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (670,2,19,'Fiindcă este un har&lt;f&gt;ⓤ&lt;/f&gt; dacă cineva, pentru conștiința lui față de Dumnezeu, suferă întristare și pătimește pe nedrept.');
INSERT INTO &quot;verses&quot; VALUES (670,2,20,'Căci ce&lt;f&gt;ⓥ&lt;/f&gt; laudă este să suferiți &lt;i&gt;cu răbdare&lt;/i&gt; să fiți bătuți când ați făcut răul? Dar dacă faceți binele, pătimiți &lt;i&gt;de pe urma lui&lt;/i&gt; și suferiți &lt;i&gt;cu răbdare&lt;/i&gt;, este un har înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (670,2,21,'&lt;pb/&gt;&lt;t&gt;La&lt;f&gt;ⓦ&lt;/f&gt; aceasta ați fost chemați,&lt;/t&gt; &lt;t&gt;fiindcă și Hristos&lt;f&gt;ⓧ&lt;/f&gt; a pătimit pentru voi&lt;/t&gt; &lt;t&gt;și v-a lăsat o pildă&lt;f&gt;ⓨ&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;ca să călcați pe urmele Lui.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,2,22,'&lt;t&gt;&lt;e&gt;El&lt;/e&gt; &lt;f&gt;ⓩ&lt;/f&gt; &lt;e&gt;n-a făcut păcat&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și în gura Lui nu s-a găsit vicleșug.&lt;/e&gt; &lt;f&gt;[21]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,2,23,'&lt;t&gt;Când&lt;f&gt;ⓐ&lt;/f&gt; era batjocorit, nu răspundea cu batjocuri;&lt;/t&gt; &lt;t&gt;când a pătimit, n-a rostit amenințări,&lt;/t&gt; &lt;t&gt;ci S-a&lt;f&gt;ⓑ&lt;/f&gt; supus dreptului Judecător.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,2,24,'&lt;t&gt;&lt;e&gt;Păcatele noastre le-a&lt;/e&gt; &lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;purtat&lt;/e&gt;&lt;f&gt;[22]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;în trupul Său, pe lemn,&lt;/t&gt; &lt;t&gt;pentru ca&lt;f&gt;ⓓ&lt;/f&gt; noi, murind față de păcate,&lt;/t&gt; &lt;t&gt;să trăim pentru dreptate;&lt;/t&gt; &lt;t&gt;&lt;e&gt;prin rănile&lt;/e&gt; &lt;f&gt;[23]&lt;/f&gt; &lt;e&gt;Lui ați fost vindecați&lt;/e&gt;&lt;f&gt;[24]&lt;/f&gt;.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,2,25,'&lt;t&gt;Căci erați &lt;e&gt;ca niște oi rătăcite&lt;/e&gt;&lt;f&gt;[25]&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;dar acum v-ați întors la&lt;f&gt;ⓔ&lt;/f&gt; Păstorul&lt;/t&gt; &lt;t&gt;și Episcopul&lt;f&gt;[26]&lt;/f&gt; sufletelor voastre.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,3,1,'&lt;pb/&gt;Tot astfel, femeilor&lt;f&gt;ⓕ&lt;/f&gt;, fiți supuse soților voștri; pentru ca, dacă unii nu ascultă Cuvântul, să fie câștigați fără cuvânt&lt;f&gt;ⓖ&lt;/f&gt;, prin purtarea&lt;f&gt;ⓗ&lt;/f&gt; soțiilor lor,');
INSERT INTO &quot;verses&quot; VALUES (670,3,2,'când vor vedea&lt;f&gt;ⓘ&lt;/f&gt; trăirea voastră curată și respectuoasă.');
INSERT INTO &quot;verses&quot; VALUES (670,3,3,'Podoaba&lt;f&gt;ⓙ&lt;/f&gt; voastră să nu fie cea exterioară, care constă în împletituri ale părului, în purtarea bijuteriilor de aur sau în îmbrăcarea cu haine &lt;i&gt;scumpe&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (670,3,4,'ci să fie omul&lt;f&gt;ⓚ&lt;/f&gt; &lt;i&gt;ascuns al&lt;/i&gt; inimii, anume caracterul nepieritor al unui duh blând și liniștit, care este de mare preț înaintea lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (670,3,5,'Astfel se împodobeau pe vremuri sfintele femei, care nădăjduiau în Dumnezeu și care le erau supuse soților lor;');
INSERT INTO &quot;verses&quot; VALUES (670,3,6,'ca Sara, care-l asculta pe Avraam și-l numea&lt;f&gt;ⓛ&lt;/f&gt; „domnul meu”. Fiicele ei v-ați făcut voi, dacă faceți binele fără să vă temeți de nimic.');
INSERT INTO &quot;verses&quot; VALUES (670,3,7,'&lt;pb/&gt;Tot astfel, bărbaților&lt;f&gt;ⓜ&lt;/f&gt;, ca să nu vă fie împiedicate rugăciunile, trăiți în înțelegere cu soțiile voastre, căci sunt vase mai fragile, și arătați-le prețuire, întrucât&lt;f&gt;ⓝ&lt;/f&gt; și ele vor moșteni împreună cu voi harul vieții!');
INSERT INTO &quot;verses&quot; VALUES (670,3,8,'&lt;pb/&gt;Încolo, să&lt;f&gt;ⓞ&lt;/f&gt; aveți toți aceleași gânduri, simțind cu alții, iubind ca frații&lt;f&gt;ⓟ&lt;/f&gt;, miloși&lt;f&gt;ⓠ&lt;/f&gt;, smeriți!');
INSERT INTO &quot;verses&quot; VALUES (670,3,9,'Nu întoarceți&lt;f&gt;ⓡ&lt;/f&gt; rău pentru rău, nu răspundeți la batjocură cu batjocură; dimpotrivă, binecuvântați, căci la aceasta ați fost chemați: să moșteniți binecuvântarea!');
INSERT INTO &quot;verses&quot; VALUES (670,3,10,'&lt;pb/&gt;&lt;t&gt;Căci &lt;e&gt;cine vrea să se bucure de viață&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și să vadă zile bune&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;să-și&lt;/e&gt; &lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;înfrâneze limba de la rău&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și buzele de la cuvinte înșelătoare.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,3,11,'&lt;t&gt;&lt;e&gt;Să se depărteze&lt;/e&gt; &lt;f&gt;ⓣ&lt;/f&gt; &lt;e&gt;de rău și să facă binele,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;să&lt;/e&gt; &lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;caute pacea și s-o urmărească.&lt;/e&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,3,12,'&lt;t&gt;&lt;e&gt;Căci ochii Domnului sunt peste cei drepți&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și&lt;/e&gt; &lt;f&gt;ⓥ&lt;/f&gt; &lt;e&gt;urechile Lui &lt;i&gt;iau aminte&lt;/i&gt; la rugăciunile lor.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Dar Fața Domnului este împotriva celor ce fac răul.&lt;/e&gt; &lt;f&gt;[27]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,3,13,'&lt;pb/&gt;Și&lt;f&gt;ⓦ&lt;/f&gt; cine vă va face rău, dacă sunteți plini de râvnă pentru bine?');
INSERT INTO &quot;verses&quot; VALUES (670,3,14,'Chiar&lt;f&gt;ⓧ&lt;/f&gt; dacă ați avea de suferit pentru dreptate, ferice de voi! &lt;e&gt;Să nu aveți&lt;/e&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;e&gt;nicio teamă de ei și să nu vă tulburați&lt;/e&gt;&lt;f&gt;[28]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (670,3,15,'ci &lt;e&gt;sfințiți-L ca Domn&lt;/e&gt; pe Hristos în inimile voastre&lt;f&gt;[29]&lt;/f&gt;! Fiți mereu gata&lt;f&gt;ⓩ&lt;/f&gt; să dați un răspuns de apărare&lt;f&gt;[30]&lt;/f&gt; oricui vă cere socoteală de nădejdea care este în voi,');
INSERT INTO &quot;verses&quot; VALUES (670,3,16,'dar cu blândețe și respect, având&lt;f&gt;ⓐ&lt;/f&gt; un cuget curat, pentru ca&lt;f&gt;ⓑ&lt;/f&gt; cei ce batjocoresc purtarea voastră bună în Hristos să rămână de rușine &lt;i&gt;tocmai&lt;/i&gt; în lucrurile în care vă vorbesc de rău!');
INSERT INTO &quot;verses&quot; VALUES (670,3,17,'Căci este mai bine, dacă așa este voia lui Dumnezeu, să suferiți pentru că faceți binele decât pentru că faceți răul.');
INSERT INTO &quot;verses&quot; VALUES (670,3,18,'&lt;pb/&gt;&lt;t&gt;Și Hristos a suferit o dată&lt;f&gt;ⓒ&lt;/f&gt; pentru păcate,&lt;/t&gt; &lt;t&gt;&lt;i&gt;El,&lt;/i&gt; Cel drept, pentru cei nedrepți,&lt;/t&gt; &lt;t&gt;ca să ne aducă la Dumnezeu.&lt;/t&gt; &lt;t&gt;A fost omorât&lt;f&gt;ⓓ&lt;/f&gt; în trup&lt;f&gt;ⓔ&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;dar a fost înviat&lt;f&gt;ⓕ&lt;/f&gt; prin Duhul,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,3,19,'&lt;pb/&gt;prin care S-a dus și le-a vestit&lt;f&gt;ⓖ&lt;/f&gt; &lt;i&gt;biruința&lt;/i&gt; Sa duhurilor din&lt;f&gt;ⓗ&lt;/f&gt; închisoare,');
INSERT INTO &quot;verses&quot; VALUES (670,3,20,'care se răzvrătiseră pe vremuri, când îndelunga răbdare a lui Dumnezeu era în așteptare&lt;f&gt;ⓘ&lt;/f&gt;, în zilele lui Noe, când se construia arca&lt;f&gt;ⓙ&lt;/f&gt; în&lt;f&gt;ⓚ&lt;/f&gt; care &lt;i&gt;doar&lt;/i&gt; câteva suflete, și anume opt, au fost salvate cu ajutorul apei.');
INSERT INTO &quot;verses&quot; VALUES (670,3,21,'Simbolul acesta&lt;f&gt;ⓛ&lt;/f&gt; vă mântuiește acum și pe voi — &lt;i&gt;anume&lt;/i&gt; botezul, care nu este o îndepărtare a murdăriei&lt;f&gt;ⓜ&lt;/f&gt; trupești, ci mărturia unui cuget curat înaintea lui Dumnezeu&lt;f&gt;[31]&lt;/f&gt;, prin învierea lui Isus Hristos,');
INSERT INTO &quot;verses&quot; VALUES (670,3,22,'care&lt;f&gt;ⓝ&lt;/f&gt; stă la dreapta lui Dumnezeu după ce S-a dus la cer, iar&lt;f&gt;ⓞ&lt;/f&gt; îngerii, autoritățile și puterile I s-au supus.&lt;f&gt;[32]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,4,1,'&lt;pb/&gt;Astfel dar, fiindcă Hristos&lt;f&gt;ⓟ&lt;/f&gt; a pătimit în trup, înarmați-vă și voi cu același fel de gândire, căci cel&lt;f&gt;ⓠ&lt;/f&gt; ce a pătimit în trup a sfârșit-o cu păcatul,');
INSERT INTO &quot;verses&quot; VALUES (670,4,2,'pentru ca&lt;f&gt;ⓡ&lt;/f&gt;, în vremea care-i mai rămâne de trăit în trup, să nu mai trăiască&lt;f&gt;ⓢ&lt;/f&gt; după poftele oamenilor, ci&lt;f&gt;ⓣ&lt;/f&gt; după voia lui Dumnezeu!');
INSERT INTO &quot;verses&quot; VALUES (670,4,3,'Ajunge că în trecut ați&lt;f&gt;ⓤ&lt;/f&gt; făcut voia păgânilor și ați trăit în dezmățuri, în pofte, în beții, în chefuri, în banchete, în slujbe idolatre neîngăduite.');
INSERT INTO &quot;verses&quot; VALUES (670,4,4,'De aceea se miră ei că nu alergați împreună cu ei la același potop de desfrâu și vă batjocoresc&lt;f&gt;ⓥ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (670,4,5,'Dar au să dea socoteală înaintea Celui ce este gata să&lt;f&gt;ⓦ&lt;/f&gt; judece viii și morții.');
INSERT INTO &quot;verses&quot; VALUES (670,4,6,'Căci tocmai de aceea li s-a vestit Evanghelia și celor &lt;i&gt;care acum sunt&lt;/i&gt; morți, ca să fie judecați în trup, în felul oamenilor, dar să trăiască în duh, în felul lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (670,4,7,'&lt;pb/&gt;Sfârșitul&lt;f&gt;ⓧ&lt;/f&gt; tuturor lucrurilor este aproape. Fiți&lt;f&gt;ⓨ&lt;/f&gt; înțelepți deci și vegheați în vederea rugăciunii!');
INSERT INTO &quot;verses&quot; VALUES (670,4,8,'Mai&lt;f&gt;ⓩ&lt;/f&gt; întâi de toate, să aveți o dragoste fierbinte unii pentru alții, căci&lt;f&gt;ⓐ&lt;/f&gt; dragostea acoperă o sumedenie de păcate!');
INSERT INTO &quot;verses&quot; VALUES (670,4,9,'Fiți&lt;f&gt;ⓑ&lt;/f&gt; primitori de oaspeți între voi, fără&lt;f&gt;ⓒ&lt;/f&gt; murmur!');
INSERT INTO &quot;verses&quot; VALUES (670,4,10,'Fiecare să-și întrebuințeze darul&lt;f&gt;ⓓ&lt;/f&gt; primit în slujirea pentru ceilalți, ca un bun ispravnic&lt;f&gt;ⓔ&lt;/f&gt; al harului&lt;f&gt;ⓕ&lt;/f&gt; felurit al lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (670,4,11,'Dacă vorbește cineva, să vorbească cuvintele lui Dumnezeu. Dacă&lt;f&gt;ⓖ&lt;/f&gt; slujește cineva, să slujească după puterea pe care i-o dă Dumnezeu, pentru ca în&lt;f&gt;ⓗ&lt;/f&gt; toate lucrurile să fie slăvit Dumnezeu prin Isus Hristos, ale căruia sunt slava și puterea în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (670,4,12,'&lt;pb/&gt;Preaiubiților, nu vă mirați de&lt;f&gt;ⓘ&lt;/f&gt; focul &lt;i&gt;suferinței&lt;/i&gt; din mijlocul vostru — care a venit peste voi ca să vă încerce — ca de ceva ciudat care s-a abătut asupra voastră;');
INSERT INTO &quot;verses&quot; VALUES (670,4,13,'dimpotrivă&lt;f&gt;ⓙ&lt;/f&gt;, dacă aveți parte&lt;f&gt;ⓚ&lt;/f&gt; de patimile lui Hristos, bucurați-vă, ca&lt;f&gt;ⓛ&lt;/f&gt; să aveți parte de bucurie și să vă veseliți când se va arăta slava Lui!');
INSERT INTO &quot;verses&quot; VALUES (670,4,14,'Dacă&lt;f&gt;ⓜ&lt;/f&gt; sunteți batjocoriți pentru Numele lui Hristos, ferice de voi, fiindcă &lt;i&gt;Duhul&lt;/i&gt; slavei, &lt;e&gt;Duhul lui Dumnezeu, Se odihnește peste voi&lt;/e&gt;&lt;f&gt;[33]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (670,4,15,'Nimeni&lt;f&gt;ⓝ&lt;/f&gt; din voi să nu sufere ca ucigaș sau ca hoț sau ca făcător de rele sau&lt;f&gt;ⓞ&lt;/f&gt; ca unul care se amestecă în treburile altuia&lt;f&gt;[34]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (670,4,16,'Dimpotrivă, dacă suferă pentru că este creștin, să nu-i fie rușine, ci&lt;f&gt;ⓟ&lt;/f&gt; să-L slăvească pe Dumnezeu în privința aceasta!');
INSERT INTO &quot;verses&quot; VALUES (670,4,17,'Căci este vremea&lt;f&gt;ⓠ&lt;/f&gt; ca judecata să înceapă de la casa lui Dumnezeu. Și, dacă începe cu noi, care va fi sfârșitul celor ce nu ascultă de Evanghelia lui Dumnezeu?');
INSERT INTO &quot;verses&quot; VALUES (670,4,18,'Și&lt;f&gt;ⓡ&lt;/f&gt;, &lt;e&gt;dacă cel drept cu greu va fi mântuit, ce se va întâmpla cu cel nelegiuit și cel păcătos?&lt;/e&gt;&lt;f&gt;[35]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,4,19,'Așa că cei ce suferă după voia lui Dumnezeu să-și&lt;f&gt;ⓢ&lt;/f&gt; încredințeze sufletele credinciosului Ziditor și să facă ce este bine.');
INSERT INTO &quot;verses&quot; VALUES (670,5,1,'&lt;pb/&gt;Pe prezbiterii&lt;f&gt;[36]&lt;/f&gt; care sunt între voi îi sfătuiesc eu, care sunt un prezbiter ca și ei, martor&lt;f&gt;ⓣ&lt;/f&gt; al patimilor lui Hristos și părtaș&lt;f&gt;ⓤ&lt;/f&gt; al slavei care va fi descoperită:');
INSERT INTO &quot;verses&quot; VALUES (670,5,2,'păstoriți&lt;f&gt;ⓥ&lt;/f&gt; turma lui Dumnezeu care se află cu voi și privegheați&lt;f&gt;[37]&lt;/f&gt; asupra ei nu din obligație, ci de bunăvoie, după &lt;i&gt;voia&lt;/i&gt; lui Dumnezeu; nu&lt;f&gt;ⓦ&lt;/f&gt; pentru un câștig murdar, ci cu înflăcărare;');
INSERT INTO &quot;verses&quot; VALUES (670,5,3,'nu ca și cum ați&lt;f&gt;ⓧ&lt;/f&gt; stăpâni peste cei ce v-au căzut la împărțeală, ci făcându-vă&lt;f&gt;ⓨ&lt;/f&gt; pilde turmei!');
INSERT INTO &quot;verses&quot; VALUES (670,5,4,'Și, când Se va arăta Păstorul&lt;f&gt;ⓩ&lt;/f&gt; cel mare, veți căpăta cununa&lt;f&gt;ⓐ&lt;/f&gt; de slavă, care nu se poate veșteji.');
INSERT INTO &quot;verses&quot; VALUES (670,5,5,'&lt;pb/&gt;Tot așa și voi, tinerilor, să le fiți supuși prezbiterilor! Și toți&lt;f&gt;ⓑ&lt;/f&gt;, în legăturile voastre, să fiți împodobiți cu smerenie, căci &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Dumnezeu&lt;/e&gt; &lt;f&gt;ⓒ&lt;/f&gt; &lt;e&gt;le stă împotrivă celor mândri,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;dar celor smeriți le dă har&lt;/e&gt; &lt;f&gt;[38]&lt;/f&gt;.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (670,5,6,'&lt;pb/&gt;Smeriți-vă&lt;f&gt;ⓓ&lt;/f&gt; deci sub mâna tare a lui Dumnezeu, pentru ca, la vremea cuvenită, El să vă înalțe,');
INSERT INTO &quot;verses&quot; VALUES (670,5,7,'și aruncați asupra Lui toate&lt;f&gt;ⓔ&lt;/f&gt; îngrijorările voastre, căci El Însuși are grijă de voi!');
INSERT INTO &quot;verses&quot; VALUES (670,5,8,'&lt;pb/&gt;Fiți&lt;f&gt;ⓕ&lt;/f&gt; treji și vegheați, &lt;i&gt;pentru că&lt;/i&gt;&lt;f&gt;ⓖ&lt;/f&gt; potrivnicul&lt;f&gt;[39]&lt;/f&gt; vostru, Diavolul, dă târcoale &lt;e&gt;ca un leu care răcnește&lt;/e&gt;&lt;f&gt;[40]&lt;/f&gt; și caută pe cine să înghită!');
INSERT INTO &quot;verses&quot; VALUES (670,5,9,'Împotriviți-vă&lt;f&gt;ⓗ&lt;/f&gt; lui tari în credință, știind&lt;f&gt;ⓘ&lt;/f&gt; că și frații voștri din &lt;i&gt;toată&lt;/i&gt; lumea au de trecut&lt;f&gt;[41]&lt;/f&gt; prin aceleași suferințe ca voi!');
INSERT INTO &quot;verses&quot; VALUES (670,5,10,'Dumnezeul oricărui har, care&lt;f&gt;ⓙ&lt;/f&gt; v-a chemat în Hristos [Isus] la slava Sa veșnică, după ce veți suferi puțină&lt;f&gt;ⓚ&lt;/f&gt; vreme, vă va desăvârși&lt;f&gt;ⓛ&lt;/f&gt;, vă va întări&lt;f&gt;ⓜ&lt;/f&gt;, vă va da putere și vă va face neclintiți.');
INSERT INTO &quot;verses&quot; VALUES (670,5,11,'Ale Lui&lt;f&gt;ⓝ&lt;/f&gt; să fie [slava și] puterea în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (670,5,12,'&lt;pb/&gt;V-am scris&lt;f&gt;ⓞ&lt;/f&gt; aceste puține &lt;i&gt;rânduri&lt;/i&gt; prin Silvan&lt;f&gt;ⓟ&lt;/f&gt;, pe care îl consider un frate credincios, ca să vă sfătuiesc și să vă dau mărturie că adevăratul har al lui Dumnezeu este harul acesta în care stați &lt;i&gt;neclintiți&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (670,5,13,'&lt;i&gt;Biserica&lt;/i&gt; din Babilon, care este aleasă împreună cu voi, vă trimite salutări. La fel și Marcu&lt;f&gt;ⓠ&lt;/f&gt;, fiul meu.');
INSERT INTO &quot;verses&quot; VALUES (670,5,14,'Salutați-vă&lt;f&gt;ⓡ&lt;/f&gt; unii pe alții cu o sărutare de dragoste! &lt;pb/&gt;Pacea să fie cu voi toți, care sunteți în Hristos [Isus! Amin!]');
INSERT INTO &quot;verses&quot; VALUES (680,1,1,'&lt;pb/&gt;Simon&lt;f&gt;[1]&lt;/f&gt; Petru, rob și apostol al lui Isus Hristos, către cei ce au căpătat o credință de același&lt;f&gt;ⓐ&lt;/f&gt; preț cu a noastră, prin dreptatea Dumnezeului și Mântuitorului nostru Isus Hristos:');
INSERT INTO &quot;verses&quot; VALUES (680,1,2,'Harul&lt;f&gt;ⓑ&lt;/f&gt; și pacea să vă fie înmulțite prin cunoașterea lui Dumnezeu și a lui Isus, Domnul nostru!');
INSERT INTO &quot;verses&quot; VALUES (680,1,3,'&lt;pb/&gt;Dumnezeiasca Lui putere ne-a dăruit tot ce &lt;i&gt;ne&lt;/i&gt; este &lt;i&gt;de trebuință&lt;/i&gt; pentru viață și evlavie, prin&lt;f&gt;ⓒ&lt;/f&gt; cunoașterea Celui ce&lt;f&gt;ⓓ&lt;/f&gt; ne-a chemat prin slava și puterea Lui,');
INSERT INTO &quot;verses&quot; VALUES (680,1,4,'prin&lt;f&gt;ⓔ&lt;/f&gt; care El ne-a dat făgăduințele Lui nespus de mari și scumpe, ca prin ele să vă faceți părtași&lt;f&gt;ⓕ&lt;/f&gt; naturii dumnezeiești, după ce ați fugit&lt;f&gt;ⓖ&lt;/f&gt; de stricăciunea care este în lume prin pofte.');
INSERT INTO &quot;verses&quot; VALUES (680,1,5,'De aceea, dați-vă&lt;f&gt;ⓗ&lt;/f&gt; și voi toată silința ca să uniți cu credința voastră fapta&lt;f&gt;[2]&lt;/f&gt;; cu fapta, cunoștința&lt;f&gt;ⓘ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (680,1,6,'cu cunoștința, înfrânarea; cu înfrânarea, răbdarea; cu răbdarea, evlavia;');
INSERT INTO &quot;verses&quot; VALUES (680,1,7,'cu evlavia, dragostea de frați&lt;f&gt;ⓙ&lt;/f&gt;; cu dragostea de frați, iubirea &lt;i&gt;de oameni&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (680,1,8,'Căci, dacă pe acestea le aveți din belșug &lt;i&gt;în voi&lt;/i&gt;, ele nu vă vor lăsa să fiți nici&lt;f&gt;ⓚ&lt;/f&gt; leneși, nici neroditori în ce privește deplina cunoaștere a Domnului nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (680,1,9,'Dar cine nu are aceste &lt;i&gt;virtuți&lt;/i&gt; este&lt;f&gt;ⓛ&lt;/f&gt; orb &lt;i&gt;sau&lt;/i&gt; nu vede bine și a uitat că a fost curățit&lt;f&gt;ⓜ&lt;/f&gt; de păcatele &lt;i&gt;făcute&lt;/i&gt; mai demult.');
INSERT INTO &quot;verses&quot; VALUES (680,1,10,'De aceea, fraților, căutați cu atât mai mult să&lt;f&gt;ⓝ&lt;/f&gt; întăriți chemarea și alegerea voastră, căci, dacă faceți astfel, nu veți aluneca niciodată!');
INSERT INTO &quot;verses&quot; VALUES (680,1,11,'&lt;i&gt;Mai mult&lt;/i&gt;, în felul acesta vi se va asigura din belșug intrare în Împărăția veșnică a Domnului și Mântuitorului nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (680,1,12,'&lt;pb/&gt;De aceea voi&lt;f&gt;ⓞ&lt;/f&gt; fi mereu gata să vă aduc aminte de aceste lucruri, măcar că le știți și sunteți tari în adevărul pe care-l aveți&lt;f&gt;[3]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (680,1,13,'Dar consider că este drept, cât&lt;f&gt;ⓟ&lt;/f&gt; voi mai fi în cortul&lt;f&gt;[4]&lt;/f&gt; acesta, să&lt;f&gt;ⓠ&lt;/f&gt; vă trezesc amintindu-vă aceste lucruri,');
INSERT INTO &quot;verses&quot; VALUES (680,1,14,'căci știu&lt;f&gt;ⓡ&lt;/f&gt; că dezbrăcarea de cortul meu va veni curând, după cum mi-a făcut cunoscut Domnul&lt;f&gt;ⓢ&lt;/f&gt; nostru Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (680,1,15,'Îmi voi da silința deci ca, și după plecarea&lt;f&gt;[5]&lt;/f&gt; mea, să vă puteți aduce totdeauna aminte de aceste lucruri.');
INSERT INTO &quot;verses&quot; VALUES (680,1,16,'&lt;pb/&gt;&lt;i&gt;Într-adevăr&lt;/i&gt;, v-am făcut cunoscute puterea și venirea Domnului nostru Isus Hristos nu întemeindu-ne pe niște mituri&lt;f&gt;ⓣ&lt;/f&gt; meșteșugit alcătuite, ci fiindcă am văzut&lt;f&gt;ⓤ&lt;/f&gt; noi înșine, cu ochii noștri, măreția Lui.');
INSERT INTO &quot;verses&quot; VALUES (680,1,17,'Căci El&lt;f&gt;ⓥ&lt;/f&gt; a primit de la Dumnezeu Tatăl cinste și slavă, atunci când pentru El s-a auzit din Slava Minunată&lt;f&gt;[6]&lt;/f&gt; un glas care zicea: „Acesta este Fiul Meu preaiubit, în care Îmi găsesc plăcerea.”');
INSERT INTO &quot;verses&quot; VALUES (680,1,18,'Și noi înșine am auzit acest glas venind din cer când eram cu El pe muntele&lt;f&gt;ⓦ&lt;/f&gt; cel sfânt.');
INSERT INTO &quot;verses&quot; VALUES (680,1,19,'Și avem cuvântul profeției făcut și mai tare, la care bine faceți că luați aminte, ca la o&lt;f&gt;ⓧ&lt;/f&gt; făclie care strălucește într-un loc întunecos până se va lumina de ziuă și va răsări luceafărul&lt;f&gt;ⓨ&lt;/f&gt; de dimineață în inimile voastre.');
INSERT INTO &quot;verses&quot; VALUES (680,1,20,'Dar să știți, înainte de toate, că nicio&lt;f&gt;ⓩ&lt;/f&gt; profeție din Scriptură nu se explică singură&lt;f&gt;[7]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (680,1,21,'Căci nicio&lt;f&gt;ⓐ&lt;/f&gt; profeție n-a fost adusă prin voia omului; ci&lt;f&gt;ⓑ&lt;/f&gt; oamenii au vorbit de la Dumnezeu, cârmuiți de Duhul Sfânt.');
INSERT INTO &quot;verses&quot; VALUES (680,2,1,'&lt;pb/&gt;Au fost însă în&lt;f&gt;ⓒ&lt;/f&gt; popor&lt;f&gt;[8]&lt;/f&gt; și profeți mincinoși, după cum și&lt;f&gt;ⓓ&lt;/f&gt; între voi vor fi învățători mincinoși, care vor aduce pe furiș erezii nimicitoare, se vor lepăda de Stăpânul care&lt;f&gt;ⓔ&lt;/f&gt; i-a răscumpărat și vor face să cadă asupra lor o nimicire năprasnică.');
INSERT INTO &quot;verses&quot; VALUES (680,2,2,'Mulți îi vor urma în destrăbălările lor și, din pricina lor, calea adevărului va fi vorbită de rău.');
INSERT INTO &quot;verses&quot; VALUES (680,2,3,'În&lt;f&gt;ⓕ&lt;/f&gt; lăcomia &lt;i&gt;lor&lt;/i&gt; vor căuta ca, prin cuvântări înșelătoare, să aibă un câștig&lt;f&gt;ⓖ&lt;/f&gt; de la voi, dar osânda îi paște de multă vreme și pierzarea lor nu doarme.');
INSERT INTO &quot;verses&quot; VALUES (680,2,4,'&lt;pb/&gt;Căci, dacă nu i-a cruțat Dumnezeu pe&lt;f&gt;ⓗ&lt;/f&gt; îngerii care au păcătuit, ci i-a aruncat&lt;f&gt;ⓘ&lt;/f&gt; în tartar&lt;f&gt;[9]&lt;/f&gt;, &lt;i&gt;legați&lt;/i&gt; în lanțurile beznei, și i-a rânduit să fie păstrați pentru judecată;');
INSERT INTO &quot;verses&quot; VALUES (680,2,5,'dacă n-a cruțat El lumea veche, ci l-a păzit pe Noe&lt;f&gt;ⓙ&lt;/f&gt;, acest propovăduitor al dreptății, împreună cu alți șapte&lt;f&gt;[10]&lt;/f&gt;, când a adus&lt;f&gt;ⓚ&lt;/f&gt; potopul peste o lume de nelegiuiți;');
INSERT INTO &quot;verses&quot; VALUES (680,2,6,'dacă a prefăcut în cenușă cetățile&lt;f&gt;ⓛ&lt;/f&gt; Sodoma și Gomora, când le-a osândit la pieire, ca să le slujească&lt;f&gt;ⓜ&lt;/f&gt; de pildă celor ce vor trăi în nelegiuire;');
INSERT INTO &quot;verses&quot; VALUES (680,2,7,'și&lt;f&gt;ⓝ&lt;/f&gt; dacă l-a scăpat pe dreptul Lot, care era foarte chinuit de trăirea destrăbălată a acestor stricați');
INSERT INTO &quot;verses&quot; VALUES (680,2,8,'— căci omul &lt;i&gt;acesta&lt;/i&gt; drept care locuia în mijlocul lor își chinuia zi de zi sufletul lui drept din pricina faptelor nelegiuite pe care le vedea și auzea —');
INSERT INTO &quot;verses&quot; VALUES (680,2,9,'&lt;i&gt;înseamnă că&lt;/i&gt; Domnul știe&lt;f&gt;ⓞ&lt;/f&gt; să-i izbăvească din încercare pe oamenii cucernici și să-i păstreze pe cei nelegiuiți ca să fie pedepsiți în ziua judecății:');
INSERT INTO &quot;verses&quot; VALUES (680,2,10,'mai ales pe&lt;f&gt;ⓟ&lt;/f&gt; cei ce, în pofta lor necurată, poftesc trupul &lt;i&gt;altuia&lt;/i&gt;&lt;f&gt;[11]&lt;/f&gt; și disprețuiesc stăpânirea. Ca niște îndrăzneți și aroganți ce sunt, nu se tem să-i batjocorească pe cei slăviți&lt;f&gt;[12]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (680,2,11,'pe când îngerii&lt;f&gt;ⓠ&lt;/f&gt;, care sunt mai mari în tărie și putere, nu aduc înaintea Domnului, împotriva lor, nicio judecată batjocoritoare.');
INSERT INTO &quot;verses&quot; VALUES (680,2,12,'&lt;pb/&gt;Dar aceștia, ca&lt;f&gt;ⓡ&lt;/f&gt; niște dobitoace necuvântătoare&lt;f&gt;[13]&lt;/f&gt;, sortite prin firea lor să fie prinse și nimicite, batjocorind ce nu cunosc, vor merge la pierzare tocmai prin stricăciunea lor');
INSERT INTO &quot;verses&quot; VALUES (680,2,13,'și își vor lua &lt;i&gt;astfel&lt;/i&gt; plata &lt;i&gt;cuvenită&lt;/i&gt; pentru nelegiuirea lor&lt;f&gt;[14]&lt;/f&gt;. Fericirea lor este să trăiască în plăceri&lt;f&gt;ⓢ&lt;/f&gt; ziua-n &lt;i&gt;amiaza mare&lt;/i&gt;. Ca niște întinați&lt;f&gt;ⓣ&lt;/f&gt; și spurcați, plini de înșelăciune, se pun pe chefuit când se ospătează&lt;f&gt;ⓤ&lt;/f&gt; împreună cu voi.');
INSERT INTO &quot;verses&quot; VALUES (680,2,14,'Ochii le scapără după femei adultere și nu se satură de păcătuit. Momesc sufletele nestatornice, au inima deprinsă la lăcomie, sunt niște blestemați&lt;f&gt;[15]&lt;/f&gt;!');
INSERT INTO &quot;verses&quot; VALUES (680,2,15,'După ce au părăsit calea cea dreaptă, s-au rătăcit și au urmat calea lui Balaam&lt;f&gt;ⓥ&lt;/f&gt;, fiul lui Bosor, care a iubit plata nelegiuirii.');
INSERT INTO &quot;verses&quot; VALUES (680,2,16,'Dar a fost mustrat pentru fărădelegea lui: o măgăriță necuvântătoare care a vorbit cu glas omenesc a pus frâu nebuniei profetului.');
INSERT INTO &quot;verses&quot; VALUES (680,2,17,'&lt;pb/&gt;Oamenii aceștia&lt;f&gt;ⓦ&lt;/f&gt; sunt fântâni fără apă, neguri purtate de vifor: lor le este păstrată bezna de nepătruns&lt;f&gt;[16]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (680,2,18,'Ei vorbesc&lt;f&gt;ⓧ&lt;/f&gt; cu trufie lucruri de nimic și, &lt;i&gt;folosind&lt;/i&gt; poftele cărnii, îi momesc în desfrânări pe cei&lt;f&gt;ⓨ&lt;/f&gt; ce abia&lt;f&gt;[17]&lt;/f&gt; au scăpat de &lt;i&gt;oamenii&lt;/i&gt; care trăiesc în rătăcire.');
INSERT INTO &quot;verses&quot; VALUES (680,2,19,'Le promit libertatea, în timp ce ei înșiși sunt robi&lt;f&gt;ⓩ&lt;/f&gt; ai stricăciunii. Căci fiecare este robul lucrului de care este biruit.');
INSERT INTO &quot;verses&quot; VALUES (680,2,20,'&lt;i&gt;Astfel&lt;/i&gt;, dacă&lt;f&gt;ⓐ&lt;/f&gt;, după ce au scăpat de murdăriile lumii, prin cunoașterea Domnului și Mântuitorului nostru Isus Hristos, se împotmolesc iarăși și sunt biruiți de ele, starea lor de pe urmă ajunge mai rea decât cea dintâi.');
INSERT INTO &quot;verses&quot; VALUES (680,2,21,'Era mai&lt;f&gt;ⓑ&lt;/f&gt; bine pentru ei să nu fi cunoscut calea dreptății decât ca, după ce au cunoscut-o, să se întoarcă de la porunca sfântă care le-a fost dată.');
INSERT INTO &quot;verses&quot; VALUES (680,2,22,'Cu ei s-a întâmplat ce spune, pe bună dreptate, proverbul: &lt;e&gt;câinele s-a întors la ce vărsase&lt;/e&gt;&lt;f&gt;[18]&lt;/f&gt; și „scroafa spălată &lt;i&gt;s-a dus iar&lt;/i&gt; să se tăvălească în mocirlă”.');
INSERT INTO &quot;verses&quot; VALUES (680,3,1,'&lt;pb/&gt;Preaiubiților, aceasta este acum a doua epistolă pe care v-o scriu. În amândouă, caut să trezesc&lt;f&gt;ⓒ&lt;/f&gt; cugetul vostru curat printr-o înștiințare&lt;f&gt;[19]&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (680,3,2,'ca să vă aduceți aminte de cuvintele rostite mai înainte de sfinții profeți și&lt;f&gt;ⓓ&lt;/f&gt; de porunca Domnului și Mântuitorului nostru, &lt;i&gt;dată&lt;/i&gt; prin apostolii voștri.');
INSERT INTO &quot;verses&quot; VALUES (680,3,3,'Înainte&lt;f&gt;ⓔ&lt;/f&gt; de toate, să știți că în zilele din urmă vor veni batjocoritori puși pe batjocură, care vor trăi&lt;f&gt;ⓕ&lt;/f&gt; după poftele lor');
INSERT INTO &quot;verses&quot; VALUES (680,3,4,'și vor zice: „Unde&lt;f&gt;ⓖ&lt;/f&gt; este făgăduința venirii Lui? Căci, de când au adormit părinții noștri, toate au rămas așa cum erau de la începutul creației!”');
INSERT INTO &quot;verses&quot; VALUES (680,3,5,'Căci înadins trec cu vederea faptul că, în vechime, erau ceruri și un pământ care a fost scos&lt;f&gt;ⓗ&lt;/f&gt; din apă și cu ajutorul apei prin&lt;f&gt;ⓘ&lt;/f&gt; Cuvântul lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (680,3,6,'&lt;i&gt;Cuvânt&lt;/i&gt; prin care lumea&lt;f&gt;ⓙ&lt;/f&gt; de atunci a pierit înecată de apă.');
INSERT INTO &quot;verses&quot; VALUES (680,3,7,'Iar cerul și pământul de acum sunt păzite prin același Cuvânt, fiind păstrate pentru focul&lt;f&gt;ⓚ&lt;/f&gt; din ziua de judecată și de pieire a oamenilor nelegiuiți.');
INSERT INTO &quot;verses&quot; VALUES (680,3,8,'&lt;pb/&gt;Dar, preaiubiților, nu uitați un lucru: la Domnul, o zi este ca o&lt;f&gt;ⓛ&lt;/f&gt; mie de ani, și o mie de ani sunt ca o zi!');
INSERT INTO &quot;verses&quot; VALUES (680,3,9,'Domnul nu&lt;f&gt;ⓜ&lt;/f&gt; zăbovește &lt;i&gt;în împlinirea&lt;/i&gt; făgăduinței Lui, cum cred unii&lt;f&gt;[20]&lt;/f&gt;, ci are o îndelungă răbdare pentru voi și dorește&lt;f&gt;ⓝ&lt;/f&gt; ca niciunul să nu piară, ci&lt;f&gt;ⓞ&lt;/f&gt; toți să vină la pocăință.');
INSERT INTO &quot;verses&quot; VALUES (680,3,10,'&lt;pb/&gt;Ziua Domnului însă va&lt;f&gt;ⓟ&lt;/f&gt; veni ca un hoț. În ziua aceea, cerurile&lt;f&gt;ⓠ&lt;/f&gt; vor trece cu vuiet &lt;i&gt;mare&lt;/i&gt;, corpurile cerești&lt;f&gt;[21]&lt;/f&gt; vor fi nimicite prin ardere și pământul, cu tot ce este pe el, nu va mai fi de găsit&lt;f&gt;[22]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (680,3,11,'Deci, fiindcă toate acestea vor fi nimicite, &lt;i&gt;gândiți-vă&lt;/i&gt; ce fel de oameni ar trebui să fiți voi, cu o&lt;f&gt;ⓡ&lt;/f&gt; purtare sfântă și evlavioasă,');
INSERT INTO &quot;verses&quot; VALUES (680,3,12,'așteptând&lt;f&gt;ⓢ&lt;/f&gt; și grăbind venirea zilei lui Dumnezeu, în care cerurile aprinse vor&lt;f&gt;ⓣ&lt;/f&gt; fi nimicite și corpurile cerești se vor topi de căldura &lt;i&gt;focului&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (680,3,13,'Dar noi, după făgăduința Lui, așteptăm &lt;e&gt;ceruri&lt;/e&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;e&gt;noi și un pământ nou&lt;/e&gt;&lt;f&gt;[23]&lt;/f&gt;, în care va locui dreptatea.');
INSERT INTO &quot;verses&quot; VALUES (680,3,14,'&lt;pb/&gt;De aceea, preaiubiților, fiindcă așteptați aceste lucruri, siliți-vă să&lt;f&gt;ⓥ&lt;/f&gt; fiți găsiți fără cusur și fără pată înaintea Lui, în pace!');
INSERT INTO &quot;verses&quot; VALUES (680,3,15,'Să credeți că îndelunga&lt;f&gt;ⓦ&lt;/f&gt; răbdare a Domnului nostru este mântuire, cum v-a scris și preaiubitul nostru frate Pavel, după înțelepciunea care i-a fost dată,');
INSERT INTO &quot;verses&quot; VALUES (680,3,16,'ca și în toate epistolele lui, când vorbește&lt;f&gt;ⓧ&lt;/f&gt; despre lucrurile acestea. În ele sunt unele lucruri greu de înțeles, pe care cei neînvățați și nestatornici le răstălmăcesc ca și pe celelalte Scripturi, spre pierzarea lor.');
INSERT INTO &quot;verses&quot; VALUES (680,3,17,'&lt;pb/&gt;Voi deci, preaiubiților, fiindcă știți mai dinainte toate acestea, păziți-vă&lt;f&gt;ⓨ&lt;/f&gt; ca nu cumva să vă lăsați târâți de rătăcirea acestor stricați și să vă pierdeți statornicia,');
INSERT INTO &quot;verses&quot; VALUES (680,3,18,'ci&lt;f&gt;ⓩ&lt;/f&gt; creșteți în harul și în cunoașterea Domnului și Mântuitorului nostru Isus Hristos! A Lui să fie slava acum și în ziua veșniciei! [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (690,1,1,'&lt;pb/&gt;Ce&lt;f&gt;ⓐ&lt;/f&gt; era de la început, ce am auzit, ce am văzut cu ochii noștri, ce&lt;f&gt;ⓑ&lt;/f&gt; am privit și ce am pipăit cu mâinile&lt;f&gt;ⓒ&lt;/f&gt; noastre cu privire la Cuvântul vieții —');
INSERT INTO &quot;verses&quot; VALUES (690,1,2,'pentru că viața&lt;f&gt;ⓓ&lt;/f&gt; s-a arătat&lt;f&gt;ⓔ&lt;/f&gt; și noi am văzut-o și&lt;f&gt;ⓕ&lt;/f&gt; mărturisim &lt;i&gt;despre ea&lt;/i&gt; și&lt;f&gt;ⓖ&lt;/f&gt; vă vestim viața veșnică, &lt;i&gt;viață&lt;/i&gt; care&lt;f&gt;ⓗ&lt;/f&gt; era la Tatăl și care ni s-a arătat —');
INSERT INTO &quot;verses&quot; VALUES (690,1,3,'deci ce&lt;f&gt;ⓘ&lt;/f&gt; am văzut și am auzit, &lt;i&gt;aceea&lt;/i&gt; vă vestim și vouă, ca și voi să aveți părtășie cu noi. Și părtășia&lt;f&gt;ⓙ&lt;/f&gt; noastră este cu Tatăl și cu Fiul Său, Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (690,1,4,'Și vă scriem aceste lucruri pentru ca bucuria&lt;f&gt;ⓚ&lt;/f&gt; voastră să fie deplină.');
INSERT INTO &quot;verses&quot; VALUES (690,1,5,'&lt;pb/&gt;Aceasta este vestea&lt;f&gt;ⓛ&lt;/f&gt; pe care am auzit-o de la El și pe care v-o propovăduim: Dumnezeu&lt;f&gt;ⓜ&lt;/f&gt; este lumină și în El nu este deloc întuneric.');
INSERT INTO &quot;verses&quot; VALUES (690,1,6,'Dacă&lt;f&gt;ⓝ&lt;/f&gt; zicem că avem părtășie cu El, dar umblăm în întuneric, mințim și nu împlinim adevărul&lt;f&gt;[1]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (690,1,7,'Dar, dacă umblăm în lumină, după cum El Însuși este în lumină, avem părtășie unii cu alții; și sângele&lt;f&gt;ⓞ&lt;/f&gt; lui Isus [Hristos], Fiul Lui, ne curățește de orice păcat.');
INSERT INTO &quot;verses&quot; VALUES (690,1,8,'Dacă&lt;f&gt;ⓟ&lt;/f&gt; zicem că nu avem păcat, ne înșelăm singuri, și&lt;f&gt;ⓠ&lt;/f&gt; adevărul nu este în noi.');
INSERT INTO &quot;verses&quot; VALUES (690,1,9,'Dacă&lt;f&gt;ⓡ&lt;/f&gt; ne mărturisim păcatele, El este credincios și drept ca să ne ierte păcatele și să&lt;f&gt;ⓢ&lt;/f&gt; ne curățească de orice nelegiuire.');
INSERT INTO &quot;verses&quot; VALUES (690,1,10,'Dacă zicem că n-am păcătuit, Îl facem mincinos, și Cuvântul Lui nu este în noi.');
INSERT INTO &quot;verses&quot; VALUES (690,2,1,'&lt;pb/&gt;Copilașilor, vă scriu aceste lucruri ca să nu păcătuiți. Dar, dacă cineva a păcătuit, avem&lt;f&gt;ⓣ&lt;/f&gt; la Tatăl un Mijlocitor&lt;f&gt;[2]&lt;/f&gt;, pe Isus Hristos cel drept.');
INSERT INTO &quot;verses&quot; VALUES (690,2,2,'El este&lt;f&gt;ⓤ&lt;/f&gt; jertfa de ispășire pentru păcatele noastre; și nu numai pentru ale noastre, ci&lt;f&gt;ⓥ&lt;/f&gt; pentru ale întregii lumi.');
INSERT INTO &quot;verses&quot; VALUES (690,2,3,'&lt;pb/&gt;Și prin aceasta știm că Îl cunoaștem: dacă păzim poruncile Lui.');
INSERT INTO &quot;verses&quot; VALUES (690,2,4,'Cine&lt;f&gt;ⓦ&lt;/f&gt; zice: „Îl cunosc”, dar nu păzește poruncile Lui este&lt;f&gt;ⓧ&lt;/f&gt; un mincinos, și adevărul nu este în el.');
INSERT INTO &quot;verses&quot; VALUES (690,2,5,'Dar în cel&lt;f&gt;ⓨ&lt;/f&gt; care păzește Cuvântul Lui dragostea lui Dumnezeu a ajuns cu adevărat desăvârșită&lt;f&gt;ⓩ&lt;/f&gt;; prin&lt;f&gt;ⓐ&lt;/f&gt; aceasta știm că suntem în El.');
INSERT INTO &quot;verses&quot; VALUES (690,2,6,'Cine&lt;f&gt;ⓑ&lt;/f&gt; zice că rămâne în El trebuie să trăiască&lt;f&gt;ⓒ&lt;/f&gt; și el cum a trăit &lt;i&gt;Isus&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (690,2,7,'&lt;pb/&gt;Preaiubiților, nu&lt;f&gt;ⓓ&lt;/f&gt; vă scriu o poruncă nouă, ci o poruncă veche, pe care&lt;f&gt;ⓔ&lt;/f&gt; ați avut-o de la început. Porunca cea veche este cuvântul pe care l-ați auzit.');
INSERT INTO &quot;verses&quot; VALUES (690,2,8,'Totuși, vă scriu o poruncă&lt;f&gt;ⓕ&lt;/f&gt; nouă, &lt;i&gt;lucru&lt;/i&gt; care este adevărat atât cu privire la El, cât și cu privire la voi; căci&lt;f&gt;ⓖ&lt;/f&gt; întunericul trece și deja strălucește lumina&lt;f&gt;ⓗ&lt;/f&gt; cea adevărată.');
INSERT INTO &quot;verses&quot; VALUES (690,2,9,'Cine&lt;f&gt;ⓘ&lt;/f&gt; zice că este în lumină, dar îl urăște pe fratele său este încă în întuneric.');
INSERT INTO &quot;verses&quot; VALUES (690,2,10,'Cine&lt;f&gt;ⓙ&lt;/f&gt; îl iubește pe fratele său rămâne în lumină, iar în el&lt;f&gt;ⓚ&lt;/f&gt; nu este niciun prilej de poticnire.');
INSERT INTO &quot;verses&quot; VALUES (690,2,11,'Dar cine îl urăște pe fratele său este în întuneric, umblă&lt;f&gt;ⓛ&lt;/f&gt; în întuneric și nu știe încotro merge, pentru că întunericul i-a orbit ochii.');
INSERT INTO &quot;verses&quot; VALUES (690,2,12,'&lt;pb/&gt;&lt;t&gt;Vă scriu, copilașilor,&lt;/t&gt; &lt;t&gt;fiindcă păcatele&lt;f&gt;ⓜ&lt;/f&gt; vă sunt iertate pentru Numele Lui.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (690,2,13,'&lt;t&gt;Vă scriu, părinților,&lt;/t&gt; &lt;t&gt;fiindcă L-ați cunoscut pe Cel &lt;i&gt;ce este&lt;/i&gt; de la&lt;f&gt;ⓝ&lt;/f&gt; început.&lt;/t&gt; &lt;t&gt;Vă scriu, tinerilor,&lt;/t&gt; &lt;t&gt;fiindcă l-ați biruit pe Cel Rău.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (690,2,14,'&lt;t&gt;V-am scris, copilașilor,&lt;/t&gt; &lt;t&gt;fiindcă L-ați cunoscut pe Tatăl.&lt;f&gt;[3]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;V-am scris, părinților,&lt;/t&gt; &lt;t&gt;fiindcă L-ați cunoscut pe Cel &lt;i&gt;ce este&lt;/i&gt; de la început.&lt;/t&gt; &lt;t&gt;V-am scris, tinerilor,&lt;/t&gt; &lt;t&gt;fiindcă sunteți&lt;f&gt;ⓞ&lt;/f&gt; tari&lt;/t&gt; &lt;t&gt;și Cuvântul lui Dumnezeu rămâne în voi&lt;/t&gt; &lt;t&gt;și l-ați biruit pe Cel Rău.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (690,2,15,'&lt;pb/&gt;Nu&lt;f&gt;ⓟ&lt;/f&gt; iubiți lumea, nici lucrurile din lume! Dacă&lt;f&gt;ⓠ&lt;/f&gt; iubește cineva lumea, dragostea Tatălui nu este în el.');
INSERT INTO &quot;verses&quot; VALUES (690,2,16,'Căci tot ce este în lume — pofta firii pământești, pofta ochilor&lt;f&gt;ⓡ&lt;/f&gt; și lăudăroșenia vieții — nu este de la Tatăl, ci din lume.');
INSERT INTO &quot;verses&quot; VALUES (690,2,17,'Iar lumea&lt;f&gt;ⓢ&lt;/f&gt; și pofta ei trec, dar cine face voia lui Dumnezeu rămâne în veac.');
INSERT INTO &quot;verses&quot; VALUES (690,2,18,'&lt;pb/&gt;Copilașilor&lt;f&gt;ⓣ&lt;/f&gt;, este&lt;f&gt;ⓤ&lt;/f&gt; ceasul de pe urmă. Ați auzit că vine antihristul&lt;f&gt;ⓥ&lt;/f&gt;; să știți că acum&lt;f&gt;ⓦ&lt;/f&gt; au apărut &lt;i&gt;deja&lt;/i&gt; mulți antihriști, de unde cunoaștem că&lt;f&gt;ⓧ&lt;/f&gt; este ceasul de pe urmă.');
INSERT INTO &quot;verses&quot; VALUES (690,2,19,'Ei au ieșit dintre&lt;f&gt;ⓨ&lt;/f&gt; noi, dar nu erau dintre ai noștri. Căci, dacă&lt;f&gt;ⓩ&lt;/f&gt; ar fi fost dintre ai noștri, ar fi rămas cu noi; dar &lt;i&gt;au ieșit&lt;/i&gt; ca&lt;f&gt;ⓐ&lt;/f&gt; să se arate că nu sunt toți dintre ai noștri.');
INSERT INTO &quot;verses&quot; VALUES (690,2,20,'Dar voi&lt;f&gt;ⓑ&lt;/f&gt; aveți ungerea de la&lt;f&gt;ⓒ&lt;/f&gt; Cel Sfânt și cunoașteți toți&lt;f&gt;ⓓ&lt;/f&gt; &lt;i&gt;adevărul&lt;/i&gt;&lt;f&gt;[4]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (690,2,21,'Nu v-am scris pentru că n-ați cunoaște adevărul, ci pentru că îl cunoașteți și știți că nicio minciună nu vine din adevăr.');
INSERT INTO &quot;verses&quot; VALUES (690,2,22,'&lt;pb/&gt;Cine&lt;f&gt;ⓔ&lt;/f&gt; este mincinosul, dacă nu cel ce tăgăduiește că Isus este Hristosul? Acesta este antihristul, care Îi tăgăduiește pe Tatăl și pe Fiul.');
INSERT INTO &quot;verses&quot; VALUES (690,2,23,'Oricine&lt;f&gt;ⓕ&lt;/f&gt; Îl tăgăduiește pe Fiul nu-L are nici pe Tatăl. Cine&lt;f&gt;ⓖ&lt;/f&gt; Îl mărturisește pe Fiul Îl are și pe Tatăl.');
INSERT INTO &quot;verses&quot; VALUES (690,2,24,'Cât despre voi, ce&lt;f&gt;ⓗ&lt;/f&gt; ați auzit de la început, aceea să rămână în voi. Dacă rămâne în voi ce ați auzit de la început, și voi veți&lt;f&gt;ⓘ&lt;/f&gt; rămâne în Fiul și în Tatăl.');
INSERT INTO &quot;verses&quot; VALUES (690,2,25,'Și&lt;f&gt;ⓙ&lt;/f&gt; făgăduința pe care ne-a făcut-o El este aceasta: viața veșnică.');
INSERT INTO &quot;verses&quot; VALUES (690,2,26,'&lt;pb/&gt;V-am scris aceste lucruri în&lt;f&gt;ⓚ&lt;/f&gt; vederea celor ce caută să vă rătăcească.');
INSERT INTO &quot;verses&quot; VALUES (690,2,27,'Cât despre voi, ungerea&lt;f&gt;ⓛ&lt;/f&gt; pe care ați primit-o de la El rămâne în voi și n-aveți&lt;f&gt;ⓜ&lt;/f&gt; nevoie să vă învețe cineva; ci, după cum ungerea Lui vă&lt;f&gt;ⓝ&lt;/f&gt; învață despre toate lucrurile și este adevărată și nu este o minciună, rămâneți în El, după cum v-a învățat ea!');
INSERT INTO &quot;verses&quot; VALUES (690,2,28,'&lt;pb/&gt;Și acum, copilașilor, rămâneți în El, pentru ca, atunci când&lt;f&gt;ⓞ&lt;/f&gt; Se va arăta El, să&lt;f&gt;ⓟ&lt;/f&gt; avem îndrăzneală și, la venirea Lui, să nu rămânem de rușine și depărtați de El!');
INSERT INTO &quot;verses&quot; VALUES (690,2,29,'Dacă&lt;f&gt;ⓠ&lt;/f&gt; știți că El este drept, să știți și că oricine&lt;f&gt;ⓡ&lt;/f&gt; trăiește în dreptate&lt;f&gt;[5]&lt;/f&gt; este născut din El.');
INSERT INTO &quot;verses&quot; VALUES (690,3,1,'&lt;pb/&gt;Vedeți ce dragoste ne-a arătat Tatăl? Să&lt;f&gt;ⓢ&lt;/f&gt; ne numim copii ai lui Dumnezeu! Și suntem. Lumea nu ne cunoaște pentru că nu L-a cunoscut &lt;i&gt;nici&lt;/i&gt; pe El.');
INSERT INTO &quot;verses&quot; VALUES (690,3,2,'Preaiubiților, acum&lt;f&gt;ⓣ&lt;/f&gt; suntem copii ai lui Dumnezeu, și ce vom fi nu s-a arătat încă. Dar știm că, atunci când Se va arăta El, vom&lt;f&gt;ⓤ&lt;/f&gt; fi asemenea Lui, pentru că Îl&lt;f&gt;ⓥ&lt;/f&gt; vom vedea așa cum este.');
INSERT INTO &quot;verses&quot; VALUES (690,3,3,'Oricine are nădejdea aceasta în El se curățește, după cum El este curat.');
INSERT INTO &quot;verses&quot; VALUES (690,3,4,'&lt;pb/&gt;Oricine face păcat face și fărădelege&lt;f&gt;ⓦ&lt;/f&gt;; căci păcatul este fărădelege.');
INSERT INTO &quot;verses&quot; VALUES (690,3,5,'Și știți că El S-a arătat ca să&lt;f&gt;ⓧ&lt;/f&gt; ridice păcatele; și în El&lt;f&gt;ⓨ&lt;/f&gt; nu este păcat.');
INSERT INTO &quot;verses&quot; VALUES (690,3,6,'Oricine rămâne în El nu păcătuiește; oricine&lt;f&gt;ⓩ&lt;/f&gt; păcătuiește nu L-a văzut, nici nu L-a cunoscut.');
INSERT INTO &quot;verses&quot; VALUES (690,3,7,'&lt;pb/&gt;Copilașilor, nimeni&lt;f&gt;ⓐ&lt;/f&gt; să nu vă înșele! Cine trăiește în dreptate este drept, după cum El &lt;i&gt;Însuși&lt;/i&gt; este drept.');
INSERT INTO &quot;verses&quot; VALUES (690,3,8,'Cine&lt;f&gt;ⓑ&lt;/f&gt; face păcat este de la Diavolul, căci Diavolul păcătuiește de la început. Fiul lui Dumnezeu S-a arătat tocmai ca&lt;f&gt;ⓒ&lt;/f&gt; să nimicească lucrările Diavolului.');
INSERT INTO &quot;verses&quot; VALUES (690,3,9,'Oricine&lt;f&gt;ⓓ&lt;/f&gt; este născut din Dumnezeu nu face păcat, pentru că sămânța&lt;f&gt;ⓔ&lt;/f&gt; Lui rămâne în el; și nu poate păcătui, fiindcă este născut din Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (690,3,10,'Prin aceasta se cunosc copiii lui Dumnezeu și copiii Diavolului. Oricine&lt;f&gt;ⓕ&lt;/f&gt; nu trăiește în dreptate nu este din Dumnezeu; nici cine nu iubește pe fratele său.');
INSERT INTO &quot;verses&quot; VALUES (690,3,11,'&lt;pb/&gt;Căci aceasta este vestea pe care ați auzit-o de la&lt;f&gt;ⓖ&lt;/f&gt; început: să ne iubim&lt;f&gt;ⓗ&lt;/f&gt; unii pe alții;');
INSERT INTO &quot;verses&quot; VALUES (690,3,12,'nu cum a fost Cain&lt;f&gt;ⓘ&lt;/f&gt;, care era de la Cel Rău și l-a ucis pe fratele său. Și de ce l-a ucis? Pentru că faptele lui erau rele, iar ale fratelui său erau drepte.');
INSERT INTO &quot;verses&quot; VALUES (690,3,13,'&lt;pb/&gt;Nu vă mirați, fraților, dacă vă&lt;f&gt;ⓙ&lt;/f&gt; urăște lumea!');
INSERT INTO &quot;verses&quot; VALUES (690,3,14,'Noi&lt;f&gt;ⓚ&lt;/f&gt; știm că am trecut din moarte la viață pentru că îi iubim pe frați. Cine nu-l iubește [pe fratele său] rămâne în moarte.');
INSERT INTO &quot;verses&quot; VALUES (690,3,15,'Oricine&lt;f&gt;ⓛ&lt;/f&gt; îl urăște pe fratele său este un ucigaș; și știți că niciun ucigaș&lt;f&gt;ⓜ&lt;/f&gt; nu are viața veșnică fiindcă &lt;i&gt;aceasta&lt;/i&gt; nu rămâne în el.');
INSERT INTO &quot;verses&quot; VALUES (690,3,16,'Noi am cunoscut&lt;f&gt;ⓝ&lt;/f&gt; dragostea Lui prin aceea că El Și-a dat viața pentru noi; &lt;i&gt;astfel&lt;/i&gt;, și noi trebuie să ne dăm viața pentru frați.');
INSERT INTO &quot;verses&quot; VALUES (690,3,17,'Dar dacă cineva&lt;f&gt;ⓞ&lt;/f&gt; are bogățiile &lt;i&gt;acestei&lt;/i&gt; lumi și îl vede pe fratele său în nevoie, dar își închide inima&lt;f&gt;[6]&lt;/f&gt; față de el, cum rămâne&lt;f&gt;ⓟ&lt;/f&gt; în el dragostea lui Dumnezeu?');
INSERT INTO &quot;verses&quot; VALUES (690,3,18,'Copilașilor, să&lt;f&gt;ⓠ&lt;/f&gt; nu iubim cu vorba, nici cu limba, ci cu fapta și cu adevărul!');
INSERT INTO &quot;verses&quot; VALUES (690,3,19,'&lt;pb/&gt;Prin aceasta vom cunoaște că&lt;f&gt;ⓡ&lt;/f&gt; suntem din adevăr și ne vom liniști inimile înaintea Lui:');
INSERT INTO &quot;verses&quot; VALUES (690,3,20,'dacă&lt;f&gt;ⓢ&lt;/f&gt; ne osândește inima noastră; Dumnezeu este mai mare decât inima noastră și cunoaște toate lucrurile.');
INSERT INTO &quot;verses&quot; VALUES (690,3,21,'Preaiubiților, dacă nu ne osândește inima noastră, avem&lt;f&gt;ⓣ&lt;/f&gt; îndrăzneală la Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (690,3,22,'Și orice&lt;f&gt;ⓤ&lt;/f&gt; vom cere, vom căpăta de la El, fiindcă păzim poruncile Lui și&lt;f&gt;ⓥ&lt;/f&gt; facem ce este plăcut înaintea Lui.');
INSERT INTO &quot;verses&quot; VALUES (690,3,23,'&lt;pb/&gt;Și&lt;f&gt;ⓦ&lt;/f&gt; porunca Lui este aceasta: să credem în Numele Fiului Său Isus Hristos și să&lt;f&gt;ⓧ&lt;/f&gt; ne iubim unii pe alții, după cum ne-a poruncit El.');
INSERT INTO &quot;verses&quot; VALUES (690,3,24,'Cine&lt;f&gt;ⓨ&lt;/f&gt; păzește poruncile Lui rămâne în Dumnezeu&lt;f&gt;[7]&lt;/f&gt; și Dumnezeu &lt;i&gt;rămâne&lt;/i&gt; în el. Și&lt;f&gt;ⓩ&lt;/f&gt; cunoaștem că El rămâne în noi prin Duhul pe care ni L-a dat.');
INSERT INTO &quot;verses&quot; VALUES (690,4,1,'&lt;pb/&gt;Preaiubiților, să nu dați crezare oricărui duh, ci să cercetați duhurile dacă sunt de la Dumnezeu; căci în lume au ieșit mulți&lt;f&gt;ⓐ&lt;/f&gt; profeți mincinoși.');
INSERT INTO &quot;verses&quot; VALUES (690,4,2,'Duhul lui Dumnezeu să-L cunoașteți după aceasta: orice&lt;f&gt;ⓑ&lt;/f&gt; duh care mărturisește că Isus Hristos a venit în trup este de la Dumnezeu;');
INSERT INTO &quot;verses&quot; VALUES (690,4,3,'și orice&lt;f&gt;ⓒ&lt;/f&gt; duh care nu-L mărturisește pe Isus nu este de la Dumnezeu, ci este duhul antihristului, despre care ați auzit că vine și care deja&lt;f&gt;ⓓ&lt;/f&gt; este în lume acum.');
INSERT INTO &quot;verses&quot; VALUES (690,4,4,'&lt;pb/&gt;Voi, copilașilor, sunteți din Dumnezeu și i-ați biruit, pentru că Cel&lt;f&gt;ⓔ&lt;/f&gt; ce este în voi este mai mare decât cel ce este în lume.');
INSERT INTO &quot;verses&quot; VALUES (690,4,5,'Ei&lt;f&gt;ⓕ&lt;/f&gt; sunt din lume; de aceea vorbesc ca din lume și lumea&lt;f&gt;ⓖ&lt;/f&gt; îi ascultă.');
INSERT INTO &quot;verses&quot; VALUES (690,4,6,'Noi suntem din Dumnezeu; cine&lt;f&gt;ⓗ&lt;/f&gt; Îl cunoaște pe Dumnezeu ne ascultă; cine nu este din Dumnezeu nu ne ascultă. Prin aceasta cunoaștem duhul&lt;f&gt;ⓘ&lt;/f&gt; adevărului și duhul rătăcirii.');
INSERT INTO &quot;verses&quot; VALUES (690,4,7,'&lt;pb/&gt;&lt;t&gt;Preaiubiților&lt;f&gt;ⓙ&lt;/f&gt;, să ne iubim unii pe alții;&lt;/t&gt; &lt;t&gt;căci dragostea este din Dumnezeu.&lt;/t&gt; &lt;t&gt;Și oricine iubește este născut din Dumnezeu&lt;/t&gt; &lt;t&gt;și Îl cunoaște pe Dumnezeu.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (690,4,8,'&lt;t&gt;Cine nu iubește nu L-a&lt;f&gt;ⓚ&lt;/f&gt; cunoscut pe Dumnezeu,&lt;/t&gt; &lt;t&gt;căci Dumnezeu este dragoste.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (690,4,9,'&lt;t&gt;Dragostea lui Dumnezeu față de noi s-a arătat prin faptul că&lt;/t&gt; &lt;t&gt;Dumnezeu&lt;f&gt;ⓛ&lt;/f&gt; L-a trimis în lume pe singurul Său Fiu&lt;/t&gt; &lt;t&gt;ca&lt;f&gt;ⓜ&lt;/f&gt; noi să trăim prin El.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (690,4,10,'&lt;t&gt;Și dragostea stă&lt;f&gt;ⓝ&lt;/f&gt; nu în faptul că&lt;/t&gt; &lt;t&gt;noi L-am iubit pe Dumnezeu,&lt;/t&gt; &lt;t&gt;ci că El ne-a iubit pe noi&lt;/t&gt; &lt;t&gt;și L-a trimis pe Fiul Său&lt;/t&gt; &lt;t&gt;ca jertfă de ispășire&lt;f&gt;ⓞ&lt;/f&gt; pentru păcatele noastre.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (690,4,11,'&lt;pb/&gt;Preaiubiților, dacă&lt;f&gt;ⓟ&lt;/f&gt; astfel ne-a iubit Dumnezeu, trebuie să ne iubim și noi unii pe alții.');
INSERT INTO &quot;verses&quot; VALUES (690,4,12,'Pe Dumnezeu nu L-a văzut nimeni&lt;f&gt;ⓠ&lt;/f&gt; niciodată; dacă ne iubim unii pe alții, Dumnezeu rămâne în noi, și dragostea&lt;f&gt;ⓡ&lt;/f&gt; Lui a ajuns desăvârșită în noi.');
INSERT INTO &quot;verses&quot; VALUES (690,4,13,'Cunoaștem&lt;f&gt;ⓢ&lt;/f&gt; că rămânem în El și că El rămâne în noi prin faptul că ne-a dat din Duhul Său.');
INSERT INTO &quot;verses&quot; VALUES (690,4,14,'Și noi&lt;f&gt;ⓣ&lt;/f&gt; am văzut și mărturisim că Tatăl&lt;f&gt;ⓤ&lt;/f&gt; L-a trimis pe Fiul ca Mântuitor al lumii.');
INSERT INTO &quot;verses&quot; VALUES (690,4,15,'Dacă cineva&lt;f&gt;ⓥ&lt;/f&gt; mărturisește că Isus este Fiul lui Dumnezeu, Dumnezeu rămâne în el, și el, în Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (690,4,16,'Și noi am cunoscut și am crezut dragostea pe care o are Dumnezeu față de noi. &lt;pb/&gt;Dumnezeu este dragoste, și cine&lt;f&gt;ⓦ&lt;/f&gt; rămâne în dragoste rămâne în Dumnezeu, iar Dumnezeu rămâne în el.');
INSERT INTO &quot;verses&quot; VALUES (690,4,17,'Prin aceasta dragostea a ajuns desăvârșită în noi&lt;f&gt;[8]&lt;/f&gt;, ca&lt;f&gt;ⓧ&lt;/f&gt; să avem îndrăzneală în ziua judecății: cum este El, așa suntem și noi în lumea aceasta.');
INSERT INTO &quot;verses&quot; VALUES (690,4,18,'În dragoste nu este frică, ci dragostea desăvârșită izgonește frica, pentru că&lt;f&gt;ⓨ&lt;/f&gt; frica are cu ea pedeapsa&lt;f&gt;[9]&lt;/f&gt;; și cine se teme n-a ajuns desăvârșit în dragoste.');
INSERT INTO &quot;verses&quot; VALUES (690,4,19,'Noi Îl iubim pentru că El ne-a iubit întâi.');
INSERT INTO &quot;verses&quot; VALUES (690,4,20,'Dacă&lt;f&gt;ⓩ&lt;/f&gt; cineva zice: „Eu Îl iubesc pe Dumnezeu”, dar îl urăște pe fratele său, este un mincinos, căci cine nu-l iubește pe fratele său, pe care îl vede, nu-L poate iubi nici pe Dumnezeu, pe care nu-L vede.');
INSERT INTO &quot;verses&quot; VALUES (690,4,21,'Și această poruncă&lt;f&gt;ⓐ&lt;/f&gt; o avem de la El: cel ce Îl iubește pe Dumnezeu &lt;i&gt;trebuie&lt;/i&gt; să îl iubească și pe fratele său.');
INSERT INTO &quot;verses&quot; VALUES (690,5,1,'&lt;pb/&gt;Oricine crede&lt;f&gt;ⓑ&lt;/f&gt; că Isus&lt;f&gt;ⓒ&lt;/f&gt; este Hristosul este născut&lt;f&gt;ⓓ&lt;/f&gt; din Dumnezeu, și&lt;f&gt;ⓔ&lt;/f&gt; oricine Îl iubește pe Cel ce l-a născut îl iubește și pe cel născut din El.');
INSERT INTO &quot;verses&quot; VALUES (690,5,2,'Cunoaștem că-i iubim pe copiii lui Dumnezeu prin aceea că-L iubim pe Dumnezeu și împlinim poruncile Lui.');
INSERT INTO &quot;verses&quot; VALUES (690,5,3,'Căci&lt;f&gt;ⓕ&lt;/f&gt; dragostea de Dumnezeu stă în păzirea poruncilor Lui. Și&lt;f&gt;ⓖ&lt;/f&gt; poruncile Lui nu sunt grele,');
INSERT INTO &quot;verses&quot; VALUES (690,5,4,'pentru că oricine&lt;f&gt;ⓗ&lt;/f&gt; este născut din Dumnezeu biruiește lumea; și cea care a biruit lumea este credința noastră.');
INSERT INTO &quot;verses&quot; VALUES (690,5,5,'Cine este cel ce&lt;f&gt;ⓘ&lt;/f&gt; biruiește lumea dacă nu cel ce crede că Isus este Fiul lui Dumnezeu?');
INSERT INTO &quot;verses&quot; VALUES (690,5,6,'&lt;pb/&gt;El, Isus Hristos, este Acela care a venit cu&lt;f&gt;ⓙ&lt;/f&gt; apă și cu sânge; nu numai cu apă, ci cu apă și cu sânge; și Duhul&lt;f&gt;ⓚ&lt;/f&gt; este Cel ce mărturisește &lt;i&gt;aceasta&lt;/i&gt;, fiindcă Duhul este adevărul.');
INSERT INTO &quot;verses&quot; VALUES (690,5,7,'Căci trei sunt care mărturisesc&lt;f&gt;[10]&lt;/f&gt;:');
INSERT INTO &quot;verses&quot; VALUES (690,5,8,'Duhul, apa și sângele, și acestea trei una sunt.');
INSERT INTO &quot;verses&quot; VALUES (690,5,9,'Dacă primim mărturia&lt;f&gt;ⓛ&lt;/f&gt; oamenilor, &lt;i&gt;să știm că&lt;/i&gt; mărturia lui Dumnezeu este și mai mare; și&lt;f&gt;ⓜ&lt;/f&gt; mărturia lui Dumnezeu &lt;i&gt;stă în faptul&lt;/i&gt; că El a dat mărturie despre Fiul Său.');
INSERT INTO &quot;verses&quot; VALUES (690,5,10,'Cine&lt;f&gt;ⓝ&lt;/f&gt; crede în Fiul lui Dumnezeu are mărturia &lt;i&gt;aceasta&lt;/i&gt; în el; cine nu-L crede pe Dumnezeu Îl&lt;f&gt;ⓞ&lt;/f&gt; face mincinos, fiindcă nu crede mărturia pe care a făcut-o Dumnezeu despre Fiul Său.');
INSERT INTO &quot;verses&quot; VALUES (690,5,11,'Și&lt;f&gt;ⓟ&lt;/f&gt; mărturia este aceasta: Dumnezeu ne-a dat viața veșnică, și&lt;f&gt;ⓠ&lt;/f&gt; această viață este în Fiul Său.');
INSERT INTO &quot;verses&quot; VALUES (690,5,12,'Cine&lt;f&gt;ⓡ&lt;/f&gt; Îl are pe Fiul are viața; cine nu-L are pe Fiul lui Dumnezeu nu are viața.');
INSERT INTO &quot;verses&quot; VALUES (690,5,13,'&lt;pb/&gt;V-am scris aceste&lt;f&gt;ⓢ&lt;/f&gt; &lt;i&gt;lucruri&lt;/i&gt; vouă, care credeți în Numele Fiului lui Dumnezeu, ca&lt;f&gt;ⓣ&lt;/f&gt; să știți că aveți viață veșnică.');
INSERT INTO &quot;verses&quot; VALUES (690,5,14,'Îndrăzneala pe care o avem la El este aceasta: dacă&lt;f&gt;ⓤ&lt;/f&gt; cerem ceva după voia Lui, ne ascultă.');
INSERT INTO &quot;verses&quot; VALUES (690,5,15,'Și, dacă știm că ne ascultă, orice I-am cere, știm că avem lucrurile pe care I le-am cerut.');
INSERT INTO &quot;verses&quot; VALUES (690,5,16,'&lt;pb/&gt;Dacă îl vede cineva pe fratele său săvârșind un păcat care nu duce la moarte, să se roage&lt;f&gt;[11]&lt;/f&gt;, și Dumnezeu&lt;f&gt;ⓥ&lt;/f&gt; îi va da viață, dacă n-a săvârșit un păcat care duce la moarte&lt;f&gt;[12]&lt;/f&gt;. Există&lt;f&gt;ⓦ&lt;/f&gt; &lt;i&gt;însă și&lt;/i&gt; păcat care duce la moarte; nu-i&lt;f&gt;ⓧ&lt;/f&gt; zic să se roage&lt;f&gt;[13]&lt;/f&gt; pentru păcatul acela.');
INSERT INTO &quot;verses&quot; VALUES (690,5,17,'Orice&lt;f&gt;ⓨ&lt;/f&gt; nelegiuire este păcat, dar există &lt;i&gt;și&lt;/i&gt; păcat care nu duce la moarte.');
INSERT INTO &quot;verses&quot; VALUES (690,5,18,'&lt;pb/&gt;Știm că oricine&lt;f&gt;ⓩ&lt;/f&gt; este născut din Dumnezeu nu păcătuiește; dimpotrivă, cel născut din Dumnezeu se păzește&lt;f&gt;ⓐ&lt;/f&gt; pe sine&lt;f&gt;[14]&lt;/f&gt;, și Cel Rău nu se atinge de el.');
INSERT INTO &quot;verses&quot; VALUES (690,5,19,'Știm că suntem din Dumnezeu și că toată&lt;f&gt;ⓑ&lt;/f&gt; lumea zace în Cel Rău.');
INSERT INTO &quot;verses&quot; VALUES (690,5,20,'Știm că Fiul lui Dumnezeu a venit și ne-a&lt;f&gt;ⓒ&lt;/f&gt; dat pricepere ca să-L&lt;f&gt;ⓓ&lt;/f&gt; cunoaștem pe Cel ce este adevărat. Și suntem în Cel ce este adevărat, adică în Isus Hristos, Fiul Lui. El&lt;f&gt;ⓔ&lt;/f&gt; este Dumnezeul adevărat și&lt;f&gt;ⓕ&lt;/f&gt; viața veșnică.');
INSERT INTO &quot;verses&quot; VALUES (690,5,21,'&lt;pb/&gt;Copilașilor, păziți-vă&lt;f&gt;ⓖ&lt;/f&gt; de idoli! Amin!');
INSERT INTO &quot;verses&quot; VALUES (700,1,1,'&lt;pb/&gt;Prezbiterul către aleasa Doamnă&lt;f&gt;[1]&lt;/f&gt; și către copiii ei, pe&lt;f&gt;ⓐ&lt;/f&gt; care-i iubesc în adevăr — și nu numai eu, ci toți cei ce cunosc adevărul&lt;f&gt;ⓑ&lt;/f&gt; —');
INSERT INTO &quot;verses&quot; VALUES (700,1,2,'pentru adevărul care rămâne în noi și care va fi cu noi în veac:');
INSERT INTO &quot;verses&quot; VALUES (700,1,3,'Harul, îndurarea și pacea de la Dumnezeu Tatăl și de la [Domnul] Isus Hristos, Fiul Tatălui, să fie&lt;f&gt;ⓒ&lt;/f&gt; cu voi în adevăr&lt;f&gt;ⓓ&lt;/f&gt; și în dragoste!');
INSERT INTO &quot;verses&quot; VALUES (700,1,4,'&lt;pb/&gt;M-am bucurat mult să aflu că &lt;i&gt;unii&lt;/i&gt; dintre copiii tăi umblă&lt;f&gt;ⓔ&lt;/f&gt; în adevăr, după porunca pe care am primit-o de la Tatăl.');
INSERT INTO &quot;verses&quot; VALUES (700,1,5,'Și acum, te rog, Doamnă, nu&lt;f&gt;ⓕ&lt;/f&gt; ca și cum ți-aș scrie o poruncă nouă, căci este cea pe care am avut-o de la început: să&lt;f&gt;ⓖ&lt;/f&gt; ne iubim unii pe alții!');
INSERT INTO &quot;verses&quot; VALUES (700,1,6,'Și dragostea stă&lt;f&gt;ⓗ&lt;/f&gt; în umblarea după poruncile Lui. Aceasta este porunca, după cum&lt;f&gt;ⓘ&lt;/f&gt; ați auzit de la început: să umblați în dragoste.&lt;f&gt;[2]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (700,1,7,'&lt;pb/&gt;În lume s-au răspândit mulți&lt;f&gt;ⓙ&lt;/f&gt; amăgitori, care&lt;f&gt;ⓚ&lt;/f&gt; nu mărturisesc că Isus Hristos a venit în trup. Iată&lt;f&gt;ⓛ&lt;/f&gt; amăgitorul și antihristul!');
INSERT INTO &quot;verses&quot; VALUES (700,1,8,'Luați seama&lt;f&gt;ⓜ&lt;/f&gt; la voi înșivă, ca să&lt;f&gt;ⓝ&lt;/f&gt; nu pierdeți rodul muncii voastre, ci să primiți o răsplată deplină!');
INSERT INTO &quot;verses&quot; VALUES (700,1,9,'&lt;pb/&gt;Oricine&lt;f&gt;ⓞ&lt;/f&gt; o ia înainte și nu rămâne în învățătura lui Hristos nu-L are pe Dumnezeu. Cine rămâne în învățătura &lt;i&gt;aceasta&lt;/i&gt; Îl are și pe Tatăl, și pe Fiul.');
INSERT INTO &quot;verses&quot; VALUES (700,1,10,'Dacă vine cineva la voi și nu &lt;i&gt;vă&lt;/i&gt; aduce învățătura aceasta, să nu-l primiți în casă și să nu-i&lt;f&gt;ⓟ&lt;/f&gt; ziceți: „Bun venit!”&lt;f&gt;[3]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (700,1,11,'Căci cine-i zice: „Bun venit!” se face părtaș faptelor lui rele.');
INSERT INTO &quot;verses&quot; VALUES (700,1,12,'&lt;pb/&gt;Deși am multe&lt;f&gt;ⓠ&lt;/f&gt; lucruri să vă scriu, n-aș vrea &lt;i&gt;să vi le scriu&lt;/i&gt; pe hârtie, cu cerneală, ci nădăjduiesc să vin la voi și să vă vorbesc gură către gură, pentru ca&lt;f&gt;ⓡ&lt;/f&gt; bucuria noastră să fie deplină.');
INSERT INTO &quot;verses&quot; VALUES (700,1,13,'&lt;pb/&gt;Copiii surorii tale alese îți&lt;f&gt;ⓢ&lt;/f&gt; trimit salutări. [Amin!]');
INSERT INTO &quot;verses&quot; VALUES (710,1,1,'&lt;pb/&gt;Prezbiterul către preaiubitul Gaius, pe&lt;f&gt;ⓐ&lt;/f&gt; care-l iubesc în adevăr.');
INSERT INTO &quot;verses&quot; VALUES (710,1,2,'&lt;pb/&gt;Preaiubitule, îți doresc să ai sănătate și să-ți meargă bine în toate privințele, așa cum îi merge bine și sufletului tău.');
INSERT INTO &quot;verses&quot; VALUES (710,1,3,'M-am bucurat mult când au venit frații și au mărturisit că ești &lt;i&gt;credincios&lt;/i&gt; adevărului și că umbli&lt;f&gt;ⓑ&lt;/f&gt; în adevăr.');
INSERT INTO &quot;verses&quot; VALUES (710,1,4,'Mai mare bucurie nu am decât să aud despre copiii&lt;f&gt;ⓒ&lt;/f&gt; mei că umblă în adevăr.');
INSERT INTO &quot;verses&quot; VALUES (710,1,5,'&lt;pb/&gt;Preaiubitule, tu lucrezi cu credincioșie în tot ce faci pentru frați, inclusiv pentru cei străini.');
INSERT INTO &quot;verses&quot; VALUES (710,1,6,'Ei au mărturisit despre dragostea ta înaintea bisericii. Dacă te vei îngriji de călătoria lor într-un chip vrednic de Dumnezeu, bine vei face,');
INSERT INTO &quot;verses&quot; VALUES (710,1,7,'căci au plecat de dragul Numelui&lt;f&gt;[1]&lt;/f&gt;, fără să primească ceva&lt;f&gt;ⓓ&lt;/f&gt; de la neamuri.');
INSERT INTO &quot;verses&quot; VALUES (710,1,8,'Este de datoria noastră să-i primim bine pe astfel de oameni, ca să lucrăm împreună cu adevărul.');
INSERT INTO &quot;verses&quot; VALUES (710,1,9,'&lt;pb/&gt;Am scris ceva bisericii, dar Diotref, căruia îi place să aibă întâietate între ei, nu &lt;i&gt;vrea&lt;/i&gt; să ne primească.');
INSERT INTO &quot;verses&quot; VALUES (710,1,10,'De aceea, când voi veni, îi voi aduce aminte de faptele pe care le face, căci ne defăimează cu vorbe rele. Și nu se mulțumește cu atât, ci, pe lângă faptul că el însuși nu-i primește &lt;i&gt;pe frați&lt;/i&gt;, îi și împiedică pe cei ce vor &lt;i&gt;să-i primească&lt;/i&gt; și-i dă afară din biserică.');
INSERT INTO &quot;verses&quot; VALUES (710,1,11,'&lt;pb/&gt;Preaiubitule, nu&lt;f&gt;ⓔ&lt;/f&gt; lua ca exemplu răul&lt;f&gt;[2]&lt;/f&gt;, ci binele! Cine&lt;f&gt;ⓕ&lt;/f&gt; face binele este din Dumnezeu; cine face răul nu L-a văzut pe Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (710,1,12,'Despre&lt;f&gt;ⓖ&lt;/f&gt; Dimitrie dau toți — inclusiv adevărul — o &lt;i&gt;bună&lt;/i&gt; mărturie; noi de asemenea dăm mărturie, și&lt;f&gt;ⓗ&lt;/f&gt; știi că mărturia noastră este adevărată.');
INSERT INTO &quot;verses&quot; VALUES (710,1,13,'&lt;pb/&gt;Aș avea multe să-ți&lt;f&gt;ⓘ&lt;/f&gt; scriu, dar nu vreau să ți le scriu cu cerneală și condei.');
INSERT INTO &quot;verses&quot; VALUES (710,1,14,'Nădăjduiesc să te văd &lt;i&gt;cât mai&lt;/i&gt; curând, și atunci vom vorbi gură către gură.');
INSERT INTO &quot;verses&quot; VALUES (710,1,15,'&lt;pb/&gt;Pacea să fie cu tine! Prietenii îți trimit salutări. Salută-i pe prieteni, pe fiecare în parte! [Amin!]&lt;f&gt;[3]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (720,1,1,'&lt;pb/&gt;Iuda, rob al lui Isus Hristos și fratele&lt;f&gt;ⓐ&lt;/f&gt; lui Iacov, către cei&lt;f&gt;ⓑ&lt;/f&gt; chemați, care sunt iubiți de Dumnezeu Tatăl și păstrați&lt;f&gt;ⓒ&lt;/f&gt; pentru Isus Hristos&lt;f&gt;[1]&lt;/f&gt;:');
INSERT INTO &quot;verses&quot; VALUES (720,1,2,'Îndurarea, pacea&lt;f&gt;ⓓ&lt;/f&gt; și dragostea să vă fie înmulțite!');
INSERT INTO &quot;verses&quot; VALUES (720,1,3,'&lt;pb/&gt;Preaiubiților, pe când căutam cu tot dinadinsul să vă scriu despre mântuirea&lt;f&gt;ⓔ&lt;/f&gt; de care avem parte cu toții, m-am văzut nevoit să vă scriu ca&lt;f&gt;ⓕ&lt;/f&gt; să vă îndemn să luptați pentru credința care le-a fost dată sfinților o dată pentru totdeauna.');
INSERT INTO &quot;verses&quot; VALUES (720,1,4,'Căci&lt;f&gt;ⓖ&lt;/f&gt; s-au strecurat &lt;i&gt;printre voi&lt;/i&gt; unii oameni care de mult sunt rânduiți&lt;f&gt;ⓗ&lt;/f&gt; pentru osânda aceasta, oameni neevlavioși, care preschimbă&lt;f&gt;ⓘ&lt;/f&gt; în desfrânare harul&lt;f&gt;ⓙ&lt;/f&gt; Dumnezeului nostru și se leapădă&lt;f&gt;ⓚ&lt;/f&gt; de singurul nostru Stăpân și Domn, Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (720,1,5,'&lt;pb/&gt;Vreau să vă aduc aminte, măcar că știți toate acestea, că Isus&lt;f&gt;[2]&lt;/f&gt;, după&lt;f&gt;ⓛ&lt;/f&gt; ce Și-a izbăvit mai întâi poporul din țara Egiptului, i-a&lt;f&gt;ⓜ&lt;/f&gt; nimicit apoi pe cei care&lt;f&gt;ⓝ&lt;/f&gt; n-au crezut.');
INSERT INTO &quot;verses&quot; VALUES (720,1,6,'Pe&lt;f&gt;ⓞ&lt;/f&gt; îngerii care nu și-au&lt;f&gt;ⓟ&lt;/f&gt; păstrat vrednicia&lt;f&gt;[3]&lt;/f&gt;, ci și-au părăsit locuința, El i-a păstrat pentru judecata zilei celei mari puși în lanțuri veșnice, în întuneric.');
INSERT INTO &quot;verses&quot; VALUES (720,1,7,'Tot astfel, Sodoma&lt;f&gt;ⓠ&lt;/f&gt; și Gomora și cetățile dimprejurul lor, care, la fel ca &lt;i&gt;îngerii&lt;/i&gt; aceia, s-au dedat la desfrâu și au poftit trupul altuia&lt;f&gt;[4]&lt;/f&gt;, ne stau înainte ca o pildă, fiindcă au suferit pedeapsa unui foc veșnic.');
INSERT INTO &quot;verses&quot; VALUES (720,1,8,'&lt;pb/&gt;La fel&lt;f&gt;ⓡ&lt;/f&gt; și &lt;i&gt;oamenii&lt;/i&gt; aceștia, târâți de visările lor, își pângăresc trupul, nesocotesc stăpânirea și-i batjocoresc&lt;f&gt;ⓢ&lt;/f&gt; pe cei slăviți&lt;f&gt;[5]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (720,1,9,'Arhanghelul Mihail&lt;f&gt;ⓣ&lt;/f&gt;, în disputa lui cu Diavolul, când se certau cu privire la trupul lui Moise, n-a îndrăznit&lt;f&gt;ⓤ&lt;/f&gt; să rostească împotriva lui o judecată de ocară, ci a zis: &lt;e&gt;Domnul să te mustre!&lt;/e&gt;&lt;f&gt;[6]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (720,1,10,'Aceștia&lt;f&gt;ⓥ&lt;/f&gt;, dimpotrivă, batjocoresc ce nu cunosc; cât despre lucrurile pe care le înțeleg — numai în chip firesc&lt;f&gt;[7]&lt;/f&gt;, ca niște dobitoace necuvântătoare&lt;f&gt;[8]&lt;/f&gt; — &lt;i&gt;tocmai&lt;/i&gt; acestea le aduc pierzarea.');
INSERT INTO &quot;verses&quot; VALUES (720,1,11,'Vai de ei, căci au apucat-o pe calea lui&lt;f&gt;ⓦ&lt;/f&gt; Cain! Pentru câștig, s-au aruncat&lt;f&gt;ⓧ&lt;/f&gt; în rătăcirea lui Balaam! Au pierit în răscoala&lt;f&gt;ⓨ&lt;/f&gt; lui Core!');
INSERT INTO &quot;verses&quot; VALUES (720,1,12,'Sunt&lt;f&gt;ⓩ&lt;/f&gt; niște stânci ascunse la agapele&lt;f&gt;ⓐ&lt;/f&gt; voastre, unde se ospătează fără sfială fiindcă le pasă doar de ei înșiși; niște nori&lt;f&gt;ⓑ&lt;/f&gt; fără apă, duși de vânt&lt;f&gt;ⓒ&lt;/f&gt; încoace și încolo; niște pomi tomnatici fără rod, de două ori morți, dezrădăcinați&lt;f&gt;ⓓ&lt;/f&gt;;');
INSERT INTO &quot;verses&quot; VALUES (720,1,13,'niște valuri&lt;f&gt;ⓔ&lt;/f&gt; înfuriate ale mării, care își spumegă&lt;f&gt;ⓕ&lt;/f&gt; rușinile lor; niște stele rătăcitoare, cărora&lt;f&gt;ⓖ&lt;/f&gt; le este păstrată pe vecie bezna de nepătruns&lt;f&gt;[9]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (720,1,14,'&lt;pb/&gt;Și pentru ei a profețit Enoh, al&lt;f&gt;ⓗ&lt;/f&gt; șaptelea &lt;i&gt;patriarh&lt;/i&gt; de la Adam, când a zis: „Iată, a venit&lt;f&gt;ⓘ&lt;/f&gt; Domnul cu zecile de mii de sfinți ai Săi');
INSERT INTO &quot;verses&quot; VALUES (720,1,15,'ca să facă judecată împotriva tuturor și să încredințeze&lt;f&gt;[10]&lt;/f&gt; orice suflet de toate faptele&lt;f&gt;ⓙ&lt;/f&gt; nelegiuite pe care le-au făcut în chip nelegiuit &lt;i&gt;acești&lt;/i&gt; păcătoși nelegiuiți și de toate &lt;i&gt;cuvintele&lt;/i&gt; aspre pe care le-au rostit împotriva Lui.”&lt;f&gt;[11]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (720,1,16,'Ei sunt niște cârtitori, nemulțumiți cu soarta lor; trăiesc după poftele lor, gura&lt;f&gt;ⓚ&lt;/f&gt; le este plină de vorbe trufașe și se arată&lt;f&gt;ⓛ&lt;/f&gt; părtinitori față de oameni pentru câștig.');
INSERT INTO &quot;verses&quot; VALUES (720,1,17,'&lt;pb/&gt;Dar&lt;f&gt;ⓜ&lt;/f&gt; voi, preaiubiților, aduceți-vă aminte de cuvintele vestite mai dinainte de apostolii Domnului nostru Isus Hristos,');
INSERT INTO &quot;verses&quot; VALUES (720,1,18,'cum vă spuneau că în vremea din urmă vor&lt;f&gt;ⓝ&lt;/f&gt; fi batjocoritori care vor trăi după poftele lor nelegiuite!');
INSERT INTO &quot;verses&quot; VALUES (720,1,19,'Aceștia sunt cei care&lt;f&gt;ⓞ&lt;/f&gt; dau naștere la dezbinări, oameni&lt;f&gt;ⓟ&lt;/f&gt; firești&lt;f&gt;[12]&lt;/f&gt;, care n-au Duhul.');
INSERT INTO &quot;verses&quot; VALUES (720,1,20,'&lt;pb/&gt;Dar voi, preaiubiților, zidiți-vă&lt;f&gt;ⓠ&lt;/f&gt; sufletește pe credința voastră preasfântă, rugați-vă&lt;f&gt;ⓡ&lt;/f&gt; prin Duhul Sfânt,');
INSERT INTO &quot;verses&quot; VALUES (720,1,21,'țineți-vă în dragostea lui Dumnezeu și așteptați&lt;f&gt;ⓢ&lt;/f&gt; îndurarea Domnului nostru Isus Hristos pentru viața veșnică!');
INSERT INTO &quot;verses&quot; VALUES (720,1,22,'Fie-vă milă de cei cuprinși de îndoieli;');
INSERT INTO &quot;verses&quot; VALUES (720,1,23,'pe unii căutați să-i mântuiți&lt;f&gt;ⓣ&lt;/f&gt; smulgându-i&lt;f&gt;ⓤ&lt;/f&gt; din foc; de alții fie-vă milă cu frică&lt;f&gt;[13]&lt;/f&gt;, urând până și cămașa&lt;f&gt;ⓥ&lt;/f&gt; întinată de trupul &lt;i&gt;lor&lt;/i&gt;!');
INSERT INTO &quot;verses&quot; VALUES (720,1,24,'&lt;pb/&gt;Iar&lt;f&gt;ⓦ&lt;/f&gt; ale Aceluia care poate să vă păzească de orice cădere și să vă facă să&lt;f&gt;ⓧ&lt;/f&gt; vă înfățișați fără cusur și plini de bucurie înaintea slavei Sale,');
INSERT INTO &quot;verses&quot; VALUES (720,1,25,'ale singurului&lt;f&gt;ⓨ&lt;/f&gt; Dumnezeu, Mântuitorul nostru, prin Isus Hristos, Domnul nostru, sunt slava, măreția, puterea și autoritatea mai înainte de toți vecii, și acum, și în veci! Amin!');
INSERT INTO &quot;verses&quot; VALUES (730,1,1,'&lt;pb/&gt;Descoperirea&lt;f&gt;[1]&lt;/f&gt; lui Isus Hristos, pe care I-a dat-o Dumnezeu ca să le arate robilor Săi ceea ce trebuie să se întâmple&lt;f&gt;ⓐ&lt;/f&gt; în curând. Și le-a făcut-o cunoscută trimițând-o&lt;f&gt;ⓑ&lt;/f&gt; prin îngerul Său la robul Său Ioan,');
INSERT INTO &quot;verses&quot; VALUES (730,1,2,'care&lt;f&gt;ⓒ&lt;/f&gt; a mărturisit despre Cuvântul lui Dumnezeu și despre mărturia lui Isus Hristos, anume tot&lt;f&gt;ⓓ&lt;/f&gt; ce a văzut.');
INSERT INTO &quot;verses&quot; VALUES (730,1,3,'Ferice&lt;f&gt;ⓔ&lt;/f&gt; de cine citește și de cei ce ascultă cuvintele profeției și păzesc lucrurile scrise în ea! Căci vremea&lt;f&gt;ⓕ&lt;/f&gt; este aproape.');
INSERT INTO &quot;verses&quot; VALUES (730,1,4,'&lt;pb/&gt;Ioan, către cele șapte biserici care sunt în Asia&lt;f&gt;[2]&lt;/f&gt;: Har și pace vouă din partea Celui&lt;f&gt;ⓖ&lt;/f&gt; care este, care era&lt;f&gt;ⓗ&lt;/f&gt; și care vine și&lt;f&gt;ⓘ&lt;/f&gt; din partea celor șapte duhuri care stau înaintea tronului Său');
INSERT INTO &quot;verses&quot; VALUES (730,1,5,'și din partea lui Isus Hristos, Martorul&lt;f&gt;ⓙ&lt;/f&gt; credincios, Cel Întâi Născut&lt;f&gt;ⓚ&lt;/f&gt; dintre cei morți, Căpetenia&lt;f&gt;ⓛ&lt;/f&gt; împăraților pământului! &lt;pb/&gt;Ale Aceluia care ne iubește&lt;f&gt;ⓜ&lt;/f&gt;, care ne-a&lt;f&gt;ⓝ&lt;/f&gt; eliberat&lt;f&gt;[3]&lt;/f&gt; de păcatele noastre prin sângele Său');
INSERT INTO &quot;verses&quot; VALUES (730,1,6,'și care a făcut&lt;f&gt;ⓞ&lt;/f&gt; din noi o împărăție și preoți pentru Dumnezeu, Tatăl Său, ale Lui&lt;f&gt;ⓟ&lt;/f&gt; să fie slava și stăpânirea în vecii vecilor! Amin!');
INSERT INTO &quot;verses&quot; VALUES (730,1,7,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Iată&lt;/e&gt; &lt;f&gt;ⓠ&lt;/f&gt; &lt;e&gt;că El vine pe nori.&lt;/e&gt;&lt;f&gt;[4]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;Și orice ochi Îl &lt;e&gt;va vedea&lt;/e&gt;;&lt;/t&gt; &lt;t&gt;și cei&lt;f&gt;ⓡ&lt;/f&gt; ce L-&lt;e&gt;au străpuns&lt;/e&gt;.&lt;/t&gt; &lt;t&gt;Și toate semințiile pământului &lt;e&gt;se vor boci din pricina Lui&lt;/e&gt;&lt;f&gt;[5]&lt;/f&gt;.&lt;/t&gt; &lt;t&gt;Da, amin!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,1,8,'&lt;pb/&gt;&lt;J&gt;„Eu&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;sunt Alfa și Omega, [Începutul și Sfârșitul]”&lt;/J&gt;, zice Domnul Dumnezeu, Cel care&lt;f&gt;ⓣ&lt;/f&gt; este, care era și care vine, Cel Atotputernic.');
INSERT INTO &quot;verses&quot; VALUES (730,1,9,'&lt;pb/&gt;Eu, Ioan, fratele vostru, părtaș&lt;f&gt;ⓤ&lt;/f&gt; cu voi la&lt;f&gt;ⓥ&lt;/f&gt; necaz, la Împărăție și la răbdarea în Isus [Hristos], mă aflam în insula care se cheamă Patmos, din pricina Cuvântului lui Dumnezeu și din pricina mărturiei lui Isus [Hristos].');
INSERT INTO &quot;verses&quot; VALUES (730,1,10,'În ziua&lt;f&gt;ⓦ&lt;/f&gt; Domnului am fost luat în Duhul&lt;f&gt;ⓧ&lt;/f&gt; și am auzit în spatele meu un glas&lt;f&gt;ⓨ&lt;/f&gt; puternic, ca de trâmbiță,');
INSERT INTO &quot;verses&quot; VALUES (730,1,11,'care zicea: &lt;J&gt;„[Eu sunt Alfa și Omega, Cel-dintâi și Cel-de-pe-urmă.] Ce vezi, scrie într-o carte și trimite-o celor șapte biserici: Efes, Smirna, Pergam, Tiatira, Sardes, Filadelfia și Laodiceea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,1,12,'&lt;pb/&gt;M-am întors să văd glasul care vorbea cu mine. Și când m-am întors, am&lt;f&gt;ⓩ&lt;/f&gt; văzut șapte sfeșnice de aur.');
INSERT INTO &quot;verses&quot; VALUES (730,1,13,'Și&lt;f&gt;ⓐ&lt;/f&gt; în mijlocul celor șapte sfeșnice, &lt;i&gt;pe cineva&lt;/i&gt; care semăna cu un fiu&lt;f&gt;ⓑ&lt;/f&gt; al omului, îmbrăcat&lt;f&gt;ⓒ&lt;/f&gt; cu o haină lungă până la picioare și încins&lt;f&gt;ⓓ&lt;/f&gt; la piept cu un brâu de aur.');
INSERT INTO &quot;verses&quot; VALUES (730,1,14,'Capul și părul&lt;f&gt;ⓔ&lt;/f&gt; Lui erau albe ca lâna albă, ca zăpada, ochii&lt;f&gt;ⓕ&lt;/f&gt; Lui erau ca para focului,');
INSERT INTO &quot;verses&quot; VALUES (730,1,15,'picioarele&lt;f&gt;ⓖ&lt;/f&gt; Lui erau ca arama aprinsă și arsă în cuptor, iar glasul&lt;f&gt;ⓗ&lt;/f&gt; Lui era ca vuietul multor ape.');
INSERT INTO &quot;verses&quot; VALUES (730,1,16,'În&lt;f&gt;ⓘ&lt;/f&gt; mâna dreaptă ținea șapte stele, din&lt;f&gt;ⓙ&lt;/f&gt; gura Lui ieșea o sabie ascuțită, cu două tăișuri, iar&lt;f&gt;ⓚ&lt;/f&gt; fața Lui era ca soarele când strălucește în toată puterea lui.');
INSERT INTO &quot;verses&quot; VALUES (730,1,17,'&lt;pb/&gt;Când L-am văzut&lt;f&gt;ⓛ&lt;/f&gt;, am căzut la picioarele Lui ca mort. El Și-a pus&lt;f&gt;ⓜ&lt;/f&gt; mâna dreaptă peste mine și mi-a zis: &lt;J&gt;„Nu te teme! Eu&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;sunt Cel-dintâi și Cel-de-pe-urmă,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,1,18,'&lt;J&gt;Cel&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;Viu. Am fost mort, dar iată că sunt viu&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;în vecii vecilor. Eu țin&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;cheile morții și ale Locuinței Morților.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,1,19,'&lt;J&gt;Scrie, așadar, lucrurile pe care le-ai văzut: lucrurile&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;care sunt și cele care&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;vor urma după ele!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,1,20,'&lt;J&gt;Taina celor șapte stele pe care le-ai văzut în mâna Mea dreaptă și a celor șapte sfeșnice de aur &lt;i&gt;este aceasta&lt;/i&gt;: cele șapte stele sunt îngerii&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;celor șapte biserici și cele&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;șapte sfeșnice sunt șapte biserici.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,1,'&lt;pb/&gt;&lt;J&gt;Îngerului bisericii din Efes scrie-i:&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;«Iată ce zice Cel&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;ce ține cele șapte stele în mâna dreaptă, Cel ce umblă&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;prin mijlocul celor șapte sfeșnice de aur:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,2,'&lt;J&gt;‘Știu&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;faptele tale, osteneala ta și răbdarea ta și că nu-i poți suferi pe cei răi, că i-ai&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;pus la încercare pe cei ce&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;zic că sunt apostoli și nu sunt și i-ai găsit mincinoși,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,3,'&lt;J&gt;că ai răbdare, că ai suferit din pricina Numelui Meu și că n-ai&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;obosit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,4,'&lt;J&gt;Dar am ceva împotriva ta: &lt;i&gt;și anume&lt;/i&gt; că ți-ai părăsit dragostea dintâi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,5,'&lt;J&gt;Adu-ți deci aminte de unde ai căzut, pocăiește-te și întoarce-te la faptele tale dintâi! Dacă nu, voi veni la tine și-ți voi lua sfeșnicul din locul lui, dacă nu te pocăiești.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,6,'&lt;J&gt;Ai însă lucrul acesta bun — că urăști faptele nicolaiților, pe care și Eu le urăsc.’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,7,'&lt;pb/&gt;&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;are urechi să asculte ce le zice bisericilor Duhul. «Celui ce va birui îi voi da să&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;mănânce din&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;pomul vieții, care este în raiul lui Dumnezeu.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,8,'&lt;pb/&gt;&lt;J&gt;Îngerului bisericii din Smirna scrie-i:&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;«Iată ce zice Cel-dintâi&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;și Cel-de-pe-urmă, Cel ce a fost mort și a înviat:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,9,'&lt;J&gt;‘Știu necazul tău și sărăcia ta (dar ești bogat&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt;&lt;J&gt;) și batjocura din partea celor&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;ce zic că sunt iudei, dar nu sunt, ci&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;sunt o sinagogă a Satanei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,10,'&lt;J&gt;Nu te teme nicidecum&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;de ceea ce vei suferi! Iată că Diavolul îi va arunca în temniță pe unii dintre voi, ca să vă încerce, și veți avea un necaz de zece zile. Fii&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;credincios până la moarte, și-ți voi da cununa&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;vieții!’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,11,'&lt;pb/&gt;&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;are urechi să asculte ce le zice bisericilor Duhul. «Cel ce va birui nicidecum nu va fi vătămat de a&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;doua moarte.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,12,'&lt;pb/&gt;&lt;J&gt;Îngerului bisericii din Pergam scrie-i:&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;«Iată ce zice Cel&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;ce are sabia ascuțită, cu două tăișuri:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,13,'&lt;J&gt;‘Știu unde locuiești: acolo unde este tronul Satanei. Tu ții Numele Meu și n-ai lepădat credința în Mine nici chiar în zilele lui Antipa, martorul Meu credincios, care a fost ucis la voi, acolo unde locuiește Satana.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,14,'&lt;J&gt;Dar am câteva lucruri împotriva ta: că ai acolo &lt;i&gt;pe unii&lt;/i&gt; care țin învățătura lui Balaam&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt;&lt;J&gt;, care l-a învățat pe Balac să pună o piatră de poticnire înaintea fiilor lui Israel, ca să&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;mănânce din carnea jertfită idolilor și&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;să se dedea la desfrâu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,15,'&lt;J&gt;Tot așa, și tu ai câțiva care țin învățătura nicolaiților.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,16,'&lt;J&gt;Așadar, pocăiește-te! Dacă nu, voi veni la tine curând și&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;Mă voi război cu ei cu sabia gurii Mele.’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,17,'&lt;pb/&gt;&lt;J&gt;Cine are urechi să asculte ce le zice bisericilor Duhul. «Celui ce va birui îi voi da din mana ascunsă și-i voi da o piatră albă; și pe piatra aceasta este scris un nume&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;nou, pe care nu-l știe nimeni decât acela care-l primește.»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,18,'&lt;pb/&gt;&lt;J&gt;Îngerului bisericii din Tiatira scrie-i:&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;«Iată ce zice Fiul lui Dumnezeu, care&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;are ochii ca para focului și picioarele ca arama aprinsă:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,19,'&lt;J&gt;‘Cunosc faptele tale și dragostea, credința, slujirea și răbdarea ta și &lt;i&gt;știu&lt;/i&gt; că faptele tale de pe urmă sunt mai multe decât cele dintâi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,20,'&lt;J&gt;Dar am ceva împotriva ta: &lt;i&gt;și anume&lt;/i&gt; că o tolerezi pe Izabela&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt;&lt;J&gt;, femeia &lt;i&gt;aceea&lt;/i&gt; care își zice profetesă și care îi duce în rătăcire pe robii Mei și îi învață să&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;se dedea la desfrâu și să mănânce din carnea jertfită idolilor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,21,'&lt;J&gt;I-am dat vreme să&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;se pocăiască, dar nu vrea să se pocăiască de desfrâul ei.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,22,'&lt;J&gt;Iată că o voi arunca &lt;i&gt;bolnavă&lt;/i&gt; la pat, iar pe cei ce săvârșesc adulter cu ea — într-un necaz mare, dacă nu se pocăiesc de faptele lor.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,23,'&lt;J&gt;Pe copiii ei îi voi lovi cu moarte, și toate bisericile vor ști că Eu&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;sunt Cel ce cercetează rărunchii și inima: și îi voi&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;da fiecăruia dintre voi după faptele sale.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,24,'&lt;J&gt;Însă vouă celorlalți din Tiatira care nu aveți învățătura aceasta și n-ați cunoscut ‹adâncimile Satanei›, cum le numesc ei, vă zic: Nu&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt; &lt;J&gt;pun peste voi altă greutate.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,25,'&lt;J&gt;Numai țineți &lt;i&gt;cu tărie&lt;/i&gt; ce&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;aveți, până voi veni!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,26,'&lt;pb/&gt;&lt;J&gt;Celui ce va birui și celui ce va păzi până la sfârșit lucrările Mele îi voi da&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;autoritate peste neamuri&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,27,'&lt;pb/&gt;&lt;t&gt;&lt;J&gt;(&lt;i&gt;așa încât&lt;/i&gt; &lt;e&gt;le va păstori cu un toiag de fier&lt;/J&gt;&lt;/e&gt; &lt;f&gt;ⓒ&lt;/f&gt;&lt;/t&gt; &lt;t&gt;&lt;J&gt;&lt;e&gt;și le va zdrobi ca pe niște vase de lut&lt;/e&gt;)&lt;/J&gt;&lt;f&gt;[6]&lt;/f&gt; &lt;J&gt;,&lt;/J&gt;&lt;/t&gt; &lt;pb/&gt;&lt;J&gt;așa cum am primit și Eu de la Tatăl Meu;&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,28,'&lt;J&gt;și-i voi da luceafărul&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;zorilor.’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,2,29,'&lt;pb/&gt;&lt;J&gt;Cine are urechi să asculte ce le zice bisericilor Duhul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,1,'&lt;pb/&gt;&lt;J&gt;Îngerului bisericii din Sardes scrie-i:&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;«Iată ce zice Cel ce&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;are cele șapte Duhuri ale lui Dumnezeu și cele șapte stele: ‘Știu&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;faptele tale: că îți merge numele că trăiești, dar&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;ești mort.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,2,'&lt;J&gt;Veghează și întărește ce a rămas, care e pe moarte, căci n-am găsit faptele tale desăvârșite înaintea Dumnezeului Meu!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,3,'&lt;J&gt;Adu-ți&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt; &lt;J&gt;aminte deci cum ai primit și auzit! Ține și pocăiește-te! Dacă&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;nu veghezi, voi veni ca un hoț și nu vei ști în care ceas voi veni peste tine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,4,'&lt;J&gt;Totuși, ai în Sardes câteva&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;nume care nu și-au mânjit hainele&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt;&lt;J&gt;. Aceștia vor umbla împreună cu Mine, îmbrăcați în&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;alb, fiindcă sunt vrednici.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,5,'&lt;pb/&gt;&lt;J&gt;Cel ce va birui va fi&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;îmbrăcat astfel în haine albe. Nu-i voi șterge&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;nicidecum numele din cartea&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;vieții și voi&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;mărturisi numele lui înaintea Tatălui Meu și înaintea îngerilor Lui.’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,6,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;are urechi să asculte ce le zice bisericilor Duhul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,7,'&lt;pb/&gt;&lt;J&gt;Îngerului bisericii din Filadelfia scrie-i:&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;«Iată ce zice Cel Sfânt, Cel&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;Adevărat, Cel ce ține cheia&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;lui David, Cel ce&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;deschide și nimeni nu va închide, Cel ce închide și nimeni nu va deschide:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,8,'&lt;J&gt;‘Știu faptele tale: iată, ți-am pus înainte o ușă deschisă pe care nimeni n-o poate închide, căci ai puțină putere, dar ai păzit Cuvântul Meu și n-ai tăgăduit Numele Meu.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,9,'&lt;J&gt;Iată că îți dau&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;din cei ce sunt în sinagoga Satanei, care zic că sunt iudei și nu sunt, ci mint; iată că îi&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;voi face să vină și să se închine la picioarele tale, ca să știe că te-am iubit.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,10,'&lt;J&gt;Fiindcă ai păzit porunca Mea de a avea răbdare, te&lt;/J&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;J&gt;voi păzi și Eu în&lt;/J&gt;&lt;f&gt;[7]&lt;/f&gt; &lt;J&gt;ceasul încercării care va veni peste lumea întreagă, ca să-i încerce pe locuitorii pământului.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,11,'&lt;J&gt;Eu vin&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;curând. Păstrează&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;ce ai, pentru ca nimeni să nu-ți ia cununa&lt;/J&gt;&lt;f&gt;ⓩ&lt;/f&gt;&lt;J&gt;!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,12,'&lt;pb/&gt;&lt;J&gt;Pe cel ce va birui îl voi face un stâlp&lt;/J&gt;&lt;f&gt;ⓐ&lt;/f&gt; &lt;J&gt;în Templul Dumnezeului Meu și cu niciun chip nu va mai ieși din el. Voi&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt; &lt;J&gt;scrie pe el Numele Dumnezeului Meu și numele cetății Dumnezeului Meu, Noul Ierusalim&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt;&lt;J&gt;, care coboară din cer, de la Dumnezeul Meu, și&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;Numele Meu cel nou.’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,13,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓔ&lt;/f&gt; &lt;J&gt;are urechi să asculte ce le zice bisericilor Duhul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,14,'&lt;pb/&gt;&lt;J&gt;Îngerului bisericii din Laodiceea scrie-i:&lt;/J&gt; &lt;pb/&gt;&lt;J&gt;«Iată&lt;/J&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;J&gt;ce zice Cel ce este Amin, Martorul&lt;/J&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;J&gt;credincios și adevărat, Începutul&lt;/J&gt;&lt;f&gt;[8]&lt;/f&gt; &lt;J&gt;zidirii lui Dumnezeu:&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,15,'&lt;J&gt;‘Știu faptele tale: că nu ești nici rece, nici în clocot. O, dacă ai fi rece sau în clocot!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,16,'&lt;J&gt;Dar, fiindcă ești căldicel, nici rece, nici în clocot, am să te vărs din gura Mea.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,17,'&lt;J&gt;Pentru că zici: ‹Sunt bogat&lt;/J&gt;&lt;f&gt;ⓗ&lt;/f&gt;&lt;J&gt;, m-am îmbogățit și nu duc lipsă de nimic›, și nu știi că ești nenorocit, vrednic de milă, sărac, orb și gol,&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,18,'&lt;J&gt;te sfătuiesc să&lt;/J&gt;&lt;f&gt;ⓘ&lt;/f&gt; &lt;J&gt;cumperi de la Mine aur lămurit în foc, ca să te îmbogățești, și haine&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;albe, ca să te îmbraci cu ele și să nu ți se vadă rușinea goliciunii tale, și colir, ca să-ți ungi ochii și să vezi.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,19,'&lt;J&gt;Eu îi mustru&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;și îi pedepsesc pe toți aceia pe care-i iubesc. Fii plin de râvnă deci și pocăiește-te!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,20,'&lt;J&gt;Iată, Eu stau&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;la ușă și bat. Dacă&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;aude cineva glasul Meu și deschide ușa, voi&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;intra la el, voi cina cu el, și el, cu Mine.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,21,'&lt;pb/&gt;&lt;J&gt;Celui ce va birui îi&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;voi da să stea cu Mine pe tronul Meu, după cum și Eu am biruit și am stat cu Tatăl Meu pe tronul Lui.’»&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,3,22,'&lt;J&gt;Cine&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;are urechi să asculte ce le zice bisericilor Duhul.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,4,1,'&lt;pb/&gt;După aceste lucruri, m-am uitat și iată că o ușă &lt;i&gt;era&lt;/i&gt; deschisă în cer. Primul glas&lt;f&gt;ⓠ&lt;/f&gt;, ca de trâmbiță, pe care-l auzisem vorbind cu mine, mi-a zis: &lt;J&gt;„Suie-te&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt; &lt;J&gt;aici și-ți&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;voi arăta ce trebuie să se întâmple după aceste lucruri!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,4,2,'&lt;pb/&gt;Îndată am&lt;f&gt;ⓣ&lt;/f&gt; fost luat în Duhul. Și iată că în cer era așezat un tron&lt;f&gt;ⓤ&lt;/f&gt;, și pe tron stătea Cineva.');
INSERT INTO &quot;verses&quot; VALUES (730,4,3,'Cel ce stătea &lt;i&gt;pe el&lt;/i&gt; avea înfățișarea pietrei de iaspis&lt;f&gt;[9]&lt;/f&gt; și de sardiu, iar tronul era înconjurat de un curcubeu&lt;f&gt;ⓥ&lt;/f&gt; care avea înfățișarea pietrei de smarald.');
INSERT INTO &quot;verses&quot; VALUES (730,4,4,'Împrejurul&lt;f&gt;ⓦ&lt;/f&gt; tronului &lt;i&gt;erau&lt;/i&gt; douăzeci și patru de tronuri, și pe tronuri — douăzeci și patru de bătrâni îmbrăcați&lt;f&gt;ⓧ&lt;/f&gt; în haine albe, având pe capetele lor cununi de aur.');
INSERT INTO &quot;verses&quot; VALUES (730,4,5,'Din tron ieșeau fulgere&lt;f&gt;ⓨ&lt;/f&gt;, glasuri și tunete. Înaintea tronului ardeau&lt;f&gt;ⓩ&lt;/f&gt; șapte făclii de foc, care sunt cele&lt;f&gt;ⓐ&lt;/f&gt; șapte Duhuri ale lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (730,4,6,'Înaintea tronului &lt;i&gt;era&lt;/i&gt; ca o mare&lt;f&gt;ⓑ&lt;/f&gt; de sticlă, asemenea cristalului. În&lt;f&gt;ⓒ&lt;/f&gt; mijlocul tronului și împrejurul tronului &lt;i&gt;erau&lt;/i&gt; patru făpturi vii, pline cu ochi pe dinainte și pe dinapoi.');
INSERT INTO &quot;verses&quot; VALUES (730,4,7,'Prima&lt;f&gt;ⓓ&lt;/f&gt; făptură vie semăna cu un leu, a doua făptură — cu un vițel, a treia făptură avea chip ca de om, iar a patra făptură era asemenea unui vultur în zbor.');
INSERT INTO &quot;verses&quot; VALUES (730,4,8,'Fiecare dintre aceste patru făpturi vii avea câte șase&lt;f&gt;ⓔ&lt;/f&gt; aripi și era plină cu ochi de jur împrejur și pe dinăuntru. Zi și noapte, fără odihnă, zic: &lt;pb/&gt;&lt;t&gt;„&lt;e&gt;Sfânt, Sfânt, Sfânt este Domnul&lt;/e&gt;&lt;f&gt;ⓕ&lt;/f&gt; &lt;e&gt;Dumnezeu, Cel&lt;/e&gt;&lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;Atotputernic&lt;/e&gt;&lt;f&gt;[10]&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;care era, care este, care vine!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,4,9,'&lt;pb/&gt;Când făpturile vii Îi dau slavă, cinste și mulțumiri Celui ce stă pe tron și care&lt;f&gt;ⓗ&lt;/f&gt; este viu în vecii vecilor,');
INSERT INTO &quot;verses&quot; VALUES (730,4,10,'cei&lt;f&gt;ⓘ&lt;/f&gt; douăzeci și patru de bătrâni se aruncă înaintea Celui ce stă pe tron și se închină Celui ce este viu în vecii vecilor, își pun cununile jos înaintea tronului și zic:');
INSERT INTO &quot;verses&quot; VALUES (730,4,11,'&lt;pb/&gt;&lt;t&gt;„Vrednic&lt;f&gt;ⓙ&lt;/f&gt; ești Tu, Domnul și Dumnezeul nostru,&lt;/t&gt; &lt;t&gt;să primești slava, cinstea și puterea,&lt;/t&gt; &lt;t&gt;căci&lt;f&gt;ⓚ&lt;/f&gt; Tu ai creat toate lucrurile&lt;/t&gt; &lt;t&gt;și prin voia Ta sunt în ființă și au fost create!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,5,1,'&lt;pb/&gt;Apoi am văzut în mâna dreaptă a Celui așezat pe tron o&lt;f&gt;ⓛ&lt;/f&gt; carte&lt;f&gt;[11]&lt;/f&gt; scrisă pe dinăuntru și pe dinafară, pecetluită&lt;f&gt;ⓜ&lt;/f&gt; cu șapte peceți.');
INSERT INTO &quot;verses&quot; VALUES (730,5,2,'Și am văzut un înger puternic care striga cu glas tare: „Cine este vrednic să deschidă cartea și să-i rupă pecețile?”');
INSERT INTO &quot;verses&quot; VALUES (730,5,3,'Și nimeni, nici din cer&lt;f&gt;ⓝ&lt;/f&gt;, nici de pe pământ, nici de sub pământ, nu putea să deschidă cartea și să se uite în ea.');
INSERT INTO &quot;verses&quot; VALUES (730,5,4,'Și am plâns mult pentru că nimeni n-a fost găsit vrednic să deschidă cartea și să se uite în ea.');
INSERT INTO &quot;verses&quot; VALUES (730,5,5,'Și unul dintre bătrâni mi-a zis: „Nu plânge! Iată că Leul&lt;f&gt;ⓞ&lt;/f&gt; din seminția lui Iuda, Rădăcina&lt;f&gt;ⓟ&lt;/f&gt; lui David, a biruit ca să deschidă cartea și&lt;f&gt;ⓠ&lt;/f&gt; cele șapte peceți ale ei.”');
INSERT INTO &quot;verses&quot; VALUES (730,5,6,'&lt;pb/&gt;Și în mijlocul tronului și al celor patru făpturi vii și în mijlocul bătrânilor, am văzut stând în picioare un&lt;f&gt;ⓡ&lt;/f&gt; Miel. Părea înjunghiat și avea șapte coarne și șapte&lt;f&gt;ⓢ&lt;/f&gt; ochi, care sunt cele&lt;f&gt;ⓣ&lt;/f&gt; șapte Duhuri ale lui Dumnezeu, trimise pe tot pământul.');
INSERT INTO &quot;verses&quot; VALUES (730,5,7,'El a venit și a luat &lt;i&gt;cartea&lt;/i&gt; din mâna dreaptă a Celui așezat&lt;f&gt;ⓤ&lt;/f&gt; pe tron.');
INSERT INTO &quot;verses&quot; VALUES (730,5,8,'&lt;pb/&gt;Când a luat cartea, cele patru făpturi vii și cei&lt;f&gt;ⓥ&lt;/f&gt; douăzeci și patru de bătrâni au căzut înaintea Mielului, având fiecare câte o harfă&lt;f&gt;ⓦ&lt;/f&gt; și cupe de aur pline cu tămâie, care&lt;f&gt;ⓧ&lt;/f&gt; sunt rugăciunile sfinților.');
INSERT INTO &quot;verses&quot; VALUES (730,5,9,'Ei cântau&lt;f&gt;ⓨ&lt;/f&gt; o cântare nouă și ziceau: &lt;pb/&gt;&lt;t&gt;„Vrednic&lt;f&gt;ⓩ&lt;/f&gt; ești să iei cartea și să-i rupi pecețile,&lt;/t&gt; &lt;t&gt;căci ai fost înjunghiat&lt;/t&gt; &lt;t&gt;și ai&lt;f&gt;ⓐ&lt;/f&gt; răscumpărat pentru Dumnezeu, cu sângele Tău,&lt;/t&gt; &lt;t&gt;&lt;i&gt;oameni&lt;/i&gt; din&lt;f&gt;ⓑ&lt;/f&gt; orice seminție, de orice limbă,&lt;/t&gt; &lt;t&gt;din orice popor și de orice neam!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,5,10,'&lt;t&gt;Ai&lt;f&gt;ⓒ&lt;/f&gt; făcut din ei o împărăție și preoți&lt;/t&gt; &lt;t&gt;pentru Dumnezeul nostru;&lt;/t&gt; &lt;t&gt;ei vor împărăți pe pământ.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,5,11,'&lt;pb/&gt;M-am uitat și am auzit glasul multor îngeri în jurul&lt;f&gt;ⓓ&lt;/f&gt; tronului, al făpturilor vii și al bătrânilor. Numărul lor era de zece mii&lt;f&gt;ⓔ&lt;/f&gt; de ori zece mii și mii de mii.');
INSERT INTO &quot;verses&quot; VALUES (730,5,12,'Ei ziceau cu glas tare: &lt;pb/&gt;&lt;t&gt;„Vrednic&lt;f&gt;ⓕ&lt;/f&gt; este Mielul, care a fost înjunghiat, să primească&lt;/t&gt; &lt;t&gt;puterea, bogăția, înțelepciunea, tăria,&lt;/t&gt; &lt;t&gt;cinstea, slava și binecuvântarea!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,5,13,'&lt;pb/&gt;Și pe toate&lt;f&gt;ⓖ&lt;/f&gt; făpturile din cer, de pe pământ, de sub pământ și de pe mare — pe toate câte se află în aceste locuri — le-am auzit zicând: &lt;pb/&gt;&lt;t&gt;„Ale Celui ce&lt;f&gt;ⓗ&lt;/f&gt; stă pe tron și ale Mielului&lt;/t&gt; &lt;t&gt;să fie binecuvântarea&lt;f&gt;ⓘ&lt;/f&gt;, cinstea, slava și stăpânirea&lt;/t&gt; &lt;t&gt;în vecii vecilor!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,5,14,'&lt;pb/&gt;Și&lt;f&gt;ⓙ&lt;/f&gt; cele patru făpturi vii ziceau: „Amin!” Și cei douăzeci și patru de bătrâni s-au aruncat &lt;i&gt;la pământ&lt;/i&gt; și s-au închinat [Celui ce&lt;f&gt;ⓚ&lt;/f&gt; este viu în vecii vecilor].');
INSERT INTO &quot;verses&quot; VALUES (730,6,1,'&lt;pb/&gt;M-am uitat când a rupt Mielul prima din cele șapte peceți&lt;f&gt;ⓛ&lt;/f&gt; și am auzit-o pe prima&lt;f&gt;ⓜ&lt;/f&gt; din cele patru făpturi vii zicând cu glas ca de tunet: „Vino!”&lt;f&gt;[12]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,6,2,'M-am uitat și iată &lt;i&gt;că s-a arătat&lt;/i&gt; un&lt;f&gt;ⓝ&lt;/f&gt; cal alb. Cel ce&lt;f&gt;ⓞ&lt;/f&gt; stătea pe el avea un arc; i&lt;f&gt;ⓟ&lt;/f&gt; s-a dat o cunună și a pornit biruitor și ca să biruiască.');
INSERT INTO &quot;verses&quot; VALUES (730,6,3,'&lt;pb/&gt;Când a rupt &lt;i&gt;Mielul&lt;/i&gt; a doua pecete, am&lt;f&gt;ⓠ&lt;/f&gt; auzit-o pe a doua făptură vie zicând: „Vino!”');
INSERT INTO &quot;verses&quot; VALUES (730,6,4,'Și&lt;f&gt;ⓡ&lt;/f&gt; a ieșit un alt cal, roșu aprins. Celui ce stătea pe el i s-a dat să ia pacea de pe pământ, pentru ca &lt;i&gt;oamenii&lt;/i&gt; să se înjunghie unii pe alții; și i s-a dat o sabie mare.');
INSERT INTO &quot;verses&quot; VALUES (730,6,5,'&lt;pb/&gt;Când a rupt &lt;i&gt;Mielul&lt;/i&gt; pecetea a treia, am&lt;f&gt;ⓢ&lt;/f&gt; auzit-o pe a treia făptură vie zicând: „Vino!” M-am uitat și iată &lt;i&gt;că s-a arătat&lt;/i&gt; un&lt;f&gt;ⓣ&lt;/f&gt; cal negru. Cel ce stătea pe el avea în mână o cumpănă.');
INSERT INTO &quot;verses&quot; VALUES (730,6,6,'Și, din mijlocul celor patru făpturi vii, am auzit ca un glas care zicea: „O măsură de grâu pentru un dinar&lt;f&gt;[13]&lt;/f&gt; și trei măsuri de orz pentru un dinar! Dar să nu vatămi untdelemnul&lt;f&gt;ⓤ&lt;/f&gt; și vinul!”');
INSERT INTO &quot;verses&quot; VALUES (730,6,7,'&lt;pb/&gt;Când a rupt &lt;i&gt;Mielul&lt;/i&gt; pecetea a patra, am&lt;f&gt;ⓥ&lt;/f&gt; auzit glasul făpturii a patra zicând: „Vino!”');
INSERT INTO &quot;verses&quot; VALUES (730,6,8,'M-am uitat&lt;f&gt;ⓦ&lt;/f&gt; și iată că s-a arătat un cal galben-verzui. Cel ce stătea pe el se numea Moartea și în urma lui venea Locuința Morților. Li s-a dat autoritate peste a patra parte a pământului, ca să&lt;f&gt;ⓧ&lt;/f&gt; ucidă cu sabie, cu foamete, cu molimă și&lt;f&gt;ⓨ&lt;/f&gt; cu fiarele pământului.');
INSERT INTO &quot;verses&quot; VALUES (730,6,9,'&lt;pb/&gt;Când a rupt &lt;i&gt;Mielul&lt;/i&gt; pecetea a cincea, am văzut sub altar&lt;f&gt;ⓩ&lt;/f&gt; sufletele&lt;f&gt;ⓐ&lt;/f&gt; celor ce fuseseră înjunghiați din pricina&lt;f&gt;ⓑ&lt;/f&gt; Cuvântului lui Dumnezeu și din pricina mărturisirii&lt;f&gt;ⓒ&lt;/f&gt; pe care o făcuseră.');
INSERT INTO &quot;verses&quot; VALUES (730,6,10,'Ei strigau cu glas tare și ziceau: „Până&lt;f&gt;ⓓ&lt;/f&gt; când, Stăpâne&lt;f&gt;ⓔ&lt;/f&gt; sfânt și adevărat, &lt;i&gt;zăbovești&lt;/i&gt; să&lt;f&gt;ⓕ&lt;/f&gt; judeci și să răzbuni sângele nostru asupra locuitorilor pământului?”');
INSERT INTO &quot;verses&quot; VALUES (730,6,11,'Fiecăruia dintre ei i s-a dat o haină&lt;f&gt;ⓖ&lt;/f&gt; albă și li s-a spus să&lt;f&gt;ⓗ&lt;/f&gt; se mai odihnească puțină vreme, până vor ajunge la deplinătate&lt;f&gt;[14]&lt;/f&gt; tovarășii lor &lt;i&gt;de slujbă&lt;/i&gt; și frații lor, care aveau să fie omorâți ca și ei.');
INSERT INTO &quot;verses&quot; VALUES (730,6,12,'&lt;pb/&gt;M-am uitat când a rupt &lt;i&gt;Mielul&lt;/i&gt; pecetea a șasea și s-a făcut&lt;f&gt;ⓘ&lt;/f&gt; un mare cutremur. Soarele&lt;f&gt;ⓙ&lt;/f&gt; s-a făcut negru ca un sac de păr, luna s-a făcut toată ca sângele');
INSERT INTO &quot;verses&quot; VALUES (730,6,13,'și&lt;f&gt;ⓚ&lt;/f&gt; stelele au căzut din cer pe pământ cum cad smochinele târzii din pom când este scuturat de un vânt puternic.');
INSERT INTO &quot;verses&quot; VALUES (730,6,14,'Cerul&lt;f&gt;ⓛ&lt;/f&gt; s-a strâns ca o carte pe care o faci sul. Și toți&lt;f&gt;ⓜ&lt;/f&gt; munții și toate insulele s-au mutat din locurile lor.');
INSERT INTO &quot;verses&quot; VALUES (730,6,15,'Împărații pământului, mai-marii, căpitanii oștilor, cei bogați și cei puternici, toți robii și toți oamenii liberi s-au&lt;f&gt;ⓝ&lt;/f&gt; ascuns în peșteri și în stâncile munților.');
INSERT INTO &quot;verses&quot; VALUES (730,6,16,'&lt;e&gt;Și&lt;/e&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;e&gt;ziceau munților și stâncilor: „Cădeți peste noi și ascundeți-ne&lt;/e&gt;&lt;f&gt;[15]&lt;/f&gt; de Fața Celui ce stă pe tron și de mânia Mielului!');
INSERT INTO &quot;verses&quot; VALUES (730,6,17,'Căci&lt;f&gt;ⓟ&lt;/f&gt; a venit ziua cea mare a mâniei Lor și&lt;f&gt;ⓠ&lt;/f&gt; cine poate sta în picioare?”');
INSERT INTO &quot;verses&quot; VALUES (730,7,1,'&lt;pb/&gt;După aceea am văzut patru îngeri care stăteau în picioare în cele patru colțuri ale pământului. Ei țineau&lt;f&gt;ⓡ&lt;/f&gt; cele patru vânturi ale pământului, ca&lt;f&gt;ⓢ&lt;/f&gt; să nu sufle vânt pe pământ, nici pe mare, nici peste vreun copac.');
INSERT INTO &quot;verses&quot; VALUES (730,7,2,'Și am văzut un alt înger, care se suia dinspre răsăritul soarelui și care avea pecetea Dumnezeului celui viu. El a strigat cu glas tare către cei patru îngeri cărora le fusese dat să vatăme pământul și marea,');
INSERT INTO &quot;verses&quot; VALUES (730,7,3,'zicând: „Nu&lt;f&gt;ⓣ&lt;/f&gt; vătămați nici pământul, nici marea, nici copacii până nu punem pecetea&lt;f&gt;ⓤ&lt;/f&gt; pe fruntea&lt;f&gt;ⓥ&lt;/f&gt; slujitorilor Dumnezeului nostru!”');
INSERT INTO &quot;verses&quot; VALUES (730,7,4,'&lt;pb/&gt;Și&lt;f&gt;ⓦ&lt;/f&gt; am auzit numărul celor ce fuseseră pecetluiți: o sută patruzeci și patru de mii&lt;f&gt;ⓧ&lt;/f&gt;, pecetluiți din toate semințiile fiilor lui Israel:');
INSERT INTO &quot;verses&quot; VALUES (730,7,5,'&lt;pb/&gt;din seminția lui Iuda, pecetluiți douăsprezece mii; &lt;pb/&gt;din seminția lui Ruben, douăsprezece mii; &lt;pb/&gt;din seminția lui Gad, douăsprezece mii;');
INSERT INTO &quot;verses&quot; VALUES (730,7,6,'&lt;pb/&gt;din seminția lui Așer, douăsprezece mii; &lt;pb/&gt;din seminția lui Neftali, douăsprezece mii; &lt;pb/&gt;din seminția lui Manase, douăsprezece mii;');
INSERT INTO &quot;verses&quot; VALUES (730,7,7,'&lt;pb/&gt;din seminția lui Simeon, douăsprezece mii; &lt;pb/&gt;din seminția lui Levi, douăsprezece mii; &lt;pb/&gt;din seminția lui Isahar, douăsprezece mii;');
INSERT INTO &quot;verses&quot; VALUES (730,7,8,'&lt;pb/&gt;din seminția lui Zabulon, douăsprezece mii; &lt;pb/&gt;din seminția lui Iosif, douăsprezece mii; &lt;pb/&gt;din seminția lui Beniamin, pecetluiți douăsprezece mii.');
INSERT INTO &quot;verses&quot; VALUES (730,7,9,'&lt;pb/&gt;După aceea m-am uitat și iată că era o mare&lt;f&gt;ⓨ&lt;/f&gt; mulțime, pe care nu putea s-o numere nimeni, &lt;i&gt;cu oameni&lt;/i&gt; de&lt;f&gt;ⓩ&lt;/f&gt; orice neam, din orice seminție, din orice popor și de orice limbă, care stăteau în picioare înaintea tronului și înaintea Mielului, îmbrăcați&lt;f&gt;ⓐ&lt;/f&gt; în haine albe, cu ramuri de palmier în mâini,');
INSERT INTO &quot;verses&quot; VALUES (730,7,10,'strigând cu glas tare și zicând: &lt;pb/&gt;&lt;t&gt;„Mântuirea&lt;f&gt;ⓑ&lt;/f&gt; este a Dumnezeului nostru,&lt;/t&gt; &lt;t&gt;care&lt;f&gt;ⓒ&lt;/f&gt; stă pe tron,&lt;/t&gt; &lt;t&gt;și a Mielului!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,7,11,'&lt;pb/&gt;Și&lt;f&gt;ⓓ&lt;/f&gt; toți îngerii stăteau împrejurul tronului, al bătrânilor și al celor patru făpturi vii. Și s-au aruncat cu fața &lt;i&gt;la pământ&lt;/i&gt; înaintea tronului, s-au închinat lui Dumnezeu');
INSERT INTO &quot;verses&quot; VALUES (730,7,12,'și au zis&lt;f&gt;ⓔ&lt;/f&gt;: &lt;pb/&gt;&lt;t&gt;„Amin! Ale Dumnezeului nostru să fie&lt;/t&gt; &lt;t&gt;binecuvântarea, slava, înțelepciunea, mulțumirea,&lt;/t&gt; &lt;t&gt;cinstea, puterea și tăria în vecii vecilor! Amin!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,7,13,'&lt;pb/&gt;Și unul dintre bătrâni a luat cuvântul și mi-a zis: „Aceștia, care sunt îmbrăcați în haine&lt;f&gt;ⓕ&lt;/f&gt; albe, cine sunt oare? Și de unde au venit?”');
INSERT INTO &quot;verses&quot; VALUES (730,7,14,'„Domnul meu”, i-am răspuns eu, „tu știi.” Și el mi-a zis: &lt;pb/&gt;&lt;t&gt;„Aceștia&lt;f&gt;ⓖ&lt;/f&gt; sunt cei ce vin din necazul cel mare;&lt;/t&gt; &lt;t&gt;ei și-au spălat&lt;f&gt;ⓗ&lt;/f&gt; hainele&lt;/t&gt; &lt;t&gt;și le-au albit în sângele Mielului.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,7,15,'&lt;t&gt;De aceea stau ei înaintea tronului lui Dumnezeu&lt;/t&gt; &lt;t&gt;și-I slujesc zi și noapte în Templul Lui.&lt;/t&gt; &lt;t&gt;Cel ce stă pe tron Își va întinde&lt;f&gt;ⓘ&lt;/f&gt; cortul peste ei.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,7,16,'&lt;t&gt;&lt;e&gt;Nu le va mai fi foame, nu le va mai fi sete și&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nu-i va mai dogori nici soarele&lt;/e&gt; &lt;f&gt;ⓙ&lt;/f&gt; &lt;e&gt;,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;nici vreo altă arșiță.&lt;/e&gt; &lt;f&gt;[16]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,7,17,'&lt;t&gt;Căci Mielul, &lt;i&gt;care stă&lt;/i&gt; în mijlocul tronului,&lt;/t&gt; &lt;t&gt;îi va păstori&lt;f&gt;ⓚ&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;îi va conduce la izvoarele apelor vieții&lt;/t&gt; &lt;t&gt;și&lt;f&gt;ⓛ&lt;/f&gt; &lt;e&gt;Dumnezeu va șterge orice lacrimă din ochii lor&lt;/e&gt;.&lt;f&gt;[17]&lt;/f&gt;”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,8,1,'&lt;pb/&gt;Când a rupt&lt;f&gt;ⓜ&lt;/f&gt; Mielul pecetea a șaptea, s-a făcut în cer o tăcere de aproape o jumătate de ceas.');
INSERT INTO &quot;verses&quot; VALUES (730,8,2,'Și&lt;f&gt;ⓝ&lt;/f&gt; i-am văzut pe cei șapte îngeri care stau înaintea lui Dumnezeu; și&lt;f&gt;ⓞ&lt;/f&gt; li s-au dat șapte trâmbițe.');
INSERT INTO &quot;verses&quot; VALUES (730,8,3,'&lt;pb/&gt;&lt;i&gt;Apoi&lt;/i&gt; a venit un alt înger, care s-a oprit în fața altarului cu o cădelniță de aur. I s-a dat tămâie multă, ca s-o aducă, împreună cu rugăciunile&lt;f&gt;ⓟ&lt;/f&gt; tuturor sfinților, pe altarul&lt;f&gt;ⓠ&lt;/f&gt; de aur, care este înaintea tronului.');
INSERT INTO &quot;verses&quot; VALUES (730,8,4,'Fumul&lt;f&gt;ⓡ&lt;/f&gt; de tămâie s-a ridicat din mâna îngerului înaintea lui Dumnezeu, împreună cu rugăciunile sfinților.');
INSERT INTO &quot;verses&quot; VALUES (730,8,5,'Apoi, îngerul a luat cădelnița, a umplut-o cu foc de pe altar și l-a aruncat pe pământ. Și s-au&lt;f&gt;ⓢ&lt;/f&gt; stârnit tunete, glasuri, fulgere și un cutremur&lt;f&gt;ⓣ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (730,8,6,'&lt;pb/&gt;Și cei șapte îngeri care aveau cele șapte trâmbițe s-au pregătit să sune din ele.');
INSERT INTO &quot;verses&quot; VALUES (730,8,7,'&lt;pb/&gt;Primul &lt;i&gt;înger&lt;/i&gt; a sunat din trâmbiță. Și&lt;f&gt;ⓤ&lt;/f&gt; au venit grindină și foc amestecat cu sânge, care au fost aruncate pe&lt;f&gt;ⓥ&lt;/f&gt; pământ: și a treia parte din pământ a fost ars și a treia parte din&lt;f&gt;ⓦ&lt;/f&gt; copaci au fost arși și toată iarba verde a fost arsă.');
INSERT INTO &quot;verses&quot; VALUES (730,8,8,'&lt;pb/&gt;Al doilea înger a sunat din trâmbiță. Și&lt;f&gt;ⓧ&lt;/f&gt; ceva ca un munte mare de foc aprins a fost aruncat în mare; și&lt;f&gt;ⓨ&lt;/f&gt; a treia parte din mare s-a&lt;f&gt;ⓩ&lt;/f&gt; făcut sânge');
INSERT INTO &quot;verses&quot; VALUES (730,8,9,'și&lt;f&gt;ⓐ&lt;/f&gt; a treia parte din făpturile vii care erau în mare au murit și a treia parte din corăbii au fost distruse.');
INSERT INTO &quot;verses&quot; VALUES (730,8,10,'&lt;pb/&gt;Al treilea înger a sunat din trâmbiță. Și&lt;f&gt;ⓑ&lt;/f&gt; a căzut din cer o stea mare, care ardea ca o torță; a&lt;f&gt;ⓒ&lt;/f&gt; căzut peste a treia parte din râuri și peste izvoarele apelor.');
INSERT INTO &quot;verses&quot; VALUES (730,8,11,'Steaua&lt;f&gt;ⓓ&lt;/f&gt; se chema Pelin; și&lt;f&gt;ⓔ&lt;/f&gt; a treia parte din ape s-au prefăcut în pelin. Și mulți oameni au murit din pricina apelor, pentru că se făcuseră amare.');
INSERT INTO &quot;verses&quot; VALUES (730,8,12,'&lt;pb/&gt;Al&lt;f&gt;ⓕ&lt;/f&gt; patrulea înger a sunat din trâmbiță. Și a treia parte din soare, a treia parte din lună și a treia parte din stele au fost lovite, ca să se întunece a treia parte din ele, ziua să-și piardă a treia parte din lumina ei și noaptea, de asemenea.');
INSERT INTO &quot;verses&quot; VALUES (730,8,13,'&lt;pb/&gt;M-am uitat și am&lt;f&gt;ⓖ&lt;/f&gt; auzit un vultur care zbura în înaltul cerului&lt;f&gt;[18]&lt;/f&gt; și zicea cu glas tare: „Vai&lt;f&gt;ⓗ&lt;/f&gt;, vai, vai de locuitorii pământului din pricina celorlalte sunete de trâmbiță ale celor trei îngeri care urmează să sune!”');
INSERT INTO &quot;verses&quot; VALUES (730,9,1,'&lt;pb/&gt;Al cincilea înger a sunat din trâmbiță. Și&lt;f&gt;ⓘ&lt;/f&gt; am văzut o stea care căzuse din cer pe pământ. I s-a dat cheia fântânii&lt;f&gt;ⓙ&lt;/f&gt; Adâncului');
INSERT INTO &quot;verses&quot; VALUES (730,9,2,'și a deschis fântâna Adâncului. Din fântână s-a&lt;f&gt;ⓚ&lt;/f&gt; ridicat fum ca fumul unui cuptor mare, încât soarele și văzduhul s-au întunecat de fumul fântânii.');
INSERT INTO &quot;verses&quot; VALUES (730,9,3,'Din fum au ieșit niște lăcuste&lt;f&gt;ⓛ&lt;/f&gt; pe pământ și li s-a dat putere ca&lt;f&gt;ⓜ&lt;/f&gt; puterea scorpionilor de pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (730,9,4,'Li s-a zis să&lt;f&gt;ⓝ&lt;/f&gt; nu vatăme iarba&lt;f&gt;ⓞ&lt;/f&gt; pământului, nici vreo verdeață, nici vreun copac, ci numai pe oamenii care n-aveau pe frunte pecetea&lt;f&gt;ⓟ&lt;/f&gt; lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (730,9,5,'Li s-a dat putere nu să-i omoare, ci&lt;f&gt;ⓠ&lt;/f&gt; să-i chinuiască cinci luni; și chinul lor era cum e chinul &lt;i&gt;pricinuit&lt;/i&gt; de scorpion, când înțeapă un om.');
INSERT INTO &quot;verses&quot; VALUES (730,9,6,'În acele zile, oamenii vor căuta&lt;f&gt;ⓡ&lt;/f&gt; moartea și cu niciun chip n-o vor găsi; își vor dori să moară, și moartea va fugi de ei.');
INSERT INTO &quot;verses&quot; VALUES (730,9,7,'&lt;pb/&gt;Lăcustele semănau&lt;f&gt;ⓢ&lt;/f&gt; cu niște cai pregătiți de luptă. Pe&lt;f&gt;ⓣ&lt;/f&gt; cap aveau un fel de cununi ca de aur, iar fețele&lt;f&gt;ⓤ&lt;/f&gt; lor păreau fețe de oameni.');
INSERT INTO &quot;verses&quot; VALUES (730,9,8,'Aveau părul ca părul de femeie și dinții&lt;f&gt;ⓥ&lt;/f&gt; lor erau ca ai leilor.');
INSERT INTO &quot;verses&quot; VALUES (730,9,9,'Aveau platoșe ca niște platoșe de fier; și vuietul &lt;i&gt;pe care-l făceau&lt;/i&gt; aripile lor era ca&lt;f&gt;ⓦ&lt;/f&gt; vuietul carelor trase de mulți cai care se avântă în luptă.');
INSERT INTO &quot;verses&quot; VALUES (730,9,10,'Aveau&lt;f&gt;ⓧ&lt;/f&gt; cozi ca de scorpion, cu bolduri. Și în cozile lor stătea puterea lor de a-i vătăma pe oameni cinci luni.');
INSERT INTO &quot;verses&quot; VALUES (730,9,11,'Peste ele aveau ca împărat&lt;f&gt;ⓨ&lt;/f&gt; pe îngerul&lt;f&gt;ⓩ&lt;/f&gt; Adâncului, care pe evreiește se cheamă Abadon&lt;f&gt;[19]&lt;/f&gt;, iar pe grecește, Apolion&lt;f&gt;[20]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (730,9,12,'&lt;pb/&gt;Primul&lt;f&gt;ⓐ&lt;/f&gt; vai a trecut. Iată că mai vin încă două vaiuri după el.');
INSERT INTO &quot;verses&quot; VALUES (730,9,13,'&lt;pb/&gt;Al șaselea înger a sunat din trâmbiță. Și din cele patru coarne ale altarului de aur care este înaintea lui Dumnezeu am auzit un glas');
INSERT INTO &quot;verses&quot; VALUES (730,9,14,'zicând îngerului al șaselea, care avea trâmbița: „Dezleagă-i pe cei patru îngeri care sunt legați la&lt;f&gt;ⓑ&lt;/f&gt; râul cel mare, Eufrat!”');
INSERT INTO &quot;verses&quot; VALUES (730,9,15,'Și cei patru îngeri care stăteau gata pentru ceasul, ziua, luna și anul acela au fost dezlegați, ca să omoare a treia parte din oameni.');
INSERT INTO &quot;verses&quot; VALUES (730,9,16,'Oștirea&lt;f&gt;ⓒ&lt;/f&gt; lor era în număr de douăzeci de mii de ori câte zece mii de călăreți&lt;f&gt;ⓓ&lt;/f&gt;; le-am&lt;f&gt;ⓔ&lt;/f&gt; auzit numărul.');
INSERT INTO &quot;verses&quot; VALUES (730,9,17,'Și iată cum mi s-au arătat în vedenie caii și călăreții: aveau platoșe ca focul, hiacintul și pucioasa. Capetele&lt;f&gt;ⓕ&lt;/f&gt; cailor erau ca niște capete de lei și din gurile lor ieșea foc, fum și pucioasă.');
INSERT INTO &quot;verses&quot; VALUES (730,9,18,'A treia parte din oameni au fost uciși de aceste trei urgii: de focul, fumul și pucioasa care ieșeau din gurile lor.');
INSERT INTO &quot;verses&quot; VALUES (730,9,19,'Căci puterea cailor stătea în gurile și în cozile lor. Cozile&lt;f&gt;ⓖ&lt;/f&gt; lor erau ca niște șerpi cu capete și cu ele vătămau.');
INSERT INTO &quot;verses&quot; VALUES (730,9,20,'&lt;pb/&gt;Ceilalți oameni, care n-au fost uciși de aceste urgii, nu&lt;f&gt;ⓗ&lt;/f&gt; s-au pocăit de faptele mâinilor lor, ca să nu se închine demonilor&lt;f&gt;ⓘ&lt;/f&gt; și idolilor&lt;f&gt;ⓙ&lt;/f&gt; de aur, de argint, de aramă, de piatră și de lemn, care nu pot nici să vadă, nici să audă, nici să umble.');
INSERT INTO &quot;verses&quot; VALUES (730,9,21,'Și nu s-au pocăit de uciderile lor, nici de vrăjitoriile&lt;f&gt;ⓚ&lt;/f&gt; lor, nici de desfrâul lor, nici de hoțiile lor.');
INSERT INTO &quot;verses&quot; VALUES (730,10,1,'&lt;pb/&gt;Apoi am văzut un alt înger puternic, care se cobora din cer învăluit într-un nor. Deasupra capului lui era curcubeul&lt;f&gt;ⓛ&lt;/f&gt;, fața&lt;f&gt;ⓜ&lt;/f&gt; lui era ca soarele și picioarele&lt;f&gt;ⓝ&lt;/f&gt; lui erau ca niște stâlpi de foc.');
INSERT INTO &quot;verses&quot; VALUES (730,10,2,'În mână ținea o cărticică deschisă. El și-a pus piciorul&lt;f&gt;ⓞ&lt;/f&gt; drept pe mare și pe cel stâng pe pământ');
INSERT INTO &quot;verses&quot; VALUES (730,10,3,'și a strigat cu glas tare, cum rage un leu. Când a strigat el, au răsunat glasurile celor șapte&lt;f&gt;ⓟ&lt;/f&gt; tunete.');
INSERT INTO &quot;verses&quot; VALUES (730,10,4,'Și când au răsunat glasurile celor șapte tunete, eram gata să mă apuc să scriu; atunci am auzit din cer un glas, care zicea: „Pecetluiește&lt;f&gt;ⓠ&lt;/f&gt; ce au spus cele șapte tunete și nu scrie!”');
INSERT INTO &quot;verses&quot; VALUES (730,10,5,'&lt;pb/&gt;Iar îngerul pe care-l văzusem stând în picioare pe mare și pe pământ și-a&lt;f&gt;ⓡ&lt;/f&gt; ridicat mâna dreaptă spre cer');
INSERT INTO &quot;verses&quot; VALUES (730,10,6,'și a jurat pe Cel ce este viu în vecii vecilor, care&lt;f&gt;ⓢ&lt;/f&gt; a creat cerul și lucrurile din el, pământul și lucrurile de pe el, marea și lucrurile din ea, că nu va mai fi nicio zăbavă&lt;f&gt;ⓣ&lt;/f&gt;,');
INSERT INTO &quot;verses&quot; VALUES (730,10,7,'ci că, în&lt;f&gt;ⓤ&lt;/f&gt; zilele în care îngerul al șaptelea va suna din trâmbiță, se va împlini taina lui Dumnezeu, după vestea bună pe care le-a dat-o El robilor Săi profeți.');
INSERT INTO &quot;verses&quot; VALUES (730,10,8,'&lt;pb/&gt;Și glasul&lt;f&gt;ⓥ&lt;/f&gt; pe care-l auzisem din cer mi-a vorbit din nou și mi-a zis: „Du-te și ia cărticica deschisă din mâna îngerului care stă în picioare pe mare și pe pământ!”');
INSERT INTO &quot;verses&quot; VALUES (730,10,9,'M-am dus la înger și i-am cerut să-mi dea cărticica. „Ia-o&lt;f&gt;ⓦ&lt;/f&gt;”, mi-a zis el, „și mănânc-o; ea îți va amărî pântecul, dar în gura ta va fi dulce ca mierea.”');
INSERT INTO &quot;verses&quot; VALUES (730,10,10,'&lt;pb/&gt;Am luat cărticica din mâna îngerului și am mâncat-o: în gura&lt;f&gt;ⓧ&lt;/f&gt; mea a fost dulce ca mierea, dar, după ce am mâncat-o, mi s-a umplut pântecul de amărăciune&lt;f&gt;ⓨ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (730,10,11,'Apoi mi s-a zis: „Trebuie să profețești din nou multor popoare, neamuri, limbi și împărați.”');
INSERT INTO &quot;verses&quot; VALUES (730,11,1,'&lt;pb/&gt;Apoi mi s-a dat o trestie&lt;f&gt;ⓩ&lt;/f&gt; asemenea unui toiag și mi s-a zis: „Scoală-te&lt;f&gt;ⓐ&lt;/f&gt; și măsoară Templul lui Dumnezeu, altarul și pe cei ce se închină acolo!');
INSERT INTO &quot;verses&quot; VALUES (730,11,2,'Dar curtea&lt;f&gt;ⓑ&lt;/f&gt; de afară a Templului las-o afară și n-o măsura, căci&lt;f&gt;ⓒ&lt;/f&gt; a fost dată neamurilor; și ele vor călca&lt;f&gt;ⓓ&lt;/f&gt; în picioare cetatea sfântă patruzeci&lt;f&gt;ⓔ&lt;/f&gt; și două de luni.');
INSERT INTO &quot;verses&quot; VALUES (730,11,3,'&lt;pb/&gt;Iar celor doi martori&lt;f&gt;ⓕ&lt;/f&gt; ai Mei le voi da să profețească&lt;f&gt;ⓖ&lt;/f&gt; îmbrăcați în sac&lt;f&gt;[21]&lt;/f&gt; o&lt;f&gt;ⓗ&lt;/f&gt; mie două sute șaizeci de zile.”');
INSERT INTO &quot;verses&quot; VALUES (730,11,4,'Aceștia sunt cei doi&lt;f&gt;ⓘ&lt;/f&gt; măslini și cele două sfeșnice care stau înaintea Domnului pământului.');
INSERT INTO &quot;verses&quot; VALUES (730,11,5,'Dacă vrea cineva să le facă rău, din gura lor iese un foc&lt;f&gt;ⓙ&lt;/f&gt; care îi mistuie pe vrăjmașii lor; dacă&lt;f&gt;ⓚ&lt;/f&gt; cineva vrea să le facă rău, trebuie să fie ucis în felul acesta.');
INSERT INTO &quot;verses&quot; VALUES (730,11,6,'Ei au&lt;f&gt;ⓛ&lt;/f&gt; puterea să închidă cerul, ca să nu cadă ploaie în zilele când vor profeți, și au&lt;f&gt;ⓜ&lt;/f&gt; putere să prefacă apele în sânge și să lovească pământul cu tot felul de urgii ori de câte ori vor dori.');
INSERT INTO &quot;verses&quot; VALUES (730,11,7,'&lt;pb/&gt;Când își vor isprăvi mărturia, fiara&lt;f&gt;ⓝ&lt;/f&gt; care se ridică din&lt;f&gt;ⓞ&lt;/f&gt; Adânc va&lt;f&gt;ⓟ&lt;/f&gt; face război cu ei, îi va birui și-i va omorî.');
INSERT INTO &quot;verses&quot; VALUES (730,11,8,'Și trupurile lor moarte &lt;i&gt;vor zăcea&lt;/i&gt; în piața cetății&lt;f&gt;ⓠ&lt;/f&gt; celei mari, care, în înțeles duhovnicesc, se cheamă Sodoma și Egipt, unde&lt;f&gt;ⓡ&lt;/f&gt; a fost răstignit și Domnul lor.');
INSERT INTO &quot;verses&quot; VALUES (730,11,9,'Și&lt;f&gt;ⓢ&lt;/f&gt;, timp de trei zile și jumătate, &lt;i&gt;oameni&lt;/i&gt; din orice popor, din orice seminție, de orice limbă și de orice neam vor privi trupurile lor moarte și&lt;f&gt;ⓣ&lt;/f&gt; nu vor da voie ca trupurile lor să fie puse în mormânt.');
INSERT INTO &quot;verses&quot; VALUES (730,11,10,'Și&lt;f&gt;ⓤ&lt;/f&gt; locuitorii pământului se vor bucura din pricina lor, se vor veseli și își vor trimite daruri&lt;f&gt;ⓥ&lt;/f&gt; unii altora, pentru că&lt;f&gt;ⓦ&lt;/f&gt; acești doi profeți îi chinuiseră pe locuitorii pământului.');
INSERT INTO &quot;verses&quot; VALUES (730,11,11,'Dar, după cele trei zile și jumătate, &lt;e&gt;duh de viață&lt;/e&gt; de la Dumnezeu &lt;e&gt;a intrat în ei și s-au ridicat în picioare&lt;/e&gt;&lt;f&gt;[22]&lt;/f&gt; și o mare frică i-a apucat pe cei ce i-au văzut.');
INSERT INTO &quot;verses&quot; VALUES (730,11,12,'Și au auzit din cer un glas puternic, care le zicea: „Suiți-vă aici!” Și&lt;f&gt;ⓧ&lt;/f&gt; s-au suit la cer în nor&lt;f&gt;ⓨ&lt;/f&gt;, iar&lt;f&gt;ⓩ&lt;/f&gt; vrăjmașii lor i-au văzut.');
INSERT INTO &quot;verses&quot; VALUES (730,11,13,'În ceasul acela s-a&lt;f&gt;ⓐ&lt;/f&gt; făcut un mare cutremur și a&lt;f&gt;ⓑ&lt;/f&gt; zecea parte din cetate s-a prăbușit. Șapte mii de oameni au fost uciși în cutremurul &lt;i&gt;acela&lt;/i&gt;, iar ceilalți s-au îngrozit și au&lt;f&gt;ⓒ&lt;/f&gt; dat slavă Dumnezeului cerului.');
INSERT INTO &quot;verses&quot; VALUES (730,11,14,'&lt;pb/&gt;Al doilea&lt;f&gt;ⓓ&lt;/f&gt; vai a trecut. Iată că al treilea vai vine curând.');
INSERT INTO &quot;verses&quot; VALUES (730,11,15,'&lt;pb/&gt;Al șaptelea înger&lt;f&gt;ⓔ&lt;/f&gt; a sunat din trâmbiță. Și&lt;f&gt;ⓕ&lt;/f&gt; în cer s-au auzit glasuri puternice, care ziceau: &lt;pb/&gt;&lt;t&gt;„Împărăția&lt;f&gt;ⓖ&lt;/f&gt; lumii a trecut în mâinile Domnului nostru&lt;/t&gt; &lt;t&gt;și ale Unsului Său.&lt;/t&gt; &lt;t&gt;Și&lt;f&gt;ⓗ&lt;/f&gt; El va împărăți în vecii vecilor.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,11,16,'&lt;pb/&gt;Și cei&lt;f&gt;ⓘ&lt;/f&gt; douăzeci și patru de bătrâni, care stăteau înaintea lui Dumnezeu pe tronurile lor, s-au aruncat cu fețele &lt;i&gt;la pământ&lt;/i&gt;, I s-au închinat lui Dumnezeu');
INSERT INTO &quot;verses&quot; VALUES (730,11,17,'și au zis: &lt;pb/&gt;&lt;t&gt;„Îți mulțumim, Doamne Dumnezeule atotputernic,&lt;/t&gt; &lt;t&gt;care&lt;f&gt;ⓙ&lt;/f&gt; ești și care erai [și care vii],&lt;/t&gt; &lt;t&gt;fiindcă ai luat puterea Ta cea mare&lt;/t&gt; &lt;t&gt;și&lt;f&gt;ⓚ&lt;/f&gt; ai început să împărățești!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,11,18,'&lt;t&gt;Neamurile se mâniaseră,&lt;/t&gt; &lt;t&gt;dar a venit&lt;f&gt;ⓛ&lt;/f&gt; mânia Ta;&lt;/t&gt; &lt;t&gt;a venit vremea să-i judeci pe cei morți,&lt;/t&gt; &lt;t&gt;să-i răsplătești pe robii Tăi profeți,&lt;/t&gt; &lt;t&gt;pe sfinți și pe cei ce se tem de Numele Tău,&lt;/t&gt; &lt;t&gt;pe cei mici&lt;f&gt;ⓜ&lt;/f&gt; și pe cei mari,&lt;/t&gt; &lt;t&gt;și să-i&lt;f&gt;ⓝ&lt;/f&gt; prăpădești pe cei ce prăpădesc&lt;f&gt;[23]&lt;/f&gt; pământul.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,11,19,'&lt;pb/&gt;Și Templul&lt;f&gt;ⓞ&lt;/f&gt; lui Dumnezeu, care este în cer, s-a deschis și s-a văzut chivotul legământului Său în Templul Său. Și au&lt;f&gt;ⓟ&lt;/f&gt; fost fulgere, glasuri, tunete, cutremur și&lt;f&gt;ⓠ&lt;/f&gt; grindină mare.');
INSERT INTO &quot;verses&quot; VALUES (730,12,1,'&lt;pb/&gt;În cer s-a arătat un semn mare: o femeie învăluită în soare, cu luna sub picioare și cu o cunună de douăsprezece stele pe cap.');
INSERT INTO &quot;verses&quot; VALUES (730,12,2,'Ea era însărcinată, țipa în durerile nașterii și se chinuia&lt;f&gt;ⓡ&lt;/f&gt; să nască.');
INSERT INTO &quot;verses&quot; VALUES (730,12,3,'În cer s-a arătat un alt semn: iată, un&lt;f&gt;ⓢ&lt;/f&gt; mare balaur roșu, cu&lt;f&gt;ⓣ&lt;/f&gt; șapte capete și zece coarne, iar&lt;f&gt;ⓤ&lt;/f&gt; pe capete — șapte diademe.');
INSERT INTO &quot;verses&quot; VALUES (730,12,4,'Cu coada&lt;f&gt;ⓥ&lt;/f&gt; trăgea a treia parte din&lt;f&gt;ⓦ&lt;/f&gt; stelele cerului și&lt;f&gt;ⓧ&lt;/f&gt; le arunca pe pământ. &lt;pb/&gt;Balaurul stătea înaintea&lt;f&gt;ⓨ&lt;/f&gt; femeii care urma să nască, pentru ca&lt;f&gt;ⓩ&lt;/f&gt;, atunci când va naște, să-i mănânce copilul.');
INSERT INTO &quot;verses&quot; VALUES (730,12,5,'Ea a născut un fiu, un copil de parte bărbătească. El&lt;f&gt;ⓐ&lt;/f&gt; &lt;e&gt;va păstori toate neamurile cu un toiag de fier&lt;/e&gt;&lt;f&gt;[24]&lt;/f&gt;. Copilul ei a fost răpit la Dumnezeu și la tronul Lui,');
INSERT INTO &quot;verses&quot; VALUES (730,12,6,'iar femeia&lt;f&gt;ⓑ&lt;/f&gt; a fugit în pustiu, unde are un loc pregătit de Dumnezeu, ca să fie hrănită acolo o mie două sute șaizeci&lt;f&gt;ⓒ&lt;/f&gt; de zile.');
INSERT INTO &quot;verses&quot; VALUES (730,12,7,'&lt;pb/&gt;Iar în cer s-a făcut război: Mihail&lt;f&gt;ⓓ&lt;/f&gt; și îngerii lui s-au luptat cu&lt;f&gt;ⓔ&lt;/f&gt; balaurul. Balaurul cu îngerii lui s-au luptat &lt;i&gt;și ei&lt;/i&gt;,');
INSERT INTO &quot;verses&quot; VALUES (730,12,8,'dar n-au putut birui, și nu s-a mai găsit loc pentru ei în cer.');
INSERT INTO &quot;verses&quot; VALUES (730,12,9,'&lt;i&gt;Astfel&lt;/i&gt;, balaurul cel mare, șarpele cel vechi&lt;f&gt;ⓕ&lt;/f&gt;, numit Diavolul și Satana, cel care&lt;f&gt;ⓖ&lt;/f&gt; înșală întreaga lume, a fost&lt;f&gt;ⓗ&lt;/f&gt; aruncat pe pământ și împreună cu el au fost&lt;f&gt;ⓘ&lt;/f&gt; aruncați și îngerii lui.');
INSERT INTO &quot;verses&quot; VALUES (730,12,10,'Și am auzit în cer un glas tare, care zicea: &lt;pb/&gt;&lt;t&gt;„Acum&lt;f&gt;ⓙ&lt;/f&gt; au venit mântuirea, puterea&lt;/t&gt; &lt;t&gt;și împărăția Dumnezeului nostru&lt;/t&gt; &lt;t&gt;și stăpânirea Hristosului Său,&lt;/t&gt; &lt;t&gt;pentru că acuzatorul fraților noștri a fost aruncat &lt;i&gt;jos&lt;/i&gt;,&lt;/t&gt; &lt;t&gt;el, care îi&lt;f&gt;ⓚ&lt;/f&gt; acuza înaintea Dumnezeului nostru&lt;/t&gt; &lt;t&gt;zi și noapte.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,12,11,'&lt;t&gt;Ei&lt;f&gt;ⓛ&lt;/f&gt; l-au biruit prin sângele Mielului&lt;/t&gt; &lt;t&gt;și prin cuvântul mărturiei lor&lt;/t&gt; &lt;t&gt;și nu&lt;f&gt;ⓜ&lt;/f&gt; și-au iubit viața până la moarte.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,12,12,'&lt;t&gt;De aceea bucurați-vă&lt;f&gt;ⓝ&lt;/f&gt;, ceruri&lt;/t&gt; &lt;t&gt;și voi, care locuiți în ceruri!&lt;/t&gt; &lt;t&gt;Vai&lt;f&gt;ⓞ&lt;/f&gt; &lt;i&gt;de voi&lt;/i&gt;, pământ și mare!&lt;/t&gt; &lt;t&gt;Căci Diavolul s-a coborât la voi&lt;/t&gt; &lt;t&gt;cuprins de o mânie mare,&lt;/t&gt; &lt;t&gt;fiindcă&lt;f&gt;ⓟ&lt;/f&gt; știe că are puțină vreme.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,12,13,'&lt;pb/&gt;Când s-a văzut balaurul aruncat pe pământ, a început să o urmărească pe&lt;f&gt;ⓠ&lt;/f&gt; femeia care născuse copilul de parte bărbătească.');
INSERT INTO &quot;verses&quot; VALUES (730,12,14,'Și&lt;f&gt;ⓡ&lt;/f&gt; femeii i-au fost date cele două aripi ale vulturului celui mare, ca&lt;f&gt;ⓢ&lt;/f&gt; să zboare cu ele în&lt;f&gt;ⓣ&lt;/f&gt; pustiu, în locul &lt;i&gt;pregătit&lt;/i&gt; pentru ea, unde este hrănită o&lt;f&gt;ⓤ&lt;/f&gt; vreme, vremi și o jumătate de vreme&lt;f&gt;[25]&lt;/f&gt;, departe de fața șarpelui.');
INSERT INTO &quot;verses&quot; VALUES (730,12,15,'Atunci, șarpele a&lt;f&gt;ⓥ&lt;/f&gt; aruncat în urma femeii apă din gură ca un râu, ca s-o ia râul.');
INSERT INTO &quot;verses&quot; VALUES (730,12,16,'Dar pământul i-a venit în ajutor femeii: pământul și-a deschis gura și a înghițit râul pe care-l aruncase balaurul din gură.');
INSERT INTO &quot;verses&quot; VALUES (730,12,17,'Balaurul s-a mâniat pe femeie și s-a&lt;f&gt;ⓦ&lt;/f&gt; dus să facă război cu rămășița seminței ei, cu cei care&lt;f&gt;ⓧ&lt;/f&gt; păzesc poruncile lui Dumnezeu și au mărturia&lt;f&gt;ⓨ&lt;/f&gt; lui Isus Hristos.');
INSERT INTO &quot;verses&quot; VALUES (730,12,18,'&lt;pb/&gt;Apoi a stat pe nisipul mării.&lt;f&gt;[26]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,13,1,'&lt;pb/&gt;Și am văzut ridicându-se din mare o&lt;f&gt;ⓩ&lt;/f&gt; fiară cu&lt;f&gt;ⓐ&lt;/f&gt; zece coarne și șapte capete; pe coarne avea zece diademe și pe capete avea nume de hulă.');
INSERT INTO &quot;verses&quot; VALUES (730,13,2,'Fiara&lt;f&gt;ⓑ&lt;/f&gt; pe care am văzut-o semăna cu un leopard, avea labe&lt;f&gt;ⓒ&lt;/f&gt; ca de urs și o gură&lt;f&gt;ⓓ&lt;/f&gt; ca de leu. Balaurul i-a&lt;f&gt;ⓔ&lt;/f&gt; dat puterea lui, tronul&lt;f&gt;ⓕ&lt;/f&gt; lui&lt;f&gt;ⓖ&lt;/f&gt; și o stăpânire mare.');
INSERT INTO &quot;verses&quot; VALUES (730,13,3,'Unul din capetele ei părea înjunghiat&lt;f&gt;ⓗ&lt;/f&gt; de moarte, dar rana de moarte fusese vindecată. &lt;pb/&gt;Și tot&lt;f&gt;ⓘ&lt;/f&gt; pământul se mira după fiară.');
INSERT INTO &quot;verses&quot; VALUES (730,13,4,'Și au început să se închine balaurului pentru că îi dăduse fiarei autoritatea &lt;i&gt;lui&lt;/i&gt;. Și au început să i se închine fiarei, zicând: „Cine&lt;f&gt;ⓙ&lt;/f&gt; este asemenea fiarei și cine se poate lupta cu ea?”');
INSERT INTO &quot;verses&quot; VALUES (730,13,5,'&lt;pb/&gt;Și i s-a dat o gură&lt;f&gt;ⓚ&lt;/f&gt;, care rostea vorbe mari și blasfemii. Și i s-a dat autoritate să lucreze patruzeci&lt;f&gt;ⓛ&lt;/f&gt; și două de luni.');
INSERT INTO &quot;verses&quot; VALUES (730,13,6,'Ea și-a deschis gura ca să rostească blasfemii împotriva lui Dumnezeu, să-I hulească Numele, cortul&lt;f&gt;ⓜ&lt;/f&gt; și pe cei ce locuiesc în cer.');
INSERT INTO &quot;verses&quot; VALUES (730,13,7,'I s-a dat să facă&lt;f&gt;ⓝ&lt;/f&gt; război cu sfinții și&lt;f&gt;ⓞ&lt;/f&gt; să-i biruiască. Și i s-a dat autoritate peste orice seminție, peste orice popor, peste orice limbă și peste orice neam.');
INSERT INTO &quot;verses&quot; VALUES (730,13,8,'Și toți locuitorii pământului i&lt;f&gt;ⓟ&lt;/f&gt; se vor închina, toți aceia al căror nume n-a fost scris de la&lt;f&gt;ⓠ&lt;/f&gt; întemeierea lumii în cartea vieții Mielului, care a fost înjunghiat.');
INSERT INTO &quot;verses&quot; VALUES (730,13,9,'&lt;pb/&gt;Cine&lt;f&gt;ⓡ&lt;/f&gt; are urechi, să audă!');
INSERT INTO &quot;verses&quot; VALUES (730,13,10,'&lt;pb/&gt;&lt;t&gt;&lt;e&gt;Cine este sortit robiei&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;în robie va merge.&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Cine este sortit sabiei&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;De sabie&lt;/e&gt; va cădea ucis.&lt;f&gt;[27]&lt;/f&gt;&lt;/t&gt; &lt;pb/&gt;Aici&lt;f&gt;ⓢ&lt;/f&gt; este răbdarea&lt;f&gt;[28]&lt;/f&gt; și credincioșia sfinților.');
INSERT INTO &quot;verses&quot; VALUES (730,13,11,'&lt;pb/&gt;Apoi am văzut ridicându-se&lt;f&gt;ⓣ&lt;/f&gt; din pământ o altă fiară, care avea două coarne ca de miel, dar vorbea ca un balaur.');
INSERT INTO &quot;verses&quot; VALUES (730,13,12,'Ea lucra cu toată autoritatea primei fiare, în numele ei&lt;f&gt;[29]&lt;/f&gt;, și făcea ca pământul și locuitorii lui să se închine primei fiare, a&lt;f&gt;ⓤ&lt;/f&gt; cărei rană de moarte fusese vindecată.');
INSERT INTO &quot;verses&quot; VALUES (730,13,13,'Săvârșea&lt;f&gt;ⓥ&lt;/f&gt; semne mari, până acolo&lt;f&gt;ⓦ&lt;/f&gt; că făcea chiar să se coboare foc din cer pe pământ, înaintea oamenilor.');
INSERT INTO &quot;verses&quot; VALUES (730,13,14,'Și-i amăgea&lt;f&gt;ⓧ&lt;/f&gt; pe locuitorii pământului prin&lt;f&gt;ⓨ&lt;/f&gt; semnele pe care i se dăduse să le facă în numele fiarei. Ea le-a zis locuitorilor pământului să-i facă un chip fiarei care avea rana de sabie și&lt;f&gt;ⓩ&lt;/f&gt; trăia.');
INSERT INTO &quot;verses&quot; VALUES (730,13,15,'&lt;pb/&gt;I s-a dat putere să dea suflare chipului fiarei, pentru ca acesta să vorbească și&lt;f&gt;ⓐ&lt;/f&gt; să facă să fie omorâți toți cei ce nu se vor închina chipului fiarei.');
INSERT INTO &quot;verses&quot; VALUES (730,13,16,'Și a făcut ca toți — mici și mari, bogați și săraci, liberi și robi — să&lt;f&gt;ⓑ&lt;/f&gt; primească un semn pe mâna dreaptă sau pe frunte');
INSERT INTO &quot;verses&quot; VALUES (730,13,17,'și nimeni să nu poată cumpăra sau vinde fără să aibă semnul acela, adică numele&lt;f&gt;ⓒ&lt;/f&gt; fiarei, sau&lt;f&gt;ⓓ&lt;/f&gt; numărul numelui ei.');
INSERT INTO &quot;verses&quot; VALUES (730,13,18,'&lt;pb/&gt;Aici&lt;f&gt;ⓔ&lt;/f&gt; e înțelepciunea. Cine are pricepere, să socotească numărul&lt;f&gt;ⓕ&lt;/f&gt; fiarei, căci&lt;f&gt;ⓖ&lt;/f&gt; este un număr de om. Și numărul ei este șase sute șaizeci și șase.');
INSERT INTO &quot;verses&quot; VALUES (730,14,1,'&lt;pb/&gt;Apoi m-am uitat și iată că Mielul&lt;f&gt;ⓗ&lt;/f&gt; stătea pe muntele Sionului și împreună cu El erau o&lt;f&gt;ⓘ&lt;/f&gt; sută patruzeci și patru de mii, care&lt;f&gt;ⓙ&lt;/f&gt; aveau scris pe frunte Numele Său și Numele Tatălui Său.');
INSERT INTO &quot;verses&quot; VALUES (730,14,2,'Și am auzit venind din cer un glas ca&lt;f&gt;ⓚ&lt;/f&gt; un vuiet de ape mari, ca bubuitul unui tunet puternic; și glasul pe care l-am auzit era ca al celor ce cântă&lt;f&gt;ⓛ&lt;/f&gt; cu harfa&lt;f&gt;ⓜ&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (730,14,3,'Cântau o cântare&lt;f&gt;ⓝ&lt;/f&gt; nouă înaintea tronului, a celor patru făpturi vii și a bătrânilor. Și nimeni nu putea să învețe cântarea în afară de cei o sută patruzeci și patru de mii, care fuseseră răscumpărați de pe pământ.');
INSERT INTO &quot;verses&quot; VALUES (730,14,4,'&lt;pb/&gt;Aceștia nu s-au întinat cu femei, căci&lt;f&gt;ⓞ&lt;/f&gt; sunt feciori&lt;f&gt;[30]&lt;/f&gt;, și-L&lt;f&gt;ⓟ&lt;/f&gt; urmează pe Miel oriunde merge El. Au fost&lt;f&gt;ⓠ&lt;/f&gt; răscumpărați dintre oameni, ca prim rod&lt;f&gt;ⓡ&lt;/f&gt; pentru Dumnezeu și pentru Miel.');
INSERT INTO &quot;verses&quot; VALUES (730,14,5,'Și&lt;f&gt;ⓢ&lt;/f&gt; &lt;e&gt;în gura lor nu s-a găsit minciună&lt;/e&gt;&lt;f&gt;[31]&lt;/f&gt;, căci sunt&lt;f&gt;ⓣ&lt;/f&gt; fără vină.');
INSERT INTO &quot;verses&quot; VALUES (730,14,6,'&lt;pb/&gt;Și am văzut un alt înger, care zbura&lt;f&gt;ⓤ&lt;/f&gt; cu o&lt;f&gt;ⓥ&lt;/f&gt; Evanghelie veșnică în înaltul cerului ca s-o vestească locuitorilor pământului, de orice&lt;f&gt;ⓦ&lt;/f&gt; neam, din orice seminție, de orice limbă și din orice popor.');
INSERT INTO &quot;verses&quot; VALUES (730,14,7,'El zicea cu glas tare: &lt;pb/&gt;„Temeți-vă&lt;f&gt;ⓧ&lt;/f&gt; de Dumnezeu și dați-I slavă, căci a venit ceasul judecății Lui, și închinați-vă&lt;f&gt;ⓨ&lt;/f&gt; Celui ce a făcut cerul și pământul, marea și izvoarele apelor!”');
INSERT INTO &quot;verses&quot; VALUES (730,14,8,'&lt;pb/&gt;Apoi a urmat&lt;f&gt;[32]&lt;/f&gt; un alt înger, al doilea, care a zis: &lt;pb/&gt;„A&lt;f&gt;ⓩ&lt;/f&gt; căzut, a căzut Babilonul, cetatea&lt;f&gt;ⓐ&lt;/f&gt; cea mare, care a dat de băut tuturor neamurilor din vinul desfrâului ei pătimaș!”');
INSERT INTO &quot;verses&quot; VALUES (730,14,9,'&lt;pb/&gt;Apoi a urmat un alt înger, al treilea, care a zis cu glas tare: &lt;pb/&gt;„Dacă se închină cineva&lt;f&gt;ⓑ&lt;/f&gt; fiarei și chipului ei și primește semnul ei pe frunte sau pe mână,');
INSERT INTO &quot;verses&quot; VALUES (730,14,10,'va bea&lt;f&gt;ⓒ&lt;/f&gt; și el din vinul mâniei lui Dumnezeu, turnat&lt;f&gt;ⓓ&lt;/f&gt; neamestecat în&lt;f&gt;ⓔ&lt;/f&gt; potirul mâniei Lui, și va fi chinuit&lt;f&gt;ⓕ&lt;/f&gt; în foc&lt;f&gt;ⓖ&lt;/f&gt; și în pucioasă înaintea sfinților îngeri și înaintea Mielului.');
INSERT INTO &quot;verses&quot; VALUES (730,14,11,'Și fumul&lt;f&gt;ⓗ&lt;/f&gt; chinului lor se suie în sus în vecii vecilor. Și nici ziua, nici noaptea n-au odihnă cei ce se închină fiarei și chipului ei și oricine primește semnul numelui ei.');
INSERT INTO &quot;verses&quot; VALUES (730,14,12,'Aici&lt;f&gt;ⓘ&lt;/f&gt; este răbdarea sfinților, care&lt;f&gt;ⓙ&lt;/f&gt; păzesc poruncile lui Dumnezeu și credința în Isus.”');
INSERT INTO &quot;verses&quot; VALUES (730,14,13,'&lt;pb/&gt;Și am auzit un glas din cer care zicea: „Scrie: «Ferice&lt;f&gt;ⓚ&lt;/f&gt; de morții care&lt;f&gt;ⓛ&lt;/f&gt; mor în Domnul de acum încolo!»” &lt;pb/&gt;„Da”, zice Duhul, „ei&lt;f&gt;ⓜ&lt;/f&gt; se vor odihni de ostenelile lor, căci faptele lor îi urmează!”');
INSERT INTO &quot;verses&quot; VALUES (730,14,14,'&lt;pb/&gt;Apoi m-am uitat, și iată un nor alb; și pe nor stătea &lt;i&gt;Cineva&lt;/i&gt; care semăna&lt;f&gt;ⓝ&lt;/f&gt; cu un fiu al omului; pe cap avea&lt;f&gt;ⓞ&lt;/f&gt; o cunună de aur, iar în mână, o seceră ascuțită.');
INSERT INTO &quot;verses&quot; VALUES (730,14,15,'Și un alt înger a&lt;f&gt;ⓟ&lt;/f&gt; ieșit din Templu și a strigat cu glas tare Celui ce stătea pe nor: &lt;pb/&gt;„Trimite-ți secera&lt;f&gt;ⓠ&lt;/f&gt; și seceră, pentru că a venit ceasul să seceri și secerișul pământului&lt;f&gt;ⓡ&lt;/f&gt; este copt!”');
INSERT INTO &quot;verses&quot; VALUES (730,14,16,'&lt;pb/&gt;Atunci, Cel ce stătea pe nor Și-a aruncat secera pe pământ. Și pământul a fost secerat.');
INSERT INTO &quot;verses&quot; VALUES (730,14,17,'&lt;pb/&gt;Și din Templul care este în cer a ieșit un alt înger, care avea și el o seceră ascuțită.');
INSERT INTO &quot;verses&quot; VALUES (730,14,18,'Și un alt înger, care avea stăpânire asupra focului, a ieșit&lt;f&gt;ⓢ&lt;/f&gt; din altar și a strigat cu glas tare către cel ce avea secera ascuțită: &lt;pb/&gt;„Trimite-ți&lt;f&gt;ⓣ&lt;/f&gt; secera cea ascuțită și culege strugurii viei pământului, căci strugurii ei s-au copt!”');
INSERT INTO &quot;verses&quot; VALUES (730,14,19,'&lt;pb/&gt;Și îngerul și-a aruncat secera pe pământ, a cules via pământului și a aruncat &lt;i&gt;strugurii&lt;/i&gt; în teascul&lt;f&gt;ⓤ&lt;/f&gt; cel mare al mâniei lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (730,14,20,'Și teascul&lt;f&gt;ⓥ&lt;/f&gt; a fost călcat în picioare în afara&lt;f&gt;ⓦ&lt;/f&gt; cetății; și din teasc a ieșit sânge până la zăbalele cailor&lt;f&gt;ⓧ&lt;/f&gt; pe o întindere de o mie șase sute de stadii.');
INSERT INTO &quot;verses&quot; VALUES (730,15,1,'&lt;pb/&gt;Apoi am&lt;f&gt;ⓨ&lt;/f&gt; văzut în cer un alt semn mare și minunat: șapte&lt;f&gt;ⓩ&lt;/f&gt; îngeri care aveau șapte urgii, cele din urmă, căci&lt;f&gt;ⓐ&lt;/f&gt; cu ele s-a isprăvit mânia lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (730,15,2,'&lt;pb/&gt;Și am văzut un fel de&lt;f&gt;ⓑ&lt;/f&gt; mare de sticlă amestecată&lt;f&gt;ⓒ&lt;/f&gt; cu foc și&lt;f&gt;ⓓ&lt;/f&gt; pe biruitorii fiarei, ai chipului ei și ai numărului numelui ei stând pe marea de sticlă cu&lt;f&gt;ⓔ&lt;/f&gt; harfele lui Dumnezeu în mână.');
INSERT INTO &quot;verses&quot; VALUES (730,15,3,'Ei cântau cântarea&lt;f&gt;ⓕ&lt;/f&gt; lui Moise, robul lui Dumnezeu, și cântarea Mielului. Și ziceau: &lt;pb/&gt;&lt;t&gt;&lt;e&gt;Mari și minunate sunt lucrările Tale&lt;/e&gt; &lt;f&gt;[33]&lt;/f&gt; &lt;e&gt;,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Doamne Dumnezeule atotputernic&lt;/e&gt; &lt;f&gt;[34]&lt;/f&gt; &lt;e&gt;!&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Drepte&lt;/e&gt; &lt;f&gt;ⓖ&lt;/f&gt; &lt;e&gt;și adevărate sunt căile Tale&lt;/e&gt;&lt;f&gt;[35]&lt;/f&gt;&lt;e&gt;,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;Împăratul neamurilor!&lt;/e&gt; &lt;f&gt;[36]&lt;/f&gt;&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,15,4,'&lt;t&gt;&lt;e&gt;Cine&lt;/e&gt; &lt;f&gt;ⓗ&lt;/f&gt; &lt;e&gt;nu se va teme, Doamne&lt;/e&gt;&lt;f&gt;[37]&lt;/f&gt;&lt;e&gt;,&lt;/e&gt;&lt;/t&gt; &lt;t&gt;&lt;e&gt;și cine nu va slăvi Numele Tău&lt;/e&gt; &lt;f&gt;[38]&lt;/f&gt; &lt;e&gt;?&lt;/e&gt;&lt;/t&gt; &lt;t&gt;Căci numai Tu ești Sfânt&lt;/t&gt; &lt;t&gt;&lt;e&gt;și toate&lt;/e&gt; &lt;f&gt;ⓘ&lt;/f&gt; &lt;e&gt;neamurile vor veni&lt;/e&gt;&lt;f&gt;[39]&lt;/f&gt;&lt;/t&gt; &lt;t&gt;și se vor închina înaintea Ta,&lt;/t&gt; &lt;t&gt;pentru că judecățile Tale au fost arătate!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,15,5,'&lt;pb/&gt;După aceea, am văzut deschizându-se în cer Templul&lt;f&gt;ⓙ&lt;/f&gt; cortului mărturiei.');
INSERT INTO &quot;verses&quot; VALUES (730,15,6,'Și&lt;f&gt;ⓚ&lt;/f&gt; din Templu au ieșit cei șapte îngeri care țineau cele șapte urgii. Erau îmbrăcați&lt;f&gt;ⓛ&lt;/f&gt; în in curat, strălucitor, și erau încinși la piept cu brâie de aur.');
INSERT INTO &quot;verses&quot; VALUES (730,15,7,'Și&lt;f&gt;ⓜ&lt;/f&gt; una din cele patru făpturi vii le-a dat celor șapte îngeri șapte cupe de aur pline de mânia lui Dumnezeu, care este&lt;f&gt;ⓝ&lt;/f&gt; viu în vecii vecilor.');
INSERT INTO &quot;verses&quot; VALUES (730,15,8,'Și Templul&lt;f&gt;ⓞ&lt;/f&gt; s-a umplut de fum din pricina&lt;f&gt;ⓟ&lt;/f&gt; slavei lui Dumnezeu și a puterii Lui. Și nimeni nu putea să intre în Templu până nu aveau să se sfârșească cele șapte urgii ale celor șapte îngeri.');
INSERT INTO &quot;verses&quot; VALUES (730,16,1,'&lt;pb/&gt;Și am auzit un glas tare, care venea din Templu și care le spunea celor&lt;f&gt;ⓠ&lt;/f&gt; șapte îngeri: „Duceți-vă și vărsați pe pământ cele șapte cupe ale mâniei&lt;f&gt;ⓡ&lt;/f&gt; lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (730,16,2,'&lt;pb/&gt;Primul s-a dus și a vărsat cupa lui pe&lt;f&gt;ⓢ&lt;/f&gt; pământ. Și o rană rea și dureroasă &lt;i&gt;a apărut&lt;/i&gt;&lt;f&gt;ⓣ&lt;/f&gt; pe oamenii care&lt;f&gt;ⓤ&lt;/f&gt; aveau semnul fiarei și care&lt;f&gt;ⓥ&lt;/f&gt; se închinau chipului ei.');
INSERT INTO &quot;verses&quot; VALUES (730,16,3,'&lt;pb/&gt;Al doilea a vărsat cupa lui în&lt;f&gt;ⓦ&lt;/f&gt; mare. Și marea s-a&lt;f&gt;ⓧ&lt;/f&gt; făcut sânge ca de mort. Și&lt;f&gt;ⓨ&lt;/f&gt; orice făptură vie care era în mare a murit.');
INSERT INTO &quot;verses&quot; VALUES (730,16,4,'&lt;pb/&gt;Al treilea a vărsat cupa lui în&lt;f&gt;ⓩ&lt;/f&gt; râuri și în izvoarele apelor. Și ele s-au&lt;f&gt;ⓐ&lt;/f&gt; făcut sânge.');
INSERT INTO &quot;verses&quot; VALUES (730,16,5,'Și l-am auzit pe îngerul apelor zicând: &lt;pb/&gt;&lt;t&gt;„Drept&lt;f&gt;ⓑ&lt;/f&gt; ești Tu, care&lt;f&gt;ⓒ&lt;/f&gt; ești și care erai, Cel Sfânt,&lt;/t&gt; &lt;t&gt;pentru că ai judecat aceste lucruri.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,16,6,'&lt;t&gt;Fiindcă ei au&lt;f&gt;ⓓ&lt;/f&gt; vărsat sângele sfinților&lt;f&gt;ⓔ&lt;/f&gt; și al profeților,&lt;/t&gt; &lt;t&gt;tot sânge&lt;f&gt;ⓕ&lt;/f&gt; le-ai dat și Tu să bea.&lt;/t&gt; &lt;t&gt;Așa și merită!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,16,7,'&lt;pb/&gt;Și am auzit altarul zicând: &lt;pb/&gt;&lt;t&gt;„Da, Doamne&lt;f&gt;ⓖ&lt;/f&gt; Dumnezeule atotputernic,&lt;/t&gt; &lt;t&gt;adevărate&lt;f&gt;ⓗ&lt;/f&gt; și drepte sunt judecățile Tale!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,16,8,'&lt;pb/&gt;Al patrulea a vărsat cupa lui peste&lt;f&gt;ⓘ&lt;/f&gt; soare. Și&lt;f&gt;ⓙ&lt;/f&gt; &lt;i&gt;soarelui&lt;/i&gt; i s-a dat să-i dogorească pe oameni cu focul &lt;i&gt;lui&lt;/i&gt;.');
INSERT INTO &quot;verses&quot; VALUES (730,16,9,'Și oamenii au fost dogoriți cu arșiță mare și au&lt;f&gt;ⓚ&lt;/f&gt; hulit Numele lui Dumnezeu, care are stăpânire peste aceste urgii, și&lt;f&gt;ⓛ&lt;/f&gt; nu s-au pocăit ca să-I&lt;f&gt;ⓜ&lt;/f&gt; dea slavă.');
INSERT INTO &quot;verses&quot; VALUES (730,16,10,'&lt;pb/&gt;Al cincilea a vărsat cupa lui peste&lt;f&gt;ⓝ&lt;/f&gt; tronul fiarei. Și&lt;f&gt;ⓞ&lt;/f&gt; împărăția fiarei a fost cuprinsă de întuneric. Oamenii își&lt;f&gt;ⓟ&lt;/f&gt; mușcau limbile de durere.');
INSERT INTO &quot;verses&quot; VALUES (730,16,11,'Și L-au&lt;f&gt;ⓠ&lt;/f&gt; hulit pe Dumnezeul cerului din pricina durerilor lor și din pricina rănilor&lt;f&gt;ⓡ&lt;/f&gt; lor rele și&lt;f&gt;ⓢ&lt;/f&gt; nu s-au pocăit de faptele lor.');
INSERT INTO &quot;verses&quot; VALUES (730,16,12,'&lt;pb/&gt;Al șaselea a vărsat cupa lui peste&lt;f&gt;ⓣ&lt;/f&gt; râul cel mare, Eufrat. Și&lt;f&gt;ⓤ&lt;/f&gt; apa lui a secat, ca&lt;f&gt;ⓥ&lt;/f&gt; să fie pregătită calea împăraților de la răsăritul soarelui.');
INSERT INTO &quot;verses&quot; VALUES (730,16,13,'Apoi am văzut &lt;i&gt;ieșind&lt;/i&gt; din gura balaurului și din gura fiarei și din gura profetului mincinos&lt;f&gt;ⓦ&lt;/f&gt; trei duhuri necurate&lt;f&gt;ⓧ&lt;/f&gt;, ca niște broaște.');
INSERT INTO &quot;verses&quot; VALUES (730,16,14,'Acestea sunt&lt;f&gt;ⓨ&lt;/f&gt; duhuri de demoni, care fac&lt;f&gt;ⓩ&lt;/f&gt; semne nemaipomenite și care se duc la împărații din întreaga lume ca să-i strângă pentru războiul&lt;f&gt;ⓐ&lt;/f&gt; din ziua cea mare a Dumnezeului celui atotputernic.');
INSERT INTO &quot;verses&quot; VALUES (730,16,15,'&lt;J&gt;„Iată&lt;/J&gt;&lt;f&gt;ⓑ&lt;/f&gt;&lt;J&gt;, Eu vin ca un hoț. Ferice de cel ce veghează și își păzește hainele ca&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt; &lt;J&gt;să nu umble gol și să i se vadă rușinea!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,16,16,'&lt;i&gt;Duhurile cele rele&lt;/i&gt;&lt;f&gt;ⓓ&lt;/f&gt; i-au strâns în locul care pe evreiește se cheamă Armaghedon.');
INSERT INTO &quot;verses&quot; VALUES (730,16,17,'&lt;pb/&gt;Al șaptelea a vărsat cupa lui în văzduh. Și din Templu, de la tron, a ieșit un glas tare, care zicea: „S-a sfârșit!”');
INSERT INTO &quot;verses&quot; VALUES (730,16,18,'Și au urmat&lt;f&gt;ⓔ&lt;/f&gt; fulgere, glasuri, tunete și s-a&lt;f&gt;ⓕ&lt;/f&gt; făcut un așa de mare cutremur, că&lt;f&gt;ⓖ&lt;/f&gt; de când este omul pe pământ n-a fost un cutremur așa de mare.');
INSERT INTO &quot;verses&quot; VALUES (730,16,19,'Cetatea&lt;f&gt;ⓗ&lt;/f&gt; cea mare s-a împărțit în trei și cetățile neamurilor s-au prăbușit. Și Babilonul cel mare a&lt;f&gt;ⓘ&lt;/f&gt; fost amintit înaintea lui Dumnezeu, ca să-i&lt;f&gt;ⓙ&lt;/f&gt; dea potirul de vin al mâniei Lui aprinse&lt;f&gt;[40]&lt;/f&gt;.');
INSERT INTO &quot;verses&quot; VALUES (730,16,20,'Toate&lt;f&gt;ⓚ&lt;/f&gt; insulele au fugit și munții n-au mai fost de găsit.');
INSERT INTO &quot;verses&quot; VALUES (730,16,21,'Grindină&lt;f&gt;ⓛ&lt;/f&gt; mare cât un talant a căzut din cer peste oameni. Și oamenii L-au hulit pe Dumnezeu din pricina urgiei&lt;f&gt;ⓜ&lt;/f&gt; grindinei, pentru că această urgie era foarte mare.');
INSERT INTO &quot;verses&quot; VALUES (730,17,1,'&lt;pb/&gt;Apoi unul&lt;f&gt;ⓝ&lt;/f&gt; din cei șapte îngeri care țineau cele șapte cupe a venit și a vorbit cu mine, zicând: „Vino! Îți&lt;f&gt;ⓞ&lt;/f&gt; voi arăta judecata desfrânatei&lt;f&gt;ⓟ&lt;/f&gt; celei mari, care&lt;f&gt;ⓠ&lt;/f&gt; stă pe ape multe.');
INSERT INTO &quot;verses&quot; VALUES (730,17,2,'Cu&lt;f&gt;ⓡ&lt;/f&gt; ea s-au desfrânat împărații pământului, iar locuitorii pământului s-au&lt;f&gt;ⓢ&lt;/f&gt; îmbătat de vinul desfrânării ei!”');
INSERT INTO &quot;verses&quot; VALUES (730,17,3,'Și m-a dus în Duhul într-un&lt;f&gt;ⓣ&lt;/f&gt; pustiu. &lt;pb/&gt;Și am văzut o femeie stând pe&lt;f&gt;ⓤ&lt;/f&gt; o fiară de culoare stacojie, plină cu nume&lt;f&gt;ⓥ&lt;/f&gt; de hulă și având șapte capete și zece coarne.');
INSERT INTO &quot;verses&quot; VALUES (730,17,4,'Femeia era&lt;f&gt;ⓦ&lt;/f&gt; îmbrăcată în purpură și stacojiu și era împodobită cu aur, pietre scumpe și mărgăritare. Ținea&lt;f&gt;ⓧ&lt;/f&gt; în mână un potir de aur plin&lt;f&gt;ⓨ&lt;/f&gt; de spurcăciuni și de necurățiile desfrâului ei.');
INSERT INTO &quot;verses&quot; VALUES (730,17,5,'Pe frunte purta scris un nume, o taină&lt;f&gt;ⓩ&lt;/f&gt;: „Babilonul cel&lt;f&gt;ⓐ&lt;/f&gt; mare, mama&lt;f&gt;ⓑ&lt;/f&gt; desfrânatelor și a spurcăciunilor pământului”.');
INSERT INTO &quot;verses&quot; VALUES (730,17,6,'Și am văzut-o pe&lt;f&gt;ⓒ&lt;/f&gt; femeie îmbătată de&lt;f&gt;ⓓ&lt;/f&gt; sângele sfinților și de sângele martirilor&lt;f&gt;ⓔ&lt;/f&gt; lui Isus. Când am văzut-o, m-am mirat minune mare.');
INSERT INTO &quot;verses&quot; VALUES (730,17,7,'Și îngerul mi-a zis: „De ce te miri? Îți voi spune eu taina femeii și a fiarei care o poartă &lt;i&gt;în spinare&lt;/i&gt; și care are cele șapte capete și cele zece coarne.');
INSERT INTO &quot;verses&quot; VALUES (730,17,8,'&lt;pb/&gt;Fiara pe care ai văzut-o era și nu &lt;i&gt;mai&lt;/i&gt; este. Ea se va&lt;f&gt;ⓕ&lt;/f&gt; ridica din Adânc și se&lt;f&gt;ⓖ&lt;/f&gt; va duce la pierzare. Și locuitorii pământului ale&lt;f&gt;ⓗ&lt;/f&gt; căror nume n-au fost scrise de la întemeierea lumii în cartea vieții se&lt;f&gt;ⓘ&lt;/f&gt; vor mira când vor vedea că fiara era, nu &lt;i&gt;mai&lt;/i&gt; este și va veni.');
INSERT INTO &quot;verses&quot; VALUES (730,17,9,'Aici&lt;f&gt;ⓙ&lt;/f&gt; este mintea care are înțelepciune: cele&lt;f&gt;ⓚ&lt;/f&gt; șapte capete sunt șapte munți pe care stă femeia.');
INSERT INTO &quot;verses&quot; VALUES (730,17,10,'Sunt și șapte împărați: cinci au căzut, unul este, celălalt n-a venit încă, dar, când va veni, trebuie să rămână puțină vreme.');
INSERT INTO &quot;verses&quot; VALUES (730,17,11,'&lt;i&gt;Cât despre&lt;/i&gt; fiara care era și nu mai este, ea este al optulea: este dintre cei șapte și merge la pierzare.');
INSERT INTO &quot;verses&quot; VALUES (730,17,12,'Cele&lt;f&gt;ⓛ&lt;/f&gt; zece coarne pe care le-ai văzut sunt zece împărați care n-au primit încă împărăția, ci vor primi stăpânire, ca împărați, timp de un ceas împreună cu fiara.');
INSERT INTO &quot;verses&quot; VALUES (730,17,13,'Toți au același gând și îi dau fiarei puterea și stăpânirea lor.');
INSERT INTO &quot;verses&quot; VALUES (730,17,14,'Ei&lt;f&gt;ⓜ&lt;/f&gt; se vor război cu Mielul, dar Mielul îi va birui, pentru că&lt;f&gt;ⓝ&lt;/f&gt; El este Domnul domnilor și Împăratul împăraților. Și&lt;f&gt;ⓞ&lt;/f&gt; cu El vor fi cei chemați, aleși și credincioși.”');
INSERT INTO &quot;verses&quot; VALUES (730,17,15,'&lt;pb/&gt;Apoi mi-a zis: „Apele&lt;f&gt;ⓟ&lt;/f&gt; pe care le-ai văzut, pe care stă desfrânata, sunt&lt;f&gt;ⓠ&lt;/f&gt; popoare, mulțimi, neamuri și limbi.');
INSERT INTO &quot;verses&quot; VALUES (730,17,16,'Cele zece coarne pe care le-ai văzut și fiara, tocmai aceștia,&lt;f&gt;[41]&lt;/f&gt; o vor&lt;f&gt;ⓡ&lt;/f&gt; urî pe desfrânată, o vor pustii și o vor lăsa goală&lt;f&gt;ⓢ&lt;/f&gt;. Carnea i-o vor mânca, și pe ea o&lt;f&gt;ⓣ&lt;/f&gt; vor arde în foc.');
INSERT INTO &quot;verses&quot; VALUES (730,17,17,'Căci&lt;f&gt;ⓤ&lt;/f&gt; Dumnezeu le-a pus în inimă să-I aducă la îndeplinire planul: să fie într-un singur gând și să-i dea fiarei împărăția până&lt;f&gt;ⓥ&lt;/f&gt; se vor împlini cuvintele lui Dumnezeu.');
INSERT INTO &quot;verses&quot; VALUES (730,17,18,'Și femeia pe care ai văzut-o este&lt;f&gt;ⓦ&lt;/f&gt; cetatea cea mare, care&lt;f&gt;ⓧ&lt;/f&gt; domnește peste împărații pământului.”');
INSERT INTO &quot;verses&quot; VALUES (730,18,1,'&lt;pb/&gt;După&lt;f&gt;ⓨ&lt;/f&gt; aceea, am văzut coborându-se din cer un alt înger, care avea o mare putere; și&lt;f&gt;ⓩ&lt;/f&gt; pământul s-a luminat de slava lui.');
INSERT INTO &quot;verses&quot; VALUES (730,18,2,'El a strigat cu glas tare și a zis: &lt;pb/&gt;&lt;t&gt;„A&lt;f&gt;ⓐ&lt;/f&gt; căzut, a căzut Babilonul cel mare!&lt;/t&gt; &lt;t&gt;A&lt;f&gt;ⓑ&lt;/f&gt; ajuns locuință a demonilor,&lt;/t&gt; &lt;t&gt;închisoare a oricărui duh necurat,&lt;/t&gt; &lt;t&gt;închisoare&lt;f&gt;ⓒ&lt;/f&gt; a oricărei păsări necurate,&lt;/t&gt; &lt;t&gt;închisoare a oricărei fiare necurate și respingătoare,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,3,'&lt;t&gt;pentru că din vinul mâniei desfrâului ei&lt;/t&gt; &lt;t&gt;au băut toate neamurile&lt;f&gt;ⓓ&lt;/f&gt;&lt;/t&gt; &lt;t&gt;și împărații pământului s-au desfrânat cu ea&lt;/t&gt; &lt;t&gt;și negustorii&lt;f&gt;ⓔ&lt;/f&gt; pământului prin risipa desfătării ei s-au îmbogățit.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,4,'&lt;pb/&gt;Apoi am auzit din cer un alt glas, care zicea: &lt;pb/&gt;&lt;t&gt;„Ieșiți&lt;f&gt;ⓕ&lt;/f&gt; din mijlocul ei, poporul Meu,&lt;/t&gt; &lt;t&gt;ca să nu fiți părtași la păcatele ei&lt;/t&gt; &lt;t&gt;și să nu fiți loviți cu urgiile ei!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,5,'&lt;t&gt;Căci&lt;f&gt;ⓖ&lt;/f&gt; până la cer s-au îngrămădit păcatele ei&lt;/t&gt; &lt;t&gt;și Dumnezeu&lt;f&gt;ⓗ&lt;/f&gt; Și-a adus aminte de nelegiuirile ei.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,6,'&lt;t&gt;Răsplătiți-i&lt;f&gt;ⓘ&lt;/f&gt; cum v-a răsplătit și ea!&lt;/t&gt; &lt;t&gt;Dați-i de două ori pentru faptele ei!&lt;/t&gt; &lt;t&gt;În potirul amestecat&lt;f&gt;ⓙ&lt;/f&gt; de ea turnați îndoit&lt;f&gt;ⓚ&lt;/f&gt;!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,7,'&lt;t&gt;Pe cât&lt;f&gt;ⓛ&lt;/f&gt; s-a slăvit și s-a desfătat,&lt;/t&gt; &lt;t&gt;pe atât dați-i chin și tânguire!&lt;/t&gt; &lt;t&gt;Fiindcă zice în inima ei:&lt;/t&gt; &lt;t&gt;«Sunt împărăteasă&lt;f&gt;ⓜ&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;nu sunt văduvă,&lt;/t&gt; &lt;t&gt;nu voi cunoaște nicidecum tânguirea.»&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,8,'&lt;t&gt;De aceea, într-o&lt;f&gt;ⓝ&lt;/f&gt; singură zi vor veni urgiile ei:&lt;/t&gt; &lt;t&gt;molima, tânguirea și foametea.&lt;/t&gt; &lt;t&gt;Și va fi mistuită de&lt;f&gt;ⓞ&lt;/f&gt; foc,&lt;/t&gt; &lt;t&gt;pentru că&lt;f&gt;ⓟ&lt;/f&gt; Domnul Dumnezeu, care a judecat-o, este tare.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,9,'&lt;pb/&gt;Vor&lt;f&gt;ⓠ&lt;/f&gt; plânge și vor boci după ea împărații&lt;f&gt;ⓡ&lt;/f&gt; pământului, care s-au desfrânat și s-au desfătat cu ea, când&lt;f&gt;ⓢ&lt;/f&gt; vor vedea fumul arderii ei.');
INSERT INTO &quot;verses&quot; VALUES (730,18,10,'Vor sta departe, de teama chinului ei, și vor zice: &lt;pb/&gt;&lt;t&gt;«Vai!&lt;f&gt;ⓣ&lt;/f&gt; vai! cetatea cea mare,&lt;/t&gt; &lt;t&gt;Babilon, cetatea cea tare!&lt;/t&gt; &lt;t&gt;Într-un&lt;f&gt;ⓤ&lt;/f&gt; ceas ți-a venit judecata!»&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,11,'&lt;pb/&gt;Negustorii&lt;f&gt;ⓥ&lt;/f&gt; pământului plâng și jelesc după ea, pentru că nimeni nu le mai cumpără marfa:');
INSERT INTO &quot;verses&quot; VALUES (730,18,12,'marfă&lt;f&gt;ⓦ&lt;/f&gt; de aur, argint, pietre scumpe, mărgăritare, in subțire, purpură, mătase și &lt;i&gt;țesături&lt;/i&gt; stacojii, felurite soiuri de lemn parfumat, felurite vase de fildeș, de lemn scump, de aramă, de fier și de marmură,');
INSERT INTO &quot;verses&quot; VALUES (730,18,13,'scorțișoară, mirodenii, ierburi aromate, mir, tămâie, vin, ulei, floarea făinii, grâu, vite, oi, cai, căruțe, trupuri și suflete&lt;f&gt;ⓧ&lt;/f&gt; de oameni.');
INSERT INTO &quot;verses&quot; VALUES (730,18,14,'&lt;pb/&gt;&lt;t&gt;Și roadele râvnite de sufletul tău&lt;/t&gt; &lt;t&gt;s-au dus de la tine.&lt;/t&gt; &lt;t&gt;Toate lucrurile alese, strălucite,&lt;/t&gt; &lt;t&gt;au pierit de la tine&lt;/t&gt; &lt;t&gt;și nicidecum nu le vei mai găsi.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,15,'&lt;pb/&gt;Cei ce fac negoț&lt;f&gt;ⓨ&lt;/f&gt; cu aceste lucruri, care s-au îmbogățit de pe urma ei, vor sta departe, de teama chinului ei. Vor plânge, se vor tângui');
INSERT INTO &quot;verses&quot; VALUES (730,18,16,'și vor zice: &lt;pb/&gt;&lt;t&gt;«Vai! vai! Cetatea cea mare,&lt;/t&gt; &lt;t&gt;care&lt;f&gt;ⓩ&lt;/f&gt; era îmbrăcată în in subțire,&lt;/t&gt; &lt;t&gt;în purpură și stacojiu,&lt;/t&gt; &lt;t&gt;care era împodobită cu aur,&lt;/t&gt; &lt;t&gt;pietre scumpe și mărgăritare!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,17,'&lt;t&gt;Într-un&lt;f&gt;ⓐ&lt;/f&gt; ceas s-au prăpădit atâtea bogății!»&lt;/t&gt; &lt;pb/&gt;Și&lt;f&gt;ⓑ&lt;/f&gt; toți cârmacii, toți cei ce merg cu corabia în &lt;i&gt;vreun&lt;/i&gt; loc, corăbieri și toți cei ce lucrează pe mare stăteau departe');
INSERT INTO &quot;verses&quot; VALUES (730,18,18,'și&lt;f&gt;ⓒ&lt;/f&gt;, când au văzut fumul arderii ei, au început să strige: «Cine&lt;f&gt;ⓓ&lt;/f&gt; mai este asemenea cetății celei mari?»');
INSERT INTO &quot;verses&quot; VALUES (730,18,19,'Și își aruncau&lt;f&gt;ⓔ&lt;/f&gt; țărână în cap, plângeau, se tânguiau, țipau și ziceau: &lt;pb/&gt;&lt;t&gt;«Vai! vai! Cetatea cea mare!&lt;/t&gt; &lt;t&gt;Toți cei ce aveau corăbii pe mare s-au îmbogățit&lt;/t&gt; &lt;t&gt;în urma belșugului ei,&lt;/t&gt; &lt;t&gt;dar într-un ceas&lt;f&gt;ⓕ&lt;/f&gt; a fost pustiită!»&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,20,'&lt;t&gt;Bucură-te de ea, cerule&lt;f&gt;ⓖ&lt;/f&gt;,&lt;/t&gt; &lt;t&gt;și &lt;i&gt;voi&lt;/i&gt;, sfinților, apostolilor și profeților,&lt;/t&gt; &lt;t&gt;pentru că&lt;f&gt;ⓗ&lt;/f&gt; Dumnezeu a judecat-o și v-a făcut dreptate!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,21,'&lt;pb/&gt;Atunci, un înger puternic a ridicat o piatră ca o piatră uriașă de moară, a aruncat-o în mare și a zis: &lt;pb/&gt;&lt;t&gt;„Cu&lt;f&gt;ⓘ&lt;/f&gt; așa repeziciune va fi aruncat&lt;/t&gt; &lt;t&gt;Babilonul, cetatea cea mare,&lt;/t&gt; &lt;t&gt;și nu&lt;f&gt;ⓙ&lt;/f&gt; va mai fi găsit!&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,22,'&lt;t&gt;Sunetul&lt;f&gt;ⓚ&lt;/f&gt; celor ce cântă din harfă,&lt;/t&gt; &lt;t&gt;sunetul muzicanților,&lt;/t&gt; &lt;t&gt;al celor ce cântă din fluiere și trâmbițe,&lt;/t&gt; &lt;t&gt;nu se va mai auzi în tine.&lt;/t&gt; &lt;t&gt;Niciun meșter în vreun meșteșug oarecare&lt;/t&gt; &lt;t&gt;nu se va mai găsi la tine,&lt;/t&gt; &lt;t&gt;și uruitul morii&lt;/t&gt; &lt;t&gt;nu se va mai auzi în tine.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,23,'&lt;t&gt;Lumina&lt;f&gt;ⓛ&lt;/f&gt; lămpii&lt;/t&gt; &lt;t&gt;nu va mai lumina în tine&lt;/t&gt; &lt;t&gt;și glasul mirelui&lt;f&gt;ⓜ&lt;/f&gt; și al miresei&lt;/t&gt; &lt;t&gt;nu se va mai auzi în tine,&lt;/t&gt; &lt;t&gt;pentru că negustorii&lt;f&gt;ⓝ&lt;/f&gt; tăi erau mai-marii pământului,&lt;/t&gt; &lt;t&gt;pentru că&lt;f&gt;ⓞ&lt;/f&gt; prin vrăjitoria ta&lt;/t&gt; &lt;t&gt;au fost amăgite toate neamurile.”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,18,24,'&lt;t&gt;Și pentru că&lt;f&gt;ⓟ&lt;/f&gt; în ea a fost găsit sângele profeților și al sfinților&lt;/t&gt; &lt;t&gt;și al tuturor celor ce au fost înjunghiați&lt;f&gt;ⓠ&lt;/f&gt; pe pământ.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,1,'&lt;pb/&gt;După aceea, am auzit&lt;f&gt;ⓡ&lt;/f&gt; ca un glas puternic, al unei mari mulțimi, în cer, care zicea: &lt;pb/&gt;&lt;t&gt;„Aleluia!&lt;/t&gt; &lt;t&gt;Mântuirea&lt;f&gt;ⓢ&lt;/f&gt;, slava și puterea&lt;/t&gt; &lt;t&gt;sunt ale Domnului Dumnezeului nostru,&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,2,'&lt;t&gt;pentru că adevărate&lt;f&gt;ⓣ&lt;/f&gt; și drepte sunt judecățile Lui:&lt;/t&gt; &lt;t&gt;El a judecat-o pe desfrânata cea mare,&lt;/t&gt; &lt;t&gt;care strica pământul cu desfrâul ei,&lt;/t&gt; &lt;t&gt;și a răzbunat&lt;f&gt;ⓤ&lt;/f&gt; sângele robilor Săi&lt;/t&gt; &lt;t&gt;din mâna ei!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,3,'&lt;pb/&gt;Și au zis a doua oară: &lt;pb/&gt;&lt;t&gt;„Aleluia!&lt;/t&gt; &lt;t&gt;Fumul&lt;f&gt;ⓥ&lt;/f&gt; ei se înalță în vecii vecilor!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,4,'&lt;pb/&gt;Și cei&lt;f&gt;ⓦ&lt;/f&gt; douăzeci și patru de bătrâni și cele patru făpturi vii s-au aruncat &lt;i&gt;la pământ&lt;/i&gt; și I s-au închinat lui Dumnezeu, care stătea pe tron. Și au zis: &lt;pb/&gt;&lt;t&gt;„Amin&lt;f&gt;ⓧ&lt;/f&gt;! Aleluia!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,5,'&lt;pb/&gt;Și dinspre tron a ieșit un glas, care zicea: &lt;pb/&gt;&lt;t&gt;„Lăudați-L&lt;f&gt;ⓨ&lt;/f&gt; pe Dumnezeul nostru,&lt;/t&gt; &lt;t&gt;toți robii Lui,&lt;/t&gt; &lt;t&gt;cei ce vă temeți de El,&lt;/t&gt; &lt;t&gt;mici și&lt;f&gt;ⓩ&lt;/f&gt; mari!”&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,6,'&lt;pb/&gt;Și&lt;f&gt;ⓐ&lt;/f&gt; am auzit ca un glas al unei mulțimi mari, ca vuietul multor ape, ca bubuitul unor tunete puternice, care zicea: &lt;pb/&gt;&lt;t&gt;„Aleluia!&lt;/t&gt; &lt;t&gt;Domnul&lt;f&gt;ⓑ&lt;/f&gt;, Dumnezeul nostru cel atotputernic,&lt;/t&gt; &lt;t&gt;a început să împărățească.&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,7,'&lt;t&gt;Să ne bucurăm, să ne veselim&lt;/t&gt; &lt;t&gt;și să-I dăm slavă!&lt;/t&gt; &lt;t&gt;Căci a&lt;f&gt;ⓒ&lt;/f&gt; venit nunta Mielului&lt;/t&gt; &lt;t&gt;și mireasa Lui s-a pregătit:&lt;/t&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,19,8,'&lt;t&gt;i s-a dat&lt;f&gt;ⓓ&lt;/f&gt; să se îmbrace&lt;/t&gt; &lt;t&gt;cu in subțire, strălucitor și curat.”&lt;/t&gt; &lt;pb/&gt;(Inul&lt;f&gt;ⓔ&lt;/f&gt; subțire sunt faptele drepte ale sfinților.)');
INSERT INTO &quot;verses&quot; VALUES (730,19,9,'&lt;pb/&gt;Apoi mi-a zis: „Scrie: «Ferice&lt;f&gt;ⓕ&lt;/f&gt; de cei chemați la ospățul nunții Mielului!»” Apoi mi-a zis: „Acestea&lt;f&gt;ⓖ&lt;/f&gt; sunt cuvintele adevărate ale lui Dumnezeu!”');
INSERT INTO &quot;verses&quot; VALUES (730,19,10,'Și m-am&lt;f&gt;ⓗ&lt;/f&gt; aruncat la picioarele lui ca să mă închin lui, dar el mi-a zis: „Nicidecum&lt;f&gt;ⓘ&lt;/f&gt;! Eu sunt un slujitor împreună cu tine și cu frații tăi, care&lt;f&gt;ⓙ&lt;/f&gt; păstrează mărturia lui Isus. Lui Dumnezeu să I te închini!” (Căci mărturia lui Isus este duhul profeției.)');
INSERT INTO &quot;verses&quot; VALUES (730,19,11,'&lt;pb/&gt;Apoi&lt;f&gt;ⓚ&lt;/f&gt; am văzut cerul deschis și iată că &lt;i&gt;s-a arătat&lt;/i&gt; un&lt;f&gt;ⓛ&lt;/f&gt; cal alb. Cel ce stătea pe el este numit credincios&lt;f&gt;ⓜ&lt;/f&gt; și adevărat; El judecă&lt;f&gt;ⓝ&lt;/f&gt; și Se luptă cu dreptate.');
INSERT INTO &quot;verses&quot; VALUES (730,19,12,'Ochii&lt;f&gt;ⓞ&lt;/f&gt; Lui erau ca para focului; pe cap&lt;f&gt;ⓟ&lt;/f&gt; purta multe diademe, având&lt;f&gt;ⓠ&lt;/f&gt; scris un nume pe care nu-l știe nimeni, ci numai El.');
INSERT INTO &quot;verses&quot; VALUES (730,19,13,'Era&lt;f&gt;ⓡ&lt;/f&gt; îmbrăcat cu o haină înmuiată în sânge. Și numele Lui este „Cuvântul&lt;f&gt;ⓢ&lt;/f&gt; lui Dumnezeu”.');
INSERT INTO &quot;verses&quot; VALUES (730,19,14,'&lt;pb/&gt;Oștirile&lt;f&gt;ⓣ&lt;/f&gt; din cer Îl urmau călări pe cai albi, îmbrăcate&lt;f&gt;ⓤ&lt;/f&gt; cu in subțire, alb și curat.');
INSERT INTO &quot;verses&quot; VALUES (730,19,15,'Din gura&lt;f&gt;ⓥ&lt;/f&gt; Lui ieșea o sabie ascuțită, pentru a lovi cu ea neamurile, pe care&lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;le va păstori cu un toiag de fier&lt;/e&gt;&lt;f&gt;[42]&lt;/f&gt;. Și&lt;f&gt;ⓧ&lt;/f&gt; va călca în picioare teascul vinului mâniei aprinse a Dumnezeului celui atotputernic.');
INSERT INTO &quot;verses&quot; VALUES (730,19,16,'Pe haină și pe coapsă avea scris numele acesta: „Împăratul&lt;f&gt;ⓨ&lt;/f&gt; împăraților și Domnul domnilor”.');
INSERT INTO &quot;verses&quot; VALUES (730,19,17,'&lt;pb/&gt;Apoi am văzut un înger care stătea în soare. El a strigat cu glas puternic și a zis tuturor păsărilor care zburau în înaltul cerului: „Veniți&lt;f&gt;ⓩ&lt;/f&gt;, adunați-vă la ospățul cel mare al lui Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (730,19,18,'ca&lt;f&gt;ⓐ&lt;/f&gt; să mâncați carnea împăraților, carnea căpeteniilor, carnea celor puternici, carnea cailor și a călăreților și carnea &lt;i&gt;oamenilor&lt;/i&gt; de tot felul: liberi și robi, mici și mari!”');
INSERT INTO &quot;verses&quot; VALUES (730,19,19,'Și&lt;f&gt;ⓑ&lt;/f&gt; am văzut fiara și pe împărații pământului și oștirile lor adunate ca să facă război cu Cel ce era călare pe cal și cu oastea Lui.');
INSERT INTO &quot;verses&quot; VALUES (730,19,20,'Și&lt;f&gt;ⓒ&lt;/f&gt; fiara a fost prinsă. Și, împreună cu ea, a fost prins profetul mincinos, care făcuse în numele ei semnele cu care îi amăgise pe cei ce primiseră semnul fiarei și&lt;f&gt;ⓓ&lt;/f&gt; se închinaseră chipului ei. Aceștia&lt;f&gt;ⓔ&lt;/f&gt; doi au fost aruncați de vii în iazul de foc, care&lt;f&gt;ⓕ&lt;/f&gt; arde cu pucioasă.');
INSERT INTO &quot;verses&quot; VALUES (730,19,21,'Iar ceilalți au fost uciși cu sabia care ieșea din gura Celui ce era călare pe cal. Și toate păsările s-au săturat din carnea&lt;f&gt;ⓖ&lt;/f&gt; lor.');
INSERT INTO &quot;verses&quot; VALUES (730,20,1,'&lt;pb/&gt;Apoi am văzut un înger care se cobora din cer și care&lt;f&gt;ⓗ&lt;/f&gt; avea în mână cheia Adâncului și un lanț mare.');
INSERT INTO &quot;verses&quot; VALUES (730,20,2,'El a înșfăcat balaurul&lt;f&gt;ⓘ&lt;/f&gt;, șarpele cel vechi, care este Diavolul și Satana, și l-a legat pentru o mie de ani.');
INSERT INTO &quot;verses&quot; VALUES (730,20,3,'L-a aruncat în Adânc, l-a închis acolo și a pecetluit&lt;f&gt;ⓙ&lt;/f&gt; &lt;i&gt;intrarea&lt;/i&gt; deasupra lui, ca&lt;f&gt;ⓚ&lt;/f&gt; să nu mai înșele neamurile, până se vor împlini cei o mie de ani. După aceea, trebuie să fie dezlegat pentru puțină vreme.');
INSERT INTO &quot;verses&quot; VALUES (730,20,4,'&lt;pb/&gt;Și am văzut niște tronuri&lt;f&gt;ⓛ&lt;/f&gt;; și celor ce s-au așezat pe ele li s-a dat judecata&lt;f&gt;ⓜ&lt;/f&gt;. Și &lt;i&gt;am văzut&lt;/i&gt; sufletele&lt;f&gt;ⓝ&lt;/f&gt; celor ce li se tăiase capul din pricina mărturiei lui Isus și din pricina Cuvântului lui Dumnezeu și ale celor ce&lt;f&gt;ⓞ&lt;/f&gt; nu se închinaseră fiarei și chipului ei&lt;f&gt;ⓟ&lt;/f&gt; și nu primiseră semnul ei pe frunte și pe mână. Ei au înviat și au&lt;f&gt;ⓠ&lt;/f&gt; împărățit cu Hristos o mie de ani.');
INSERT INTO &quot;verses&quot; VALUES (730,20,5,'Ceilalți morți n-au înviat până nu s-au sfârșit cei o mie de ani. &lt;pb/&gt;Aceasta este prima înviere.');
INSERT INTO &quot;verses&quot; VALUES (730,20,6,'Fericiți și sfinți sunt cei ce au parte de prima înviere! Asupra lor a doua moarte&lt;f&gt;ⓡ&lt;/f&gt; n-are nicio putere; și ei vor fi preoți&lt;f&gt;ⓢ&lt;/f&gt; ai lui Dumnezeu și ai lui Hristos și vor împărăți cu El o mie de ani.');
INSERT INTO &quot;verses&quot; VALUES (730,20,7,'&lt;pb/&gt;Când se vor împlini cei o mie de ani, Satana va fi dezlegat din temnița lui');
INSERT INTO &quot;verses&quot; VALUES (730,20,8,'și va ieși ca să înșele neamurile care sunt în cele patru colțuri ale pământului, pe Gog&lt;f&gt;ⓣ&lt;/f&gt; și pe Magog, ca să-i&lt;f&gt;ⓤ&lt;/f&gt; adune pentru război. Numărul lor va fi ca nisipul mării.');
INSERT INTO &quot;verses&quot; VALUES (730,20,9,'Și&lt;f&gt;ⓥ&lt;/f&gt; ei s-au suit pe fața pământului și au înconjurat tabăra sfinților și cetatea preaiubită. &lt;e&gt;Dar din cer s-a coborât foc și i-a mistuit.&lt;/e&gt;&lt;f&gt;[43]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,20,10,'Și Diavolul, care-i înșela, a fost aruncat în iazul de foc și de pucioasă, unde&lt;f&gt;ⓦ&lt;/f&gt; erau fiara și profetul mincinos. Și vor fi chinuiți&lt;f&gt;ⓧ&lt;/f&gt; zi și noapte în vecii vecilor.');
INSERT INTO &quot;verses&quot; VALUES (730,20,11,'&lt;pb/&gt;Apoi am văzut un tron mare și alb și pe Cel ce stătea pe el. Pământul și cerul au&lt;f&gt;ⓨ&lt;/f&gt; fugit dinaintea Lui și&lt;f&gt;ⓩ&lt;/f&gt; nu și-au mai găsit locul.');
INSERT INTO &quot;verses&quot; VALUES (730,20,12,'Și i-am văzut pe morți, mari&lt;f&gt;ⓐ&lt;/f&gt; și mici, stând în picioare înaintea tronului. Niște&lt;f&gt;ⓑ&lt;/f&gt; cărți au fost deschise. Și a fost deschisă o altă&lt;f&gt;ⓒ&lt;/f&gt; carte, care este &lt;i&gt;cartea&lt;/i&gt; vieții. Și morții au fost judecați potrivit celor scrise în cărțile acelea, după&lt;f&gt;ⓓ&lt;/f&gt; faptele lor.');
INSERT INTO &quot;verses&quot; VALUES (730,20,13,'Marea i-a dat înapoi pe morții care erau în ea; Moartea&lt;f&gt;ⓔ&lt;/f&gt; și Locuința Morților i-au dat înapoi pe morții care erau în ele; și au fost judecați: fiecare după faptele lui.');
INSERT INTO &quot;verses&quot; VALUES (730,20,14,'Apoi Moartea&lt;f&gt;ⓕ&lt;/f&gt; și Locuința Morților au fost aruncate în iazul de foc. Aceasta&lt;f&gt;ⓖ&lt;/f&gt; este moartea a doua — iazul de foc.');
INSERT INTO &quot;verses&quot; VALUES (730,20,15,'Oricine n-a fost găsit scris în cartea vieții a fost aruncat&lt;f&gt;ⓗ&lt;/f&gt; în iazul de foc.');
INSERT INTO &quot;verses&quot; VALUES (730,21,1,'&lt;pb/&gt;Apoi am&lt;f&gt;ⓘ&lt;/f&gt; văzut un cer nou și un pământ nou; pentru că&lt;f&gt;ⓙ&lt;/f&gt; cerul dintâi și pământul dintâi trecuseră, și marea nu mai era.');
INSERT INTO &quot;verses&quot; VALUES (730,21,2,'Și eu am văzut&lt;f&gt;ⓚ&lt;/f&gt; coborându-se din cer, de la Dumnezeu, cetatea sfântă, Noul Ierusalim, gătită&lt;f&gt;ⓛ&lt;/f&gt; ca o mireasă împodobită pentru bărbatul ei.');
INSERT INTO &quot;verses&quot; VALUES (730,21,3,'Și am auzit un glas tare, &lt;i&gt;care venea&lt;/i&gt; dinspre tron și zicea: &lt;pb/&gt;„Iată, cortul&lt;f&gt;ⓜ&lt;/f&gt; lui Dumnezeu &lt;i&gt;este&lt;/i&gt; cu oamenii! El va locui cu ei și ei vor fi poporul Lui, și Dumnezeu Însuși va fi cu ei. El va fi Dumnezeul lor.');
INSERT INTO &quot;verses&quot; VALUES (730,21,4,'&lt;e&gt;El&lt;/e&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;e&gt;va șterge orice lacrimă din ochii&lt;/e&gt;&lt;f&gt;[44]&lt;/f&gt; lor. Și&lt;f&gt;ⓞ&lt;/f&gt; moartea nu va mai fi. Nu va mai fi nici&lt;f&gt;ⓟ&lt;/f&gt; tânguire, nici țipăt, nici durere, pentru că lucrurile dintâi au trecut.”');
INSERT INTO &quot;verses&quot; VALUES (730,21,5,'&lt;pb/&gt;Cel ce&lt;f&gt;ⓠ&lt;/f&gt; stătea pe tron a zis: &lt;J&gt;„Iată&lt;/J&gt;&lt;f&gt;ⓡ&lt;/f&gt;&lt;J&gt;, Eu fac toate lucrurile noi.”&lt;/J&gt; Și a adăugat: &lt;J&gt;„Scrie: «Aceste&lt;/J&gt;&lt;f&gt;ⓢ&lt;/f&gt; &lt;J&gt;cuvinte sunt vrednice de crezut și adevărate.»”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,21,6,'Apoi mi-a zis: &lt;J&gt;„S-a&lt;/J&gt;&lt;f&gt;ⓣ&lt;/f&gt; &lt;J&gt;sfârșit! Eu&lt;/J&gt;&lt;f&gt;ⓤ&lt;/f&gt; &lt;J&gt;sunt Alfa și Omega, Începutul și Sfârșitul. Celui căruia îi este sete îi voi&lt;/J&gt;&lt;f&gt;ⓥ&lt;/f&gt; &lt;J&gt;da să bea fără plată din izvorul apei vieții.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,21,7,'&lt;J&gt;Cel ce va birui va moșteni aceste lucruri. &lt;e&gt;Eu&lt;/J&gt;&lt;/e&gt;&lt;f&gt;ⓦ&lt;/f&gt; &lt;e&gt;&lt;J&gt;voi fi Dumnezeul lui și el va fi fiul Meu.&lt;/J&gt;&lt;/e&gt;&lt;f&gt;[45]&lt;/f&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,21,8,'&lt;J&gt;Cât&lt;/J&gt;&lt;f&gt;ⓧ&lt;/f&gt; &lt;J&gt;despre lași, necredincioși, scârboși, ucigași, desfrânați, vrăjitori, idolatri și toți mincinoșii, partea lor este în iazul&lt;/J&gt;&lt;f&gt;ⓨ&lt;/f&gt; &lt;J&gt;care arde cu foc și cu pucioasă, adică moartea a doua.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,21,9,'&lt;pb/&gt;Apoi unul din cei&lt;f&gt;ⓩ&lt;/f&gt; șapte îngeri care țineau cele șapte cupe pline cu cele din urmă șapte urgii a venit și a vorbit cu mine, zicând: „Vino să-ți arăt mireasa&lt;f&gt;ⓐ&lt;/f&gt;, soția Mielului!”');
INSERT INTO &quot;verses&quot; VALUES (730,21,10,'Și m-a dus în&lt;f&gt;ⓑ&lt;/f&gt; Duhul pe un munte mare și înalt. Și mi-a arătat cetatea&lt;f&gt;ⓒ&lt;/f&gt; sfântă, Ierusalimul, care se cobora din cer, de la Dumnezeu,');
INSERT INTO &quot;verses&quot; VALUES (730,21,11,'având&lt;f&gt;ⓓ&lt;/f&gt; slava lui Dumnezeu. Strălucirea ei era ca o piatră foarte scumpă, ca o piatră de iaspis cristalin.');
INSERT INTO &quot;verses&quot; VALUES (730,21,12,'Era înconjurată cu un zid mare și înalt, avea douăsprezece&lt;f&gt;ⓔ&lt;/f&gt; porți, și la porți — doisprezece îngeri. Și &lt;i&gt;pe porți&lt;/i&gt; erau scrise niște nume: numele celor douăsprezece seminții ale fiilor lui Israel.');
INSERT INTO &quot;verses&quot; VALUES (730,21,13,'Spre&lt;f&gt;ⓕ&lt;/f&gt; răsărit erau trei porți, spre miazănoapte — trei porți, spre miazăzi — trei porți și spre apus — trei porți.');
INSERT INTO &quot;verses&quot; VALUES (730,21,14,'Zidul cetății avea douăsprezece temelii și pe ele&lt;f&gt;ⓖ&lt;/f&gt; erau cele douăsprezece nume ale celor doisprezece apostoli ai Mielului.');
INSERT INTO &quot;verses&quot; VALUES (730,21,15,'&lt;pb/&gt;&lt;i&gt;Îngerul&lt;/i&gt; care vorbea cu mine avea&lt;f&gt;ⓗ&lt;/f&gt; ca măsurătoare o trestie de aur, ca să măsoare cetatea, porțile și zidul ei.');
INSERT INTO &quot;verses&quot; VALUES (730,21,16,'Cetatea avea formă pătrată și lungimea ei era cât lățimea. A măsurat cetatea cu trestia: douăsprezece mii de stadii&lt;f&gt;[46]&lt;/f&gt;. Lungimea, lărgimea și înălțimea erau egale.');
INSERT INTO &quot;verses&quot; VALUES (730,21,17,'I-a măsurat și zidul și a găsit o sută patruzeci și patru de coți, după măsura oamenilor, &lt;i&gt;folosită&lt;/i&gt; și de înger.');
INSERT INTO &quot;verses&quot; VALUES (730,21,18,'Zidul era făcut din iaspis și cetatea era de aur curat, ca sticla curată.');
INSERT INTO &quot;verses&quot; VALUES (730,21,19,'Temeliile&lt;f&gt;ⓘ&lt;/f&gt; zidului cetății erau împodobite cu pietre scumpe de tot felul: prima temelie era de iaspis; a doua, de safir; a treia, de calcedonie; a patra, de smarald;');
INSERT INTO &quot;verses&quot; VALUES (730,21,20,'a cincea, de sardonix; a șasea, de sardiu; a șaptea, de crisolit; a opta, de beril; a noua, de topaz; a zecea, de crisopraz; a unsprezecea, de hiacint; a douăsprezecea, de ametist.');
INSERT INTO &quot;verses&quot; VALUES (730,21,21,'Cele douăsprezece porți erau douăsprezece mărgăritare. Fiecare poartă era dintr-un singur mărgăritar. Strada&lt;f&gt;ⓙ&lt;/f&gt; cetății&lt;f&gt;[47]&lt;/f&gt; era de aur curat, ca sticla străvezie.');
INSERT INTO &quot;verses&quot; VALUES (730,21,22,'&lt;pb/&gt;Templu&lt;f&gt;ⓚ&lt;/f&gt; n-am văzut în &lt;i&gt;cetate&lt;/i&gt;, pentru că Domnul Dumnezeul atotputernic și Mielul sunt Templul ei.');
INSERT INTO &quot;verses&quot; VALUES (730,21,23,'Cetatea&lt;f&gt;ⓛ&lt;/f&gt; n-are nevoie nici de soare, nici de lună care să-i dea lumină, căci o luminează slava lui Dumnezeu, și făclia ei este Mielul.');
INSERT INTO &quot;verses&quot; VALUES (730,21,24,'Neamurile&lt;f&gt;ⓜ&lt;/f&gt; vor umbla în lumina ei și împărații pământului își vor aduce slava lor în ea.');
INSERT INTO &quot;verses&quot; VALUES (730,21,25,'Porțile&lt;f&gt;ⓝ&lt;/f&gt; ei nu se vor închide ziua, fiindcă acolo nu va mai fi&lt;f&gt;ⓞ&lt;/f&gt; noapte.');
INSERT INTO &quot;verses&quot; VALUES (730,21,26,'În ea vor aduce slava și cinstea neamurilor.');
INSERT INTO &quot;verses&quot; VALUES (730,21,27,'Nimic întinat nu&lt;f&gt;ⓟ&lt;/f&gt; va intra în ea, nimeni care trăiește în spurcăciune și în minciună, ci numai cei scriși în cartea&lt;f&gt;ⓠ&lt;/f&gt; vieții Mielului.');
INSERT INTO &quot;verses&quot; VALUES (730,22,1,'&lt;pb/&gt;Și mi-a arătat râul&lt;f&gt;ⓡ&lt;/f&gt; apei vieții, limpede ca cristalul, care curgea de la tronul lui Dumnezeu și al Mielului');
INSERT INTO &quot;verses&quot; VALUES (730,22,2,'prin&lt;f&gt;ⓢ&lt;/f&gt; mijlocul cetății; iar pe cele două maluri ale râului era pomul&lt;f&gt;[48]&lt;/f&gt; vieții&lt;f&gt;ⓣ&lt;/f&gt;, rodind douăsprezece feluri de rod și dând rod în fiecare lună; și frunzele pomului slujesc la&lt;f&gt;ⓤ&lt;/f&gt; vindecarea popoarelor.');
INSERT INTO &quot;verses&quot; VALUES (730,22,3,'Nu&lt;f&gt;ⓥ&lt;/f&gt; va mai fi nimic supus blestemului. Tronul lui Dumnezeu și al Mielului va fi în ea, și robii Lui Îi vor sluji.');
INSERT INTO &quot;verses&quot; VALUES (730,22,4,'Ei vor&lt;f&gt;ⓦ&lt;/f&gt; vedea fața Lui și&lt;f&gt;ⓧ&lt;/f&gt; Numele Lui va fi pe frunțile lor.');
INSERT INTO &quot;verses&quot; VALUES (730,22,5,'Și noapte nu va mai fi&lt;f&gt;ⓨ&lt;/f&gt;; nu vor mai avea nevoie nici de lumina lămpii, nici de lumina soarelui, pentru că Domnul Dumnezeu va lumina peste ei. Și&lt;f&gt;ⓩ&lt;/f&gt; vor împărăți în vecii vecilor.');
INSERT INTO &quot;verses&quot; VALUES (730,22,6,'&lt;pb/&gt;Și &lt;i&gt;îngerul&lt;/i&gt; mi-a zis: „Aceste&lt;f&gt;ⓐ&lt;/f&gt; cuvinte sunt vrednice de crezare și adevărate. Și Domnul, Dumnezeul duhurilor profeților, l-a&lt;f&gt;ⓑ&lt;/f&gt; trimis pe îngerul Său să le arate robilor Săi ceea ce trebuie să se întâmple în curând.”');
INSERT INTO &quot;verses&quot; VALUES (730,22,7,'&lt;J&gt;„Și, iată&lt;/J&gt;&lt;f&gt;ⓒ&lt;/f&gt;&lt;J&gt;, Eu vin curând! Ferice&lt;/J&gt;&lt;f&gt;ⓓ&lt;/f&gt; &lt;J&gt;de cine păzește cuvintele profeției din cartea aceasta!”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,22,8,'&lt;pb/&gt;Eu, Ioan, am auzit și am văzut &lt;i&gt;lucrurile&lt;/i&gt; acestea. Și când le-am auzit și le-am văzut, am&lt;f&gt;ⓔ&lt;/f&gt; căzut la picioarele îngerului care mi le arăta, ca să mă închin lui.');
INSERT INTO &quot;verses&quot; VALUES (730,22,9,'Dar el mi-a zis: „Nicidecum&lt;f&gt;ⓕ&lt;/f&gt;! Eu sunt un slujitor împreună cu tine și cu frații tăi, profeții, și cu cei ce păzesc cuvintele din cartea aceasta. Lui Dumnezeu să I te închini!”');
INSERT INTO &quot;verses&quot; VALUES (730,22,10,'Apoi&lt;f&gt;ⓖ&lt;/f&gt; mi-a zis: „Să nu pecetluiești cuvintele profeției din cartea&lt;f&gt;ⓗ&lt;/f&gt; aceasta, căci vremea este aproape!');
INSERT INTO &quot;verses&quot; VALUES (730,22,11,'Cine&lt;f&gt;ⓘ&lt;/f&gt; este nedrept să fie nedrept și mai departe, cine este întinat să se întineze și mai departe, cine este drept să trăiască și mai departe în dreptate, iar cine este sfânt să se sfințească și mai departe!”');
INSERT INTO &quot;verses&quot; VALUES (730,22,12,'&lt;pb/&gt;&lt;J&gt;„Iată, Eu vin curând, și răsplata&lt;/J&gt;&lt;f&gt;ⓙ&lt;/f&gt; &lt;J&gt;Mea este cu Mine, ca să&lt;/J&gt;&lt;f&gt;ⓚ&lt;/f&gt; &lt;J&gt;dau fiecăruia după fapta lui!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,22,13,'&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓛ&lt;/f&gt; &lt;J&gt;sunt Alfa și Omega, Cel-dintâi și Cel-de-pe-urmă, Începutul și Sfârșitul.&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,22,14,'&lt;pb/&gt;&lt;J&gt;Ferice de cei ce își spală hainele, ca să aibă drept la&lt;/J&gt;&lt;f&gt;ⓜ&lt;/f&gt; &lt;J&gt;pomul vieții și&lt;/J&gt;&lt;f&gt;ⓝ&lt;/f&gt; &lt;J&gt;să intre pe porți în cetate!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,22,15,'&lt;J&gt;Afară&lt;/J&gt;&lt;f&gt;ⓞ&lt;/f&gt; &lt;J&gt;sunt câinii, vrăjitorii, desfrânații, ucigașii, idolatrii și oricine iubește minciuna și trăiește în minciună!&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,22,16,'&lt;pb/&gt;&lt;J&gt;Eu&lt;/J&gt;&lt;f&gt;ⓟ&lt;/f&gt; &lt;J&gt;, Isus, l-am trimis pe îngerul Meu să vă adeverească aceste lucruri pentru biserici. Eu sunt Rădăcina și Sămânța lui David, Luceafărul&lt;/J&gt;&lt;f&gt;ⓠ&lt;/f&gt; &lt;J&gt;strălucitor de dimineață.”&lt;/J&gt;');
INSERT INTO &quot;verses&quot; VALUES (730,22,17,'&lt;pb/&gt;Și Duhul și Mireasa&lt;f&gt;ⓡ&lt;/f&gt; zic: „Vino!” Și cine aude să zică: „Vino!” Și&lt;f&gt;ⓢ&lt;/f&gt; cel căruia îi este sete să vină; cine vrea, să ia apa vieții fără plată!');
INSERT INTO &quot;verses&quot; VALUES (730,22,18,'&lt;pb/&gt;O spun clar oricui aude cuvintele profeției din cartea aceasta că, dacă&lt;f&gt;ⓣ&lt;/f&gt; va adăuga cineva &lt;i&gt;ceva&lt;/i&gt; la ele, îi va adăuga &lt;i&gt;și&lt;/i&gt; Dumnezeu urgiile descrise în cartea&lt;f&gt;ⓤ&lt;/f&gt; aceasta.');
INSERT INTO &quot;verses&quot; VALUES (730,22,19,'Și, dacă va scoate cineva ceva din cuvintele cărții acestei profeții, îi va scoate &lt;i&gt;și&lt;/i&gt; Dumnezeu partea lui de la pomul vieții și din cetatea sfântă, descrise în cartea aceasta.');
INSERT INTO &quot;verses&quot; VALUES (730,22,20,'&lt;pb/&gt;Cel ce mărturisește aceste lucruri zice: &lt;J&gt;„Da, Eu vin curând.”&lt;/J&gt; Amin!&lt;f&gt;ⓥ&lt;/f&gt; Vino&lt;f&gt;ⓦ&lt;/f&gt;, Doamne Isuse!');
INSERT INTO &quot;verses&quot; VALUES (730,22,21,'&lt;pb/&gt;Harul&lt;f&gt;ⓧ&lt;/f&gt; Domnului Isus [Hristos] să fie cu toți! [Amin!]');
INSERT INTO &quot;introductions&quot; VALUES (0,'&lt;p&gt;Prezenta ediție a Noului Testament reprezintă o revizuire a textului din Biblia tradusă de Dumitru Cornilescu și publicată în 1924 de British and Foreign Bible Society / Societatea Biblică pentru Răspândirea Bibliei în Anglia și Străinătate.&lt;/p&gt; &lt;p&gt;Istoria Bibliei traduse de Dumitru Cornilescu (1891–1975) se împletește strâns cu istoria din ultimul secol a bisericilor neoprotestante românești. Publicată în 1921 și revizuită în perioada 1923–1924, la cererea și sub îndrumarea directă a Societății Biblice Britanice (SBB), versiunea Cornilescu s-a impus relativ repede în conștiința cititorilor români, fiind folosită, în perioada comunistă, inclusiv de protestanții istorici ori de catolici și ortodocși.&lt;/p&gt; &lt;p&gt;Deși în general SBB nu a adoptat versiuni realizate de un singur traducător, pentru spațiul românesc a făcut o excepție datorită primirii entuziaste de care a avut parte versiunea lui Dumitru Cornilescu în primii ani de la tipărirea și difuzarea ei. Tot în această perioadă, conducerea executivă a SBB a avut în intenție o revizuire atentă a textului. Acest proces ar fi urmat să se realizeze prin compararea minuțioasă a textului românesc cu originalele ebraic și aramaic (pentru VT), respectiv grec (pentru NT). Din cauza bolii care l-a afectat o vreme, Dumitru Cornilescu, stabilit între timp în Elveția, nu a mai reușit să înceapă laborioasa activitate planificată de SBB, astfel că ideea a căzut în uitare. Problema revizuirii a revenit în actualitate după evenimentele prilejuite de a 90-a aniversare a Bibliei Cornilescu, în 2014.&lt;/p&gt; &lt;p&gt;În primăvara anului 2015, Societatea Biblică Interconfesională din România (SBIR) a făcut demersurile necesare constituirii unui Comitet de Revizuire, alcătuit din specialiști desemnați de confesiunile neoprotestante din România (Biserica Adventistă, Biserica Baptistă, Biserica Penticostală). În echipa de revizori au fost desemnați următorii cercetători: Emanuel Conțac, Laurențiu Ionescu, Florin Lăiu, Laurențiu Moț, Gelu Păcurar, Dragoș Ștefănică, Silviu Tatu, Ciprian Terinte, Cristinel Sava (revizor literar).&lt;/p&gt; &lt;p&gt;În aceeași perioadă s-a cristalizat și Comitetul Pastoral al revizuirii, alcătuit din patru pastori aparținând principalelor confesiuni neoprotestante: Ioan Brie (Biserica Penticostală), Corneliu Boingeanu (Biserica Baptistă), Ieremia Rusu (Biserica Creștină după Evanghelie), Iacob Pop (Biserica Adventistă).&lt;/p&gt; &lt;p&gt;În vederea unei verificări și diortosiri atente, fiecărei cărți din Scriptură i-au fost alocați doi revizori care să parcurgă succesiv textul: sarcina celui dintâi a fost de a se asigura că traducerea se aliniază fidel la original; rolul celui de al doilea revizor a fost de a verifica munca făcută de colegul său. Eventualele divergențe survenite în urma acestei prime etape au fost aduse pe masa Comitetului de Revizuire, soluțiile fiind adoptate prin consens, în urma unor discuții ample, sau, în mod excepțional, prin supunere la vot. Îmbunătățiri de ordin stilistic au fost introduse de revizorul literar, care s-a asigurat că textul corespunde așteptărilor cititorului educat. După această etapă, textul a fost trimis spre lectură Comitetului Pastoral, ale cărui sugestii au fost analizate în ședințe de lucru separate.&lt;/p&gt;');
